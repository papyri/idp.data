    <lb n="3" break="no"/><supplied reason="lost">των </supplied><gap reason="lost" quantity="9" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="1" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="2" unit="character"/><gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" quantity="5" unit="character"/><gap reason="illegible" quantity="1" unit="character"/> <app type="editorial"><lem resp="BL 1.308"><supplied reason="lost">ἀρχισωματο</supplied>φύλακ<supplied reason="lost">ο</supplied>ς καὶ δι<supplied reason="lost">ὰ</supplied></lem><rdg><gap reason="lost" quantity="10" unit="character"/> <unclear>φ</unclear>υ<gap reason="illegible" quantity="1" unit="character"/><unclear>λ</unclear><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="1" unit="character"/><unclear>ς</unclear> καὶ <gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" quantity="1" unit="character"/></rdg></app> 

