    <lb n="4" type="inWord"/>βας πεντακοσίας <num value="500"/> καὶ κριθῆς καθαρᾶς 

    <lb n="5"/>ἀρτάβας τριακοσίας εἴσοσι <num value="320"/>, <expan><ex>γίνονται</ex></expan> <expan>σί<ex>του</ex></expan> <expan><ex>ἀρτάβαι</ex></expan> <num value="500">φ</num>, <expan>κριθ<ex>ῆς</ex></expan> <expan><ex>ἀρτάβαι</ex></expan> <num value="320">τκ</num> 

