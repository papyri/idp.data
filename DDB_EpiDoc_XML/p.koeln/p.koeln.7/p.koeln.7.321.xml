         <head n="38726" xml:lang="en">
            <date>VI/VII spc</date>
            <placeName>Herakleopolite</placeName>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><ab>
    <lb n="1"/><gap reason="lost" extent="unknown" unit="line"/> 

    <lb n="1"/><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="45" unit="character"/>ωμ<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="15" unit="character"/> 

    <lb n="2"/>ε<unclear>τ</unclear><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="40" unit="character"/><gap reason="illegible" quantity="3" unit="character"/> ἐν κ<unclear>αὶ</unclear> Ἡρα<unclear>κλ</unclear><supplied reason="lost">έους πόλει </supplied><gap reason="lost" quantity="7" unit="character"/> 

    <lb n="3"/><g type="stauros"/> ε<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="40" unit="character"/><gap reason="illegible" quantity="3" unit="character"/>ημ<gap reason="illegible" quantity="2" unit="character"/><unclear>ι</unclear><gap reason="illegible" quantity="3" unit="character"/><gap reason="lost" quantity="14" unit="character"/> 

    <lb n="4"/>πρὸς ἀλλήλους ἑκουσίᾳ γ<supplied reason="lost">νώμῃ </supplied><gap reason="lost" quantity="20" unit="character"/>τω<unclear>με</unclear><gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" quantity="17" unit="character"/> 

    <lb n="5"/>υἱο<unclear>ῦ</unclear> <gap reason="lost" quantity="4" unit="character"/> ἀπὸ κώμης Πεενπεβί<supplied reason="lost">χεως τοῦ Ἡρ</supplied><unclear>ακλεοπο</unclear><supplied reason="lost">λίτου νομοῦ </supplied><gap reason="lost" quantity="3" unit="character"/><unclear>ε</unclear>ι μετ<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="15" unit="character"/> 

    <lb n="6"/>ἀπὸ τῆς <supplied reason="lost">αὐ</supplied>τῆς κώμης <expan><ex>καὶ</ex></expan> ἔστιν <gap reason="lost" quantity="2" unit="character"/> ἑξῆς ὑ<unclear>π</unclear>οδ<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="3" unit="character"/><supplied reason="lost"> ἐ</supplied>πείπερ τοῦ προγεγραμ<supplied reason="lost">μένου </supplied><gap reason="lost" quantity="14" unit="character"/> 

    <lb n="7"/><gap reason="lost" quantity="2" unit="character"/><gap reason="illegible" quantity="3" unit="character"/>ιαν τίθεσθαι καὶ ἐνθεῖναι τ<unclear>ο</unclear><gap reason="lost" quantity="1" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="1" unit="character"/>μ<gap reason="illegible" quantity="4" unit="character"/> <unclear>εἰς</unclear> <unclear>τὸ</unclear> <unclear>μ</unclear>έσον <expan>αὐτ<ex>ῶν</ex></expan>, ἐκ τούτου π<unclear>α</unclear>ραλαβ<supplied reason="lost">εῖν </supplied><gap reason="lost" quantity="11" unit="character"/> 

    <lb n="8"/>ἐν τῷ <choice><reg>οἴκῳ</reg><orig>οἴκου</orig></choice> <expan>αὐτ<ex>οῦ</ex></expan> μετὰ τῶν καταλειφθέντων <expan>αὐτ<ex>ῷ</ex></expan> οἱωνδήποτε πραγμάτ<unclear>ω</unclear><supplied reason="lost">ν κινη</supplied><unclear>τῶν</unclear> 

    <lb n="9"/>ἀκινήτων αὐτοκ<supplied reason="lost">ινήτ</supplied><unclear>ων</unclear> <gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="1" unit="character"/><unclear>κ</unclear>ο<gap reason="illegible" quantity="3" unit="character"/> <supplied reason="lost">ἀ</supplied>ρ<unclear>ο</unclear>υ<unclear>ρ</unclear><supplied reason="lost">ῶν</supplied> σπορίμων <expan>γῃδ<ex>ίων</ex></expan> καὶ κτην<supplied reason="lost">ῶν </supplied><gap reason="lost" quantity="10" unit="character"/> 

    <lb n="10"/>καὶ <choice><reg>ἐπαύλεων</reg><orig>ἐπαύλαιων</orig></choice> καὶ ἄλλων διακειμένων ἔν <choice><reg>τε</reg><orig>δε</orig></choice> <choice><reg>τῷ</reg><orig>δῷ</orig></choice> ἀγρῷ <expan>α<supplied reason="lost">ὐ</supplied>τ<ex>οῦ</ex></expan> κ<unclear>αὶ</unclear> ἐν τ<supplied reason="lost">ῇ κώμῃ </supplied><gap reason="lost" quantity="8" unit="character"/> 

    <lb n="11"/>καὶ διʼ ἣν ἐπείσθησαν γενέσθαι ἀμφότεροι εἰς <expan>ἑαυτ<ex>οὺς</ex></expan> τὴν παροῦσαν ἀντισύγγ<supplied reason="lost">ραφον ὁ</supplied>μολογίαν 

    <lb n="12"/><g type="stauros"/> τῆς υἱοθεσίας <g type="stauros"/> διʼ ἧς ὁμολογοῦσιν ὁ μὲν <choice><reg>εἰρημένος</reg><orig>εἰρημενον</orig></choice> <choice><reg>Σέργιος</reg><orig>Σέργιο<unclear>ν</unclear></orig></choice> τὸν προγεγραμμέ<unclear>ν</unclear><supplied reason="lost">ον </supplied><gap reason="lost" quantity="5" unit="character"/><unclear>ν</unclear> <abbr>αὐ<unclear>τ</unclear></abbr> ιγν<gap reason="illegible" quantity="2" unit="character"/>ο<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="13"/>ἴδιον καὶ <choice><reg>πρωτότοκον</reg><orig>προτότοκον</orig></choice> <expan>αὐτ<ex>οῦ</ex></expan> υἱὸν τὴν πᾶσαν τῶν ἀκινήτων <expan>αὐτ<ex>οῦ</ex></expan> πραγμάτων ἐργα<supplied reason="lost">σίαν ἐκτε</supplied>λέσαι <supplied reason="lost">καὶ</supplied> 

    <lb n="14"/>τὴν ἅπασαν τῶν ἁπάντων <expan>αὐτ<ex>οῦ</ex></expan> πραγμάτων συντέλειαν <choice><reg>δεόντως</reg><orig>δεοντων</orig></choice> κ<supplied reason="lost">αὶ ἀκαταφ</supplied>ρονήτ<supplied reason="lost">ως</supplied> 

    <lb n="15"/>καὶ <subst><add place="inline">ἀκαταγνώστως</add><del rend="corrected">ακαταγνω<del rend="erasure">ω</del>στως</del></subst>· καὶ τὸ <choice><reg>περισσευόμενον</reg><orig>περισευόμενον</orig></choice> <expan>αὐτ<ex>οῖς</ex></expan> <choice><reg>ἐτησίως</reg><orig>ἐντησίως</orig></choice> ὁμοθυμ<unclear>α</unclear><supplied reason="lost">δὸν </supplied><choice><reg><supplied reason="lost">δαπ</supplied><unclear>α</unclear>νήσου<unclear>σ</unclear><supplied reason="lost">ιν</supplied></reg><orig><supplied reason="lost">δαπ</supplied><unclear>α</unclear>νήσω<unclear>σ</unclear><supplied reason="lost">ιν</supplied></orig></choice>, 

    <lb n="16"/>ἐπείπερ εἰς μίαν <num value="1"/> <choice><reg>ὁμοζωΐαν</reg><orig>ὁμοζωειαν</orig></choice> <choice><reg>ἀποζήσουσιν</reg><orig>ἀποζωήσουσιν</orig></choice> μετʼ <choice><reg>ἀλλήλων</reg><orig>ἀταλλ<unclear>η</unclear>λους</orig></choice>. εἰ δὲ <unclear>α</unclear><gap reason="lost" quantity="7" unit="character"/><unclear>σ</unclear>εις 

    <lb n="17"/>γένηται εἰς τὸ μέσον <expan>αὐτ<ex>ῶν</ex></expan>, ὥστε τὸ <choice><reg><expan>περισσευόμε<ex>νον</ex></expan></reg><orig><expan>περισευόμε<ex>νον</ex></expan></orig></choice> αὐτοῖς διελεῖν εἰς ἑαυτοὺς <supplied reason="lost">κατʼ ἴσην</supplied> <choice><reg>ἡμίσειαν</reg><orig>ἡ<supplied reason="lost">μ</supplied><unclear>ί</unclear>σιαν</orig></choice>, 

    <lb n="18"/><choice><reg>τὸ</reg><orig>τον</orig></choice> εὑρισκόμενον αὐτοῖς ἔκτοτε κέρ<unclear>δ</unclear>ος <choice><reg>τε</reg><orig>δε</orig></choice> καὶ ὅπερ ἀπείη <choice><reg cert="low">μεί<unclear>ω</unclear><supplied reason="lost" cert="low">σιν</supplied></reg><orig>μί<unclear>ω</unclear><supplied reason="lost">σιν</supplied></orig></choice>. <supplied reason="lost"><expan>ἐπερω<ex>τη</ex>θ<ex>εὶς</ex></expan> ὡμολο</supplied>

    <lb n="19" break="no"/>γησα <expan><ex>καὶ</ex></expan> ἀπέλυσα. 

    <lb n="19,ms"/><g type="stauros"/> </ab></div>
