    <lb n="12"/>δραχμὰς <num value="288"><choice><reg>διακοσίας</reg><orig>διακουσίας</orig></choice> <choice><reg>ὀγδοήκοντα</reg><orig>ὀκτωήκοντα</orig></choice> 

    <lb n="13"/><choice><reg>ὀκτὼ</reg><orig>ὀκτὼι</orig></choice></num> καὶ <choice><reg>σπονδῆς</reg><orig>σπο<supplied reason="lost">ν</supplied><unclear>τ</unclear>ῆς</orig></choice> Φαμενὼθ οἴνου κεράμια 

    <lb n="14"/><num value="2"><choice><reg>δύω</reg><orig>δύωι</orig></choice></num>, τῶν τ<unclear>ε</unclear> <choice><reg>προσδιαγράψω</reg><orig>προσδιαγραψο</orig></choice> κατὰ μῆνα 

    <lb n="15"/><choice><reg>ἐν μηνὶ</reg><orig>ἐμ μῆνα</orig></choice> τοῦ <choice><reg>αὐτοῦ</reg><orig>α<unclear>ὐδ</unclear>οῦ</orig></choice> <expan><ex>ἔτους</ex></expan> ἀεὶ τῇ πέμπτῃ καὶ 
<choice><reg>εἰ<lb n="16" break="no"/>κάδι</reg><orig>εἰ<lb n="16" break="no"/>κάτη</orig></choice> καὶ <choice><reg>καταχωρίσω</reg><orig>καταχορίζω</orig></choice> <choice><reg>σοι</reg><orig>σου</orig></choice> διὰ <choice><reg>τετραμή

    <lb n="17" break="no"/>νου</reg><orig>τετραμη<lb n="17" break="no"/>να</orig></choice> <choice><reg>πάντας</reg><orig>πάντος</orig></choice> <choice><reg>τοὺς</reg><orig>του</orig></choice> διʼ ἐμοῦ οἰκονομηθησο

    <lb n="18" break="no"/>μένους χρηματισμοὺς ἐν <choice><reg>τόμῳ</reg><orig>τόμου</orig></choice> <choice><reg>συγκολ

    <lb n="19" break="no"/>λησίμῳ</reg><orig>συνκολ<lb n="18" break="no"/>λοσιμου</orig></choice> καὶ <choice><reg>εἰρομένῳ</reg><orig>εἰρομενι</orig></choice> <num value="1">ἑνὶ</num> καὶ ἀναγρα

    <lb n="20" break="no"/>φῇ <num value="1">μιᾷ</num> καὶ <choice><reg>δώσω</reg><orig>δοσο</orig></choice> <choice><reg>σοι</reg><orig>σου</orig></choice> <choice><reg>καταχωρισμὸν</reg><orig>καταχορισμὸν</orig></choice> 

    <lb n="21"/><choice><reg>βιβλίων</reg><orig>βυβλίων</orig></choice> δραχμὰς <choice><reg>ὀκτὼ</reg><orig>ὀκτὼι</orig></choice> <num value="8"/> καὶ <choice><reg>δώσω</reg><orig>δοσο</orig></choice> <choice><reg>σοι</reg><orig>σου</orig></choice> 

    <lb n="22"/><choice><reg>ἱκανὸν</reg><orig>εἱκανὸν</orig></choice> <choice><reg>ἀξιόχρεον</reg><orig>ἀξιωχρον</orig></choice>, ἐὰν <choice><reg>φαίνεται</reg><orig>φεναι</orig></choice> <choice><reg>ἐπιχωρῆσ<supplied reason="omitted">αι</supplied></reg><orig>ἐπιχορης</orig></choice> 

    <lb n="23"/>ἐπὶ <choice><reg>τοῖς</reg><orig>το͂ς</orig></choice> <choice><reg>προκειμένοις</reg><orig>προκειμένος</orig></choice> πᾶσι. εὐτύχει. 

