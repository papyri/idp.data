         <head n="21375" xml:lang="en">
            <date>VIIspc</date>
            <placeName>Aphrodito, Aphroditopolis</placeName>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><ab>
    <lb n="1"/><supplied reason="lost"><g type="stauros"/> ἐν</supplied> ὀνόματι τῆς ἁγίας καὶ ζῳοποιοῦ καὶ ὁμοουσίου τριάδος πατρὸς καὶ υἱοῦ καὶ ἁγίου πνεύματο<supplied reason="lost">ς</supplied> 

    <lb n="2"/><expan>ἐ<unclear>γ</unclear><supplied reason="lost">ρά</supplied><unclear>φ</unclear><ex>η</ex></expan> <expan><unclear>μ</unclear><ex>ηνὶ</ex></expan> <supplied reason="lost">Ἁθ</supplied>ὺρ <num value="3"><unclear>γ</unclear></num> <expan>ἰνδ<ex>ικτίονος</ex></expan> <unclear>τ</unclear>ε<supplied reason="lost">τ</supplied><unclear>άρτ</unclear>η<supplied reason="lost">ς</supplied>. 

    <lb n="3"/><g type="stauros"/> Αὐρηλία Εὐδοξία θυγάτηρ Γεωργίου μ<unclear>η</unclear>τρὸς Ῥοῦθ <gap reason="illegible" quantity="4" unit="character"/> <gap reason="lost" quantity="3" unit="character"/><gap reason="illegible" quantity="2" unit="character"/> καὶ <choice><reg>συναινοῦντο<supplied reason="lost">ς</supplied></reg><orig>συνενουντο<supplied reason="lost">ς</supplied></orig></choice> 

    <lb n="4"/>κ<supplied reason="lost">αὶ συμπειθομ</supplied>ένου ταύτῃ τῇ παρούσῃ ἐμ<supplied reason="lost">ῇ πράσει ἀνδ</supplied>ρὸς <surplus>αὐτῆς</surplus> ἐμ<supplied reason="lost">οῦ Ἀνουφίου υἱοῦ</supplied> 

    <lb n="5"/><supplied reason="lost">Ἰσακίου συντελεστοῦ      ἀπὸ κώμης Ἀφροδίτης τοῦ</supplied> 

    <lb n="6"/>Ἀνταιοπολίτου νομοῦ <g type="stauros"/> Αὐρηλ<supplied reason="lost">ίῳ Μηνᾷ υἱῷ τοῦ</supplied> <expan>μακαριω<supplied reason="lost">τ<ex>άτου</ex></supplied></expan> Χρ<unclear>ιστο</unclear>φ<supplied reason="lost">ό</supplied><unclear>ρ</unclear><supplied reason="lost">ου ὁ</supplied><unclear>ρ</unclear><supplied reason="lost">μωμένου</supplied> 

    <lb n="7"/>ἀπὸ τῆς αὐτῆς κώμης Ἀφροδίτης <expan>χαίρ<ex>ειν</ex></expan>. ὁμολ<supplied reason="lost">ογ</supplied><unclear>ῶ</unclear> <choice><reg>εἰπο<supplied reason="lost">ῦ</supplied>σα</reg><orig>η<unclear>π</unclear><supplied reason="lost">υ</supplied>σα</orig></choice> ἡ <expan>προγεγρ<unclear>α</unclear>μμέ<ex>νη</ex></expan> Εὐδοξία θ<unclear>υγάτηρ</unclear> 

    <lb n="8"/><unclear>Γεωργίου</unclear> καὶ <unclear>μετ</unclear>ὰ <choice><reg>συναινέσεως</reg><orig>συνενησεως</orig></choice> ἀνδρὸς <surplus>α<supplied reason="lost">ὐτ</supplied><unclear>ῆς</unclear></surplus> ἐμοῦ <choice><reg>Ἀνοῦφιος</reg><orig>ανουφις</orig></choice> <choice><reg>υἱοῦ</reg><orig>υιος</orig></choice> <hi rend="diaeresis">Ἰ</hi>σακίου <expan>συντ<ex>ελεστοῦ</ex></expan> διὰ ταύτη<supplied reason="lost">ς</supplied> 

    <lb n="9"/>μου τῆ<unclear>ς</unclear> ἐγγράφου ἀσφαλείας <choice><reg>ἤτοι</reg><orig>ητι</orig></choice> πράσε<supplied reason="lost">ως</supplied> ἑκουσίᾳ <choice><reg>γνώμῃ</reg><orig>γνομην</orig></choice> <choice><reg>αὐθαιρέτῳ</reg><orig>αυθερετω</orig></choice> βουλήσει 

    <lb n="10"/>καὶ ἀμετανοήτῳ καὶ ἀμεταβλ<unclear>ήτῳ</unclear> καὶ ἀμετ<unclear>ατρ</unclear><supplied reason="lost">έπ</supplied><unclear>τῳ</unclear> λογ<unclear>ισμ</unclear>ῷ <choice><reg>καθερωτάτῃ</reg><orig>καθεροτατη</orig></choice> προθέσ<unclear>ει</unclear> 

    <lb n="11"/><unclear>καὶ</unclear> <unclear>καθ</unclear>αρᾷ <choice><reg>συνειδήσει</reg><orig>συνεδυσει</orig></choice> καὶ <choice><reg>ἀδόλῳ</reg><orig>αδωλο</orig></choice> <choice><reg>σκοπῷ</reg><orig>σκοπο</orig></choice> ὀρθῇ τῇ <choice><reg>διανοίᾳ</reg><orig>διανια</orig></choice> ἅμα <choice><reg>τε</reg><orig>τα</orig></choice> καὶ <choice><reg>ὀμνῦσα</reg><orig>ομνυς</orig></choice> <choice><reg>τόν</reg><orig>των</orig></choice> τε 

    <lb n="12"/><choice><reg>παντοκράτορα</reg><orig>παντοκρατωρα</orig></choice> <unclear>θεὸν</unclear> καὶ τὴν νίκην τῆς <choice><reg>βασιλικῆς</reg><orig><unclear>βασι</unclear>λεικης</orig></choice> σωτηρίας πεπρακέναι σ<supplied reason="lost">οι τῷ</supplied> <expan>π<unclear>ρ</unclear><supplied reason="lost">ο</supplied><unclear>ειρη</unclear><supplied reason="lost">μέ<ex>νῳ</ex></supplied></expan> 

    <lb n="13"/>Μηνᾷ υ<hi rend="diaeresis">ἱ</hi>ῷ τοῦ <expan>μακαριωτ<ex>άτου</ex></expan> Χριστοφόρου πραγματευτῇ καὶ <choice><reg>καταγεγραφηκέναι</reg><orig>κατακεγραφηκεναι</orig></choice> σοι καὶ 

    <lb n="14"/>παρακεχωρηκέναι καὶ <choice><reg>ἐκκεχωρηκέναι</reg><orig>εγκηχωρηκεναι</orig></choice> σ<supplied reason="lost">ο</supplied><unclear>ι</unclear> ἀπὸ τοῦ νῦν <choice><reg>ἐπὶ</reg><orig>επει</orig></choice> <choice><reg>τὸν</reg><orig>των</orig></choice> ἀεὶ ἑξῆς ἅπαντα 

    <lb n="15"/>καὶ <choice><reg>διηνεκῆ</reg><orig>διηνεγκη</orig></choice> χρόνον <unclear>π</unclear>ληρεστάτῳ παντὶ <choice><reg>δεσποτείας</reg><orig>δεσποτιας</orig></choice> δικαίῳ καὶ καλῇ <choice><reg>πίστει</reg><orig>πειστι</orig></choice> 

    <lb n="16"/>καὶ <choice><reg>πάσῃ</reg><orig>πασι</orig></choice> ἐξουσίᾳ καὶ <choice><reg>ἐπὶ</reg><orig>επει</orig></choice> αἰωνίᾳ δεσποτικῇ κατοχῇ τὸ ὑπάρχον μοι καὶ περιελθ<unclear>ὸν</unclear> 

    <lb n="17"/>εἰς ἐμὲ ἀπὸ δικαίας κληρονομίας παρὰ τοῦ <expan>μακαριωτ<ex>άτου</ex></expan> μου παρτρὸς ὁλόκληρόν 

    <lb n="18"/>μου μέρος ἀπὸ τῆς παλαιᾶς <choice><reg>σεσαθρ<supplied reason="lost">ω</supplied>μένης</reg><orig>σεσαθρ<supplied reason="lost">ο</supplied><unclear>μεν</unclear>ης</orig></choice> οἰκίας <expan>λεγομέ<ex>νης</ex></expan> <hi rend="diaeresis">Ἰ</hi>ωναθὰν οἰκ<unclear>ο</unclear>δόμου 

    <lb n="19"/><choice><reg>τῆς</reg><orig>του</orig></choice> <expan>διακειμέ<ex>νης</ex></expan> ἐν τῇ βορινῇ <choice><reg>μέρει</reg><orig>μερεσει</orig></choice> τῆς εἰρημένης κώμης <expan>Ἀφροδ<ex>ίτης</ex></expan> <choice><reg>ἐπὶ</reg><orig>επει</orig></choice> ῥύμης 

    <lb n="20"/>τῆς ἁγίας καθολικῆς ἐκκλησ<unclear>ί</unclear><supplied reason="lost">ας      </supplied><expan><supplied reason="lost">ἀν</supplied>εῳγμέ<ex>νης</ex></expan> εἰ<unclear>ς</unclear> <unclear>λίβα</unclear> εἰς τὴν 

    <lb n="21"/>δημοσίαν ῥύμην μετὰ παντὸς <choice><reg>ἀπτώτου</reg><orig>απτητου</orig></choice> δικαίου ἀπʼ ἐδάφους μέχρι <choice><reg>ἀέρος</reg><orig>αερως</orig></choice> ἄνω 

    <lb n="22"/>ἁπαξ<unclear>απλῶς</unclear> <choice><reg>καθὼς</reg><orig><unclear>κα</unclear>θος</orig></choice> <choice><reg>ἐνεμόμην</reg><orig>ενεμωμαι</orig></choice> <choice><reg>τοῦ</reg><orig>ο</orig></choice> <expan>μακαριωτ<ex>άτου</ex></expan> μου πατρὸς ἔτι περιόντος οὐδὲν 

    <lb n="23"/><choice><reg>καταλείψας</reg><orig>καταλυιψας</orig></choice> ἐκ τοῦ <choice><reg>προδιωμολογημένου</reg><orig>πρωτηωμολογιμε</orig></choice> <unclear>ὁ</unclear>λοκλήρου μου <choice><reg>μέρους</reg><orig>μερος</orig></choice> ἀπὸ <choice><reg>τῆς</reg><orig>του</orig></choice> <expan>προκ<ex>ειμένης</ex></expan> παλαιᾶς 

    <lb n="24"/><choice><reg>σεσαθρωμένης</reg><orig>σεσαθρομενης</orig></choice> οἰκίας καὶ ἐξ <choice><reg>οἱουδήποτε</reg><orig>ιουδηποται</orig></choice> δικαίου ἀπʼ ἐδάφ<unclear>ο</unclear>υς μέχρι <choice><reg>ἀ

    <lb n="25" type="inWord"/>έρος</reg><orig>αερως</orig></choice> ἄνω ὡς πρὸ <choice><reg>ἀνωτέρω</reg><orig>ανωτερο</orig></choice> τέταγται, οἱ δὲ γείτονες <choice><reg>τῆς</reg><orig>του</orig></choice> <expan>προκ<ex>ειμένης</ex></expan> παλαιᾶς οἰκίας 

    <lb n="26"/>ἐκ <choice><reg>τετραγώνου</reg><orig>τετρακονου</orig></choice> εἴσιν <choice><reg>οὖτοι</reg><orig>ουτι</orig></choice> νότου <choice><reg>πεσσὸς</reg><orig>πεσου</orig></choice> οἰκίας Μηνᾶ Χιχόιτος <choice><reg>χαλκοπράτου</reg><orig>χαλχοπρατη</orig></choice> 

    <lb n="27"/>ἀπηλιώτου <choice><reg>μεγάλη</reg><orig>μεγαλης</orig></choice> <choice><reg>ἔπαυλις</reg><orig>επαυλεως</orig></choice> τῶν κληρωνόμων 

    <lb n="28"/>Προμαυῶτος βορρᾶ οἰκία 

    <lb n="29"/>τῶν κληρονόμων <choice><reg>Πέτρου</reg><orig>πετρος</orig></choice> ἢ οἷοι <choice><reg>ἐὰν</reg><orig>ειαν</orig></choice> <choice><reg>ὦσι</reg><orig>οσει</orig></choice> γείτονες 

πάντῃ πάντοθεν 

    <lb n="30"/>τιμῆς τε τῆς πρὸς ἀλλήλους συν<unclear>πεφ</unclear>ωνημένης καὶ <choice><reg>συναρεσάσης</reg><orig>συναρεσησης</orig></choice> τελείας 

    <lb n="31"/>καὶ ἀξίας χρυσοῦ <choice><reg>δεσποτικοῦ</reg><orig>δεσποτικων</orig></choice> <choice><reg>δοκ<supplied reason="lost">ί</supplied>μου</reg><orig>δωκ<supplied reason="lost">ι</supplied>μων</orig></choice> 

    <lb n="32"/><subst><add place="inline">νομισματίου</add><del rend="corrected">νομισματαου</del></subst> δίμοιρον <num value="2/3"/> 

    <lb n="33"/><choice><reg>συναλλαγῇ</reg><orig>συναλλαγης</orig></choice> πρὸς <expan>παρασ<ex>ταθμίαν</ex></expan> <choice><reg>δύο</reg><orig>δυω</orig></choice> <num value="2"/> τῷ σταθμῷ τῆς αὐτῆς κώμης, <expan>γί<ex>νεται</ex></expan> <expan>χρ<ex>υσοῦ</ex></expan> <expan>νο<ex>μισματίου</ex></expan> <num value="2/3">𐅷</num> <expan>συναλλ<ex>αγῇ</ex></expan> <expan><ex>κεράτια</ex></expan> <subst><add place="inline"><num value="2">β</num></add><del rend="corrected"><num value="1">α</num></del></subst>, 

    <lb n="34"/>ἅπερ τοῦτο τῆς τιμῆς χρύσιν<unclear>ον</unclear> ἀπείληφα <choice><reg>εἰποῦσα</reg><orig>ηπυσα</orig></choice> ἡ <choice><reg><supplied reason="omitted">ἀ</supplied>ποδομένη</reg><orig>ποδομενη</orig></choice> Εὐδοξία θυγάτηρ 

    <lb n="35"/>Γεωργίου <subst><add place="inline">καὶ</add><del rend="corrected">αια</del></subst> μετὰ <choice><reg>συναινέσεως</reg><orig>συνενησεως</orig></choice> ἀνδρὸς <surplus>αὐτῆς</surplus> ἐμοῦ <choice><reg>Ἀνούφιος</reg><orig>ανουφις</orig></choice> Ἰ<supplied reason="lost">σα</supplied><unclear>κί</unclear><supplied reason="lost">ο</supplied>υ παρὰ σοῦ 

    <lb n="36"/>τοῦ <choice><reg><expan>ἐωνημ<ex>ένου</ex></expan></reg><orig>αιωνεμμ</orig></choice> Μηνᾶ <choice><reg>υἱοῦ</reg><orig>υιω</orig></choice> <choice><reg>Χριστοφόρου</reg><orig>χριστοφορος</orig></choice> διὰ χειρός σου εἰς χεῖράς μου 

    <lb n="37"/><unclear>ἐ</unclear>πʼ <choice><reg>ὄψεσι</reg><orig>οψεσει</orig></choice> τῶν ἑξῆς ὑπογραφόντων νομίμων μαρτύρων εἰς τὸ ἀπὸ τοῦ 

    <lb n="38"/>νῦν <choice><reg>ἐπὶ</reg><orig>επει</orig></choice> <choice><reg>τὸ</reg><orig>τω</orig></choice> <choice><reg>διηνεκὲς</reg><orig>διηνεγκες</orig></choice> ἀριθμῷ σταθμῷ πλήρη πρήλη ἅμα τῇ πράσει <choice><reg><expan>γενομέ<ex>νῃ</ex></expan></reg><orig>γενωμε</orig></choice> ἐφʼ ᾗ 

    <lb n="39"/>τιμῇ καὶ <choice><reg>πεπεισμένη</reg><orig>πεπεισμενος</orig></choice> πρὸς τὸ <choice><reg>ἐντεῦθέν</reg><orig>εντευθην</orig></choice> σε <choice><reg>τὸ<supplied reason="omitted">ν</supplied></reg><orig>το</orig></choice> <choice><reg>αὐτὸ<supplied reason="omitted">ν</supplied></reg><orig>αυτο</orig></choice> <choice><reg>πριάμενον</reg><orig>πριαμενος</orig></choice> <choice><reg>Μηνᾶν</reg><orig>μηνα</orig></choice> 

    <lb n="40"/><choice><reg>υἱὸν</reg><orig><hi rend="diaeresis">υ</hi>ιω</orig></choice> Χριστορόρου κρατεῖν καὶ κυριεύειν καὶ δεσπόζειν ἀπὸ τοῦ <choice><reg>προδι

    <lb n="41" type="inWord"/><supplied reason="lost">ωμ</supplied>ολογημένου</reg><orig>πρωτη<supplied reason="lost">ωμ</supplied><unclear>ηλ</unclear>ε<unclear>γο</unclear>μενου</orig></choice> ὁλοκλήρου μου <choice><reg>μέρους</reg><orig>μερος</orig></choice> ἀπὸ <choice><reg>τῆς</reg><orig>του</orig></choice> <expan>προκ<ex>ειμένης</ex></expan> <choice><reg>σεσαθρωμένης</reg><orig>σεσαθρομενης</orig></choice> 

    <lb n="42"/>παλαιᾶς οἰ<unclear>κ</unclear>ίας <expan>λεγομέ<ex>νης</ex></expan> <hi rend="diaeresis">Ἰ</hi>ωναθὰν οἰκοδόμου καὶ ἐξ <choice><reg>οἱουδήποτε</reg><orig>ιουδηποται</orig></choice> δικαίου 

    <lb n="43"/>ἀπὸ <choice><reg><supplied reason="omitted">ἐ</supplied>δάφους</reg><orig>δαφους</orig></choice> μέχρι <choice><reg>ἀέρος</reg><orig>αερως</orig></choice> ἄνω κατὰ τὴ<unclear>ν</unclear> <unclear>ἀνώ</unclear>τεραν διαστολὴν καὶ οἰκεῖν 

    <lb n="44"/>καὶ διοικεῖν καὶ οἰκονομεῖν καὶ οἰκοδομεῖν ἢ καὶ <choice><reg>ἐποικοδομεῖν</reg><orig>επεικοδ<unclear>ο</unclear>μειν</orig></choice> καὶ μισθοῦν 

    <lb n="45"/>καὶ <choice><reg>μεταμισθοῦν</reg><orig>μηταμισθουν</orig></choice> καὶ παραπέμπειν <choice><reg>ἐπὶ</reg><orig>επει</orig></choice> κληρονόμους ἰδίους καὶ <choice><reg>διαδόχους</reg><orig>διατοχους</orig></choice> καὶ <choice><reg>διακατόχο<supplied reason="omitted">υ</supplied>ς</reg><orig>διακατοχος</orig></choice> 

    <lb n="46"/>καὶ <choice><reg>χρήσασθαι</reg><orig>χρυσασθαι</orig></choice> καὶ <choice><reg>νέμεσθαι</reg><orig>νεμαισθαι</orig></choice> κατὰ <choice><reg>τὸν</reg><orig>των</orig></choice> <unclear>δο</unclear>κοῦντά σοι <choice><reg>κυριευτικὸν</reg><orig>κυριευτικων</orig></choice> τρόπον <choice><reg>ἀκωλύτως</reg><orig>α<unclear>γ</unclear>ορητος</orig></choice> 

    <lb n="47"/>καὶ <choice><reg>ἀνεμποδίστως</reg><orig>ανεμποτιστος</orig></choice> <choice><reg>μετʼ</reg><orig>μητε</orig></choice> ἑνός <num value="1"/> σοι ἀντιποιουμένου ἢ ἀντιποιηθησομένου τῶν μήτε 

    <lb n="48"/>ἐμὲ κληρονόμων ἢ συνκληρονόμων ἢ <choice><reg>τῶν</reg><orig>τον</orig></choice> ἐκ γένους μοι ἀνηκόντων 

    <lb n="49"/>ἢ <choice><reg>ἀγχιστευόντων</reg><orig>αχειστευωντων</orig></choice> καὶ <choice><reg>τὸν</reg><orig>των</orig></choice> <choice><reg>ἐπελευσόμενον</reg><orig>επειλευσεμενη</orig></choice> παντὶ καιρῷ ἐπάναγκες <supplied reason="omitted">ἀποτρέψω</supplied>· εἰ δὲ 

    <lb n="50"/>καὶ <subst><add place="inline">ἧττον</add><del rend="corrected">ηττος</del></subst> <choice><reg>φροντίζω</reg><orig>φροντισο</orig></choice> τῆς βεβαιώσεως <choice><reg>τριπλασίως</reg><orig>τριπλασεως</orig></choice> τὸ τίμημα <choice><reg>ἀποδώσω</reg><orig>απ<unclear>οδ</unclear>ωσο</orig></choice> 

    <lb n="51"/>μετὰ καὶ <choice><reg>τῶν</reg><orig>του</orig></choice> διαφερόντων καὶ ἀναλωμάτων πρὸς <subst><add place="inline">τῷ</add><del rend="corrected">το</del></subst> βεβαίαν εἶναι καὶ <choice><reg>ἰσχυρὰν</reg><orig>ανισχυραν</orig></choice> 

    <lb n="52"/>ταύτην τὴν πρᾶσιν πανταχοῦ <expan>προφερομέ<ex>νην</ex></expan> ἥνπερ ἁπλῆν <choice><reg>γραφεῖσαν</reg><orig>γραφισαν</orig></choice> <choice><reg>ἑκοῦσα</reg><orig>εκοντες</orig></choice> 

    <lb n="53"/>καὶ <choice><reg>πεπεισμένη</reg><orig>πεπεισμενος</orig></choice> ἐθέμην σοι ἄνευ βίας καὶ ἀπάτης καὶ ἀνάγκης καὶ πλάνης 

    <lb n="54"/>πάσης καὶ <choice><reg>φόβου</reg><orig>φωβου</orig></choice> καὶ <choice><reg>δόλου</reg><orig>δωλου</orig></choice> τινὸς ἐν δημοσίῳ ἀρχείῳ καὶ κατὰ νόμους 

    <lb n="55"/><choice><reg>τετελειωμένη<supplied reason="omitted">ν</supplied></reg><orig>τετελειωμενη</orig></choice> μεθʼ <expan>ὑπ<unclear>ογ</unclear>ραφ<ex>ῆς</ex></expan> τοῦ ὑπὲρ ἐμοῦ <expan>ὑπογράφ<ex>οντος</ex></expan> καὶ τῶν ἑξῆς 

    <lb n="56"/><expan>συνηθ<ex>ῶν</ex></expan> κατὰ <expan>παράκλ<ex>ησιν</ex></expan> ἐμὴν <expan>μαρτυρ<ex>ησάντων</ex></expan> <expan>μαρτύρ<ex>ων</ex></expan> καὶ ἐξεδόμην σοι πρὸς 

