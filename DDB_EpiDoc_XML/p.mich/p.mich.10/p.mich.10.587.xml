    <lb n="1"/><gap reason="lost" extent="unknown" unit="line"/> 

    <lb n="1"/><gap reason="illegible" extent="unknown" unit="character"><desc>vestiges</desc></gap> 

    <lb n="2"/><unclear>Πα</unclear><supplied reason="lost">βελλ</supplied>ηοῦ<unclear>ς</unclear> Ὀ<supplied reason="lost">νν</supplied>ώ<unclear>φ</unclear><supplied reason="lost">ρ</supplied><unclear>ι</unclear><supplied reason="lost">ος</supplied> Π<supplied reason="lost">έρσης τῆ</supplied>ς <unclear>ἐπ</unclear><supplied reason="lost">ιγο</supplied>ν<supplied reason="lost">ῆς</supplied> 

    <lb n="3"/>ὡς ἐτῶν τεσσαρ<unclear>ά</unclear>κοντα <num value="40"/> <choice><reg>οὐλὴ</reg><orig>οὐληι</orig></choice> <choice><reg>μετώπῳ</reg><orig>μετόπωι</orig></choice> ἐξ ἀρισ

    <lb n="4" break="no"/>τερῶν Ἁρμιύ<unclear>σ</unclear>ι Ὀννώφριος ὡς ἐτῶν εἴκοσι ἓξ <num value="26"/> 

    <lb n="5"/><choice><reg>οὐλὴ</reg><orig>οὐληι</orig></choice> γαστροκνημίᾳ ἀριστερᾷ <choice><reg>ἔχειν</reg><orig>ἔχιν</orig></choice> παρʼ αὐτοῦ 

    <lb n="6"/>παραχρῆμα διὰ <choice><reg>χειρὸς</reg><orig>χιρὸς</orig></choice> ἐξ οἴκου ἀργυρίου ἐπισήμου 

    <lb n="7"/>δραχμὰς τεσσαράκοντα <choice><reg>ὀκτὼ</reg><orig>ὀκτὼι</orig></choice> <num value="48"/>, καὶ ἀντὶ τῶν τού

    <lb n="8" break="no"/>των τόκων καὶ δεόντων καὶ <unclear>ἱ</unclear>ματισμοῦ παρ

    <lb n="9" break="no"/>έξεσθαι τὸν ὁμολογοῦντα τὴν ἑαυτοῦ <choice><reg>θυγατέρα</reg><orig>θυγατέραν</orig></choice> 

    <lb n="10"/>Κολλεῦθιν <choice><reg>παραμ<unclear>έ</unclear>νουσα<supplied reason="omitted">ν</supplied></reg><orig>παραμενουσα</orig></choice> τῶι Ἁρμιύσει καὶ τοῖς 

    <lb n="11"/>παρʼ αὐτοῦ ἐπʼ ἐνιαυτὸν ἕνα <num value="1"/> ἀπὸ τοῦ προκιμένου 

    <lb n="12"/>χρόνου διατρίβ<supplied reason="lost">ου</supplied>σαν καὶ ποιοῦσαν τὰ <choice><reg>ἐπιστ<unclear>ε</unclear>λ<supplied reason="omitted">λ</supplied>όμενα</reg><orig>ἐπιστ<unclear>ε</unclear>λόμενα</orig></choice> 

    <lb n="13"/>αὐτῆ<unclear>ι</unclear> πάν<unclear>τα</unclear>, <supplied reason="lost">οὐ</supplied> <choice><reg>γινομένην</reg><orig>γεινομενηι</orig></choice> <choice><reg>ἀπόκοιτον</reg><orig>ἀποκοιτος</orig></choice> οὐδʼ <choice><reg>ἀφή

    <lb n="14" break="no"/><supplied reason="lost">με</supplied>ρον</reg><orig>ἀφη<lb n="14" break="no"/>μ<supplied reason="lost">ερ</supplied>ος</orig></choice> ἀπὸ τῆς τοῦ Ἁρμιύσιος οἰκίας ἄνευ τῆς 

    <lb n="15"/>αὐτοῦ <choice><reg>γνώμης</reg><orig>γνόμη<unclear>ς</unclear></orig></choice>, ἀλλὰ καὶ <choice><reg>συνακο<supplied reason="lost">λ</supplied>ουθοῦσα<supplied reason="omitted">ν</supplied></reg><orig>συνακ<supplied reason="lost">λ</supplied>ουθουσα</orig></choice> αὐτῶι 

    <lb n="16"/>π<unclear>αν</unclear>ταχῆι κα<unclear>τ</unclear>ʼ <supplied reason="lost">Α</supplied>ἴγυπτον, καὶ μὴ <choice><reg>ἐξέστω</reg><orig>ἐξέστωι</orig></choice> αὐτῶι 

    <lb n="17"/>Παβελληοῦ ἐ<unclear>ν</unclear><supplied reason="lost">τ</supplied>ὸς τοῦ χρόνου ἀποσπάσαι τὴν θυ

    <lb n="18" break="no"/>γατέραν, ἐὰν δὲ ἀ<unclear>π</unclear>οσπάσηι ἢ καὶ <choice><reg>αὐτὴ</reg><orig>αὐτηι</orig></choice> ἑκοῦσα 

    <lb n="19"/>ἀπαλλαγῆι ἢ ἄλλ<supplied reason="lost">ως</supplied> παραβῶσί τι τῶν <expan>προγεγραμμέν<ex>ων</ex></expan> 

    <lb n="20"/>ἢ <unclear>α</unclear><gap reason="illegible" quantity="1" unit="character"/><unclear>η</unclear><gap reason="illegible" quantity="4" unit="character"/>ου<supplied reason="lost">σ</supplied><unclear>α</unclear> ἢ <choice><reg>καταβλάπτουσα</reg><orig><unclear>κατ</unclear>αβ<unclear>λα</unclear>πτουσι</orig></choice> ἢ <choice><reg>νοσφιζ<supplied reason="omitted">ομ</supplied>ένη</reg><orig>νοσφιζενος</orig></choice> 

    <lb n="21"/>ἁλίσκητα<unclear>ι</unclear> τῶ<unclear>ν</unclear> <supplied reason="lost">Ἁ</supplied>ρμιύσιος ἢ τῶν παρʼ αὐτοῦ, ἢ λα

    <lb n="22" break="no"/>βοῦσά τι φυλάσσ<supplied reason="lost">ει</supplied><unclear>ν</unclear> μὴ παράσχηται σῶον, ἐὰν 

    <lb n="23"/>μή τι μετὰ βία<unclear>ς</unclear> <unclear>ἀ</unclear>φαιρεθῆι, <choice><reg>ἀποτισάτω</reg><orig>ἀποτισάτωι</orig></choice> <supplied reason="omitted">ὁ</supplied> <expan>ὁμολογῶ<ex>ν</ex></expan> 

    <lb n="24"/>Ἁρμιύσι παραχ<supplied reason="lost">ρ</supplied><unclear>ῆ</unclear>μα ἅς τʼ εἴληφεν παρʼ αὐτοῦ καθότι 

    <lb n="25"/>πρόκιται <expan>ἀργ<ex>υρίου</ex></expan> <supplied reason="lost"><expan><ex>δραχμὰς</ex></expan> τεσ</supplied>σαράκοντα <choice><reg>ὀκτὼ</reg><orig>ὀκτὼι</orig></choice> <num value="48"/> μεθʼ ἡμιολίας 

    <lb n="26"/>καὶ τόκων καὶ ἐπ<supplied reason="lost">ί</supplied>τιμον <expan>ἀργ<ex>υρίου</ex></expan> <expan><ex>δραχμὰς</ex></expan> ἑκατὸν <num value="100"/> καὶ εἰς τὸ <expan>δημό<ex>σιον</ex></expan> <supplied reason="omitted">τὰς ἴσας</supplied> 

    <lb n="27"/>καὶ τὸ βλάβος, κ<supplied reason="lost">αὶ ἑ</supplied>κάστης <unclear>ἡ</unclear>μέρ<unclear>α</unclear>ς ἧς ἐὰν <expan>ἀτ<unclear>ακτ</unclear>ή<ex>σῃ</ex></expan> 

    <lb n="28"/>τῆς <gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><unclear>ς</unclear> χαλκοῦ ὀβολοὺς τρεῖς <num value="3"/> καὶ τὸ <expan>κεφ<ex>άλαιον</ex></expan> 

    <lb n="29"/>καὶ τὸ ἀπόκ<unclear>λεμμ</unclear>α πενταπλοῦν, τὸ δὲ νοσφισμὸν 

    <lb n="30"/>ἡμιόλιον ἡμιόλιον τοὺς δὲ τόκους ἁπλοῦς. ἐὰν δὲ μὴ 

    <lb n="31"/>παραδῷ ἢ μὴ παράσχηται σῶον, τὴν ἀξίαν τὴν <expan><ex>κατὰ</ex></expan> 

    <lb n="32"/>τὴν <choice><reg>ἐπι<supplied reason="lost">γ</supplied>νώμην</reg><orig>ἐπι<supplied reason="lost">γ</supplied><unclear>ν</unclear>όμην</orig></choice> Ἁρμιύσιος, οὔσης τῶι Ἁρμιύσει 

    <lb n="33"/>τῆς πράξεως ἔκ τε τοῦ <expan>ὑποχ<ex>ρέου</ex></expan> καὶ ἐκ τῶν <expan>ὑπαρχ<ex>όντων</ex></expan> καθά

    <lb n="34" break="no"/>περ <choice><reg>ἐκ</reg><orig>ἐγ</orig></choice> δίκης. τοῦ δὲ χρόνου <choice><reg>πληρωθέντος</reg><orig>πληροθέντος</orig></choice> <choice><reg><expan>ἀποδό<ex>τω</ex></expan></reg><orig><expan>ἀποδώ<ex>τω</ex></expan></orig></choice> 

    <lb n="35"/><supplied reason="omitted">ὁ</supplied> ὁμολογῶν τὰς <expan>προκιμ<ex>ένας</ex></expan> <expan>ἀργ<ex>υρίου</ex></expan> <expan><ex>δραχμὰς</ex></expan> τεσσαράκοντα <choice><reg>ὀκτὼ</reg><orig>ὀκτὼι</orig></choice> <num value="48"/>, 

    <lb n="36"/>ἐὰν δὲ μὴ ἀποδῷ, ἀ<unclear>π</unclear>οτισάτω μεθʼ ἡμιολίας καὶ <expan>τόκω<ex>ν</ex></expan> 

    <lb n="37"/>καὶ ἐπίτιμον ἄλλας ἀργυρίου δραχμὰς ἑκατὸν <num value="100"/> καὶ εἰς τὸ <expan>δημό<ex>σιον</ex></expan> <supplied reason="omitted">τὰς ἴσας</supplied>. 

    <lb n="38"/><expan>ὑπογρ<ex>αφεὺς</ex></expan> τοῦ μὲν <expan>ὁμο<ex>λογοῦντος</ex></expan> <expan>Κολλοῦθο<ex>ς</ex></expan> <expan>Μιεῦτο<ex>ς</ex></expan> <expan><ex>ὡς ἐτῶν</ex></expan> <num value="20">κ</num> <expan>οὐ<ex>λὴ</ex></expan> <expan>καρ<ex>πῷ</ex></expan> <expan>δεξ<ex>ιῷ</ex></expan> 

    <lb n="39"/><handShift new="m2"/> Παβελληοῦς <unclear>Ὀννώ</unclear>φριος Πέρσης τῆς ἐπιγονῆς 

