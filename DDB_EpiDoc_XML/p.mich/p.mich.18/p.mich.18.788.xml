         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="1" type="textpart"><ab>

    <lb n="1"/><unclear>ἐ</unclear>μίσθωσεν Θῶνις Φατρέως τοῦ Ἁρθώνιος <unclear>ἀ</unclear><supplied reason="lost">π </supplied><gap reason="lost" quantity="6" unit="character"/>

    <lb n="2"/>ων πόλεως ἱερεὺς κα<supplied reason="lost">ὶ</supplied> <subst><add place="inline"><unclear>π</unclear>τεραφόρος</add><del rend="corrected"><unclear>π</unclear>τελαφορος</del></subst> κα<supplied reason="lost">ὶ</supplied> <unclear>ἱεροτέ</unclear><supplied reason="lost">κτων</supplied>

    <lb n="3"/>Θοήριδος καὶ <choice><reg>Ἴσιδος</reg><orig>Εἴσιδος</orig></choice> καὶ Σαράπιδος <unclear>καὶ</unclear> ναο<supplied reason="lost">ῦ θεοῦ</supplied>

    <lb n="4"/>Σεβ<unclear>αστοῦ</unclear> <unclear>Καίσαρος</unclear> <supplied reason="lost">καὶ</supplied> τῶ<supplied reason="lost">ν συ</supplied>ν<supplied reason="lost">ν</supplied><unclear>ά</unclear>ων <unclear>θε</unclear><supplied reason="lost">ῶν</supplied>

    <lb n="5"/>καὶ μοσχοσφραγιστὴς Διονυσίῳ τῷ καὶ Καπίτ<unclear>ω</unclear><supplied reason="lost">νι</supplied>

    <lb n="6"/>ἀπε<unclear>λευ</unclear>θέρῳ Πλου<unclear>τί</unclear><supplied reason="lost">ω</supplied><unclear>ν</unclear>ος Ὠφελίωνος <unclear>ἀπ</unclear><supplied reason="lost">ὸ τῆς</supplied>

    <lb n="7"/>αὐτῆς πόλεως ἐπὶ χρόνον ἔτη τρία <num value="3"/> ἀπὸ νε<supplied reason="lost">ομη</supplied>

    <lb n="8" break="no"/>νίας Φαῶφι τοῦ εἰσιό<unclear>ντ</unclear>ος <choice><reg>τεσσαρεσκαιδεκ<supplied reason="lost">άτου</supplied></reg><orig>τεσαρεσκαιδεκ<supplied reason="lost">άτου</supplied></orig></choice><supplied reason="lost"> <expan><ex>ἔτους</ex></expan></supplied>

    <lb n="9"/>Αὐρηλίου Ἀντωνίνου Καίσαρος τ<unclear>οῦ</unclear> <app type="editorial"><lem resp="BL 12.126"><unclear>κυρ</unclear><supplied reason="lost">ίου ἀφ' οὗ</supplied></lem><rdg><unclear>κυρ</unclear><supplied reason="lost">ίου οὗ</supplied></rdg></app>

    <lb n="10"/>ἔχει ἐπʼ ἀμφόδου Ἑρμαίου καμηλῶν<supplied reason="lost">ος </supplied><gap reason="lost" quantity="3" unit="character"/>

    <lb n="11"/>ρου λεγομένου τὰς ἐν τῷ α<unclear>ὐ</unclear>τῷ <supplied reason="lost">ἐποι</supplied><unclear>κ</unclear>ί<unclear>ω</unclear>` <unclear>ἀ</unclear><supplied reason="lost">ρού</supplied>

    <lb n="12" break="no"/>ρας ἑπτὰ εἰς <choice><reg>ὀρνιθ<supplied reason="lost">ῶ</supplied><unclear>ν</unclear><supplied reason="lost">α</supplied></reg><orig>ὀρ<unclear>ν</unclear>ε<unclear>ι</unclear>θ<supplied reason="lost">ῶ</supplied><unclear>ν</unclear><supplied reason="lost">α</supplied></orig></choice><gap reason="lost" quantity="1" unit="character"/><gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" quantity="15" unit="character"/>

    <lb n="13"/><unclear>ων</unclear>, εἰς ἃς εἰσοδεύσει καὶ ἐξοδεύσει ὁ με<supplied reason="lost">μισθω</supplied>

    <lb n="14" break="no"/>μένος διὰ τῆ<unclear>ς</unclear> ἐκ λι<unclear>β</unclear>ὸς ἐν τῷ βορινῷ μέρει <supplied reason="lost">τοῦ</supplied>

    <lb n="15"/>αὐτοῦ καμηλῶνος θύρας <unclear>ἐξ</unclear>ουσίας αὐτ<unclear>ῶ</unclear>` σ<unclear>υ</unclear><supplied reason="lost">γ</supplied>

    <lb n="16" break="no"/>χρᾶσθαι τῷ ἐνόντι <unclear>φρ</unclear><supplied reason="lost">έ</supplied><unclear>α</unclear><add place="above">τι</add> ἀκ<supplied reason="lost">ω</supplied><unclear>λ</unclear>ύτως <unclear>ἐκ</unclear><supplied reason="lost">φο</supplied><unclear>ρί</unclear><supplied reason="lost">ου κατʼ ἔ</supplied>

    <lb n="17" break="no"/><app type="editorial"><lem resp="BL 12.126">ἀργυρί<supplied reason="lost">ου δ</supplied><unclear>ρα</unclear>χ<unclear>μ</unclear><supplied reason="lost">ῶν</supplied> <num><gap reason="lost" extent="unknown" unit="character"/></num> <supplied reason="lost">καὶ ἐκ</supplied></lem><rdg>τος ἀργυρί<supplied reason="lost">ου δραχμῶν</supplied> <num value="300"><supplied reason="lost">τρια</supplied><unclear>κ</unclear>οσίω<supplied reason="lost">ν</supplied></num> <supplied reason="lost">καὶ ἐκ</supplied></rdg></app>

    <lb n="18" break="no"/>τάκτων κατʼ <unclear>ἔ</unclear><supplied reason="lost">τος</supplied> <choice><reg><unclear>ὀ</unclear>ρνί<supplied reason="lost">θων</supplied></reg><orig><unclear>ὀ</unclear>ρν<unclear>ε</unclear><supplied reason="lost">ίθων</supplied></orig></choice><gap reason="lost" quantity="1" unit="character"/><supplied reason="lost"> τελείων τοκάδων ἀλε</supplied>

