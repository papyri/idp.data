    <lb n="23"/><gap reason="lost" quantity="1" unit="character"/><gap reason="illegible" quantity="1" unit="character"/>ς ἐὰν ἄλλας αἱρῆ<supplied reason="lost">ται </supplied><gap reason="lost" extent="unknown" unit="character"/>

    <lb n="24"/><gap reason="lost" quantity="1" unit="character"/>ε συνφω<unclear>ν</unclear><gap reason="illegible" quantity="4" unit="character"/><gap reason="lost" extent="unknown" unit="character"/>

    <lb n="25"/>φυλάκτρο<unclear>υ</unclear> <supplied reason="lost">καὶ πλινθευομένης ὄντων πρὸς τὸν</supplied>

