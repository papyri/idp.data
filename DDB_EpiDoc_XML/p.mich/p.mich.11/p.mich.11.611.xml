    <lb n="27"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="4" unit="character"/> αχ<gap reason="illegible" quantity="1" unit="character"/><unclear>ξ</unclear><gap reason="illegible" quantity="1" unit="character"/>κ<gap reason="illegible" quantity="1" unit="character"/>λλ<gap reason="illegible" quantity="4" unit="character"/> </ab></div></div>
