    <lb n="30"/><del rend="erasure"><gap reason="illegible" quantity="3" unit="character"/>ι</del><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="31"/>λομε<unclear>ν</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="32"/>ναιπ<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="33"/>ἐπεὶ <gap reason="illegible" quantity="4" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="34"/>μέχρι νῦν ο<unclear>υ</unclear><gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="35"/>καὶ παρα<gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

