    <lb n="1"/>Ἀμμων<unclear>ίῳ</unclear> <expan>στρα<ex>τηγῷ</ex></expan> <expan>Ἀρσ<unclear>ι</unclear><ex>νοίτου</ex></expan> <expan>Ἡρακλ<ex>είδου</ex></expan> 

