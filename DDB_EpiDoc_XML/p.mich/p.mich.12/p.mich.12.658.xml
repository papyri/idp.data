    <lb n="11" type="inWord"/>θι τ<supplied reason="lost">οῦ</supplied> <choice><corr>εἰσιόντος</corr><sic>ισιοντος</sic></choice> <num value="2">β</num><gap reason="lost" quantity="1" unit="character"/> 

