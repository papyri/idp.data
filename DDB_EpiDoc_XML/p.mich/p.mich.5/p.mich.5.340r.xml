    <lb n="13"/><expan><supplied reason="lost">Πολέ</supplied>μωνο<ex>ς</ex></expan> <expan>μερίδο<ex>ς</ex></expan> κλήρου <choice><reg>κατοικικοῦ</reg><orig>κατυκικοῦ</orig></choice> ἀρούρας <num value="5">ε</num> 

    <lb n="14"/><supplied reason="lost">ἢ </supplied><choice><reg><supplied reason="lost">ὅσ</supplied>αι</reg><orig>ὁσων</orig></choice> ἐὰν ὦσιν <del rend="erasure">ἐπὶ ταῖς οὔσει</del> ὧν αἱ <choice><reg>γειτνίαι</reg><orig>γιτνείαι</orig></choice> 

