    <lb n="1,msup"/><handShift new="m5"/> <num value="23">κγ</num> <expan><ex>ἔτους</ex></expan> <supplied reason="lost">Τιβ</supplied>ερίου Καίσαρος Σεβ<supplied reason="lost">αστ</supplied>οῦ. Μεχὶρ <num value="13">ιγ</num>. ἐκδόσιμον 

    <lb n="1"/><supplied reason="lost"><expan><ex>ἔτους</ex></expan> τρίτ</supplied><unclear>ο</unclear><supplied reason="lost">υ καὶ εἰκοστοῦ Τιβερίου</supplied> <unclear>Κα</unclear>ίσαρος Σεβα<unclear>σ</unclear><supplied reason="lost">τοῦ </supplied><gap reason="lost" quantity="13" unit="character"/> α<unclear>π</unclear><gap reason="lost" quantity="3" unit="character"/>κ<gap reason="illegible" quantity="3" unit="character"/><gap reason="lost" quantity="17" unit="character"/> <unclear>Π</unclear>ολέμ<unclear>ωνος</unclear> 

    <lb n="2"/><unclear>μερ</unclear>ί<unclear>δ</unclear><supplied reason="lost">ος τοῦ Ἀρσινοίτου νο</supplied>μοῦ. ὁμολογ<unclear>εῖ</unclear> <supplied reason="lost">Θενπετερμοῦθις Ἁτρήους, ὡ</supplied><unclear>ς</unclear> <unclear>ἐ</unclear>τῶν <gap reason="illegible" quantity="3" unit="character"/>κ<unclear>ο</unclear>ν<supplied reason="lost">τα </supplied><gap reason="lost" quantity="7" unit="character"/><supplied reason="lost"> οὐλὴ</supplied> <unclear>τ</unclear><supplied reason="lost">ρ</supplied><unclear>α</unclear>χήλωι 

