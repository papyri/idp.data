         <head n="2903" xml:lang="en">
            <date>242/1 BC</date>
            <placeName>Gurob</placeName>
            <ref n="p.petr;3;74" type="reprint-from">cfPPetr3,74,B</ref>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="ctr" type="textpart"><ab>

    <lb n="1"/><gap reason="lost" extent="unknown" unit="line"/> 

    <lb n="1"/>Φιλοπα<supplied reason="lost">τόρων</supplied> ἀθ<supplied reason="lost">λο</supplied>φόρου Βερ<supplied reason="lost">ενίκης <app type="editorial"><lem>Εὐεργετίδος</lem><rdg/></app></supplied> 

    <lb n="2"/>Εἰρήνης τῆς Μητροφάνους, <supplied reason="lost">κανηφόρου Ἀρσινόης</supplied> 

    <lb n="3"/>Φιλαδέλφου Ἰαμνείας τῆς <app type="editorial"><lem resp="BL cf. 3.143"><supplied reason="lost">Περιγένους</supplied></lem><rdg><gap reason="lost" extent="unknown" unit="character"/></rdg></app> 

    <lb n="4"/>Δαισίου πέντε καὶ <app type="editorial"><lem resp="BL 3.143">δεκάτη<supplied reason="lost">ι ἐν Ἱερᾶι Νήσωι</supplied></lem><rdg>δεκάτη<supplied reason="lost">ι </supplied><gap reason="lost" extent="unknown" unit="character"/></rdg></app> 

    <lb n="5"/>Θεῶν Σωτήρων τοῦ Ἀρσινοίτου <app type="editorial"><lem resp="BL 1.376"><supplied reason="lost">νομοῦ</supplied></lem><rdg><gap reason="lost" extent="unknown" unit="character"/></rdg></app>. 

    <lb n="6"/>ὁμολογεῖ <app type="editorial"><lem>Δονομάζις</lem><rdg>Δονομαζυς</rdg></app> Ταωτίου ε<gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> τῆς</supplied> 

    <lb n="7"/>ἐπιγονῆς ἀπέχειν παρὰ Ἱστιαίου <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="8"/>τῶν <unclear>Μαραίου</unclear> τακτομίσθου κληρούχου <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="9"/><app type="editorial"><lem resp="BL cf. 1.376"><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><unclear>νδ</unclear>εως<gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="3" unit="character"/><gap reason="lost" extent="unknown" unit="character"/></lem><rdg>Β<supplied reason="lost" cert="low">ου</supplied>βάστεως εἰσιν <note xml:lang="en">?</note> <gap reason="illegible" extent="unknown" unit="character"><desc>vestiges</desc></gap> <gap reason="lost" extent="unknown" unit="character"/><certainty match=".." locus="value"/></rdg></app> 

    <lb n="10"/>εἰς τὸ τοῦ βασιλέως <supplied reason="lost">ὄνομα</supplied> υρ<gap reason="illegible" quantity="3" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="11"/>Ἱστιαίου ἐν τῶι τ<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="12"/>ἐνεκάλεσεν Δονο<supplied reason="lost">μάζις</supplied> Ἱστιαίωι <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="13"/>ε<gap reason="lost" quantity="7" unit="character"/>εως εἰσιν <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="14"/><gap reason="lost" extent="unknown" unit="character"/>ασωι τῶν <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="15"/>Ἱστιαῖος ἐπειρη<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="16"/>τριακοσίας τριάκοντα <num value="330"/> <supplied reason="lost">Δον</supplied>ομ<supplied reason="lost">άζει </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="17"/>ἐπὶ η<gap reason="lost" extent="unknown" unit="character"/>αυτ<gap reason="illegible" quantity="1" unit="character"/>ος<gap reason="illegible" quantity="3" unit="character"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ὑ</supplied>

    <lb n="18" break="no"/><app type="editorial"><lem>πὲρ</lem><rdg>περὶ</rdg></app> ὧν ἀνήλωκεν Δονομάζει <app type="editorial"><lem>Θεοδοτ<unclear>ω</unclear><supplied reason="lost" cert="low"> </supplied><gap reason="lost" extent="unknown" unit="character"/> </lem><rdg>εσ<gap reason="illegible" quantity="1" unit="character"/>ατο<gap reason="lost" extent="unknown" unit="character"/></rdg></app> 

    <lb n="19"/>καὶ ἀπέχειν τὸν λόγον Ἱστιαῖος παρὰ Δ<supplied reason="lost">ονομάζεως </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="20"/>τῶν τριακοσίων τριάκοντα <num value="330"/> δραχμῶν <app type="editorial"><lem resp="BL 1.377"><supplied reason="lost">καὶ μὴ ἐξεῖναι</supplied></lem><rdg><gap reason="lost" extent="unknown" unit="character"/></rdg></app> 

    <lb n="21"/>Δονομάζει μήδε Ἱστιαίωι περὶ τῶν χρ<supplied reason="lost">ημάτων <app type="editorial"><lem resp="BL 1.377">ἀλλήλοις</lem><rdg><supplied reason="lost">ἔξηι </supplied><gap reason="lost" extent="unknown" unit="character"/></rdg></app></supplied> 

    <lb n="22"/>ἐπελθεῖν, ἐπιόντι <app type="editorial"><lem resp="BL 1.377"><supplied reason="lost">δὲ </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ἐὰν δέ τις</supplied></lem><rdg><supplied reason="lost">δὲ </supplied><gap reason="lost" extent="unknown" unit="character"/></rdg></app> 

    <lb n="23"/>παρασυγγραφήσηι καὶ μὴ ἐμμείνηι <app type="editorial"><lem>τ<supplied reason="lost">ῆι ὁμολογίαι</supplied></lem><rdg>τ<gap reason="lost" extent="unknown" unit="character"/></rdg></app> 

    <lb n="24"/><app type="editorial"><lem>ἥ τε ἔφοδος</lem><rdg><gap reason="illegible" quantity="1" unit="character"/> ἐπέφοδος</rdg></app> αὐτῶι ἄκυρος ἔστω καὶ <app type="editorial"><lem>προσ<supplied reason="lost">αποτισάτω</supplied></lem><rdg>προς<gap reason="lost" extent="unknown" unit="character"/></rdg></app> 

    <lb n="25"/>ὁ ἐπιπορευ<supplied reason="lost">όμενος </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="26"/><unclear>Διόνυσος</unclear> <gap reason="lost" quantity="5" unit="character"/> καὶ <app type="editorial"><lem>μηθὲν ἧττον ἡ <supplied reason="lost">συγγραφὴ κυρία</supplied></lem><rdg><gap reason="illegible" quantity="1" unit="character"/>η<gap reason="illegible" quantity="3" unit="character"/>ρητη<gap reason="illegible" quantity="1" unit="character"/>η</rdg></app> 

    <lb n="27"/>ἔστω. μάρτυρες <supplied reason="lost">Στ</supplied>ρατωνίδης Χα<supplied reason="lost">λκηδόνιος,</supplied> 

    <lb n="28"/>Φιλοκύδ<supplied reason="lost" cert="low">ης</supplied><supplied reason="lost"> Ἀ</supplied>μφιπολίτης, Νικολάο<supplied reason="lost">ς </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="29"/>οἱ τρεῖς <num value="3"/> τῶν Μαραίου τακτόμισθοι <app type="editorial"><lem resp="BL 7.160">κ<supplied reason="lost">ληροῦχοι</supplied></lem><rdg><gap reason="lost" extent="unknown" unit="character"/></rdg></app> 

    <lb n="30"/>Ἀπολλώνιος <app type="editorial"><lem resp="BL 6.114">Αἴνιος</lem><rdg><gap reason="illegible" quantity="1" unit="character"/>νιδε<gap reason="illegible" quantity="1" unit="character"/></rdg></app>, Πτολεμαῖος Μ<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="31"/>Ἀπολλώνιος Ἡρακλεώτης οἱ τρεῖς <num value="3"/> τ<supplied reason="lost">ῶν </supplied><app type="editorial"><lem><supplied reason="lost">Ἱππο</supplied>

    <lb n="32" break="no"/>κράτους</lem><rdg><gap reason="lost" extent="unknown" unit="character"/>κράτους</rdg></app> τῆς δευτέρας ἱππαρχίας <app type="editorial"><lem resp="BL 7.161"><supplied reason="lost">ἑκατον</supplied>

    <lb n="33" break="no"/>τάρουροι</lem><rdg><gap reason="lost" extent="unknown" unit="character"/> τούτων</rdg></app>, συγγραφοφύλαξ Ἀπολλ<supplied reason="lost">ώνιος</supplied> 
</ab></div><div n="subscrrp" type="textpart"><ab>

    <lb n="34"/><handShift new="m2"/> ἐχρηματίσθη ἀνενεχθὲν τὸ <supplied reason="lost">ἀντί</supplied>

    <lb n="35" break="no"/>γραφον τοῦτο <add place="above">ὑπὸ Ἱστιαίου</add> ἐν Κροκοδίλων <supplied reason="lost">πόλει</supplied> 

    <lb n="36"/><expan><ex>ἔτους</ex></expan> <num value="13">ιγ</num> Παῦνι πεντεκαιδεκά<supplied reason="lost">τηι</supplied> 

    <lb n="37"/>διὰ τὸ τὸν συγγραφοφύλακα <app type="editorial"><lem resp="BL cf. 1.377">λει<supplied reason="lost" cert="low">τουργεῖν</supplied> <certainty match=".." locus="value"/></lem><rdg>λεί<supplied reason="lost" cert="low">πεσθαι</supplied><certainty match=".." locus="value"/></rdg></app> 

    <lb n="38"/>ἐν Ἀλαβάστρων πόλει. </ab></div></div>
