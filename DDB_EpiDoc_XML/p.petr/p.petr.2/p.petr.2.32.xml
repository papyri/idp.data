    <lb n="8"/><supplied reason="lost">ἀν</supplied>ήλωμα 

    <lb n="9"/>εἰς τοὺς σάκκους <app type="editorial"><lem>τρί<unclear>χ</unclear>ες</lem><rdg>τρ<gap reason="illegible" quantity="2" unit="character"/>ες</rdg></app> <num value="3"><expan><ex>τριώβολον</ex></expan></num> 

    <lb n="10"/><num value="3">γ</num> ἄρτων τῶν ἀποσταλέντων σοι 

