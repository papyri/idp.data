         <head n="7455" xml:lang="en">
            <date>III sac</date>
            <placeName>Gurob</placeName>
            <ref type="reprint-from">cf. P.Petr. 3 45,3</ref>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="1,FrA" type="textpart"><ab>

    <lb n="1"/>ἐπιστο<gap reason="lost" quantity="3" unit="character"/>στερουσι<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="2"/>εἰ δὲ μὴ παλιτραχηλιοῦσι <app type="editorial"><lem>ἐν τ<supplied reason="lost">ῶ</supplied>ι β<gap reason="lost" extent="unknown" unit="character"/></lem><rdg>εντ<gap reason="lost" extent="unknown" unit="character"/></rdg></app> 

    <lb n="3"/>ναύτας πρότερον δὲ ἕκαστος <app type="editorial"><lem>τῶν λ<supplied reason="lost">αῶν </supplied><gap reason="lost" extent="unknown" unit="character"/></lem><rdg>αὐτῶν λ<gap reason="lost" extent="unknown" unit="character"/></rdg></app> 

    <lb n="4"/>δωρεὰν μὲν <app type="editorial"><lem>ἀω<supplied reason="lost">ί</supplied>λια <num value="31">λα</num> κατασ<supplied reason="lost">τήσεται </supplied><gap reason="lost" extent="unknown" unit="character"/></lem><rdg>αω<gap reason="illegible" quantity="1" unit="character"/>λια <gap reason="lost" quantity="3" unit="character"/>βα<gap reason="illegible" quantity="1" unit="character"/>και τας<certainty match=".." locus="value"/></rdg></app> 

    <lb n="5"/>καὶ πρὸς τοῖς ἔργοις ἐγίν<supplied reason="lost">ο</supplied>ντο <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="6"/>ἵνα ἐκ τοῦ βασιλικοῦ χορηγῆται αὐτοῖς <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="7"/>δεῖ φροντίσαι μὴ πότε τῶν ναυτῶν <supplied reason="lost">ἀπο </supplied><gap reason="lost" extent="unknown" unit="character"/><foreign xml:lang="la">-</foreign>

    <lb n="8"/>πέμπτων γενομένων οὐκέτι ῥᾴ<supplied reason="lost">διον ἀνακ</supplied>

    <lb n="9" break="no"/><unclear>τήσασθαι</unclear> ἵνα ὦσι πρὸς τοῖς ἔργοις. <add place="above">εἰ γὰρ δὲ</add> <g type="parens-deletion-opening"/><app type="editorial"><lem>ὅτε δ<gap reason="lost" extent="unknown" unit="character"/></lem><rdg>ον εδ<gap reason="lost" extent="unknown" unit="character"/></rdg></app><g type="parens-deletion-closing"/> 

    <lb n="10"/>συντελεῖσθαι διάτι οὐ καταφρονοῦντες πλεονάκις <app type="editorial"><lem>ποι<gap reason="lost" extent="unknown" unit="character"/></lem><rdg>μου</rdg></app> 

    <lb n="11"/>εἰδῆσαι ἀκριβῶς ὑπό τινος <app type="editorial"><lem>τὸ ὕδωρ π<gap reason="lost" extent="unknown" unit="character"/></lem><rdg>τοῦ δωρπ<gap reason="lost" extent="unknown" unit="character"/></rdg></app> 

    <lb n="12"/>ὥσπερ νῦν Πολύδωρος γράφει σοι ὅτι τὰ ἐν τῶι <app type="editorial"><lem><num value="10">ι</num> <expan><ex>ἔτει</ex></expan> γι<supplied reason="lost" cert="low">νόμενα </supplied><gap reason="lost" extent="unknown" unit="character"/></lem><rdg>ι<gap reason="illegible" quantity="3" unit="character"/><gap reason="lost" extent="unknown" unit="character"/></rdg></app> 

    <lb n="13"/>ἔγραφόν σοι τὴν ἐπιστολὴν <app type="editorial"><lem>ἐλογευόμην</lem><rdg>επογε τομων<certainty match=".." locus="value"/></rdg></app> τὰ δ<gap reason="illegible" quantity="4" unit="character"/> 

    <lb n="14"/>ἐπεὶ σὺ γράφεις μοι ἐναντίως 
</ab></div><div n="1,FrB" type="textpart"><ab>

    <lb n="1"/><gap reason="lost" extent="unknown" unit="character"/>δου τὸ δια<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="2"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> τ</supplied>ῆι <num value="11">ια</num> διὰ τριῶν <num value="3"/> θυ<supplied reason="lost">ρῶν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="3"/><gap reason="lost" extent="unknown" unit="character"/>ιν καὶ τοὺς λαοὺς α<gap reason="lost" extent="unknown" unit="character"/> 
</ab></div></div>
