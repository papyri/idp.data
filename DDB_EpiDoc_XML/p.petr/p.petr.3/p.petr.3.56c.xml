         <div xml:lang="grc" type="edition" xml:space="preserve"><ab>

    <lb n="1"/>εἰς τὴν γῆν ὀρθῶς καὶ δικ<supplied reason="lost">αίως </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="2"/><gap reason="lost" quantity="4" unit="character"/><gap reason="illegible" quantity="4" unit="character"/>ν νοσφίσασθαι <gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="3"/>ἀλλʼ ἐάν τινα αἴσθωμαι κακο<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="4"/>τὰ σπέρματα ἀποστελε<supplied reason="lost">ῖν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="5"/>μετὰ φυλακῆς ἐπιμελῶς <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="6"/>ἡ περὶ τὴν κώμην γῆ π<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="7"/>κληρουχικὴ κατὰ τρό<supplied reason="lost">πον </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="8"/>ἀλλʼ ἑξῆς ποτιεῖν <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="9"/><gap reason="lost" quantity="2" unit="character"/> ἐξέσται φροντιει<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="10"/>καὶ ἀφέσεων καὶ γ<supplied reason="lost">ε</supplied>φ<supplied reason="lost">υρῶν</supplied> 

    <lb n="11"/>κατὰ τρόπον <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="12"/><gap reason="lost" quantity="2" unit="character"/>α καθαρθως<gap reason="lost" extent="unknown" unit="character"/> 
