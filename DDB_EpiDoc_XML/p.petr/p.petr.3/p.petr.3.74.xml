         <head n="7530" xml:lang="en">
            <date>III sac</date>
            <placeName>Gurob</placeName>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="A" type="textpart"><ab>

    <lb n="1"/>Φιλοκύδης Κερ<unclear>τ</unclear><gap reason="illegible" quantity="1" unit="character"/><unclear>μ</unclear>ου <add place="above">Ἀμφιπολίτης τῶν <gap reason="lost" extent="unknown" unit="character"/></add> Εὐμ<unclear>ή</unclear><supplied reason="lost">λωι </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="2"/>ὁμολογῶ ἐπικεχωρηκέν<supplied reason="lost">αι σοι τὸν</supplied> 

    <lb n="3"/>κλῆρον <add place="above">μου</add> πυρῶν ἀκινδύνων <gap reason="lost" quantity="7" unit="character"/> 

    <lb n="4"/>τεσσαράκοντα πέντε <num value="45"/> ἀ<supplied reason="lost">ρταβῶν</supplied> 

    <lb n="5"/>ἀπὸ τοῦ <num value="14">ιδ</num> <expan><ex>ἔτους</ex></expan> εἰς τὸ πέν<supplied reason="lost">τε καὶ δέκατον</supplied> 

    <lb n="6"/>ἐφʼ ὧι <del rend="erasure">κομισάμενος τὰς τ<supplied reason="lost">εσσαρά</supplied>

    <lb n="7" break="no"/>κοντα πέντε <num value="45"/></del> <add place="above">ἀρτάβας</add> τῶν πυρῶν ἀκινδύνων 

    <lb n="8"/>βεβαιώσω σοι ἕως <choice><reg>ἂν</reg><orig>ἂγ</orig></choice> κομίσωμ<supplied reason="lost">αι</supplied> 

    <lb n="9"/>τὰς <choice><reg>τεσσαράκοντα</reg><orig>τεσαράκοντα</orig></choice> πέντε <num value="45"/> ἀ<supplied reason="lost">ρτάβας</supplied> 

    <lb n="10"/><choice><reg>τῶν</reg><orig>τῶμ</orig></choice> πυρ<supplied reason="lost">ῶν</supplied>· <unclear>ἕω</unclear>ς δὲ τοῦ κομίσασθα<supplied reason="lost">ι</supplied> 

    <lb n="11"/>μὴ ἐγβα<supplied reason="lost">λεῖ</supplied>ν σε μή<unclear>δ</unclear><supplied reason="lost">ε</supplied> <gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" quantity="5" unit="character"/> 

    <lb n="12"/>μηθὲν <gap reason="lost" quantity="4" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="13"/>αὐτὸς <app type="editorial"><lem><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ἐὰν</supplied> 

    <lb n="14"/>δὲ μὴ</lem><rdg><gap reason="lost" extent="unknown" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="2" unit="character"/></rdg></app> β<supplied reason="lost">ε</supplied>βαιώσω ἀπ<supplied reason="lost">οτίνειν </supplied><app type="editorial"><lem><supplied reason="lost">δρα</supplied>

    <lb n="15" break="no"/>χμὰς</lem><rdg><gap reason="lost" quantity="3" unit="character"/>ας</rdg></app> <num value="500"><unclear>φ</unclear></num>. 
</ab></div><div n="B" type="textpart"><ab>

