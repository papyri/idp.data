         <head n="3345" xml:lang="en">
            <date>221 BC</date>
            <placeName>Magdola</placeName>
            <ref n="chr.mitt;;10" type="reprint-from">PLille2,21;MChr10</ref>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r" type="textpart"><ab>

    <lb n="1"/>βασιλεῖ Πτολεμαίωι χαίρειν Πτολεμαῖος, Μακεδὼν τ<gap reason="illegible" quantity="3" unit="character"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ἀδικοῦμαι</supplied> 

    <lb n="2"/>ὑπὸ Ὥρου ἰ<supplied reason="lost">σ</supplied>ιονόμου καὶ Θοτορταίου καὶ Παθει<gap reason="illegible" quantity="2" unit="character"/> <add place="above">καὶ <unclear>Π</unclear>αλου<unclear>τ</unclear><gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" extent="unknown" unit="character"/></add> <supplied reason="lost">κατοι</supplied>κούν<supplied reason="lost">των </supplied><gap reason="lost" extent="unknown" unit="character"/><gap reason="illegible" quantity="1" unit="character"/>ν<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> Ἀρσι</supplied>

    <lb n="3" break="no"/>νοίτου νο<supplied reason="lost">μοῦ</supplied>. τηρήσαντές με τῆι <num value="3">γ</num> το<supplied reason="lost">ῦ </supplied><gap reason="lost" extent="unknown" unit="character"/>ε<unclear>μι</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="4"/>ὅρκον ευ<gap reason="lost" extent="unknown" unit="character"/>ας<gap reason="lost" extent="unknown" unit="character"/><gap reason="illegible" quantity="1" unit="character"/>οτει<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="5"/>νεμομ<gap reason="lost" quantity="8" unit="character"/>ην συνδιώξαντες α<gap reason="lost" extent="unknown" unit="character"/>εχο<gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="6"/>πικοῖς <unclear>δι</unclear><gap reason="lost" extent="unknown" unit="character"/><gap reason="illegible" quantity="1" unit="character"/> ἐνευροκόπησαν ταῦτα <gap reason="lost" extent="unknown" unit="character"/><unclear>των</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="7"/>παραπορευ<supplied reason="lost">ο</supplied>μένοις τισὶν εἰς τὰ Ὀξόρυγχα ἔδωκαν <gap reason="lost" extent="unknown" unit="character"/><unclear>κ</unclear>όψαι τη<gap reason="lost" extent="unknown" unit="character"/><gap reason="illegible" quantity="1" unit="character"/>μν<unclear>ι</unclear><gap reason="illegible" quantity="1" unit="character"/>ν<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="8"/>ἐν τῶι πεδίωι· ὕστερον δὲ αὐτὴν εὕρομεν ὑπὸ τῶν <supplied reason="lost">κυνῶν καὶ τ</supplied>ῶν κοράκω<supplied reason="lost">ν διαβεβρ</supplied>ωμένην, 

    <lb n="9"/>ἣν καὶ τοῖς φυλακίταις καὶ ἄλλοις πλέοσιν ἐπεδείξα<unclear>μ</unclear><supplied reason="lost">εν. δέομ</supplied>αι οὖν σου, βασ<supplied reason="lost">ιλεῦ, εἴ σ</supplied>οι δοκεῖ, 

    <lb n="10"/>προστάξαι Διοφάνει τῶι στρατηγῶι γράψαι Μοσ<supplied reason="lost">χίωνι</supplied> τῶι ἐν <supplied reason="lost">Ὀ</supplied>ξορύγχοις ἐπι<supplied reason="lost">στάτ</supplied>ηι ἀποστεῖ

    <lb n="11" break="no"/>λαι τοὺς ἐγκεκλημένους ὑπό μου πρὸς αὐτόν, ἵνα ἐπισκέψηται περὶ το<supplied reason="lost">ύτων</supplied> καί, ἐὰν ἦι 

    <lb n="12"/>τὰ διὰ τῆς ἐντεύξεως ἀληθῆ, πραχθῆ<supplied reason="lost">ν</supplied>αι αὐτοὺς παραχρῆμα καὶ ἀποδοῦ<supplied reason="lost">ναί</supplied> μοι 

    <lb n="13"/>τ<supplied reason="lost">ὴ</supplied>ν προδεδηλωμένην τιμὴν. τούτου γὰρ γενομένου, βασιλεῦ, ἐπὶ σὲ κ<supplied reason="lost">ατα</supplied>φυγών, 

    <lb n="14"/>τ<supplied reason="lost">ὸν</supplied> κοινὸν εὐεργέτην καὶ βοηθόν, <supplied reason="lost">τεύ</supplied>ξομαι τῆς παρὰ σου βοηθείας. 

    <lb n="15"/>εὐτύχει. 

    <lb n="16"/><space extent="unknown" unit="character"/> 

    <lb n="16"/><handShift new="m2"/> Μοσχίωνι. <expan>μά<ex>λιστα</ex></expan> <expan>δι<ex>άλυσον</ex></expan> <supplied reason="lost">αὐτούς· εἰ δὲ μή, <expan>ἀπό<ex>στειλον</ex></expan></supplied> ὅπως ἐπὶ τοῦ <expan>κοινοδι<ex>κίου</ex></expan> <expan>δι<ex>ακριθῶσιν</ex></expan>. 

    <lb n="17"/><expan><ex>ἔτους</ex></expan> <num value="1">α</num>, Γορπιαίου <num value="28">κη</num>, Τῦβι <num value="12">ιβ</num>. 
</ab></div><div n="v" type="textpart"><ab>

    <lb n="1"/><expan><ex>ἔτους</ex></expan> <num value="1">α</num>, Γ<supplied reason="lost">ο</supplied>ρ<supplied reason="lost">πιαίου <num value="28">κη</num>, Τῦβι <num value="12">ιβ</num></supplied>. 

    <lb n="2"/>Πτολε<supplied reason="lost">μαῖος πρὸς ωρον ἰσιο</supplied>

    <lb n="3" break="no"/>νόμον καὶ τοὺς <gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> περὶ</supplied> 

    <lb n="4"/>ἱερείων. </ab></div></div>
