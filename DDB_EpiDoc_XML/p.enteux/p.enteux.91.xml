         <head n="3381" xml:lang="en">
            <date>221 BC</date>
            <placeName>Magdola</placeName>
            <ref type="reprint-from">PLille2,22</ref>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r" type="textpart"><ab>

    <lb n="1"/><supplied reason="lost">βα</supplied>σιλεῖ Πτολεμαίωι χαίρειν <gap reason="lost" quantity="12" unit="character"/><supplied reason="lost"> ἀδικο</supplied>ῦμαι ὑπὸ Αὐτοβούλου <expan><ex>ἑκατονταρούρου</ex></expan> τῆς <num value="3">γ</num> <supplied reason="lost">ἱππαρχίας </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="2"/><gap reason="lost" quantity="2" unit="character"/>ναχέως. συνεγδόμενος γὰρ <gap reason="lost" quantity="18" unit="character"/>ατρὸς τῆς γυναικὸς ἧι συνοικῶ κ<supplied reason="lost">ατὰ συγγραφὴν</supplied> 

    <lb n="3"/><supplied reason="lost">συ</supplied>νοικισίου τὴν κειμένην <gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="16" unit="character"/><supplied reason="lost"> δε</supplied>δωκότος εἰς τὸ αὑτοῦ μέρος πρ<supplied reason="lost">όβατα </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="4"/><supplied reason="lost">ἐπ</supplied>ίποκα καὶ τὰ <unclear>εἰ</unclear>ς τὴν <unclear>π</unclear><gap reason="illegible" quantity="2" unit="character"/>μ<gap reason="lost" quantity="13" unit="character"/><supplied reason="lost"> Σεμθέ</supplied>α τὸν ποιμένα ἐκέλευσε ἐγλεξα<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="5"/><gap reason="lost" quantity="16" unit="character"/>ω<gap reason="lost" quantity="1" unit="character"/> Ἡρακ<gap reason="lost" quantity="18" unit="character"/>ι τούτου κώμη<gap reason="illegible" quantity="1" unit="character"/> παρέδωκα Σεμ<supplied reason="lost">θε </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="6"/><gap reason="lost" extent="unknown" unit="character"/>τωι <gap reason="illegible" quantity="3" unit="character"/><supplied reason="lost">Αὐ</supplied>τοβουλ<gap reason="lost" quantity="5" unit="character"/><supplied reason="lost"> τῶ</supplied>ν νομῶν τ<unclear>η</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="7"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> παρ</supplied>ὰ τὸ μὴ ἰσχύειν με <supplied reason="lost">τ</supplied><unclear>ὰς</unclear> νομὰ<supplied reason="lost">ς </supplied><gap reason="lost" quantity="6" unit="character"/><supplied reason="lost"> ἔ</supplied>γραψε δέ μοι α<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="8"/><gap reason="lost" quantity="14" unit="character"/>κλήσειν κατὰ ταῦτα ἔδωκα δέ σοι καὶ πρ<supplied reason="lost">ότε</supplied>ρ<supplied reason="lost">ον ἔντευ</supplied>ξιν εἰς τὸ σὸ<supplied reason="lost">ν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="9"/><gap reason="lost" quantity="7" unit="character"/><supplied reason="lost"> κ</supplied>αταπλεῦσαί με εἰς τὴν πόλιν ἐπὶ τὴν παράστασιν τὴν 

    <lb n="10"/>γε<supplied reason="lost">νομ</supplied>ένην ἡμῶ<supplied reason="lost">ν </supplied><gap reason="lost" quantity="6" unit="character"/> 

    <lb n="11"/><gap reason="lost" quantity="7" unit="character"/>μενος δὲ ἠξίουν αὐτὸν ἀποδοῦναί μοι· ὃ δὲ οὐ προσέσχεν ἀλλα <gap reason="lost" quantity="2" unit="character"/> δικεῖν <gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="8" unit="character"/> 

    <lb n="12"/>δέομαι <supplied reason="lost">ο</supplied>ὖν σου, βασιλεῦ, προστάξαι Διοφάνει τῶι στρατηγῶι γράψαι Πύρρωι τῶι ἐπισ<supplied reason="lost">τάτη</supplied>ι ἀπ<supplied reason="lost">ο</supplied>σ

    <lb n="13" break="no"/>τεῖλαι Αὐτόβουλον καὶ Σεμθέα τὸν ποιμένα ἐπὶ Διοφάνην καί, ἐὰν ᾖ καθότι γράφω ἀληθῆ, ἐπαναγκάσαι 

    <lb n="14"/>αὐτὸν τὰ δίκαιά μοι ποιῆσαι. τούτου γὰρ γενομένου, ἔσομαι διὰ σέ, βασιλεῦ, τετευχὼς ὧν ἀξιῶ. 

    <lb n="15"/>εὐτύχει. 

    <lb n="16"/><handShift new="m2"/> Πύρρωι. μάλιστα μὲν διάλυσον αὐτούς· εἰ δὲ μή, <expan>ἀπ<ex>όστειλον</ex></expan> <expan>ὅπ<ex>ως</ex></expan> ἐπὶ τοῦ καθήκοντος κριτηρίου 

    <lb n="17"/>διακριθῶσιν. <expan><ex>ἔτους</ex></expan> <num value="1">α</num>, Γορπιαίου <num value="28">κη</num>, Τῦ<supplied reason="lost">βι</supplied> <num value="12">ιβ</num>. 
</ab></div><div n="v" type="textpart"><ab>

    <lb n="1"/><supplied reason="lost"><expan><ex>ἔτους</ex></expan> <num value="1">α</num>, Γορπιαίου <num value="28">κη</num>, Τῦβι <num value="12">ιβ</num></supplied>. 

    <lb n="2"/><gap reason="lost" extent="unknown" unit="character"/> <expan>πρ<ex>ὸς</ex></expan> Αὐτόβουλ<supplied reason="lost">ον </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="3"/><supplied reason="lost">περ</supplied>ὶ προβάτων. </ab></div></div>
