    <lb n="1"/><gap reason="lost" extent="unknown" unit="character"/> ὀρφανὸς, Θραῖ<supplied reason="lost">ξ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="2"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ἀδικοῦ</supplied>μαι ὑπὸ Ζ<supplied reason="lost">ωίλου </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="3"/><gap reason="lost" extent="unknown" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><unclear>α</unclear>μου ἔτι ζῶντο<unclear>ς</unclear> ἐ<unclear>λα</unclear>β<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="4"/><gap reason="lost" extent="unknown" unit="character"/> Πτολεμαίδα τὴν <gap reason="illegible" quantity="3" unit="character"/>τ<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="5"/><gap reason="lost" extent="unknown" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><unclear>υ</unclear>μ<gap reason="illegible" quantity="2" unit="character"/> <unclear>κ</unclear>αὶ καταφυτεύεσ<supplied reason="lost">θαι </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="6"/><gap reason="lost" extent="unknown" unit="character"/> <choice><reg>μυρίκινα</reg><orig>μυρύκινα</orig></choice> καὶ καρπιζομένου τοῦ πατρ<supplied reason="lost">ὸς </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="7"/><gap reason="lost" extent="unknown" unit="character"/>ς τῶν ξύλων ἕως ἔζη, καὶ μετὰ τὴν τελε<supplied reason="lost">υτὴν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="8"/><gap reason="lost" extent="unknown" unit="character"/>ευκότος μου καὶ καρπιζομένου μου ἕως τοῦ <num value="2"><unclear>β</unclear></num><gap reason="lost" quantity="1" unit="character"/><supplied reason="lost"> <expan><ex>ἔτους</ex></expan> </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="9"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> λ</supplied>αβόντος τὸν πατρικὸν κλῆρον ἐν τῶι <num value="26">κϛ</num> <expan><ex>ἔτει</ex></expan> <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="10"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> τ</supplied>ὴν κώμην ἐνποιεῖται τοῦ χώματος κ<unclear>α</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="11"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ἀ</supplied>ποφέρεται, βιαζόμενός με, <choice><reg>καταφρονῶν</reg><orig>καταφρονῶμ</orig></choice> <supplied reason="lost">μου </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="12"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> δέο</supplied>μαι οὖν σου, βασιλεῦ, εἴ σοι δοκεῖ, προστάξα<supplied reason="lost">ι </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="13"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> γραμ</supplied>ματεῖ γράψαι τῶι γεωμέτρηι ὧι καθήκει <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="14"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> κ</supplied>λῆρόν μου καὶ τὸν τοῦ Ζωίλου τοῦ ἐνποιουμ<supplied reason="lost">ένου </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="15"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ἀ</supplied>ποδοῦναι ἑκάστωι <add place="above">τὰ ὅρια</add> κατὰ τὴν ὑπάρχουσαν <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="16"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> τετ</supplied>άρτηι, καὶ ἐὰν φαίνηται ἐπιβ<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="17"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> πρ</supplied>ᾶξαι αὐτὸν τιμὴν ξύλων <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="18"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> τ</supplied>ριάκοντα <num value="30"/>. τούτου γὰρ γ<supplied reason="lost">ενομένου </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="19"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> τοῦ δι</supplied>καίου τετευχώς.  εὐ<supplied reason="lost">τύχει</supplied>. 

    <lb n="20"/><space extent="unknown" unit="character"/> 

