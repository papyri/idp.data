    <lb n="1"/>βασιλ<supplied reason="lost">εῖ</supplied> Πτολεμαίωι χαίρειν Αἰσχρίω<supplied reason="lost">ν </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ἀδι</supplied>

    <lb n="2" break="no"/>κοῦμα<supplied reason="lost">ι</supplied> ὑπὸ Ἡλιοδώρου. τοῦ γὰρ <num value="3">γ</num> <expan><ex>ἔτους</ex></expan>, <add place="above">μη<supplied reason="lost">νὸς</supplied></add> Π<supplied reason="lost" cert="low">αῦνι </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="3"/>ἱμάτιον οὐκ ἀπ<supplied reason="lost">ο</supplied>δίδωσιν οὔτε τῶι παι<unclear>δ</unclear><gap reason="lost" extent="unknown" unit="character"/> 

