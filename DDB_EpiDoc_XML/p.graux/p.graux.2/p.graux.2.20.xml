    <lb n="5" break="no"/>κοντα <choice><reg><expan>τέσσαρα<ex>ς</ex></expan></reg><orig><expan>τέσσαρε<ex>ς</ex></expan></orig></choice> <expan><ex>γίνονται</ex></expan> <expan><ex>δραχμαὶ</ex></expan> <num value="44">μδ</num> <expan><ex>ἡμιωβέλιον</ex></expan> <num value="1/2"/> <expan><ex>χαλκοῖ 2</ex></expan><num value="2"/> <expan>ὑικ<ex>ῆς</ex></expan> <expan><ex>δραχμὴν</ex></expan> <num value="1">α</num> 
<num value="1"><expan><ex>ὀβολὸν</ex></expan></num>  <expan>μαγ<ex>δωλοφυλάκων</ex></expan> <num value="3"><expan><ex>τριώβολον</ex></expan></num> <expan><ex>χαλκοῦς 2</ex></expan><num value="2"/> <expan>δεσμ<ex>οφυλακίας</ex></expan> <gap reason="illegible" quantity="1" unit="character"/> 

    <lb n="6"/><expan>ποτ<ex>αμοφυλακίδος</ex></expan> <num value="1"><expan><ex>ὀβολὸν</ex></expan></num> <expan><ex>χαλκοῦς 2</ex></expan><num value="2"/> <expan>φυλ<ex>άκων</ex></expan> <expan><ex>ὀβολοὺς</ex></expan> <num value="5">ε</num> <num value="1/2">𐅵</num> καὶ δ <gap reason="illegible" extent="unknown" unit="character"><desc>vestiges</desc></gap> <abbr>τ</abbr> <expan><ex>ὀβολοὺς</ex></expan> <num value="4">δ</num> <num value="1/2">𐅵</num> καὶ ἐ<supplied reason="lost">πι</supplied>μερισμοῦ 

    <lb n="7"/>ἀπόρων τοῦ αὐτοῦ ἔτους πρώτου καὶ εἰκοστοῦ 

    <lb n="8"/><expan>ἀργ<ex>υρίου</ex></expan> <expan><ex>δραχμὰς</ex></expan> δεκαοκτὼ <expan><ex>ἡμιωβέλιον</ex></expan> <num value="1/2"/> <expan><ex>γίνονται</ex></expan> <expan><ex>δραχμαὶ</ex></expan> <num value="18">ιη</num> <expan><ex>ἡμιωβέλιον</ex></expan> <num value="1/2"/> μὴ προσχρησάμενος 

    <lb n="9"/>ἑτέρῳ <expan>συμβόλ<ex>ῳ</ex></expan> μέχρι τῆς ἐνεστώσης ἡμέρας 

    <lb n="10"/>διὰ τὸ ἐμὲ ἀνειληφέναι τὴν ὅλην διαγραφὴν 

    <lb n="11"/>τοῦ ἔτους μηδενὸς ἄλλου γράμματ<unclear>ο</unclear>ς. </ab></div>
