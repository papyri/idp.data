         <head n="11308" xml:lang="en">
            <date>AD 155</date>
            <placeName>Arsinoe</placeName>
            <ref n="bgu;13;2270" type="reprint-from">P.Berl.Frisk. 1 col. 39 (P.Graux 3,30 col. 1 only)</ref>
            <ref type="reprint-in">cf. SB 16,13060 + BGU 13,2270 + P.Berl.Frisk 1 cols. 1-38 + P.Col. 2,1 recto 4</ref>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><!--3--><ab>

    <lb n="1"/>Δείῳ καὶ Σαβείνῳ <subst><add place="inline"><expan>γεγυμ<ex>νασιαρχηκόσι</ex></expan></add><del rend="corrected">γεγ<gap reason="illegible" quantity="1" unit="character"/>μ</del></subst> καὶ <expan>μετόχ<ex>οις</ex></expan> <subst><add place="inline"><expan>δημοσίο<ex>ις</ex></expan></add><del rend="corrected">δημ<unclear>ων</unclear>σιο</del></subst> <choice><reg>τραπεζίταις</reg><orig>τραπεζείταις</orig></choice>

    <lb n="2"/>ο<supplied reason="lost">ἱ </supplied><expan><supplied reason="lost">ὑπο</supplied>γεγρα<ex>μμένοι</ex></expan> δημόσιοι <subst><add place="inline">καὶ</add><del rend="corrected"><gap reason="illegible" quantity="1" unit="character"/>αι</del></subst> ἰδιωτικοὶ <expan>κτηνοτ<ex>ρόφοι</ex></expan> κώμης Ναρμούθεως διὰ

    <lb n="3"/>Τρύ<supplied reason="lost">φ</supplied>ωνος <expan>ἀποσυσταθ<ex>έντος</ex></expan>· ἀπέχομεν παρʼ <subst><add place="inline">ὑμῶν</add><del rend="corrected"><subst><add place="inline">ϋμων</add><del rend="corrected">αμων</del></subst></del></subst> τὰς ἐπισταλείσας <choice><reg>ἡμῖν</reg><orig>ἡμεῖν</orig></choice>

    <lb n="4"/>ὑπὸ <surplus>τε</surplus> Πτολεμαίου <expan>βασιλ<ex>ικοῦ</ex></expan> <expan>γρα<ex>μματέως</ex></expan> Πολέμωνος μερίδος <expan>διαδεχο<ex>μένου</ex></expan> καὶ <supplied reason="omitted">τὰ</supplied> κατὰ τὴν

    <lb n="5"/><expan>στρα<ex>τηγίαν</ex></expan> Θεμίστου καὶ Πολέμωνος μερίδων, ἃς ᾐτησάμεθα ἐπισταλῆναι,

    <lb n="6"/>ὑπὲρ φορέτρων πυροῦ καὶ κριθῆς ὧν κατήξαμεν ἀπὸ θησαυρῶν Πο

    <lb n="7" break="no"/>λέμωνος μερίδος εἰς τοὺς ὅρμους ἀπεργασίας <expan>γενή<ex>ματος</ex></expan> <num value="17">ιζ</num> <expan><ex>ἔτους</ex></expan> Ἀντωνίνου

    <lb n="8"/>Καίσαρος τοῦ κυρίου πυροῦ <expan><ex>ἀρτάβας</ex></expan> <num value="82">πβ</num>, κριθῆς <expan><ex>ἀρτάβας</ex></expan> <num value="6">ϛ</num> <num value="1/2">𐅵</num>, <choice><reg>τιμῆς</reg><orig>τειμῆς</orig></choice> ὡς τῆς <expan><ex>ἀρτάβης</ex></expan> <subst><add place="inline">τοῦ</add><del rend="corrected">το<gap reason="illegible" quantity="1" unit="character"/></del></subst>

    <lb n="9"/>μὲν πυροῦ ἐκ <expan><ex>δραχμῶν</ex></expan> <num value="8">η</num>, <expan><ex>δραχμὰς</ex></expan> <num value="656">χνϛ</num>, τῆς δὲ κριθῆς ὡ<unclear>ς</unclear> τῶν <num value="100">ρ</num> <expan><ex>ἀρταβῶν</ex></expan> <expan><ex>δραχμῶν</ex></expan> <num value="480">υπ</num>, <expan><ex>δραχμὰς</ex></expan> <num value="31">λα</num> <num value="1"><expan><ex>ὀβολὸν</ex></expan></num>,

    <lb n="10"/>τὰς ἐπὶ τὸ αὐτὸ ἀργυρίου δραχμὰς ἑξακοσίας <unclear>ὀ</unclear><supplied reason="lost">γδ</supplied><unclear>ο</unclear>ήκοντα ἑπτὰ <num value="1"><expan><ex>ὀβολὸν</ex></expan></num>, <expan><ex>γίνονται</ex></expan> <expan><ex>δραχμαὶ</ex></expan> <num value="687">χπζ</num> <num value="1"><expan><ex>ὀβολὸς</ex></expan></num>.

    <lb n="11"/>εἰσὶ δέ· Πεεῦς Μύσθου <expan><ex>ἀρτάβας</ex></expan> <num value="6">ϛ</num> <num value="1/6" rend="tick">ϛ</num> <expan>κριθ<ex>ῆς</ex></expan> <expan><ex>ἀρτάβης</ex></expan> <num value="1/2">𐅵</num> <num value="1/24" rend="tick">κδ</num>, Ὀν<supplied reason="lost">ν</supplied>ῶφρις καὶ <expan>Μύσθ<ex>ης</ex></expan> <expan><ex>ἀρτάβας</ex></expan> <num value="6">ϛ</num> <num value="1/2">𐅵</num> <num value="1/12" rend="tick">ιβ</num>

    <lb n="12"/><expan>κριθ<ex>ῆς</ex></expan> <expan><ex>ἀρτάβης</ex></expan> <num value="1/2">𐅵</num> <num value="1/24" rend="tick">κδ</num>, Ἥρων καὶ Πασίων <expan><ex>ἀρτάβας</ex></expan> <num value="6">ϛ</num> <num value="1/3" rend="tick">γ</num> <num value="1/8" rend="tick">η</num> <expan>κριθ<ex>ῆς</ex></expan> <expan><ex>ἀρτάβης</ex></expan> <supplied reason="lost"><num value="1/2">𐅵</num></supplied> <num value="1/24" rend="tick">κδ</num>, Π<unclear>ε</unclear><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="1" unit="character"/>ις καὶ Ἥρων <expan><ex>ἀρτάβας</ex></expan> <num value="6">ϛ</num> <num value="1/6" rend="tick">ϛ</num>

    <lb n="13"/><expan>κριθ<ex>ῆς</ex></expan> <expan><ex>ἀρτάβης</ex></expan> <num value="1/2">𐅵</num> <num value="1/24" rend="tick">κδ</num>, Αὐνῆς καὶ <choice><reg>Νεῖλος</reg><orig>Νῖλος</orig></choice> <expan><ex>ἀρτάβας</ex></expan> <num value="6">ϛ</num> <num value="1/2">𐅵</num> <num value="1/3" rend="tick">γ</num> <expan>κριθ<ex>ῆς</ex></expan> <expan><ex>ἀρτάβης</ex></expan> <supplied reason="lost"><num value="1/2">𐅵</num></supplied> <num value="1/24" rend="tick"><unclear>κδ</unclear></num>, Τεσενοῦφις <expan>Ἥρω<unclear>ν</unclear><ex>ος</ex></expan>

    <lb n="14"/><expan><ex>ἀρτάβας</ex></expan> <num value="6">ϛ</num> <num value="1/2">𐅵</num> <num value="1/3" rend="tick">γ</num> <num value="1/8" rend="tick">η</num> <expan>κριθ<ex>ῆς</ex></expan> <expan><ex>ἀρτάβης</ex></expan> <num value="1/2">𐅵</num> <num value="1/24" rend="tick">κδ</num>, Ἀκιᾶρις <expan>Πετερμούθ<ex>εως</ex></expan> <expan><ex>ἀρτάβας</ex></expan> <num value="6">ϛ</num> <num value="1/2">𐅵</num> <num value="1/12" rend="tick">ιβ</num> <expan>κρι<ex>θῆς</ex></expan> <expan><ex>ἀρτάβης</ex></expan> <num value="1/2">𐅵</num> <num value="1/24" rend="tick">κδ</num>,

    <lb n="15"/>Ἥρων Ἑριέως <expan><ex>ἀρτάβας</ex></expan> <num value="6">ϛ</num> <num value="1/2">𐅵</num> <num value="1/3" rend="tick">γ</num> <num value="1/8" rend="tick">η</num> <expan>κριθ<ex>ῆς</ex></expan> <expan><ex>ἀρτάβης</ex></expan> <num value="1/2">𐅵</num> <num value="1/24" rend="tick">κδ</num>, Σανσν<supplied reason="lost">εῦ</supplied>ς <expan>Μύσθ<ex>ου</ex></expan> <expan><ex>ἀρτάβας</ex></expan> <num value="6">ϛ</num> <num value="1/6" rend="tick">ϛ</num>

    <lb n="16"/><expan>κριθ<ex>ῆς</ex></expan> <expan><ex>ἀρτάβης</ex></expan> <num value="1/2">𐅵</num> <num value="1/24" rend="tick">κδ</num>, Ἥρων Ὀννώφρεως <expan><ex>ἀρτάβας</ex></expan> <num value="5">ε</num> <expan>κρι<supplied reason="lost">θ<ex>ῆς</ex></supplied></expan><supplied reason="lost"> <expan><ex>ἀρτάβης</ex></expan></supplied> <num value="1/2">𐅵</num> <num value="1/24" rend="tick">κδ</num>, <unclear>Ἥρ</unclear><supplied reason="lost">ω</supplied><unclear>ν</unclear> <expan>Κρονίω<ex>νος</ex></expan> <expan><ex>ἀρτάβας</ex></expan> <num value="6">ϛ</num> <num value="1/3" rend="tick">γ</num> <num value="1/8" rend="tick">η</num>

    <lb n="17"/><expan>κριθ<ex>ῆς</ex></expan> <expan><ex>ἀρτάβης</ex></expan> <num value="1/2">𐅵</num> <num value="1/24" rend="tick">κδ</num>, Ὀννῶφρις Ὀρσέως <expan><ex>ἀρτάβας</ex></expan> <num value="6">ϛ</num> <expan>κρι<supplied reason="lost">θ<ex>ῆς</ex></supplied></expan> <expan><ex cert="low">ἀρτάβης</ex></expan> <supplied reason="lost"><num value="1/2">𐅵</num></supplied> <num value="1/24" rend="tick">κδ</num>, <gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="3" unit="character"/><gap reason="illegible" quantity="3" unit="character"/>ς Ἑριέως

    <lb n="18"/><expan><ex>ἀρτάβας</ex></expan> <num value="2">β</num> <num value="1/12" rend="tick">ιβ</num>. <subst><add place="inline"><expan>ἰδιωτικ<ex>ῶν</ex></expan></add><del rend="corrected"><gap reason="illegible" quantity="1" unit="character"/>διωτικ</del></subst> <expan>ὁμ<ex>οίως</ex></expan>· Μύσθης Ἀβοίκεως <expan><ex>ἀρτάβην</ex></expan> <num value="1">α</num> <num value="1/2">𐅵</num> <num value="1/3" rend="tick">γ</num> <num value="1/12" rend="tick">ιβ</num>. <expan><ex>γίνονται</ex></expan> <unclear>α</unclear><supplied reason="lost">ἱ <expan>π<ex>ροκείμεναι</ex></expan></supplied> <expan>ἀκολ<ex>ούθως</ex></expan> τῷ <expan>ἐπιστάλ<ex>ματι</ex></expan>.

    <lb n="19"/>Τρύφων ὁ <expan>ἀποσυσταθ<ex>εὶς</ex></expan> ὑπὸ τῶν <expan>πρ<supplied reason="lost">ογεγρ</supplied>α<ex>μμένων</ex></expan> ἀ<supplied reason="lost">π</supplied><unclear>έ</unclear>χω τὰς <expan>πρ<unclear>ο</unclear>κ<ex>ειμένας</ex></expan>

    <lb n="20"/>ἀργυρίου δραχμὰς ἑξακοσίας ὀγηδο<supplied reason="lost">ή</supplied>κοντα ἑπ<supplied reason="lost">τὰ</supplied> <num value="1"><expan><ex>ὀβολὸς</ex></expan></num>, <expan><ex>γίνονται</ex></expan> <expan><ex>δραχμαὶ</ex></expan> <num value="687">χπζ</num> <num value="1"><expan><ex>ὀβολὸς</ex></expan></num>.

    <lb n="21"/><expan><ex>ἔτους</ex></expan> <num value="18">ιη</num> Αὐτοκράτορος Καίσαρος <subst><add place="inline">Τίτου</add><del rend="corrected">τ<gap reason="illegible" quantity="1" unit="character"/>του <certainty match=".." locus="value"/></del></subst> Αἰ<supplied reason="lost">λ</supplied>ίου Ἁδρ<supplied reason="lost">ι</supplied>ανοῦ Ἀντωνίνου

    <lb n="22"/>Σεβαστοῦ Εὐσεβοῦς Μεχεὶρ <num value="20">κ</num> <del rend="erasure"><num value="1">α</num></del>.
