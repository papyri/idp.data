    <lb n="26"/><num value="26">κϛ</num> <expan>ἀνέβ<ex>η</ex></expan> <expan>δ<ex>ακτύλους</ex></expan> <num value="7">ζ</num> 

    <lb n="27"/><expan>ὁ<ex>μοῦ</ex></expan> <expan>γί<ex>νονται</ex></expan> <expan>δάκ<ex>τυλοι</ex></expan> <num value="28">κη</num> 

