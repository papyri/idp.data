    <lb n="23"/><num value="23">κγ</num> <expan>ἀνέβ<ex>η</ex></expan> <expan>δ<ex>ακτύλους</ex></expan> <num value="7">ζ</num> 

    <lb n="24"/><num value="24">κδ</num> <expan>ἀνέβ<ex>η</ex></expan> <expan>δ<ex>ακτύλους</ex></expan> <num value="7">ζ</num> 

