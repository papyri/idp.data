         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r" type="textpart"><ab>

    <lb n="1"/><g type="stauros"/> ἀνάγκης γεναμένης <unclear>γέ</unclear>γραφ<supplied reason="lost">α π</supplied><unclear>ρὸς</unclear> τὴ<unclear>ν</unclear> <gap reason="lost" quantity="9" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><unclear>ε</unclear><gap reason="illegible" quantity="2" unit="character"/>ε<unclear>πι</unclear><gap reason="illegible" quantity="2" unit="character"/>κατελθ<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="2"/>καὶ καλῶς ποιήσ<unclear>ε</unclear><supplied reason="lost">τ</supplied>ε μὴ <unclear>κ</unclear><gap reason="lost" quantity="1" unit="character"/>τελ<gap reason="illegible" quantity="2" unit="character"/>τες<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="6" unit="character"/><supplied reason="lost"> παρ</supplied><unclear>α</unclear>κ<unclear>α</unclear>λῶ αὐτὴ<supplied reason="lost">ν μὴ</supplied> <unclear>κατελ</unclear>θεῖν ἀλλὰ 

    <lb n="3"/>καθείσαι <unclear>κ</unclear>αὶ φροντίσα<unclear>ι</unclear> <unclear>τ</unclear><gap reason="illegible" quantity="3" unit="character"/>ρ<gap reason="illegible" quantity="2" unit="character"/><unclear>μ</unclear><gap reason="illegible" quantity="1" unit="character"/><unclear>τ</unclear>ος σο<gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" quantity="3" unit="character"/><gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" quantity="2" unit="character"/><supplied reason="lost"> ἐ</supplied>θέλησεν ὁ θεὸς απ<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="3" unit="character"/><gap reason="illegible" quantity="3" unit="character"/><unclear>ση</unclear><gap reason="illegible" quantity="1" unit="character"/><unclear>εκ</unclear><gap reason="illegible" quantity="1" unit="character"/>λ<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="4"/>πρὸς τὸν κόμιτα Ἰωαννάκιν καὶ τοῦτο <gap reason="lost" quantity="4" unit="character"/>σαμ<gap reason="illegible" quantity="3" unit="character"/> τοῦ μὴ γενέσθαι <gap reason="lost" quantity="6" unit="character"/> ου<unclear>το</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="5"/><choice><reg>τὸ</reg><orig>τοὺ</orig></choice> δὲ χρυσίον τὸ ἀπαιτηθὲν καταξι<unclear>ώ</unclear><supplied reason="lost">σ</supplied>ατ<unclear>ε</unclear> <choice><reg>ἀποστ<supplied reason="omitted">ε</supplied>ῖλαί</reg><orig>ἀποστῖλαί</orig></choice> μοι <choice><reg>ἐπ<supplied reason="omitted">ε</supplied>ιδὴ</reg><orig>ἐπιδὴ</orig></choice> <gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="2" unit="character"/><supplied reason="lost"> ἐπ</supplied>α<unclear>χ</unclear>θῶς φέρε<unclear>τ</unclear><supplied reason="lost">α</supplied><unclear>ί</unclear> 

    <lb n="6"/>μοι ὁ μεγαλοπρεπέστατος κόμις Παῦλος, καὶ μὴ ἀναμείνητε ἀλ<unclear>λ</unclear>ʼ <unclear>ὅτ</unclear><supplied reason="lost">αν</supplied> κατὰ τὴν ὁδὸν 

    <lb n="7"/>ἀπαντήσῃ ὑμῖν ὁ γραμματηφόρος κατʼ αὐτῆς <hi rend="diaeresis">ὑ</hi>ποστρέφετε, τὸ δὲ <choice><reg>χρυσίον</reg><orig><unclear>χρυ</unclear>σινον</orig></choice> πέμψητε 

    <lb n="8"/><choice><reg>ἐπ<supplied reason="omitted">ε</supplied>ιδὴ</reg><orig>ἐπιδὴ</orig></choice> ὡς <expan>εἴρητ<ex>αι</ex></expan> πάνυ <choice><reg>ἀναγκάζομαι</reg><orig>ἀναγκάζωμαι</orig></choice> περὶ <choice><reg>ἀπογραφῶν</reg><orig>ἀπογραφο͂ν</orig></choice> ω<gap reason="illegible" quantity="1" unit="character"/>δε<unclear>τ</unclear><gap reason="lost" extent="unknown" unit="character"/><unclear>α</unclear><gap reason="illegible" quantity="2" unit="character"/> ἐποίησες κ<gap reason="illegible" quantity="2" unit="character"/> τοπω<add place="above">ς</add> 

    <lb n="9"/>ἢ διαγώγησον περὶ τῶν ἐκεῖσε καὶ τὸ τί θέλεις <hi rend="diaeresis">ἵ</hi>να ποιήσω, <supplied reason="lost">ἢ</supplied> ἀναμείνω ὧδε 

    <lb n="10"/>ἢ ἀνέλθω εὐθέως, <choice><reg>ἐπ<supplied reason="omitted">ε</supplied>ιδὴ</reg><orig>ἐπιδὴ</orig></choice> <choice><reg>οὐδεμία</reg><orig>οὐτεμία</orig></choice> <unclear>ἀπ</unclear>όκρισις ἕως ἄρτι ἦλ<supplied reason="lost">θε</supplied>ν περὶ τοῦ 

    <lb n="11"/>ὑπερφυεστάτου στρατηλάτου <g type="chirho"/> 
</ab></div><div n="v" type="textpart"><ab>

