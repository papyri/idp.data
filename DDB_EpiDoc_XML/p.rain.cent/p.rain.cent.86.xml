         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r" type="textpart"><ab>

    <lb n="1"/>ὑπατείας <choice><reg>Φλαουίω<supplied reason="omitted">ν</supplied></reg><orig>Φλα<unclear>υ</unclear><hi rend="diaeresis">ι</hi>ω</orig></choice> <choice><reg>Εὐχαιρίου</reg><orig>Εὐχαιργίου</orig></choice> κ<unclear>α</unclear>ὶ Συαγρίου <add place="above"><unclear>τ</unclear>οῦ</add> λαμπρο

    <lb n="2" break="no"/>τάτου ἐπάρχου τοῦ <choice><reg>ἱεροῦ</reg><orig>εἱεροῦ</orig></choice> <choice><reg>πραιτωρίου</reg><orig>πρετωρίου</orig></choice> <g type="slanting-stroke"/> <g type="slanting-stroke"/> Φαμεν<unclear>ὼθ</unclear> <num value="1">α</num>. 

    <lb n="3"/><choice><reg>Αὐρήλιος</reg><orig>Αὐρύλιος</orig></choice> Κεφάλων Θεοδώρου <choice><reg><expan>μη<ex>τρὸς</ex></expan></reg><orig><expan>μυ<ex>τρὸς</ex></expan></orig></choice> <choice><reg>Σοφία<supplied reason="omitted">ς</supplied></reg><orig>Σοφία</orig></choice> <expan>διάκον<ex>ος</ex></expan> 

    <lb n="4"/><choice><reg>καθολικῆς</reg><orig>καθωλικῆς</orig></choice> <choice><reg>ἐκ<supplied reason="lost">κλ</supplied>ησία<supplied reason="omitted">ς</supplied></reg><orig>ἐκ<supplied reason="lost">κλ</supplied>ησια</orig></choice>ἀπὸ κώμης Ταμωρῶ τοῦ 
<choice><reg>Ἡρακλε<lb n="5" break="no"/>οπολίτου</reg><orig>Ὑρακλε<lb n="5" break="no"/>ουπολίτου</orig></choice> νομοῦ <choice><reg>Αὐρηλίῳ</reg><orig>Αὐρήλιος</orig></choice> <choice><reg>Λεοντίῳ</reg><orig>Λεωντίου</orig></choice> κεκωστ<unclear>αρηπ</unclear>α

    <lb n="6" break="no"/>των τὴν <choice><reg>τέχνην</reg><orig>τεχυνυν</orig></choice> <choice><reg>ἀφʼ</reg><orig>ἀπφ</orig></choice> <choice><reg>Ἡρακλέους</reg><orig>Ὑρακλέους</orig></choice> πόλεως χαίρειν. 

    <lb n="7"/><choice><reg>ὁμολογῶ</reg><orig>ὁμωλογῶ</orig></choice> ἐσχηκέναι παρὰ σοῦ εἰς ἰδίαν μου καὶ <unclear>ἀ</unclear>
<choice><reg>ν<lb n="8" break="no"/>αγκαίαν</reg><orig><unclear>α</unclear>ννανκεαν</orig></choice> <choice><reg>χρείαν</reg><orig>χρίαν</orig></choice> διὰ <choice><reg>χειρὸς</reg><orig>χιρὸς</orig></choice> <choice><reg>ἐξ</reg><orig>ἐκξ</orig></choice> <choice><reg>οἴκου</reg><orig>οἴκω</orig></choice> σου χρυσοῦ 

    <lb n="9"/><choice><reg>διζῴδια</reg><orig>δισῴτια</orig></choice> ἁπ<unclear>λ</unclear>ᾶ <g type="slanting-stroke"/> νομισμάτια πέντε, <expan><ex>γίνεται</ex></expan> <expan>νο<ex>μισμάτια</ex></expan> <num value="5">ε</num> 

    <lb n="10"/><choice><reg>κεφαλαίου</reg><orig>κεφαλέου</orig></choice> καὶ ἐφʼ ᾧτε <choice><reg>μὲ</reg><orig>μαὶ</orig></choice> <choice><reg>παρασχεῖν</reg><orig>παρασχῖν</orig></choice> σοι ἀντὶ λό

    <lb n="11" break="no"/>γου ἐπικερδίας κατὰ <choice><reg>μῆνα</reg><orig>μοιναν</orig></choice> <choice><reg>ἕκαστον</reg><orig>ἕκαστων</orig></choice> σιππί

    <lb n="12" break="no"/>ου <choice><reg>πενταμναῖα</reg><orig>πενταμνιεα</orig></choice> δεσμ<supplied reason="lost">ίδι</supplied><unclear>α</unclear> <unclear>π</unclear>έ<unclear>ντ</unclear>ε <num value="5"/> <unclear>καὶ</unclear> 

    <lb n="13"/>ἀπὸ τοῦ ὄντος <choice><reg>μηνὸς</reg><orig>μοινὸς</orig></choice> Φα<unclear>μ</unclear><supplied reason="lost">ε</supplied><unclear>ν</unclear>ὼθ τοῦ <choice><reg>ἐνεστῶτος</reg><orig>ἐνεστῶτους</orig></choice> 

    <lb n="14"/>ἔτους <hi rend="diaeresis">ἰ</hi>νδικτίωνος <choice><reg>ὑ<supplied reason="lost">πο</supplied>τίθημί</reg><orig>ὑ<supplied reason="lost">πο</supplied><unclear>τ</unclear>ίθυμί</orig></choice> σοι ἐνέχ<unclear>υρον</unclear> 

    <lb n="15"/><choice><reg>ἀσήμου</reg><orig>ἀσύμου</orig></choice> <choice><reg>καύκιον</reg><orig>κα<unclear>υκ</unclear>ιν</orig></choice> καὶ <unclear>ξ</unclear>ε<unclear>σ</unclear><supplied reason="lost">τ</supplied><gap reason="lost" quantity="2" unit="character"/><gap reason="illegible" quantity="2" unit="character"/>ς λίτρας <gap reason="lost" quantity="1" unit="character"/> 

    <lb n="16"/>ἅπερ ἀπ<unclear>ο</unclear><supplied reason="lost">δώσω σοι </supplied><gap reason="lost" extent="unknown" unit="character"/> 

