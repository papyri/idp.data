    <lb n="9"/><gap reason="illegible" extent="unknown" unit="character"><desc>vestiges</desc></gap> θέντων ναυ<gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="10"/><gap reason="lost" extent="unknown" unit="character"/> 

