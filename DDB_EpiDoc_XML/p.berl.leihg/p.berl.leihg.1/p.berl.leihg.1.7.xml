    <lb n="16"/>Παποντῶς καὶ <choice><reg>Πανοῦφις</reg><orig><unclear>Π</unclear><supplied reason="lost">α</supplied><unclear>ν</unclear>ούφεως</orig></choice> 

    <lb n="17"/><expan>ἀμφότ<ex>εροι</ex></expan> Ἀμά<hi rend="diaeresis">ι</hi><unclear>τ</unclear><supplied reason="lost">ο</supplied><unclear>ς</unclear> <expan><ex>ἄρουρα</ex></expan> <num value="1">α</num> <num value="1/2">𐅵</num> <num value="1/8" rend="tick">η</num> <num value="1/16" rend="tick">ιϛ</num> 

