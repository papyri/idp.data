    <lb n="21"/><choice><reg>δίδω</reg><orig>διδο</orig></choice><expan><ex>μι</ex></expan> <abbr>Ἁρποχ</abbr> Βάσσου <expan><ex>ἄρουραι</ex></expan> <num value="2">β</num> <num value="3/4">𐅸</num> 

