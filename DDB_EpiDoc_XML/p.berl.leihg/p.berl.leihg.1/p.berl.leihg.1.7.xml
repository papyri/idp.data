    <lb n="53"/>καὶ ἀντὶ Πελῶ<hi rend="diaeresis">ι</hi>ς <expan>Ἀνουβίωνο<ex>ς</ex></expan> <expan><ex>ἄρουραι</ex></expan> <num value="3">γ</num> <num value="1/2">𐅵</num> 

