    <lb n="5"/>πυροῦ <expan><ex>ἀρτάβης</ex></expan> <num value="7">ζ</num> <num value="1/20"><hi rend="supraline">κ</hi></num> <num value="1/120"><hi rend="supraline">ρκ</hi></num>, <choice><reg><expan>κριθ<ex>ῆς</ex></expan></reg><orig><expan>κριιθ<ex>ῆς</ex></expan></orig></choice> <expan><ex>ἀρτάβας</ex></expan> <num value="5">ε</num> <num value="1/10"><hi rend="supraline">ι</hi></num> <num value="1/300"><hi rend="supraline">τ</hi></num> διὰ γεωργοῦ Πεκύσεω<supplied reason="lost">ς</supplied> 

