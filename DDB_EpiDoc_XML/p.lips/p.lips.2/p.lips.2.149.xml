         <head n="78448" xml:lang="en">
            <date>AD 199</date>
            <placeName>Oxyrhynchus</placeName>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="1" type="textpart"><ab>

    <lb n="1"/>ἔτους ἑβδόμου αὐτοκρατόρων <unclear>Κ</unclear>αι<supplied reason="lost">σά</supplied><unclear>ρ</unclear>ων Λουκίου Σεπτιμίο<supplied reason="lost">υ 
Σεουήρου Εὐσεβοῦς Περτίνακος Ἀραβικοῦ Ἀδιαβηνικοῦ Παρθικοῦ 
Μεγίστου καὶ</supplied>

    <lb n="2"/>Μάρκου Αὐρηλίου Ἀντωνίνου Σεβα<unclear>στῶν</unclear> <unclear>Π</unclear>α<unclear>ῦνι</unclear> 
<num value="28"><unclear>κη</unclear></num> <supplied reason="lost">ἐ</supplied><unclear>ν</unclear> Ὀξυρύγχων π<supplied reason="lost">όλει τῆς Θηβαίδος, ἀγαθῇ τύχῃ.</supplied>

    <lb n="3"/>τὰδε διέθετο νοοῦσα καὶ φρονοῦσα <unclear>Α</unclear><gap reason="illegible" quantity="3" unit="character"/><gap reason="lost" quantity="4" unit="character"/> 
ἀπ<unclear>ελ</unclear>ευ<unclear>θ</unclear>έρα Τααμοίτος <gap reason="lost" quantity="65" unit="character"/><supplied reason="lost"> μετὰ κυρίου </supplied><gap reason="lost" extent="unknown" unit="character"/>

    <lb n="4"/>μητρὸς Σαραεῦτος ἀπὸ τῆς αὐτῆς πόλεως, ὅνπ<unclear>ε</unclear><supplied reason="lost">ρ</supplied> ᾕ<unclear>ρ</unclear>ηται καὶ συν<unclear>ε</unclear><gap reason="lost" quantity="65" unit="character"/>

    <lb n="5"/>διὰ Διονυσίου διαδόχου ἀκολούθως ᾧ <unclear>ἐ</unclear>πέδωκε <supplied reason="lost">αὐτ</supplied><unclear>ῷ</unclear> βιβλιδίῳ 
<unclear>ο</unclear><gap reason="lost" quantity="55" unit="character"/><supplied reason="lost"> τοῖς τοῦ ἀ</supplied>

    <lb n="6" break="no"/>γορανομ<unclear>ε</unclear>ίου ἐπιτηρηταῖς ἐν ἀγυιᾷ· <del rend="erasure">δι</del> ἐφʼ ὃν μὲν 
περί<unclear>ε</unclear><supplied reason="lost">ιμ</supplied><unclear>ι</unclear> χρόνον ἔχειν με τ<supplied reason="lost">ὴν τῶν ἰδίων ἐξουσίαν ὃ ἐὰν βούλωμαι 
ἐπιτελεῖν καὶ μεταδιατίθεσθαι καὶ ἀκυροῦν</supplied>

    <lb n="7"/>τήνδε τὴν διαθήκην, <unclear>ὃ</unclear> <unclear>δ</unclear>ʼ <supplied reason="lost">ἂν</supplied> <unclear>ἐ</unclear>πιτελ<supplied reason="lost">έσ</supplied><unclear>ω</unclear> <unclear>κύ</unclear><supplied reason="lost">ρ</supplied>ιον ὑπάρχειν. 
ἐ<supplied reason="lost">ὰν δʼ ἐπὶ τῇδε τῇ διαθήκῃ τελευτήσω κληρονόμους ἀπολείπω</supplied>

    <lb n="8"/>τόν τε υἱόν μου Ἄγαθον τὸν καὶ <unclear>Ἀγ</unclear>α<unclear>θει</unclear><gap reason="lost" quantity="2" unit="character"/><unclear>ν</unclear> 
Θέεω<supplied reason="lost">νος</supplied> <unclear>τ</unclear>οῦ καὶ Διογένους <supplied reason="lost">καὶ </supplied><gap reason="lost" quantity="60" unit="character"/>

    <lb n="9"/>ἀμφοτέρους ἀπὸ τῆς αὐτῆς πόλεω<supplied reason="lost">ς</supplied>, <unclear>ἑκά</unclear>τ<unclear>ε</unclear>ρο<unclear>ν</unclear> <supplied reason="lost">δὲ</supplied> <unclear>α</unclear>ὐ<supplied reason="lost">τ</supplied>ῶν 
ἐὰν ζῇ, ε<unclear>ἰ</unclear> <supplied reason="lost">δὲ μὴ </supplied><gap reason="lost" quantity="50" unit="character"/><supplied reason="lost"> τῆς ὑπαρχού</supplied>

    <lb n="10" break="no"/>σης μοι ἐπʼ <choice><reg>ἀμφόδου</reg><orig>ἀνφόδου</orig></choice> Μη<supplied reason="lost">τρῴου</supplied> οἰ<unclear>κ</unclear><supplied reason="lost">ί</supplied><unclear>α</unclear><supplied reason="lost">ς</supplied> διπυρ<unclear>γ</unclear><supplied reason="lost">ίας κα</supplied><unclear>ὶ</unclear> <unclear>α</unclear>ἰ<unclear>θρ</unclear><supplied reason="lost">ίο</supplied>υ <unclear>ὑ</unclear>φʼ ἣ<unclear>ν</unclear> <supplied reason="lost">κατάγειον 
</supplied><gap reason="lost" quantity="55" unit="character"/>

    <lb n="11"/>τον τοῦ τῆς αὐτῆς οἰ<supplied reason="lost">κία</supplied><unclear>ς</unclear> κατ<gap reason="illegible" quantity="2" unit="character"/>ο<unclear>υ</unclear><gap reason="lost" quantity="1" unit="character"/>τη<unclear>ν</unclear> <gap reason="lost" atLeast="5" atMost="6" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><unclear>ο</unclear>λειτην ευπο<unclear>ρ</unclear><gap reason="lost" extent="unknown" unit="character"/>

    <lb n="12"/>τῆς αὐτῆς οἰκίας καὶ ἀπὸ τῶν ὑπαρχ<supplied reason="lost">όντω</supplied><unclear>ν</unclear> μ<unclear>ο</unclear><supplied reason="lost">ι πε</supplied>ρὶ κώμην 
<unclear>Πα</unclear><gap reason="lost" extent="unknown" unit="character"/>

    <lb n="13"/>το<unclear>υ</unclear><supplied reason="lost">ς</supplied>, ἀμφοτέρους δὲ κοι<supplied reason="lost">ν</supplied><unclear>ῶς</unclear> ἐξ ἴ<unclear>σ</unclear><supplied reason="lost">ου</supplied> τοῦ <gap reason="illegible" quantity="2" unit="character"/>αρ<gap reason="illegible" quantity="3" unit="character"/><gap reason="lost" atLeast="1" atMost="2" unit="character"/><unclear>ι</unclear> αὐτῆς καὶ τῶν <gap reason="lost" quantity="60" unit="character"/><supplied reason="lost"> καὶ</supplied>

    <lb n="14"/>τῶν ἄλλων ὑπʼ ἐμοῦ ἀπολει<unclear>φθ</unclear><supplied reason="lost">η</supplied>σο<unclear>μ</unclear><supplied reason="lost">έ</supplied><unclear>ν</unclear>ων <supplied reason="lost">καθʼ ὁνδη</supplied>ποτοῦν 
τρόπ<supplied reason="lost">ον παντοίων πάντων </supplied><gap reason="lost" quantity="50" unit="character"/>

    <lb n="15"/>βούλομαι ὑπὸ τ<unclear>ῶ</unclear>ν π<unclear>ρο</unclear>δηλουμένων μου κ<unclear>λ</unclear><supplied reason="lost">ηρονό</supplied>μων κοινῶς 
<unclear>κ</unclear><gap reason="lost" extent="unknown" unit="character"/>

    <lb n="16"/>ἀπὸ τῆς αὐτῆς πόλεως ἐὰν ἀνθρώπινόν τι <supplied reason="lost">πάθῃ,</supplied> ἐπακολουθησα<gap reason="lost" extent="unknown" unit="character"/>

    <lb n="17"/>μ<unclear>οι</unclear>ς <unclear>μου</unclear> π<supplied reason="lost">ε</supplied><unclear>ρὶ</unclear> <unclear>ο</unclear>ὐδενὸς ἁπλῶς τῷ κα<supplied reason="lost">θόλ</supplied>ου <supplied reason="lost">παρευρέσ</supplied><unclear>ει</unclear> οὐδεμιᾷ <unclear>δ</unclear><gap reason="lost" quantity="65" unit="character"/>

    <lb n="18"/>με περιγράφ<supplied reason="lost">ει</supplied><unclear>ν</unclear>, παρὰ δὲ ταῦτα μ<unclear>η</unclear><supplied reason="lost">δε</supplied>νὶ ἐξ<unclear>ό</unclear><supplied reason="lost">ντος</supplied> παραβαίνειν, <unclear>τ</unclear><supplied reason="lost">ὸν 
δὲ παραβησόμενον ἐκτίνειν τῷ ἐμμένοντι τό τε βλάβος καὶ </supplied><gap reason="lost" quantity="15" unit="character"/>

    <lb n="19"/>εἰ<unclear>ς</unclear> <supplied reason="lost">ἐπ</supplied><unclear>ι</unclear><supplied reason="lost">τί</supplied>μου λόγον ἀ<supplied reason="lost">ργυ</supplied><unclear>ρ</unclear>ίου <unclear>δρ</unclear>αχμ<supplied reason="lost">ὰς</supplied> δι<unclear>σχει</unclear>λίας <num value="2000"/> καὶ

    <lb n="20"/>εἰς τὸ <gap reason="illegible" quantity="1" unit="character"/>δ<supplied reason="lost">ημόσιον τὰς ἴσας </supplied><gap reason="lost" quantity="50" unit="character"/>

    <lb n="21"/><gap reason="lost" quantity="45" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="lost" extent="unknown" unit="character"/>
