<app type="editorial"><lem resp="BL 1.205"><unclear>κα</unclear><supplied reason="lost">ὶ Μ</supplied>αξιμι<unclear>αν</unclear><supplied reason="lost">ο</supplied>ῦ</lem><rdg><unclear>κα</unclear><supplied reason="lost">ὶ </supplied><gap reason="lost" quantity="2" unit="character"/><supplied reason="lost"> Μ</supplied>αξιμι<unclear>α</unclear><supplied reason="lost">νο</supplied>ῦ</rdg></app> 

