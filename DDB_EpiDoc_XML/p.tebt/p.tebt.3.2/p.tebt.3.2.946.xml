         <head n="7982" xml:lang="en">
            <date>III sac</date>
            <placeName>Tebtynis</placeName>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r" type="textpart"><ab>

    <lb n="1"/>Πάσιππος Φίλωνι χαίρειν. ε<supplied reason="lost">ἰ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="2"/>τοῖς θεοῖς πολλὴν χάριν ἔχ<supplied reason="lost">ω </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="3"/>παρό<unclear>ντ</unclear>ι ἐνταῦθα μὴ ἀπο<supplied reason="lost" cert="low">δεδωκέναι με </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="4"/>ἐμοῦ δὲ προσαναπεπληρω<supplied reason="lost">κότος </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="5"/>ἀρτάβας <num value="90">ϙ</num> ὥστʼ ἀπέχει<supplied reason="lost">ν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="6"/>Μεσορὴ καὶ ἔτι προέχειν <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="7"/>α<unclear>ποητ</unclear>ου ἀπέχοντος ὁμοίω<supplied reason="lost">ς </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="8"/>ἐληλύθην εἰς Διονυσιάδα κα<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="9"/><expan>χα<ex>λκοῦ</ex></expan> <expan><ex>δραχμ  </ex></expan> <num value="650">χν</num>, νυνὶ δὲ οὐθενὸς ἀτό<supplied reason="lost" cert="low">που </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="10"/>ἐπεὶ οὖν καὶ ὁ Λαμέδοντος <expan>κλ<ex>ῆρος</ex></expan> <unclear>ε</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="11"/>διὰ τὸ ἐπιδεῖσθαι σπέρματος <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="12"/>σπείρειν ὑπὸ <choice><reg>χεῖρα</reg><orig>χε͂ρα</orig></choice>. ἐὰν οὖν σοι <supplied reason="lost">φαίνηται </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="13"/>τῶι σησάμωι ὅπως μηκ<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="14"/>τὸ ἄχυρον οὐχ ὑπομεμενη<supplied reason="lost">κ </supplied><gap reason="lost" extent="unknown" unit="character"/> 
</ab></div><div n="v" type="textpart"><ab>

    <lb n="15"/>Φίλωνι </ab></div></div>
