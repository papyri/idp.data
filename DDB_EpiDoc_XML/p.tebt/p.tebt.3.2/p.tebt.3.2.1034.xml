    <lb n="16"/><unclear>ἐν</unclear> <unclear>τῶι</unclear> <num value="31">λα</num> <expan><ex>ἔτει</ex></expan> <gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="1" unit="character"/>, ἃς καὶ ἀναφέρεσθαι <expan>ἐμβε<ex>βλημένας</ex></expan> 

