    <lb n="1"/><gap reason="illegible" quantity="1" unit="line"/> 

    <lb n="2"/>ἐκ <gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="4" unit="character"/> διὰ 

