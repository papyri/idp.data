    <lb n="11"/>ἀνὰ <num value="3">γ</num>  <num value="227">σκζ</num> <num value="1/2">𐅵</num> <num value="1/4" rend="tick">δ</num>, <num value="61">ξα</num> <num value="1/2">𐅵</num> <expan>ἀν<ex>ὰ</ex></expan> <num value="2">β</num> <num value="1/2">𐅵</num> <expan><ex>ἀρτάβαι</ex></expan> <num value="153">ρνγ</num> <num value="1/2">𐅵</num> <num value="1/4" rend="tick">δ</num>, 

    <lb n="12"/><num value="7">ζ</num> <expan>ἀν<ex>ὰ</ex></expan> <num value="2">β</num>  <num value="14">ιδ</num>, 

