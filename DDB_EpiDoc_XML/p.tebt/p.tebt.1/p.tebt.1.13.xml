         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="v" type="textpart"><ab>

    <lb n="1"/>Μεγχῆς <expan>κω<ex>μο</ex>γρ<ex>αμματεὺς</ex></expan> <expan>Κερκ<ex>εο</ex>σί<ex>ρεως</ex></expan> τῆς <expan>Πο<ex>λέμωνος</ex></expan> <expan>με<ex>ρίδος</ex></expan> 

    <lb n="2"/>Πτολεμαίωι <expan>χαί<ex>ρειν</ex></expan>. τῆι <num value="16">ιϛ</num> 

    <lb n="3"/>τοῦ Ἐπεὶφ τοῦ <num value="3">γ</num> <expan><ex>ἔτους</ex></expan> <subst><add place="inline">ἐφοδεύοντός</add><del rend="corrected">εφοδευοντων</del></subst> μου 

    <lb n="4"/>σὺν Ὥρωι <expan>κω<ex>μάρχηι</ex></expan> καὶ Πατάνι καὶ 

    <lb n="5"/>ἄλλοις <expan>πρεσ<ex>βυτέροις</ex></expan> <subst><add place="inline">τῶν</add><del rend="corrected">γεω</del></subst> <expan>γεω<ex>ργῶν</ex></expan> <del rend="erasure">τὰ ἐν</del> περὶ τὴν 

    <lb n="6"/><subst><add place="inline">κώμην</add><del rend="corrected"><gap reason="illegible" quantity="1" unit="character"/>ωμην</del></subst> χωματικὰ ἔργα 

    <lb n="6"/><gap reason="illegible" quantity="1" unit="character"/><add place="above">ν</add><gap reason="illegible" quantity="1" unit="character"/><abbr>θ<unclear>ει</unclear><gap reason="illegible" quantity="1" unit="character"/><unclear>χθη</unclear></abbr> π<unclear>ρὸς</unclear> <gap reason="illegible" quantity="2" unit="character"/><unclear>ου</unclear> 

    <lb n="6a"/><del rend="erasure">ἐν τῶι <expan>Θε<ex>μίστου</ex></expan> <expan>λεγο<ex>μένωι</ex></expan> <expan>περιχώ<ex>ματι</ex></expan></del> 

    <lb n="7"/>καὶ γενομένων κατὰ τὸν ἐξαγωγὸν <unclear>τὴν</unclear> <expan>ἐ<unclear>πι</unclear>βο<ex>λὴν</ex></expan> 

    <lb n="7b"/><del rend="erasure"><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="3" unit="character"/></del> 

    <lb n="8"/>τοῦ <choice><reg>Σοκνεβτύνι<supplied reason="omitted">ος</supplied></reg><orig>Σοκνεβτυνι</orig></choice> <unclear>θε</unclear><supplied reason="lost">ο</supplied><unclear>ῦ</unclear> μεγάλου 

    <lb n="8a"/><subst><add place="inline">ὄντ<unclear>ων</unclear></add><del rend="corrected">οντ<unclear>ης</unclear><certainty match=".." locus="value"/></del></subst> <gap reason="illegible" quantity="3" unit="character"/> 

    <lb n="8b"/>ἀνὰ μέσον οὔ<unclear>σης</unclear> <subst><add place="inline">τῶν</add><del rend="corrected">της</del></subst> περὶ τ<supplied reason="lost">ὴ</supplied>ν <expan>κώ<ex>μην</ex></expan> <del rend="erasure">γῆς</del> πεδίων 

    <lb n="9"/><expan>περ<supplied reason="lost">ι</supplied>χώ<ex>ματος</ex></expan> <del rend="erasure">ἐν τῶι</del> <subst><add place="inline">εὕρομέν</add><del rend="corrected">ευρον</del></subst> τινας 

    <lb n="10"/>τ<unclear>ῶν</unclear> παρὰ <expan>Φιλ<unclear>ο</unclear>ναύ<ex>του</ex></expan> <del rend="erasure">ἐκ<gap reason="illegible" quantity="3" unit="character"/>αντας</del> <add place="above">τοῦ Λέοντος τῶν</add> <add place="above">ἐκ <expan>Βε<ex>ρενικίδος</ex></expan> <expan>Θε<ex>σμοφόρου</ex></expan> <expan>κα<ex>τοίκων</ex></expan> <expan>ἱπ<ex>πέων</ex></expan></add> <add place="above">ὑπ<unclear>ω</unclear>ρυχότας</add> 

    <lb n="10a"/><gap reason="illegible" quantity="4" unit="character"/><unclear>το</unclear>υ <unclear>δε</unclear><gap reason="illegible" quantity="2" unit="character"/> μέρος τοῦ <expan>δεδη<ex>λωμένου</ex></expan> <subst><add place="inline">ἐξαγωγοῦ</add><del rend="corrected"><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="4" unit="character"/></del></subst> 

    <lb n="11"/><del rend="erasure"><unclear>ἐκ</unclear> <unclear>τ</unclear><supplied reason="lost">οῦ αὐ</supplied>τοῦ χώματος τοῦ <expan>δεδη<ex>λωμένου</ex></expan> ἐξαγωγοῦ</del> 

    <lb n="12"/><gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" quantity="3" unit="character"/><supplied reason="lost"> τ</supplied><unclear>ῶ</unclear>ν <subst><add place="inline"><expan>χω<ex>μάτων</ex></expan></add><del rend="corrected">θεογο</del></subst> τοῦ <del rend="erasure"><expan>δεδη<ex>λωμένου</ex></expan></del> <expan>λεγο<ex>μένου</ex></expan> Θεμίστου <expan>περιχώ<ex>ματος</ex></expan> 

    <lb n="13"/>εἰ<supplied reason="lost">ς</supplied> σχοινία ὀκτὼ <num value="8"/> καὶ <subst><add place="inline">τὸν</add><del rend="corrected">των</del></subst> <subst><add place="inline">ἀπʼ</add><del rend="corrected"><unclear>τ</unclear><gap reason="illegible" quantity="2" unit="character"/></del></subst> αὐτοῦ 

    <lb n="14"/>χοῦν ἀναβεβλη<add place="above">κότας</add> εἰς <subst><add place="inline">τὰ</add><del rend="corrected"><gap reason="illegible" quantity="2" unit="character"/></del></subst> <add place="above">τοῦ</add> <expan>σημαινο<ex>μένου</ex></expan> <expan>Φιλ<unclear>ο</unclear>ναύ<ex>του</ex></expan> 

    <lb n="15"/><expan>κ<unclear>λή</unclear><ex>ρου</ex></expan> χώματα.  <subst><add place="inline">πάραυτα</add><del rend="corrected"><gap reason="illegible" quantity="2" unit="character"/>ραυτα</del></subst> δὲ <subst><add place="inline">συμψήσαντες</add><del rend="corrected">επιψησαντες</del></subst> 

    <lb n="16"/><unclear>ἀπ</unclear>ὸ τῶν <expan>προγεγρ<ex>αμμένων</ex></expan> ἕνα <num value="1"/> 

    <lb n="17"/><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="3" unit="character"/><unclear>ι</unclear> <unclear>ἐπ</unclear>εδώ<add place="above"><unclear>καμ</unclear>εν</add> Πολέμωνι τῶι διεξάγοντι τὰ κατ<supplied reason="lost">ὰ</supplied> <supplied reason="lost">τ</supplied>ὴν <expan>ἐπισ<ex>τατείαν</ex></expan> 

    <lb n="18"/><unclear>τ</unclear><supplied reason="lost">ῆς </supplied><expan><supplied reason="lost">κ</supplied><unclear>ώ</unclear><ex>μης</ex></expan> <gap reason="illegible" quantity="2" unit="character"/> <expan>καταστῆ<ex>σαι</ex></expan> ἐπὶ σὲ τοὺς δε<gap reason="lost" quantity="2" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><unclear>α</unclear>νους 

    <lb n="19"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="1" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><unclear>ρ</unclear><gap reason="lost" quantity="1" unit="character"/>τηκέναι.  διὸ ἀναφέρω ἵνʼ ἐὰν <expan>φαίνη<ex>ται</ex></expan> 

    <lb n="20"/><gap reason="lost" quantity="2" unit="character"/><unclear>τ</unclear><gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" quantity="1" unit="character"/><gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" quantity="1" unit="character"/>ηιε<unclear>τ</unclear>ι <add place="above">πρὸ πάντων τοῦ <gap reason="illegible" quantity="3" unit="character"/><abbr>υ</abbr></add> τ<unclear>ὰ</unclear> μὲν <expan>χώ<ex>ματα</ex></expan> <subst><add place="inline">ἀσφαλισθῆναι</add><del rend="corrected">ἀσφαλισθῆ<del rend="erasure"><gap reason="illegible" quantity="2" unit="character"/></del>ναι</del></subst> 

    <lb n="21"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="lost" quantity="1" unit="character"/><gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" quantity="1" unit="character"/><abbr><gap reason="illegible" quantity="1" unit="character"/></abbr> <unclear>τ</unclear><gap reason="lost" quantity="2" unit="character"/><gap reason="illegible" quantity="1" unit="character"/> <expan>χώ<ex>ματος</ex></expan> <expan>ἐπιβο<ex>λ  </ex></expan>, τὸν δὲ <expan>Φιλοναύ<ex>την</ex></expan> καὶ τοὺς 

    <lb n="22"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" quantity="2" unit="character"/><gap reason="illegible" quantity="4" unit="character"/> τοῦ <expan>χώ<ex>ματος</ex></expan> κατασταθέντας 

