    <lb n="38"/>πυροῦ μὲν <expan><ex>ἀρτάβαι</ex></expan> <num value="15">ιε</num> 

    <lb n="39"/>φασήλου <supplied reason="lost"><expan><ex>ἀρτάβαι</ex></expan></supplied> <num value="80">π</num>, 

    <lb n="40"/>ἀπ<unclear>ὸ</unclear> <gap reason="lost" quantity="3" unit="character"/> τούτων 

