         <head n="3660" xml:lang="en">
            <date>117 BC</date>
            <placeName>Tebtynis</placeName>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="1" type="textpart"><ab>

    <lb n="1"/>ἐλα<supplied reason="lost">σ</supplied>σόνων ἐκφορίων ἀπὸ τῆς ενα<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="2"/>τοῦ νομοῦ ἐφʼ ἧς καὶ ἱκανὰ κεφάλαια <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="3"/>ἀμπέλου τῆς ἐν σιτικῆι μισθώσει α<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="4"/>ωσθαι τοῦ τοιούτου μὴ ὄντος μέχρι <unclear>ν</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="5"/>κ<unclear>α</unclear><supplied reason="lost">ὶ</supplied> μηδαμῶς ὀπτανομένων υ<unclear>π</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="6"/><supplied reason="lost">το</supplied>ῦ Ἀφροδιτοπολίτου ἀποχωρησ<unclear>α</unclear>ν<supplied reason="lost">τ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="7"/>κα<supplied reason="lost">ὶ</supplied> <unclear>τῶ</unclear>ν ὑδάτων ἐπιβαλλόντων <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="8"/>διεπέμπ<unclear>ο</unclear>μεθα <choice><reg>μεταγαγόντες</reg><orig><subst><add place="inline">μεταγα<unclear>γ</unclear>ουντες</add><del rend="corrected">μεταγ<del rend="erasure"><gap reason="illegible" quantity="1" unit="character"/></del><unclear>γ</unclear>ουντες</del></subst></orig></choice> καὶ <unclear>τ</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="9"/><gap reason="illegible" quantity="3" unit="character"/> μ<unclear>ηδὲ</unclear> <gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="1" unit="character"/> ἁπλῶς τὰς χεῖρας ἐπιβ<supplied reason="lost">αλ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="10"/><unclear>συ</unclear><supplied reason="lost">ντε</supplied><unclear>τ</unclear>ριφ<unclear>έν</unclear>αι εἰς ὕστερον ὁπότε δ<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="11"/>στρα<supplied reason="lost">τη</supplied>γοῦ κατήλ<unclear>θο</unclear>σαν συστάντες <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="12"/>καὶ εἰς τὸ λογιστήριον παραγενομέ<supplied reason="lost">νων </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="13"/>γράμματα συνταξαμένων δὲ κα<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="14"/>ὑπερτιθεμένου τοῦ χρόνου συνθ<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="15"/>τοῦ ποταμοῦ ἐπιβαλλούσης το<unclear>τ</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="16"/>ἐπιβολῆς ἐκπληρώσαντες διξ<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="17"/><gap reason="lost" quantity="2" unit="character"/><gap reason="illegible" quantity="3" unit="character"/>αι.  ἐν δὲ τῶι ἐν<unclear>εσ</unclear>τῶτι ἔ<unclear>τ</unclear><supplied reason="lost">ει </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> προσ</supplied>

    <lb n="18" break="no"/><supplied reason="lost">πί</supplied><unclear>πτ</unclear>οντος περὶ τὸν Χοίαχ μῆνα <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="19"/><gap reason="lost" quantity="3" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><unclear>τι</unclear> <unclear>ικ</unclear><gap reason="lost" quantity="2" unit="character"/><unclear>ι</unclear>τ<unclear>ο</unclear>ν πράγματος λογου<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="20"/><gap reason="lost" quantity="6" unit="character"/><gap reason="illegible" quantity="3" unit="character"/><gap reason="lost" quantity="2" unit="character"/><gap reason="illegible" quantity="1" unit="character"/>ς ἐν τῶι νομῶ<unclear>ι</unclear><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="21"/><gap reason="lost" quantity="7" unit="character"/>ανο<unclear>ι</unclear> τοῦ ἔργου <unclear>α</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="22"/><gap reason="lost" quantity="3" unit="character"/><gap reason="illegible" quantity="1" unit="character"/> ἑτοῖμοι μηδεμιᾶς ἀναβολ<supplied reason="lost">ῆς </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="23"/><gap reason="lost" quantity="3" unit="character"/>ε <unclear>ἑ</unclear>τέρας ἐπιγνώσεως καὶ τοῦ <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="24"/><gap reason="lost" quantity="3" unit="character"/> τοῦ Χοίαχ εἴς τινα κώ<supplied reason="lost">μη</supplied>ν <gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ἀπέχουσαν ἀπὸ</supplied> 
</ab></div><div n="2" type="textpart"><ab>

    <lb n="25"/><supplied reason="lost">τ</supplied>ῆς μητροπόλεως στάδια δύο <num value="2"/> ὅπως ἐπισκέψηται ταύτην, οὐδʼ οὕτως 

    <lb n="26"/>ὑπήκουσαν οἰόμενοι ἐν τῶι ἐν <subst><add place="inline">τῇ</add><del rend="corrected">τ<gap reason="illegible" quantity="1" unit="character"/></del></subst> Κροκοδίλων <subst><add place="inline">πόλει</add><del rend="corrected">πολιι</del></subst> ἱερῶι γράψειν ἄνευ 

    <lb n="27"/>ἐπισκέψεως.  πρὸς τὸ καὶ ἐν τούτοις <subst><add place="inline">ἀπροφασίστους</add><del rend="corrected">απροφασιστοσς</del></subst> αὐτοὺς καταστῆσαι 

    <lb n="28"/>ὑπηκούσαμεν καὶ πρόγραμμα ἐξεθήκαμεν παραγείνεσθαι εἰς τὸ <expan>λογι<ex>στήριον</ex></expan> 

    <lb n="29"/>τῆι <num value="1">α</num> τοῦ Τῦβι ὅπως καταταχηθῆι τὰ τῆς πραγματείας ἐν τ<supplied reason="lost">ῶ</supplied>ι <supplied reason="lost">δ</supplied>έοντι 

    <lb n="30"/>καιρῶι.  οἱ δὲ συννοήσαντες <del rend="erasure"><unclear>ε</unclear></del> <choice><reg>τὸ<supplied reason="omitted">ν</supplied></reg><orig>το</orig></choice> ἐγβησόμενον κίνδυ<supplied reason="lost">ν</supplied>ον ἐκ <supplied reason="lost">τ</supplied>οῦ <app type="editorial"><lem resp="BL 1.422">προσ
    <lb n="31" break="no"/><supplied reason="lost">η</supplied><unclear>δ</unclear>ρευκέναι</lem><rdg>προσ<supplied reason="lost">ε</supplied><unclear>δ</unclear>ρευκέναι</rdg></app> ἡμᾶς ἐν τοῖς <del rend="erasure">ε<unclear>ν</unclear></del> κατὰ τὰ προσαγγέλματα τῆς βεβρεγμένης 

    <lb n="32"/><supplied reason="lost">κα</supplied><unclear>ὶ</unclear> <choice><reg><supplied reason="lost">ν</supplied>ομίζοντες</reg><orig><supplied reason="lost">ν</supplied>ομιζειν</orig></choice> <choice><reg>ἀνα<supplied reason="omitted">φ</supplied>θείρεσθαι</reg><orig>ἀνα<unclear>θ</unclear>είρεσθαι</orig></choice> τὰ ὑπʼ αὐτῶν <choice><reg>διεσκευασμένα</reg><orig>διεσκευασμενωνα</orig></choice>, οὕτως 

    <lb n="33"/>λήγοντες τῆς ἀ<add place="above">γ</add>νοίας, διαλαμβάνοντες εἰς ἀπραξίαν ἡμᾶς περιστήσειν 

    <lb n="34"/>ὁπότε δὴ ἐνάρξασθαι, ἀνεχώρησαν εἰς τὸν <subst><add place="inline">Ἡρακλεοπολίτην</add><del rend="corrected">ηρακλε<del rend="erasure">ι</del>οπολιτην</del></subst> · καὶ διότι 

    <lb n="35"/>διʼ ἄλλων προσανενηνόχαμεν μόλις διά τε τοῦ στρατηγ<supplied reason="lost">ο</supplied>ῦ καὶ χρη

    <lb n="36" break="no"/>ματιστῶν καὶ τῶν ἄλλων, ἐναρχομένου τ<supplied reason="lost">οῦ Με</supplied>χεὶρ α<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="1" unit="character"/><choice><reg cert="low"><supplied reason="lost" cert="low">ελη</supplied>λύθεσαν</reg><orig><gap reason="lost" quantity="4" unit="character"/>λυθησαν</orig></choice> 

    <lb n="37"/><choice><reg>ἐλεγχθέντες</reg><orig>ἐλεχθέντες</orig></choice> ἐν <unclear>τ</unclear><gap reason="lost" quantity="4" unit="character"/><unclear>ρ</unclear><gap reason="lost" quantity="5" unit="character"/><unclear>ρ</unclear><gap reason="lost" quantity="6" unit="character"/><unclear>δ</unclear>ιν<gap reason="illegible" quantity="1" unit="character"/> <gap reason="lost" quantity="3" unit="character"/><gap reason="illegible" quantity="1" unit="character"/>α τ<unclear>ῆ</unclear><supplied reason="lost">ς</supplied> <unclear>ἀ</unclear>φόρου πε<unclear>π</unclear><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="2" unit="character"/><gap reason="illegible" quantity="2" unit="character"/> 

    <lb n="38"/>τυγχά<unclear>ν</unclear>ουσι.  καὶ περὶ τὴν <num value="11">ια</num> τοῦ αὐτοῦ μηνὸς εἰς τὸ λογιστήριον 

    <lb n="39"/><choice><reg>παρεβεβλήκεσαν</reg><orig>παραβεβλήκησαν</orig></choice> ἔκ τε τῆς πλείστης προσεδρείας <choice><reg>ἤδη</reg><orig>ἤδηι</orig></choice> πρὸς τῶι <app type="editorial"><lem resp="BL 7.268"><unclear>συ</unclear><supplied reason="lost">γ</supplied>

    <lb n="40" break="no"/>κλείεσ<unclear>θα</unclear>ι</lem><rdg><unclear>συ</unclear><gap reason="lost" quantity="2" unit="character"/>κλειεσ<unclear>ω</unclear>ι</rdg></app> ταύτης οὔσης ἐπί τε <choice><reg cert="low">ὧν</reg><orig>των</orig></choice> <subst><add place="inline">ἐπεσκέμμεθα</add><del rend="corrected">επεσκεμμενα</del></subst> κωμῶν οὐκ ὀλίγω<supplied reason="lost">ν</supplied> 

    <lb n="41"/>ἐν προσδοκίαι <subst><add place="inline">ὄντων</add><del rend="corrected">προσδοκιανοντων</del></subst> περιέσεσθαι παρὰ τὸν ἐπιδεδομένον ὑπʼ αὐτῶν 

    <lb n="42"/>σπόρον, ἐν τῶι μεταξὺ Θέωνο<supplied reason="lost">ς τοῦ</supplied> προκεχειρισμένου ἐπὶ τὴν γεωμετρίαν 

    <lb n="43"/>τῶν ἀμπελώνων καὶ παραδείσων ἀποδόντος ἡμῖν καὶ ἐπεσταλμέ<supplied reason="lost">νου </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="44"/>περὶ τῶν κατʼ αὐτὸν <unclear>π</unclear><supplied reason="lost">ρονο</supplied>ούμενοι ἀκολούθως ταῖς <choice><reg>δεδομέναι<supplied reason="omitted">ς</supplied></reg><orig>δεδομεναι</orig></choice> διʼ α<supplied reason="lost">ὐ</supplied>

    <lb n="45" break="no"/>τῶν <choice><reg>διαστολα<supplied reason="omitted">ῖ</supplied>ς</reg><orig>διαστολᾶς</orig></choice> ἐκ<unclear>τ</unclear><gap reason="lost" quantity="11" unit="character"/><unclear>α</unclear>ι προσκ<unclear>αλεσ</unclear>άμε<unclear>νο</unclear>ι <gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="10" unit="character"/> 

    <lb n="46"/>καὶ περ<supplied reason="lost">ὶ</supplied> ἑκάσ<supplied reason="lost">του ὅσα</supplied> ἥρμοζεν ὑποδείξαντες <choice><reg>παρεκαλέσαμεν</reg><orig>παρακαλέσαμεν</orig></choice> ἑ<unclear>αυ</unclear><supplied reason="lost">τοὺς </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="47"/><supplied reason="lost">κ</supplied><unclear>α</unclear>τʼ αὐτ<supplied reason="lost">ῶν </supplied><gap reason="lost" quantity="9" unit="character"/> ἐπιδοῦναι ἀνελύσαμεν ὡς ἐξορμήσ<unclear>ο</unclear>ντ<supplied reason="lost">ες </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="48"/><gap reason="lost" quantity="2" unit="character"/><gap reason="illegible" quantity="1" unit="character"/> τῶι Θ<supplied reason="lost">έωνι </supplied><gap reason="lost" quantity="15" unit="character"/>του κατʼ <supplied reason="lost">αὐτ</supplied><unclear>ῶ</unclear>ν καταρτισ<unclear>ό</unclear>μεθα 

    <lb n="49"/>αὐ<supplied reason="lost">τὸ</supplied>ν καὶ <gap reason="lost" quantity="19" unit="character"/><subst><add place="inline"><gap reason="illegible" quantity="1" unit="character"/><unclear>ο</unclear>υτη</add><del rend="corrected"><gap reason="illegible" quantity="1" unit="character"/>ουτη</del></subst><gap reason="lost" quantity="7" unit="character"/> τῶν δὲ στα<unclear>λ</unclear>έν<supplied reason="lost">των </supplied><gap reason="lost" extent="unknown" unit="character"/> 
</ab></div><div n="3" type="textpart"><ab>

    <lb n="50"/>διαφόρων εἰσαγωγῆι καὶ ἐν τούτοις <choice><reg>ὅπερ</reg><orig>ὅπορ</orig></choice> εἰκὸς ἐστιν <choice><reg>διελάμβανον</reg><orig>διαλάμβανον</orig></choice> ἑαυτοῖ<supplied reason="lost">ς</supplied> 

    <lb n="51"/>συνιστοροῦντες τὰ πλήθη τῶν ὑποστελλομένων καθότι προσπ<supplied reason="lost">έ</supplied>πτω<supplied reason="lost">κε</supplied><unclear>ν</unclear> 

    <lb n="52"/><choice><reg>ἀπο<supplied reason="omitted">κε</supplied>χωρηκέναι</reg><orig>ἀποχωρηκέναι</orig></choice> τῆι <num value="12">ιβ</num> αὐτὸν τὸν ἐπικεφαλαίου σπόρον ἐπιδόντες, τὰς δὲ 

    <lb n="53"/>κατʼ ἄνδρα γραφὰς οὐδαμῶς προέμενοι ἵνα καὶ ταυθʼ ἃμα καὶ τοῖς ἄλλοις 

    <lb n="54"/>τοῖς <choice><reg>ὑπʼ</reg><orig>ὑπο</orig></choice> <choice><reg>αὐτῶν</reg><orig>των</orig></choice> κατὰ τοῦ <subst><add place="inline">συμφέροντος</add><del rend="corrected">συμφεροντας</del></subst> <choice><reg>διαπρασσομένοις</reg><orig>διαπρασσομνους</orig></choice> ἐμποδοστα

    <lb n="55" break="no"/>τήσαντες ὑπὸ διασκορπισμὸν τὰ τῆς φορολογίας ἀγάγωσιν, τοῦ 

    <lb n="56"/>ἀκμαιοτάτου καιροῦ τῆς ἀναγωγῆς τῶν γενημάτων ἐνεστηκότος. 

    <lb n="57"/>ἔξεστι δὲ τὴν περὶ αὐτοὺς ἔτι πρότερον μ<supplied reason="lost">ο</supplied>χθηρὰν ἀγωγὴν συν

    <lb n="58" break="no"/><subst><add place="inline">ιδεῖν</add><del rend="corrected">συν<del rend="erasure"><gap reason="illegible" quantity="4" unit="character"/></del>ιδειν</del></subst> ἐκ τῶν <choice><reg>ὑποδεικνυμένων</reg><orig>ὑποδικνυμένον</orig></choice> <add place="above"><del rend="erasure"><num value="161">ρξα</num> <num value="1/3" rend="tick">γ</num> <num value="1/12" rend="tick">ιβ</num></del></add> καὶ ὡς ἀλλο<unclear>ῖ</unclear><supplied reason="lost">ον τὸ κατ</supplied> αὐτοὺς ἐστιν 

    <lb n="59"/>τῆς διὰ τῶν νυνὶ παρὰ σοῦ γεγραμμένω<supplied reason="lost">ν περ</supplied><unclear>ὶ</unclear> τοῦ εἴδους γεινομένης 

    <lb n="60"/><choice><reg>φροντίδος</reg><orig>φροντιδα</orig></choice> ἵνα ἐπὶ τῶν τόπων ἐπίμονοι ὑπ<supplied reason="lost">άρχω</supplied>σιν τῶν γὰρ <choice><reg>πλεί<supplied reason="omitted">σ</supplied>των</reg><orig>πλείτων</orig></choice> 

    <lb n="61"/>αὐτῶν ἄνευ διοκητικῶν χρημα<supplied reason="lost">τισμῶν κα</supplied><unclear>θε</unclear>σταμένων 

