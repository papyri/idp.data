         <head n="3799" xml:lang="en">
            <date>-118/117 BC</date>
            <placeName>Tebtynis</placeName>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="1" type="textpart"><ab>

    <lb n="1"/>ἀντί<supplied reason="lost">γραφον συγγραφῆς Αἰγυπτίας </supplied><gap reason="lost" quantity="3" unit="character"/> 

    <lb n="2"/>μεθη<supplied reason="lost">ρμηνευμένης </supplied><gap reason="lost" quantity="4" unit="character"/><gap reason="illegible" quantity="4" unit="character"/> κατὰ <supplied reason="lost">τὸ δυνα</supplied>

    <lb n="3" break="no"/>τὸν <gap reason="lost" quantity="14" unit="character"/> ἐν Κερκεοσί<supplied reason="lost">ρει τῆς Πο</supplied>

    <lb n="4" break="no"/>λ<supplied reason="lost">έμω</supplied>νος μερίδος τοῦ Ἀρσινοεί<supplied reason="lost">του νομοῦ</supplied> 

    <lb n="5"/><app type="editorial"><lem resp="BL 8.490"><unclear>Ἕ</unclear><supplied reason="lost">λλην</supplied></lem><rdg><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="4" unit="character"/></rdg></app> ἐνχώριος τῶν <app type="editorial"><lem resp="BL 8.490"><unclear>κ</unclear>ατοί<unclear>κ</unclear><supplied reason="lost">ω</supplied><unclear>ν</unclear> <gap reason="lost" quantity="7" unit="character"/></lem><rdg><unclear>κατοι</unclear><supplied reason="lost">κούντων </supplied><gap reason="lost" quantity="3" unit="character"/></rdg></app> 

    <lb n="6"/>Πε<supplied reason="lost">τεσοῦ</supplied>χος ὃς καὶ <app type="editorial"><lem resp="BL 8.490"><unclear>Ἀ</unclear><supplied reason="lost">μμώ</supplied><unclear>νιο</unclear><supplied reason="lost">ς Μεγχῆτος</supplied></lem><rdg><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="16" unit="character"/></rdg></app> 

    <lb n="7"/>το<supplied reason="lost">ῦ κα</supplied><unclear>ὶ</unclear> <unclear>Ἀ</unclear><supplied reason="lost">σ</supplied><unclear>κ</unclear><supplied reason="lost">λ</supplied>ηπιάδου μητρὸς <app type="editorial"><lem resp="BL 8.490"><gap reason="illegible" quantity="1" unit="character"/>ύγχιο<supplied reason="lost">ς</supplied></lem><rdg><unclear>Π</unclear>ύγχιο<supplied reason="lost">ς</supplied></rdg></app><gap reason="lost" quantity="5" unit="character"/> 

    <lb n="8"/><choice><reg>ὁμολογῶ</reg><orig>ὁμο<unclear>λογ</unclear>ῶι</orig></choice> <app type="editorial"><lem resp="BL 8.490"><choice><reg>Ἕλλην<supplied reason="omitted">ι</supplied></reg><orig>Ἕ<unclear>λλ</unclear>ην</orig></choice></lem><rdg>Ἕ<unclear>λλ</unclear>ην</rdg></app> ἐ<unclear>ν</unclear>χωρίωι τ<gap reason="illegible" quantity="2" unit="character"/>υτου<gap reason="lost" quantity="5" unit="character"/> 

    <lb n="9"/>Μεγχῆτι τῶι κα<supplied reason="lost">ὶ Ἀσκλ</supplied>πιάδει <app type="editorial"><lem resp="BL 8.490">Πετεσο<supplied reason="lost">ύ</supplied>χου</lem><rdg>Πετεσσο<supplied reason="lost">ύ</supplied>χου</rdg></app> 

    <lb n="10"/>τοῦ καὶ Ἀμμωνίου μητρὸς Θάσιτος <app type="editorial"><lem resp="BL 8.490">τῶ<supplied reason="lost">ι υἱῶι</supplied></lem><rdg>τω<gap reason="lost" quantity="5" unit="character"/></rdg></app> 

    <lb n="11"/>α<unclear>π</unclear><gap reason="lost" quantity="2" unit="character"/><gap reason="illegible" quantity="1" unit="character"/>ς<gap reason="illegible" quantity="1" unit="character"/> τοῦ ψιλοῦ τ<supplied reason="lost">όπ</supplied>ου τῶν ἄνευ <gap reason="lost" quantity="1" unit="character"/><gap reason="illegible" quantity="4" unit="character"/> 

    <lb n="12"/><unclear>λ</unclear>ο<unclear>υ</unclear> ἐ<supplied reason="lost">ν</supplied> <num value="2"><unclear>β</unclear></num> <supplied reason="lost">σ</supplied>φραγῖσι τῆς μὲν πρώτης σφ<supplied reason="omitted">ρ</supplied>
<choice><reg>αγῖ<lb n="13" break="no"/>δο<supplied reason="lost">ς</supplied></reg><orig>σφαγῖ<lb n="13" break="no"/>δο<supplied reason="lost">ς</supplied></orig></choice> <supplied reason="lost"><expan>νό<ex>του</ex></expan> ἐπὶ</supplied> <expan>βο<ex>ρρᾶ</ex></expan> <expan>π<ex>ήχεις</ex></expan> <num value="16">ιϛ</num>, <supplied reason="lost"><expan>λι<ex>βὸς</ex></expan></supplied> ἐπʼ <expan>ἀπη<ex>λιώτου</ex></expan> <expan>π<ex>ήχεις</ex></expan> <num value="16">ιϛ</num> <gap reason="lost" quantity="1" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="1" unit="character"/> 

    <lb n="14"/><choice><reg>σφ<supplied reason="omitted">ρ</supplied>αγῖδος</reg><orig>σφαγῖδος</orig></choice> <expan>νό<ex>του</ex></expan> ἐ<supplied reason="lost">πὶ</supplied> <expan>β<supplied reason="lost">ο<ex>ρρᾶ</ex></supplied></expan> <expan>π<ex>ήχεις</ex></expan> <num value="6">ϛ</num> <num value="1/2">𐅵</num>, <supplied reason="lost"><expan>λι<ex>βὸς</ex></expan></supplied> <unclear>ἐπ</unclear>ʼ ἀπηλιώτου <expan>π<ex>ήχεις</ex></expan> 

    <lb n="15"/><gap reason="illegible" extent="unknown" unit="character"><desc>vestiges</desc></gap> κατὰ τὸ <gap reason="lost" quantity="4" unit="character"/><supplied reason="lost"> ο</supplied>ν μέρος ἐν 

    <lb n="16"/>τῇ καὶ Κερκ<supplied reason="lost">ε</supplied>ο<supplied reason="lost">σ</supplied>είρει τῇ καὶ <unclear>Κε</unclear><supplied reason="lost">ρκεοσίρει</supplied> τῆς 

    <lb n="17"/>Πολέμωνος μερίδος ἀπὸ <choice><reg>νό<supplied reason="omitted">του</supplied></reg><orig>νο</orig></choice> τῆς Ἀ<unclear>ργ</unclear>αίτιδος 

    <lb n="18"/>διώρυγος Εὐεργέτου τοῦ Ἀρσινοείτου νομοῦ 

    <lb n="19"/><choice><reg>γείτονες</reg><orig>γιτονο</orig></choice> τούτων δύο <num value="2"/> <choice><reg>σφρα<supplied reason="omitted">γ</supplied>ίδων</reg><orig>σφραίδων</orig></choice> τῶν τόπων 

    <lb n="20"/><expan>νό<ex>του</ex></expan> <choice><reg>εἴσοδος</reg><orig>ἴσοδος</orig></choice> Ὥρου καὶ Πολέμωνος ἥ ἐστιν <expan>π<ex>ήχεις</ex></expan> <num value="2">β</num> ἐπὶ 

    <lb n="21"/>μῆκος τῆς <choice><reg>εἰσόδου</reg><orig>ἰσόδου</orig></choice> καὶ ἐξόδου Τοθίους Σενθέως 

    <lb n="22"/>καὶ Πόρτιος τοῦ Πόρτου <expan>βο<ex>ρρᾶ</ex></expan> τῶν <num value="2">β</num> <choice><reg>σφ<supplied reason="omitted">ρ</supplied>αγίδων</reg><orig>σφαγίδων</orig></choice> 

    <lb n="23"/>τῶν τόπων <supplied reason="omitted">ὧν</supplied> καὶ οἱ πηχισμοὶ πρόκεινται καὶ ἀπὸ 

    <lb n="24"/>τῆς <choice><reg>σήμερον</reg><orig>μερονήμερον</orig></choice> ἡμέρας μὴ ἐπελεύσεσθαι ἐπὶ σέ. 
</ab></div><div n="2" type="textpart"><ab>

