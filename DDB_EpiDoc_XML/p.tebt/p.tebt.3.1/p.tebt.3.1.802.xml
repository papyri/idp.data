    <lb n="1"/><expan><ex>ἔτους</ex></expan> <num value="36">λϛ</num> Ἁθὺ<supplied reason="lost" cert="low">ρ</supplied><num value="11"><supplied reason="lost">ι</supplied>α</num>. 

