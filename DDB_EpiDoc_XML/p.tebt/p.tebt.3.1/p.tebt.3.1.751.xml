         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r" type="textpart"><ab>

    <lb n="1"/>Ἀδάμας τῶι πατρὶ χαίρειν. 

    <lb n="2"/>ἐ<unclear>πε</unclear><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="4" unit="character"/> ὀφείλημα 

    <lb n="3"/><unclear>θ</unclear><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="2" unit="character"/>ε<unclear>ι</unclear><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="2" unit="character"/><unclear>ς</unclear><gap reason="lost" quantity="3" unit="character"/> 

    <lb n="4"/><unclear>ι</unclear><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="3" unit="character"/>υ<unclear>τ</unclear><gap reason="illegible" quantity="3" unit="character"/><unclear>ις</unclear> τῶι <expan>θη<ex>σαυρῷ</ex></expan> 

    <lb n="5"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><unclear>ιν</unclear> καὶ <gap reason="illegible" quantity="3" unit="character"/> <num value="150">ρν</num> καὶ 

    <lb n="6"/>ἐν <gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="1" unit="character"/> <num value="1150">Αρν</num>, καὶ <add place="above"><gap reason="illegible" quantity="4" unit="character"/> <unclear>τ</unclear>οῦ ἐ<unclear>πὶ</unclear> τούτ<unclear>ου</unclear></add> 

    <lb n="7"/>ὡς ἐν τῶι μεγίστωι κινδύνωι 

    <lb n="8"/>ἐσμὲν περὶ τοῦ ὑπάρχοντος 

    <lb n="9"/><expan>κριθοπυ<ex>ροῦ</ex></expan> ἐν Πόαν εἰς <expan>πυ<ex>ροῦ</ex></expan> <expan>ἀρτ<ex>άβας</ex></expan> <num value="2000">Β</num> 

    <lb n="10"/>ἕως τοῦ ἐφιδῆσαι τὴν τοῦ 

    <lb n="11"/>διοικητοῦ ὁρμήν, μὴ ἐπὶ τὸ χεῖρον 

    <lb n="12"/>διαλάβηι καὶ ἄλλως πως ἡμῖν 

    <lb n="13"/><gap reason="lost" quantity="6" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><unclear>τ</unclear><gap reason="lost" quantity="4" unit="character"/><unclear>σ</unclear>ερχε<gap reason="lost" quantity="5" unit="character"/> 

    <lb n="14"/><gap reason="lost" extent="unknown" unit="line"/> 
