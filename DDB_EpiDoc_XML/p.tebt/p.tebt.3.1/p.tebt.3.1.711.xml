      <body>
         <head n="5322" xml:lang="en">
            <date>ca. 125 BC</date>
            <placeName>Tebtynis</placeName>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r" type="textpart"><ab>

    <lb n="1"/>Θέων τοῖς ἐν Ὀξ<supplied reason="lost">υρύγ</supplied>χοις φύλα<supplied reason="lost">ξι</supplied> 

    <lb n="2"/>καὶ <supplied reason="lost">το</supplied>ῖς γεωργο<supplied reason="lost">ῖς</supplied> χ<supplied reason="lost">αί</supplied>ρ<supplied reason="lost">ει</supplied>ν. κατ<supplied reason="lost">έλαβον</supplied> 

    <lb n="3"/>διὰ π<supplied reason="lost">λ</supplied>ειόνων Πετωῦν τ<unclear>ο</unclear><gap reason="illegible" quantity="4" unit="character"/><gap reason="lost" quantity="3" unit="character"/> 

    <lb n="4"/>κωμάρχην τῆς κώμης <choice><reg>λιποτελῆ</reg><orig>λειποτελῆ</orig></choice> <supplied reason="lost">ὄντα</supplied> 

    <lb n="5"/>καὶ <add place="above">ἀτοποῦντα</add>, μεγάλως δὲ <choice><reg>καθυστερηκότα</reg><orig>καθυστηρ<unclear>η</unclear>κότα</orig></choice> ἐν <supplied reason="lost">τῶι</supplied> 

    <lb n="6"/>διεσταμένωι πρὸς αὐτὸ<unclear>ν</unclear> κεφαλαίωι οὗ <supplied reason="lost">δεῖ</supplied> 

    <lb n="7"/>δοθῆναι ἀπὸ προχειρισμοῦ σίτ<supplied reason="lost">ο</supplied>υ <gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" quantity="3" unit="character"/> 

    <lb n="8"/>καὶ διειλκυκότα ἕως τῆς <num value="20">κ</num> το<supplied reason="lost">ῦ </supplied><gap reason="lost" quantity="3" unit="character"/> 

    <lb n="9"/>ἀπολ<supplied reason="lost">ε</supplied>λοιπέναι πλείω τοῦ ἡμίσους. <unclear>ὅθ</unclear><supplied reason="lost">εν</supplied> 

    <lb n="10"/>ἀναδραμόντες τα<unclear>τ</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="11"/>γενέσθαι ἐν τούτοις <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="12"/>καὶ ἐν τοῖς πρότερον <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="13"/>τὰ τῆς χρείας ἐκπλη<supplied reason="lost">ρ </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ἵνα</supplied> 

    <lb n="14"/>μηθὲν ἐλάσσωμα <gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> τῶι</supplied> 

    <lb n="15"/>προγεγραμμένωι γέ<supplied reason="lost">νηται </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="16"/><supplied reason="lost">γ</supplied>ενόμενοι ἐπιδ<gap reason="illegible" quantity="3" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="17"/>εἰς <unclear>τὸ</unclear> <num value="44">μ<unclear>δ</unclear></num> <supplied reason="lost"><expan><ex cert="low">ἔτος</ex></expan></supplied> <unclear>μ</unclear>όνον τὰ <unclear>δ</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="18"/>παραδοθῆναι ἐπακ<supplied reason="lost">ολουθ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="19"/>δειξα<unclear>με</unclear><supplied reason="lost">ν</supplied><gap reason="lost" quantity="2" unit="character"/> τυγχανε<gap reason="lost" extent="unknown" unit="character"/> 
<milestone rend="paragraphos" unit="undefined"/>
    <lb n="20"/><unclear>ἔ</unclear><supplied reason="lost" cert="low">ρρωσθε. <expan><ex>ἔτους</ex></expan> <num value="44">μδ</num>  </supplied><gap reason="lost" quantity="3" unit="character"/> 
</ab></div><div n="v" type="textpart"><ab>

    <lb n="21"/>τοῖς ἐν Ὀξυρύγχοις 

    <lb n="22"/>γενηματοφύλαξι καὶ τοῖς γεωργοῖς. </ab></div></div>
