    <lb n="31"/><handShift new="m3"/> <gap reason="lost" extent="unknown" unit="character"/> Ὀννῶφρις <choice><reg>Πακήβκιος</reg><orig>Πακα<unclear>β</unclear>ις</orig></choice> μεμ

    <lb n="32" break="no"/><supplied reason="lost">ί</supplied><unclear>σ</unclear>θ<supplied reason="lost">ωκα τὰ</supplied>ς δύο <num value="2"/> ἀρούρ

    <lb n="33" break="no"/><supplied reason="lost">ας εἰς ἄ</supplied><unclear>λ</unclear><supplied reason="lost">λα</supplied> <unclear>ἔτ</unclear>η δ<unclear>ε</unclear>καέν

    <lb n="34" break="no"/><supplied reason="lost">νεα <num value="19"/>. <handShift new="m4"/> Ὀ</supplied>ννῶφ<supplied reason="lost">ρ</supplied>ις <supplied reason="lost">Π</supplied>ακήβ<supplied reason="lost">κι</supplied>ος μεμίσ

    <lb n="35" break="no"/><supplied reason="lost">θωκα</supplied> τῇ Ταορσεοῦτ<supplied reason="lost">ι τὰ</supplied>ς δύο <num value="2"/> ἀρού

    <lb n="36" break="no"/><supplied reason="lost">ρας εἰ</supplied><unclear>ς</unclear> ἄλλα ἔ<supplied reason="lost">τη </supplied><choice><reg><supplied reason="lost">δεκ</supplied>αέννεα</reg><orig><supplied reason="lost">δεκ</supplied>αένναια</orig></choice> <num value="19"/> 

    <lb n="37"/><supplied reason="lost">μετ</supplied><unclear>ὰ</unclear> τοὺς χρόνους τ<supplied reason="lost">ῶ</supplied>ν τρι

    <lb n="38" break="no"/><supplied reason="lost">ῶν <num value="3"/> </supplied><choice><reg><supplied reason="lost">μι</supplied>σθώσεων</reg><orig><supplied reason="lost">μι</supplied>σθωσεως</orig></choice> ἀντὶ τ<unclear>ῶ</unclear><supplied reason="lost">ν δη</supplied>

    <lb n="39" break="no"/><supplied reason="lost">μοσί</supplied>ων καὶ <choice><reg>ἐκπέπει<supplied reason="lost">σμαι</supplied></reg><orig>ἐκπεπυ<unclear>η</unclear><supplied reason="lost">σμαι</supplied></orig></choice> <supplied reason="lost">ὡς</supplied> 

    <lb n="40"/><supplied reason="lost">πρόκ</supplied>ιται. Πακῆβ<unclear>κ</unclear><supplied reason="lost">ις ἔγραψα</supplied> 

    <lb n="41"/><supplied reason="lost">τοῦ πατρ</supplied>ός μου βραδέω<supplied reason="lost">ς γράφοντος</supplied>. 

