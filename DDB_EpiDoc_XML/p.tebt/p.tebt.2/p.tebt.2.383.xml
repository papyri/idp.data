         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r" type="textpart"><ab>

    <lb n="1"/>ἔτους ἕκτου Τιβερίου Κλαυδίου Καίσαρος Σεβαστοῦ Γερμανικ<supplied reason="lost">οῦ Αὐτοκράτορος</supplied> 

    <lb n="2"/>μην<supplied reason="lost">ὸ</supplied>ς Γορ<supplied reason="lost">πιαί</supplied>ου ἑπτακαιδεκάτῃ Ἐπεὶφ <num value="17">ιζ</num>, ἐν Τεβ<supplied reason="lost">τύνει τῆς Πολέμωνος</supplied> 

    <lb n="3"/>μερίδος τοῦ Ἀρσινοείτου νομοῦ. ὁμολογοῦσιν ἀλλήλοις Μαρε<supplied reason="lost">ψῆμις ὁ καὶ Πατα</supplied>

    <lb n="4" break="no"/>ρεεῖς ὡς ἐτῶν τριάκοντα ἑνὸς <num value="31"/> οὐλὴ <expan>μετώπ<ex>ῳ</ex></expan> ἐξ ἀριστερῶν κ<supplied reason="lost">αὶ Ταμαρρῆς ὡς</supplied> 

    <lb n="5"/>ἐτῶν τ<supplied reason="lost">ριάκοντα</supplied> πέντε <num value="35"/> οὐλὴ κροτάφῳ ἀριστερῷ ἀμφ<supplied reason="lost">ότεροι Παώπιος</supplied> 

    <lb n="6"/>μετὰ κυρί<supplied reason="lost">ου τῆς</supplied> Τ<supplied reason="lost">α</supplied>μαρ<supplied reason="lost">ρ</supplied><unclear>ή</unclear>ους τοῦ ἑαυτῆς ἀνδρὸ<supplied reason="lost">ς Ψενκήβκιος τοῦ</supplied> 

    <lb n="7"/>Ὀ<supplied reason="lost">ν</supplied>νώφ<supplied reason="lost">ρ</supplied>εω<supplied reason="lost">ς ὡ</supplied>ς ἐτῶν τριάκοντα ὀκ<supplied reason="lost">τ</supplied><unclear>ὼ</unclear> <num value="38"/> <supplied reason="lost">οὐλὴ </supplied><gap reason="lost" quantity="10" unit="character"/><supplied reason="lost"> διῃρῆσ</supplied>

    <lb n="8" break="no"/>θαι πρὸς ἑαυτο<supplied reason="lost">ὺ</supplied>ς ἐξ εὐδοκούντων διὰ <supplied reason="lost">τῆσδε τῆς ὁμολογίας ἀπὸ τοῦ νῦν</supplied> 

    <lb n="9"/>ἐπὶ τὸν ἅπαντα χρόνον τὸν ὑπάρχον<supplied reason="lost">τα ἑκατέρῳ αὐτῶν ψιλὸν τόπον</supplied> 

    <lb n="10"/><choice><reg>περιτετειχισμένον</reg><orig>περιτετιχι<supplied reason="lost">σ</supplied>μένον</orig></choice> καὶ τὸν ἐν ἑ<supplied reason="lost">τ</supplied>έρᾳ σφραγῖδ<supplied reason="lost">ι ψιλὸν τόπον Ἀχένους</supplied> <surplus>εν ετερᾳ</surplus> 

    <lb n="11"/><surplus>σφραγιδι</surplus> κα<supplied reason="lost">ὶ τὸ </supplied><choice><reg><supplied reason="lost">ὑ</supplied>πάρχον</reg><orig><supplied reason="lost">ὑ</supplied>πάρχων</orig></choice> αὐτοῖς τ<supplied reason="lost">ρί</supplied>τον μέρος <supplied reason="lost">παστοφορίου λεγομένου </supplied><gap reason="lost" quantity="5" unit="character"/> 

    <lb n="12"/>διστέγου <choice><reg>κοινοῦ</reg><orig>κυν<supplied reason="lost">ο</supplied>ῦ</orig></choice> καὶ <choice><reg>ἀδιαιρέτου</reg><orig>ἀδιερέτου</orig></choice> ἐντὸς τοῦ ἐκτὸς <unclear>τ</unclear><gap reason="lost" quantity="22" unit="character"/> 

    <lb n="13"/>ου θεοῦ μεγάλου μεγίστου πάντα ἐν τῇ προκιμ<supplied reason="lost">έ</supplied>νῃ κ<supplied reason="lost">ώμῃ Τεβτύνει, ἐφʼ ᾧ τὸν</supplied> 

    <lb n="14"/>Μ<supplied reason="lost">α</supplied>ρεψῆμιν <choice><reg>λελογχέναι</reg><orig>λελονχέναι</orig></choice> εἰς τὸ ἐπιβαλλόν αὐτῷ μέρο<supplied reason="lost">ς τὸ προκίμενον τρίτον</supplied> 

    <lb n="15"/>μ<supplied reason="lost">έ</supplied>ρ<supplied reason="lost">ος</supplied> τοῦ προκιμένου παστ<supplied reason="lost">ο</supplied>φορίου διστέγου λεγομ<supplied reason="lost">ένου </supplied><gap reason="lost" quantity="5" unit="character"/><supplied reason="lost">, <choice><reg>γείτονες</reg><orig>γίτονες</orig></choice> νό</supplied>

    <lb n="16" break="no"/>του <choice><reg>ἄλλο</reg><orig>ἄλλου</orig></choice> <choice><reg>παστοφόριον</reg><orig>πα<supplied reason="lost">σ</supplied>τοφοριου</orig></choice> <unclear>τοῦ</unclear> <supplied reason="lost">Μαρσισ</supplied>ού<supplied reason="lost">χ</supplied>ου <unclear>τοῦ</unclear> Μαρεψήμ<supplied reason="lost">ιος βορρᾶ </supplied><gap reason="lost" quantity="8" unit="character"/> 

    <lb n="17"/>εὐρ<unclear>υ</unclear>χωρ<supplied reason="lost">ία</supplied> <unclear>κ</unclear><supplied reason="lost">αὶ</supplied> <choice><reg>εἴσοδος</reg><orig>ἴσοδος</orig></choice> καὶ ἔξοδος τοῦ <choice><reg>ἱεροῦ</reg><orig>εἱεροῦ</orig></choice> λιβὸ<supplied reason="lost">ς</supplied> τ<supplied reason="lost">ὸ </supplied><gap reason="lost" quantity="15" unit="character"/><supplied reason="lost"> τοῦ</supplied> 

    <lb n="18"/>Μαρσισούχου παστοφόριον ἀπηλιώτου Μαρρήους καὶ τῶ<supplied reason="lost">ν μετόχων παστο</supplied>

    <lb n="19" break="no"/>φόριον, <supplied reason="lost">τὴ</supplied>ν <supplied reason="lost">δ</supplied>ὲ Ταμαρρῆν καὶ αὐτὴν <choice><reg>λελογχέναι</reg><orig>λελονχέναι</orig></choice> <supplied reason="lost">εἰς τὸ ἐπιβάλλον αὐτῇ</supplied> 

    <lb n="20"/><unclear>μ</unclear><supplied reason="lost">έ</supplied><unclear>ρ</unclear><supplied reason="lost">ος τὸ</supplied><unclear>ν</unclear> προκ<supplied reason="lost">ίμ</supplied>εν<supplied reason="lost">ο</supplied>ν ψιλὸν τόπον <choice><reg>περιτετειχισμένον</reg><orig>περιτετ<supplied reason="lost">ι</supplied><unclear>χι</unclear><supplied reason="lost">σμένον</supplied></orig></choice>, <supplied reason="lost">ὧν μέτρα</supplied> 

    <lb n="21"/>το<supplied reason="lost">ύ</supplied>του νότον ἐπὶ <choice><reg>βορρᾶ<supplied reason="omitted">ν</supplied></reg><orig>βορρᾶ</orig></choice> <choice><reg>πήχεις</reg><orig>πήχις</orig></choice> ἕνδεκα <num value="11"/> λί<supplied reason="lost">βα ἐπʼ ἀπηλιώτην</supplied> <choice><reg>πήχεις</reg><orig><supplied reason="lost">πήχις</supplied></orig></choice> 

    <lb n="22"/>πέ<supplied reason="lost">ν</supplied>τε <choice><reg>ἥμισυ</reg><orig>ἥμ<unclear>υσ</unclear><supplied reason="lost">υ</supplied></orig></choice> <num value="5"/><num value="1/2"/>, ὧν μέτρα καὶ τοῦ προκιμένου τ<supplied reason="lost">οῦ ἐν ἑτέρᾳ </supplied><choice><reg><supplied reason="lost">σφρα</supplied><lb n="23" break="no"/>γῖδι</reg><orig><supplied reason="lost">σφρα</supplied><lb n="23" break="no"/>γιδει</orig></choice> ψιλοῦ τόπου Ἀχένους νότον ἐπὶ <choice><reg>βορρᾶ<supplied reason="omitted">ν</supplied></reg><orig>βορρᾶ</orig></choice> <choice><reg>πήχεις</reg><orig>πήχις</orig></choice> <unclear>ε</unclear><gap reason="lost" quantity="11" unit="character"/><supplied reason="lost"> πα</supplied><choice><reg><lb n="24" break="no"/>λαιστὰς</reg><orig><supplied reason="lost">πα</supplied>λεστὰς</orig></choice> δύο <num value="2"/> λιβὸς ἐπʼ ἀπηλιώτην <choice><reg>πήχεις</reg><orig>πήχις</orig></choice> πέντε <choice><reg>ἥμισυ</reg><orig>ἥμυσυ</orig></choice> <num value="5"/><num value="1/2"/>, <choice><reg>γ<supplied reason="lost">είτονες</supplied></reg><orig><unclear>γ</unclear><supplied reason="lost">ίτονες</supplied></orig></choice> <supplied reason="lost">τοῦ μὲν</supplied> 

    <lb n="25"/><choice><reg>περιτετειχισμένου</reg><orig>περιτετιχισμένου</orig></choice> τόπου νότου καὶ ἀπηλιώτου <subst><add place="inline">Μαρσισούχου</add><del rend="corrected">μαρη<supplied reason="lost">σουχου</supplied></del></subst> <supplied reason="lost">τοῦ </supplied><gap reason="lost" quantity="7" unit="character"/> 

    <lb n="26"/>καὶ τῶν μετόχων <unclear>ε</unclear>ἴσοδος καὶ ἔξοδος βορρᾶ ῥύμη βα<supplied reason="lost">σιλικὴ λιβὸς</supplied> 

    <lb n="27"/>Λυσιμάχου τοῦ Διδύμου τόποι, ὧν <choice><reg>γείτονες</reg><orig>γίτονες</orig></choice> καὶ τοῦ προκιμ<supplied reason="lost">ένου τόπου Ἀχένους</supplied> 

    <lb n="28"/>νότου Ψενκήβκιος του Πακήβκιος Χούσους τόποι βορρᾶ <gap reason="lost" quantity="6" unit="character"/><supplied reason="lost"> ἡ προκει</supplied>

    <lb n="29" break="no"/>μένη εἴσοδος καὶ ἔξοδος εἰς ἣν καὶ <choice><reg>ἀνοίξει</reg><orig>ἀνύξι</orig></choice> ἑαυτῇ ἡ προγεγρα<supplied reason="lost">μμένη Ταμαρρῆς</supplied> 

    <lb n="30"/>καὶ οἱ παρʼ αὐτῆς καὶ οἱ μεταλημψόμενοι θύραν ἐκ τοῦ α<gap reason="lost" quantity="10" unit="character"/> 

    <lb n="31"/>τὴν ἀπὸ <choice><reg>λιβὸς</reg><orig>λίβα</orig></choice> μέρους εἴσοδον καὶ ἔξοδον ἕλκ<supplied reason="lost">ο</supplied>υσαν ε<unclear>κ</unclear><gap reason="lost" quantity="6" unit="character"/><supplied reason="lost">, καὶ εἰσο</supplied>

    <lb n="32" break="no"/>δεύσι καὶ ἐξοδεύσι ἡ αὐτὴ <choice><reg>Ταμαρ<supplied reason="omitted">ρ</supplied>ῆς</reg><orig>Ταμαρῆς</orig></choice> καὶ οἱ παρʼ α<unclear>ὐ</unclear><supplied reason="lost">τῆς καὶ οἱ μεταλημψόμενοι</supplied> 

    <lb n="33"/>δ<supplied reason="lost">ι</supplied>ὰ τῆς ἑλκο<supplied reason="lost">ύσης</supplied> εἰς τὸν <choice><reg>βορρᾶ<supplied reason="omitted">ν</supplied></reg><orig>βορρᾶ</orig></choice> καὶ ἀπηλιώτ<supplied reason="lost">ην ἐξόδου, λιβὸς </supplied><choice><reg><supplied reason="lost">δημο</supplied><lb n="34" break="no"/>σία</reg><orig><supplied reason="lost">δημο</supplied><lb n="34" break="no"/>σιαν</orig></choice> <choice><reg>ῥύμη</reg><orig>ρὑμην</orig></choice> ἀπηλιώτου τοῦ προγεγραμμένου <unclear>Μ</unclear><supplied reason="lost">αρσισούχου τοῦ </supplied><gap reason="lost" quantity="7" unit="character"/> 

    <lb n="35"/>οἰκία. <choice><reg>κρατεῖν</reg><orig>κρατῖν</orig></choice> οὖν καὶ <choice><reg>δεσπόζειν</reg><orig>δεσπόζιν</orig></choice> ἕκαστον τῶ<supplied reason="lost">ν ὁμολογούντων καὶ τοὺς</supplied> 

    <lb n="36"/>παρʼ αὐτῶν καὶ τοὺς μεταλημψομένους ὧ<supplied reason="lost">ν ἕκαστος</supplied> <choice><reg>λέλογχεν</reg><orig><supplied reason="lost">λέλονχεν</supplied></orig></choice> <supplied reason="lost">ἀπὸ</supplied> 

    <lb n="37"/>τῆς ἐνεστώσης ἡμέρας ἐπὶ τὸν ἅπαντα χρόν<supplied reason="lost">ο</supplied>ν <gap reason="lost" quantity="14" unit="character"/> 

    <lb n="38"/>καὶ μὴ ἐπιπορεύεσθαι ἑκάτερον τῷ ἑτέρῳ <supplied reason="lost">ἐ</supplied>φʼ ἃ ὁ ἕτ<supplied reason="lost">ερος αὐτῶν κεκλή</supplied>

    <lb n="39" break="no"/>ρωται τρόπῳ μηθενί, ὅ τι δʼ ἂν τῶν προγεγραμμένω<supplied reason="lost">ν παρασυνγρα</supplied>

    <lb n="40" break="no"/>φῇ τις τῶν <subst><add place="inline">ὁμολογούντων</add><del rend="corrected">ομολογουντον</del></subst> ἀποτισάτω ὁ μὴ ἐ<unclear>ν</unclear>μ<supplied reason="lost">ένων τῷ ἐμμένον</supplied>

    <lb n="41" break="no"/>τι τά τε βλάβη καὶ δαπα<supplied reason="lost">ν</supplied>ή<supplied reason="lost">ματ</supplied>α διπ<supplied reason="lost">λ</supplied>ᾶ καὶ ἐπίτιμον <supplied reason="lost">ἀργυρίου δραχμὰς</supplied> 

    <lb n="42"/>διακοσίας <num value="200"/> καὶ εἰς τὸ δημόσιον τὰς <choice><reg>ἴσας</reg><orig>εἴσας</orig></choice>, καὶ μηδὲν ἧσσον. <supplied reason="lost">εὐδοκεῖ δὲ ἡ</supplied> 

    <lb n="43"/>Μαρεψῆμις γυνὴ Θομσαεῖς Μαρεψήμις ὡς ἐτῶν τριάκ<supplied reason="lost">οντα <num value="30"/> οὐλὴ </supplied><gap reason="lost" quantity="4" unit="character"/><supplied reason="lost"> δε</supplied>

    <lb n="44" break="no"/>ξιῷ μετὰ κυρίου αὐτοῦ Μαρεψήμιος τῇ <choice><reg>διαιρέσει</reg><orig>διερέσι</orig></choice> ταύτῃ καὶ <supplied reason="lost">μὴ ἐγκαλεῖν</supplied> 

    <lb n="45"/>μηδʼ <choice><reg>ἐνκαλέσι<supplied reason="omitted">ν</supplied></reg><orig>ἐνκαλ<supplied reason="lost">ε</supplied>σι</orig></choice> μηδὲ ἄλλον ὑπὲρ αὐτῆς. <choice><reg>ὑπογραφεῖς</reg><orig>ὑπογραφῖς</orig></choice> τοῦ Μαρεψή<supplied reason="lost">μιος</supplied> 

    <lb n="46"/>Ψ<supplied reason="lost">οῖ</supplied>φις Ὀν<supplied reason="lost">ν</supplied>ώφ<supplied reason="lost">ρε</supplied>ως ὡς <expan><ex>ἐτῶν</ex></expan> εἴκοσι ἓξ <num value="26"/> οὐλὴ μετώπῳ ἐξ <unclear>ἀ</unclear><supplied reason="lost">ριστερῶν, τῶν δὲ</supplied> 

    <lb n="47"/>ἄλλων <supplied reason="lost">Μ</supplied>αρεψῆμις ὃς κ<supplied reason="lost">αὶ</supplied> <unclear>Κ</unclear>αλ<unclear>ε</unclear>ῶς Μαρεψήμι<supplied reason="lost">ς</supplied> ὡς <expan><ex>ἐτῶν</ex></expan> <num value="38">λη</num> οὐ<supplied reason="lost">λὴ </supplied><gap reason="lost" quantity="11" unit="character"/> 

    <lb n="48"/><handShift new="m2"/> Μα<supplied reason="lost">ρ</supplied>ε<supplied reason="lost">ψῆμις</supplied> <unclear>ὃ</unclear>ς καὶ Παταρεῖς Παώπις <choice><reg>συνδιήρημαι</reg><orig>συνδιίρη<supplied reason="lost">μαι</supplied></orig></choice> <gap reason="lost" quantity="4" unit="character"/> 

    <lb n="49"/>τ<supplied reason="lost">ὸ</supplied> ἐ<supplied reason="lost">π</supplied>ιβάλλον μέρος τὸ προκίμενον τρίτον <num value="1/3"/> μέρος <unclear>τ</unclear><supplied reason="lost">οῦ παστο</supplied>

    <lb n="50" break="no"/>φορίου διστέγου. <choice><reg>εὐδοκεῖ</reg><orig>εὐδοκῖ</orig></choice> ἡ γυνή μου Θονψαεῖς Μαρε<supplied reason="lost">ψήμ</supplied><choice><reg><lb n="51" break="no"/>ιος</reg><orig>μαρε<supplied reason="lost">ψημ</supplied>εις</orig></choice> μετὰ κυρίου ἐμοῦ τῇ <choice><reg>διαιρέσει</reg><orig>διαιρέση</orig></choice> καὶ μὴ <choice><reg>ἐπελεύσεσθαι</reg><orig>ἐπαιλεύσεσ<supplied reason="lost">θαι</supplied></orig></choice> <gap reason="lost" quantity="3" unit="character"/> 

    <lb n="52"/>ση <choice><reg>καθὼς</reg><orig>καθ<unclear>ὸ</unclear>ς</orig></choice> πρόκιται. ἔγραψεν ὑπὲρ αὐτῶν Ψοῖφις 

Ὀννώφ<supplied reason="lost">ριος μὴ </supplied><choice><reg><supplied reason="lost">εἰδό</supplied><lb n="53" break="no"/>των</reg><orig><supplied reason="lost">ειδο</supplied><lb n="53" break="no"/>τον</orig></choice> <choice><reg>γράμ<supplied reason="omitted">μ</supplied>ατα</reg><orig>γράματα</orig></choice>. <handShift new="m3"/> <choice><reg>Ταμαρ<supplied reason="omitted">ρ</supplied>ῆς</reg><orig>Ταμαρῆς</orig></choice> Παώπις μετὰ κυρίου <supplied reason="lost">τοῦ ἀνδρὸς</supplied> 

    <lb n="54"/>Ψενκήβκις τοῦ <subst><add place="inline">Ὀννώφρις</add><del rend="corrected">κννωνρις</del></subst> <choice><reg>συνδιῄρημαι</reg><orig><subst><add place="inline"><unclear>σ</unclear>υδιειρημε</add><del rend="corrected">συδιιιρημε</del></subst></orig></choice> καὶ <choice><reg>λέλο<supplied reason="omitted">γ</supplied>χα</reg><orig>λέλο<unclear>χ</unclear><supplied reason="lost">α</supplied></orig></choice> <supplied reason="lost">τὸ προκείμενον</supplied> 

    <lb n="55"/>μέρος τῶν <choice><reg>προκειμέν<supplied reason="omitted">ων</supplied></reg><orig>προκειμεν</orig></choice> <choice><reg>περιτετειχισμένων</reg><orig>περτεθικ<unclear>ο</unclear>σμ<unclear>έ</unclear>ν<supplied reason="lost">ων</supplied></orig></choice> <supplied reason="lost">καὶ τῶν ἐν ἑ</supplied>

    <lb n="56" break="no"/>τέρᾳ <choice><reg>σφραγῖδι</reg><orig>σφρακῖτι</orig></choice> <choice><reg>Ἀχένους</reg><orig>Ἀχάνος</orig></choice> τόπων καὶ ἕκαστα π<supplied reason="lost">οιήσω ὡς πρό</supplied>

    <lb n="57" break="no"/>κιται. ἔγραψεν ὑπὲρ <subst><add place="inline">αὐτῶν</add><del rend="corrected">αυτον</del></subst> Μαρεψῆμις διὰ τὸ <supplied reason="lost">τὸν μὲν Ψεν</supplied>

    <lb n="58" break="no"/>κῆβκιν <choice><reg>Αἰγύπτια</reg><orig>Ἐγυπτυ<unclear>τι</unclear>α</orig></choice> <choice><reg>γράφειν</reg><orig>γράφιν</orig></choice> τὴν <choice><reg>δὲ</reg><orig>τὲ</orig></choice> <choice><reg>ἄλ<supplied reason="omitted">λ</supplied>ην</reg><orig>ἄλην</orig></choice> μὴ <unclear>ε</unclear><supplied reason="lost">ἰδέναι <expan>γράμ<ex>ματα</ex></expan>.</supplied> 

