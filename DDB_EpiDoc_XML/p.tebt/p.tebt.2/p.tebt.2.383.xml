         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r" type="textpart"><!--milestone unit="4"--><ab>

    <lb n="1"/>ἔτους ἕκτου Τιβερίου Κλαυδίου Καίσαρος Σεβαστοῦ Γερμανικ<supplied reason="lost">οῦ Αὐτοκράτορος</supplied> 

    <lb n="2"/>μην<supplied reason="lost">ὸ</supplied>ς Γορ<supplied reason="lost">πιαί</supplied>ου ἑπτακαιδεκάτῃ Ἐπεὶφ <num value="17">ιζ</num>, ἐν Τεβ<supplied reason="lost">τύνει τῆς Πολέμωνος</supplied> 

    <lb n="3"/>μερίδος τοῦ Ἀρσινοείτου νομοῦ. ὁμολογοῦσιν ἀλλήλοις Μαρε<supplied reason="lost">ψῆμις ὁ καὶ Πατα</supplied>

    <lb n="4" type="inWord"/>ρεεῖς ὡς ἐτῶν τριάκοντα ἑνὸς <num value="31"/> οὐλὴ <expan>μετώπ<ex>ῳ</ex></expan> ἐξ ἀριστερῶν κ<supplied reason="lost">αὶ Ταμαρρῆς ὡς</supplied> 

    <lb n="5"/>ἐτῶν τ<supplied reason="lost">ριάκοντα</supplied> πέντε <num value="35"/> οὐλὴ κροτάφῳ ἀριστερῷ ἀμφ<supplied reason="lost">ότεροι Παώπιος</supplied> 

    <lb n="6"/>μετὰ κυρί<supplied reason="lost">ου τῆς</supplied> Τ<supplied reason="lost">α</supplied>μαρ<supplied reason="lost">ρ</supplied><unclear>ή</unclear>ους τοῦ ἑαυτῆς ἀνδρὸ<supplied reason="lost">ς Ψενκήβκιος τοῦ</supplied> 

    <lb n="7"/>Ὀ<supplied reason="lost">ν</supplied>νώφ<supplied reason="lost">ρ</supplied>εω<supplied reason="lost">ς ὡ</supplied>ς ἐτῶν τριάκοντα ὀκ<supplied reason="lost">τ</supplied><unclear>ὼ</unclear> <num value="38"/> <supplied reason="lost">οὐλὴ </supplied><gap reason="lost" quantity="10" unit="character"/><supplied reason="lost"> διῃρῆσ</supplied>

    <lb n="8" type="inWord"/>θαι πρὸς ἑαυτο<supplied reason="lost">ὺ</supplied>ς ἐξ εὐδοκούντων διὰ <supplied reason="lost">τῆσδε τῆς ὁμολογίας ἀπὸ τοῦ νῦν</supplied> 

    <lb n="9"/>ἐπὶ τὸν ἅπαντα χρόνον τὸν ὑπάρχον<supplied reason="lost">τα ἑκατέρῳ αὐτῶν ψιλὸν τόπον</supplied> 

    <lb n="10"/><choice><corr>περιτετειχισμένον</corr><sic>περιτετιχι<supplied reason="lost">σ</supplied>μενον</sic></choice> καὶ τὸν ἐν ἑ<supplied reason="lost">τ</supplied>έρᾳ σφραγῖδ<supplied reason="lost">ι ψιλὸν τόπον Ἀχένους</supplied> <surplus>εν ετερᾳ</surplus> 

    <lb n="11"/><surplus>σφραγιδι</surplus> κα<supplied reason="lost">ὶ τὸ </supplied><choice><corr><supplied reason="lost">ὑ</supplied>πάρχον</corr><sic><supplied reason="lost">υ</supplied>παρχων</sic></choice> αὐτοῖς τ<supplied reason="lost">ρί</supplied>τον μέρος <supplied reason="lost">παστοφορίου λεγομένου </supplied><gap reason="lost" quantity="5" unit="character"/> 

    <lb n="12"/>διστέγου <choice><corr>κοινοῦ</corr><sic>κυν<supplied reason="lost">ο</supplied>υ</sic></choice> καὶ <choice><corr>ἀδιαιρέτου</corr><sic>αδιερετου</sic></choice> ἐντὸς τοῦ ἐκτὸς <unclear>τ</unclear><gap reason="lost" quantity="22" unit="character"/> 

    <lb n="13"/>ου θεοῦ μεγάλου μεγίστου πάντα ἐν τῇ προκιμ<supplied reason="lost">έ</supplied>νῃ κ<supplied reason="lost">ώμῃ Τεβτύνει, ἐφʼ ᾧ τὸν</supplied> 

    <lb n="14"/>Μ<supplied reason="lost">α</supplied>ρεψῆμιν <choice><corr>λελογχέναι</corr><sic>λελονχεναι</sic></choice> εἰς τὸ ἐπιβαλλόν αὐτῷ μέρο<supplied reason="lost">ς τὸ προκίμενον τρίτον</supplied> 

    <lb n="15"/>μ<supplied reason="lost">έ</supplied>ρ<supplied reason="lost">ος</supplied> τοῦ προκιμένου παστ<supplied reason="lost">ο</supplied>φορίου διστέγου λεγομ<supplied reason="lost">ένου </supplied><gap reason="lost" quantity="5" unit="character"/><supplied reason="lost">, <choice><corr>γείτονες</corr><sic>γιτονες</sic></choice> νό</supplied>

    <lb n="16" type="inWord"/>του <choice><corr>ἄλλο</corr><sic>αλλου</sic></choice> <choice><corr>παστοφόριον</corr><sic>πα<supplied reason="lost">σ</supplied>τοφοριου</sic></choice> <unclear>τοῦ</unclear> <supplied reason="lost">Μαρσισ</supplied>ού<supplied reason="lost">χ</supplied>ου <unclear>τοῦ</unclear> Μαρεψήμ<supplied reason="lost">ιος βορρᾶ </supplied><gap reason="lost" quantity="8" unit="character"/> 

    <lb n="17"/>εὐρ<unclear>υ</unclear>χωρ<supplied reason="lost">ία</supplied> <unclear>κ</unclear><supplied reason="lost">αὶ</supplied> <choice><corr>εἴσοδος</corr><sic>ισοδος</sic></choice> καὶ ἔξοδος τοῦ <choice><corr>ἱεροῦ</corr><sic>ειερου</sic></choice> λιβὸ<supplied reason="lost">ς</supplied> τ<supplied reason="lost">ὸ </supplied><gap reason="lost" quantity="15" unit="character"/><supplied reason="lost"> τοῦ</supplied> 

    <lb n="18"/>Μαρσισούχου παστοφόριον ἀπηλιώτου Μαρρήους καὶ τῶ<supplied reason="lost">ν μετόχων παστο</supplied>

    <lb n="19" type="inWord"/>φόριον, <supplied reason="lost">τὴ</supplied>ν <supplied reason="lost">δ</supplied>ὲ Ταμαρρῆν καὶ αὐτὴν <choice><corr>λελογχέναι</corr><sic>λελονχεναι</sic></choice> <supplied reason="lost">εἰς τὸ ἐπιβάλλον αὐτῇ</supplied> 

    <lb n="20"/><unclear>μ</unclear><supplied reason="lost">έ</supplied><unclear>ρ</unclear><supplied reason="lost">ος τὸ</supplied><unclear>ν</unclear> προκ<supplied reason="lost">ίμ</supplied>εν<supplied reason="lost">ο</supplied>ν ψιλὸν τόπον <choice><corr>περιτετειχισμένον</corr><sic>περιτετ<supplied reason="lost">ι</supplied><unclear>χι</unclear><supplied reason="lost">σμενον</supplied></sic></choice>, <supplied reason="lost">ὧν μέτρα</supplied> 

    <lb n="21"/>το<supplied reason="lost">ύ</supplied>του νότον ἐπὶ <choice><corr>βορρᾶ<supplied reason="omitted">ν</supplied></corr><sic>βορρα</sic></choice> <choice><corr>πήχεις</corr><sic>πηχις</sic></choice> ἕνδεκα <num value="11"/> λί<supplied reason="lost">βα ἐπʼ ἀπηλιώτην</supplied> <choice><corr>πήχεις</corr><sic><supplied reason="lost">πηχις</supplied></sic></choice> 

    <lb n="22"/>πέ<supplied reason="lost">ν</supplied>τε <choice><corr>ἥμισυ</corr><sic>ημ<unclear>υσ</unclear><supplied reason="lost">υ</supplied></sic></choice> <num value="5"/><num value="1/2"/>, ὧν μέτρα καὶ τοῦ προκιμένου τ<supplied reason="lost">οῦ ἐν ἑτέρᾳ σφρα</supplied>

    <lb n="23" type="inWord"/><choice><corr>γῖδι</corr><sic><supplied reason="lost">σφρα</supplied>γιδει</sic></choice> ψιλοῦ τόπου Ἀχένους νότον ἐπὶ <choice><corr>βορρᾶ<supplied reason="omitted">ν</supplied></corr><sic>βορρα</sic></choice> <choice><corr>πήχεις</corr><sic>πηχις</sic></choice> <unclear>ε</unclear><gap reason="lost" quantity="11" unit="character"/><supplied reason="lost"> πα</supplied>

    <lb n="24" type="inWord"/><choice><corr>λαιστὰς</corr><sic><supplied reason="lost">πα</supplied>λεστας</sic></choice> δύο <num value="2"/> λιβὸς ἐπʼ ἀπηλιώτην <choice><corr>πήχεις</corr><sic>πηχις</sic></choice> πέντε <choice><corr>ἥμισυ</corr><sic>ημυσυ</sic></choice> <num value="5"/><num value="1/2"/>, <choice><corr>γ<supplied reason="lost">είτονες</supplied></corr><sic><unclear>γ</unclear><supplied reason="lost">ιτονες</supplied></sic></choice> <supplied reason="lost">τοῦ μὲν</supplied> 

    <lb n="25"/><choice><corr>περιτετειχισμένου</corr><sic>περιτετιχισμενου</sic></choice> τόπου νότου καὶ ἀπηλιώτου <subst><add place="inline">Μαρσισούχου</add><del rend="corrected">μαρη<supplied reason="lost">σουχου</supplied></del></subst> <supplied reason="lost">τοῦ </supplied><gap reason="lost" quantity="7" unit="character"/> 

    <lb n="26"/>καὶ τῶν μετόχων <unclear>ε</unclear>ἴσοδος καὶ ἔξοδος βορρᾶ ῥύμη βα<supplied reason="lost">σιλικὴ λιβὸς</supplied> 

    <lb n="27"/>Λυσιμάχου τοῦ Διδύμου τόποι, ὧν <choice><corr>γείτονες</corr><sic>γιτονες</sic></choice> καὶ τοῦ προκιμ<supplied reason="lost">ένου τόπου Ἀχένους</supplied> 

    <lb n="28"/>νότου Ψενκήβκιος του Πακήβκιος Χούσους τόποι βορρᾶ <gap reason="lost" quantity="6" unit="character"/><supplied reason="lost"> ἡ προκει</supplied>

    <lb n="29" type="inWord"/>μένη εἴσοδος καὶ ἔξοδος εἰς ἣν καὶ <choice><corr>ἀνοίξει</corr><sic>ανυξι</sic></choice> ἑαυτῇ ἡ προγεγρα<supplied reason="lost">μμένη Ταμαρρῆς</supplied> 

    <lb n="30"/>καὶ οἱ παρʼ αὐτῆς καὶ οἱ μεταλημψόμενοι θύραν ἐκ τοῦ α<gap reason="lost" quantity="10" unit="character"/> 

    <lb n="31"/>τὴν ἀπὸ <choice><corr>λιβὸς</corr><sic>λιβα</sic></choice> μέρους εἴσοδον καὶ ἔξοδον ἕλκ<supplied reason="lost">ο</supplied>υσαν ε<unclear>κ</unclear><gap reason="lost" quantity="6" unit="character"/><supplied reason="lost">, καὶ εἰσο</supplied>

    <lb n="32" type="inWord"/>δεύσι καὶ ἐξοδεύσι ἡ αὐτὴ <choice><corr>Ταμαρ<supplied reason="omitted">ρ</supplied>ῆς</corr><sic>ταμαρης</sic></choice> καὶ οἱ παρʼ α<unclear>ὐ</unclear><supplied reason="lost">τῆς καὶ οἱ μεταλημψόμενοι</supplied> 

    <lb n="33"/>δ<supplied reason="lost">ι</supplied>ὰ τῆς ἑλκο<supplied reason="lost">ύσης</supplied> εἰς τὸν <choice><corr>βορρᾶ<supplied reason="omitted">ν</supplied></corr><sic>βορρα</sic></choice> καὶ ἀπηλιώτ<supplied reason="lost">ην ἐξόδου, λιβὸς δημο</supplied>

    <lb n="34" type="inWord"/><choice><corr>σία</corr><sic><supplied reason="lost">δημο</supplied>σιαν</sic></choice> <choice><corr>ῥύμη</corr><sic>ρυμην</sic></choice> ἀπηλιώτου τοῦ προγεγραμμένου <unclear>Μ</unclear><supplied reason="lost">αρσισούχου τοῦ </supplied><gap reason="lost" quantity="7" unit="character"/> 

    <lb n="35"/>οἰκία. <choice><corr>κρατεῖν</corr><sic>κρατιν</sic></choice> οὖν καὶ <choice><corr>δεσπόζειν</corr><sic>δεσποζιν</sic></choice> ἕκαστον τῶ<supplied reason="lost">ν ὁμολογούντων καὶ τοὺς</supplied> 

    <lb n="36"/>παρʼ αὐτῶν καὶ τοὺς μεταλημψομένους ὧ<supplied reason="lost">ν ἕκαστος</supplied> <choice><corr>λέλογχεν</corr><sic><supplied reason="lost">λελονχεν</supplied></sic></choice> <supplied reason="lost">ἀπὸ</supplied> 

    <lb n="37"/>τῆς ἐνεστώσης ἡμέρας ἐπὶ τὸν ἅπαντα χρόν<supplied reason="lost">ο</supplied>ν <gap reason="lost" quantity="14" unit="character"/> 

    <lb n="38"/>καὶ μὴ ἐπιπορεύεσθαι ἑκάτερον τῷ ἑτέρῳ <supplied reason="lost">ἐ</supplied>φʼ ἃ ὁ ἕτ<supplied reason="lost">ερος αὐτῶν κεκλή</supplied>

    <lb n="39" type="inWord"/>ρωται τρόπῳ μηθενί, ὅ τι δʼ ἂν τῶν προγεγραμμένω<supplied reason="lost">ν παρασυνγρα</supplied>

    <lb n="40" type="inWord"/>φῇ τις τῶν <subst><add place="inline">ὁμολογούντων</add><del rend="corrected">ομολογουντον</del></subst> ἀποτισάτω ὁ μὴ ἐ<unclear>ν</unclear>μ<supplied reason="lost">ένων τῷ ἐμμένον</supplied>

    <lb n="41" type="inWord"/>τι τά τε βλάβη καὶ δαπα<supplied reason="lost">ν</supplied>ή<supplied reason="lost">ματ</supplied>α διπ<supplied reason="lost">λ</supplied>ᾶ καὶ ἐπίτιμον <supplied reason="lost">ἀργυρίου δραχμὰς</supplied> 

    <lb n="42"/>διακοσίας <num value="200"/> καὶ εἰς τὸ δημόσιον τὰς <choice><corr>ἴσας</corr><sic>εισας</sic></choice>, καὶ μηδὲν ἧσσον. <supplied reason="lost">εὐδοκεῖ δὲ ἡ</supplied> 

    <lb n="43"/>Μαρεψῆμις γυνὴ Θομσαεῖς Μαρεψήμις ὡς ἐτῶν τριάκ<supplied reason="lost">οντα <num value="30"/> οὐλὴ </supplied><gap reason="lost" quantity="4" unit="character"/><supplied reason="lost"> δε</supplied>

    <lb n="44" type="inWord"/>ξιῷ μετὰ κυρίου αὐτοῦ Μαρεψήμιος τῇ <choice><corr>διαιρέσει</corr><sic>διερέσι</sic></choice> ταύτῃ καὶ <supplied reason="lost">μὴ ἐγκαλεῖν</supplied> 

    <lb n="45"/>μηδʼ <choice><corr>ἐνκαλέσι<supplied reason="omitted">ν</supplied></corr><sic>ενκαλ<supplied reason="lost">ε</supplied>σι</sic></choice> μηδὲ ἄλλον ὑπὲρ αὐτῆς. <choice><corr>ὑπογραφεῖς</corr><sic>υπογραφις</sic></choice> τοῦ Μαρεψή<supplied reason="lost">μιος</supplied> 

    <lb n="46"/>Ψ<supplied reason="lost">οῖ</supplied>φις Ὀν<supplied reason="lost">ν</supplied>ώφ<supplied reason="lost">ρε</supplied>ως ὡς <expan><ex>ἐτῶν</ex></expan> εἴκοσι ἓξ <num value="26"/> οὐλὴ μετώπῳ ἐξ <unclear>ἀ</unclear><supplied reason="lost">ριστερῶν, τῶν δὲ</supplied> 

    <lb n="47"/>ἄλλων <supplied reason="lost">Μ</supplied>αρεψῆμις ὃς κ<supplied reason="lost">αὶ</supplied> <unclear>Κ</unclear>αλ<unclear>ε</unclear>ῶς Μαρεψήμι<supplied reason="lost">ς</supplied> ὡς <expan><ex>ἐτῶν</ex></expan> <num value="38">λη</num> οὐ<supplied reason="lost">λὴ </supplied><gap reason="lost" quantity="11" unit="character"/> 

    <lb n="48"/><handShift new="m2"/> Μα<supplied reason="lost">ρ</supplied>ε<supplied reason="lost">ψῆμις</supplied> <unclear>ὃ</unclear>ς καὶ Παταρεῖς Παώπις <choice><corr>συνδιήρημαι</corr><sic>συνδιιρη<supplied reason="lost">μαι</supplied></sic></choice> <gap reason="lost" quantity="4" unit="character"/> 

    <lb n="49"/>τ<supplied reason="lost">ὸ</supplied> ἐ<supplied reason="lost">π</supplied>ιβάλλον μέρος τὸ προκίμενον τρίτον <num value="1/3"/> μέρος <unclear>τ</unclear><supplied reason="lost">οῦ παστο</supplied>

    <lb n="50" type="inWord"/>φορίου διστέγου. <choice><corr>εὐδοκεῖ</corr><sic>ευδοκι</sic></choice> ἡ γυνή μου Θονψαεῖς Μαρε<supplied reason="lost">ψήμ</supplied>

    <lb n="51" type="inWord"/><choice><corr>ιος</corr><sic>μαρε<supplied reason="lost">ψημ</supplied>εις</sic></choice> μετὰ κυρίου ἐμοῦ τῇ <choice><corr>διαιρέσει</corr><sic>διαιρεση</sic></choice> καὶ μὴ <choice><corr>ἐπελεύσεσθαι</corr><sic>επαιλευσεσ<supplied reason="lost">θαι</supplied></sic></choice> <gap reason="lost" quantity="3" unit="character"/> 

    <lb n="52"/>ση <choice><corr>καθὼς</corr><sic>καθ<unclear>ο</unclear>ς</sic></choice> πρόκιται. ἔγραψεν ὑπὲρ αὐτῶν Ψοῖφις 

Ὀννώφ<supplied reason="lost">ριος μὴ εἰδό</supplied>

    <lb n="53" type="inWord"/><choice><corr>των</corr><sic><supplied reason="lost">ειδο</supplied>τον</sic></choice> <choice><corr>γράμ<supplied reason="omitted">μ</supplied>ατα</corr><sic>γραματα</sic></choice>. <handShift new="m3"/> <choice><corr>Ταμαρ<supplied reason="omitted">ρ</supplied>ῆς</corr><sic>ταμαρης</sic></choice> Παώπις μετὰ κυρίου <supplied reason="lost">τοῦ ἀνδρὸς</supplied> 

    <lb n="54"/>Ψενκήβκις τοῦ <subst><add place="inline">Ὀννώφρις</add><del rend="corrected">κννωνρις</del></subst> <choice><corr>συνδιῄρημαι</corr><sic><subst><add place="inline"><unclear>σ</unclear>υδιειρημε</add><del rend="corrected">συδιιιρημε</del></subst></sic></choice> καὶ <choice><corr>λέλο<supplied reason="omitted">γ</supplied>χα</corr><sic>λελο<unclear>χ</unclear><supplied reason="lost">α</supplied></sic></choice> <supplied reason="lost">τὸ προκείμενον</supplied> 

    <lb n="55"/>μέρος τῶν <choice><corr>προκειμέν<supplied reason="omitted">ων</supplied></corr><sic>προκειμεν</sic></choice> <choice><corr>περιτετειχισμένων</corr><sic>περτεθικ<unclear>ο</unclear>σμ<unclear>ε</unclear>ν<supplied reason="lost">ων</supplied></sic></choice> <supplied reason="lost">καὶ τῶν ἐν ἑ</supplied>

    <lb n="56" type="inWord"/>τέρᾳ <choice><corr>σφραγῖδι</corr><sic>σφρακιτι</sic></choice> <choice><corr>Ἀχένους</corr><sic>αχανος</sic></choice> τόπων καὶ ἕκαστα π<supplied reason="lost">οιήσω ὡς πρό</supplied>

    <lb n="57" type="inWord"/>κιται. ἔγραψεν ὑπὲρ <subst><add place="inline">αὐτῶν</add><del rend="corrected">αυτον</del></subst> Μαρεψῆμις διὰ τὸ <supplied reason="lost">τὸν μὲν Ψεν</supplied>

    <lb n="58" type="inWord"/>κῆβκιν <choice><corr>Αἰγύπτια</corr><sic>εγυπτυ<unclear>τι</unclear>α</sic></choice> <choice><corr>γράφειν</corr><sic>γραφιν</sic></choice> τὴν <choice><corr>δὲ</corr><sic>τε</sic></choice> <choice><corr>ἄλ<supplied reason="omitted">λ</supplied>ην</corr><sic>αλην</sic></choice> μὴ <unclear>ε</unclear><supplied reason="lost">ἰδέναι <expan>γράμ<ex>ματα</ex></expan>.</supplied> 

    <lb n="59"/><handShift new="m4"/> <gap reason="ellipsis" quantity="1" unit="line"><desc>Demotic</desc></gap> 

    <lb n="59"/><handShift new="m1"/> ἔτους ἕκτου Τιβερ<supplied reason="lost">ί</supplied>ου Κλαυδίου <supplied reason="lost">Καίσαρος Σεβαστοῦ</supplied> 

    <lb n="60"/>Γερμανικοῦ Αὐτοκράτορος Ἐπεὶφ <num value="17">ιζ</num>, <expan>ἀναγέγρ<ex>απται</ex></expan> διὰ <app type="BL"><lem resp="9.356">Εὐτυχί<add place="above">δ</add>ου <supplied reason="lost">τοῦ πρὸς τῷ γραφείῳ</supplied></lem><rdg><expan>Εὐτυχείδ<ex>ου</ex></expan> <unclear>κα</unclear>ὶ <supplied reason="lost"><expan>Κρονίω<ex>νος</ex></expan> τῶν πρὸς <expan>γρ<ex>αφείῳ</ex></expan></supplied></rdg></app><supplied reason="lost"> 
