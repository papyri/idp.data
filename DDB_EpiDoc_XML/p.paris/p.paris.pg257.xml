         <head n="23611a" xml:lang="en">
            <date>AD 599</date>
            <placeName>Thinite</placeName>
            <ref type="reprint-from">cf. P.Paris 21ter</ref>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><ab>
    <lb n="1"/><gap reason="lost" extent="unknown" unit="character"/>ος Ἀρσένιος <app type="editorial"><lem resp="BL 1.341"><supplied reason="lost">Καλλινίκου</supplied></lem><rdg/></app> 

    <lb n="2"/><gap reason="lost" extent="unknown" unit="character"/> ἐφʼ ᾧ ἐπείσθην δηλαδὴ 

    <lb n="3"/><gap reason="lost" extent="unknown" unit="character"/> κατὰ νόμους ταύτης τῆς πράσεως 

    <lb n="4"/><gap reason="lost" extent="unknown" unit="character"/> κληρονόμων μου καὶ διαδόχων 

    <lb n="5"/><supplied reason="lost">καὶ διακατόχων </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> κατὰ πᾶσαν</supplied> βεβαίωσιν ἀπὸ παντὸς τοῦ 

    <lb n="6"/><choice><reg>ἐπελευσομένου</reg><orig>ἐπερευσομένου</orig></choice> <gap reason="lost" extent="unknown" unit="character"/> κληρονόμοις ἢ διαδόχοις ἢ διακατόχοις 

    <lb n="7"/><gap reason="lost" extent="unknown" unit="character"/> ἐμοῦ ὀνόματος ἢ ἄλλου τινὸς ἁπαξαπλῶς

    <lb n="8"/><supplied reason="lost">ἐν παντὶ καιρῷ ἐπάναγκες, </supplied><app type="editorial"><lem resp="BL 1.341"><supplied reason="lost">εἰ δὲ συμβαί</supplied>η, ὁ μὴ εἴη, ἐκνίκησιν</lem><rdg><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="2" unit="character"/> μήτʼ ἐκ<gap reason="lost" quantity="3" unit="character"/>κησιν</rdg></app> ἢ ἀμφισβήτησίν 

    <lb n="9"/>τινα γίνεσθαι περὶ τοῦ εἰρη<supplied reason="lost">μένου μέρ</supplied>ους τρίτου οἴκίας ἢ περὶ <app type="editorial"><lem resp="BL 1.341">μέρους αὐτοῦ <gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="1" unit="character"/></lem><rdg>μέρους <gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="1" unit="character"/></rdg></app> 

    <lb n="10"/>ἢ δεσποτείας ἢ δικαίου <app type="editorial"><lem resp="BL 1.341">οἱ<unclear>ο</unclear><supplied reason="lost">υ</supplied>δήπ<supplied reason="lost">οτε</supplied></lem><rdg>ἢ <gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="4" unit="character"/></rdg></app> παρʼ οἱουδήποτε προσώπου ἐν δικαστηρίοις 

    <lb n="11"/>ἢ ἐκτὸς δικαστηρίων ἐπὶ τῷ ἐμὲ τὸν ἀποδόμενον καὶ τοὺς ἐμοὺς κληρονόμους 

    <lb n="12"/>καὶ διαδόχους <app type="editorial"><lem resp="BL 1.341">παντοίους</lem><rdg/></app> ἰδίοις <supplied reason="lost">ἀ</supplied>ναλώμασιν καὶ δαπανήμασιν ἀποστήσειν 

    <lb n="13"/>καὶ ἀποσοβεῖν πάντα<supplied reason="lost">ς τοὺς ἐπελευσο</supplied>μένους ἐμοὶ καὶ τοῖς ἐμοῖς κληρονόμοις 

    <lb n="14"/>ἢ διαδόχοις ἢ διακατόχοις ἢ ἐπι<supplied reason="lost">γν</supplied>ῶναι τὸ προκείμενον τίμημα ἐν διπλῷ 

    <lb n="15"/>καὶ πάντα τὰ ἀναλώματα καὶ τὰ <supplied reason="lost">δ</supplied>απανήματα τὰ ἀναλωθησόμενα εἴς <app type="editorial"><lem resp="BL 1.341">τε</lem><rdg/></app> βελτίωσιν 

    <lb n="16"/>καὶ εἰς οἰκοδομὴν καὶ εἰς ἑτέραν <supplied reason="lost">χειρου</supplied>ργίαν καὶ εἰς δίκην <app type="editorial"><lem resp="BL 1.341"><supplied reason="lost" cert="low">εἴ τις συμβαίη γεν</supplied>ηθῆναι</lem><rdg><gap reason="lost" extent="unknown" unit="character"/>ηθῆναι</rdg></app> 

    <lb n="17"/>ὁμοίως ἐν διπλῷ ἀποκ<supplied reason="lost">αταστήσειν</supplied> μετὰ τοῦ ταύτην τὴν πρᾶσιν ἀρραγῆ 

    <lb n="18"/>καὶ ἀσάλευτον εἶναι διὰ παντὸς ἥνπερ ὑμῖν ἐθέμην πρὸς ἀσφάλειαν 

    <lb n="19"/><supplied reason="lost">κυρίαν οὖσαν καὶ βεβαίαν πανταχοῦ</supplied> προφερομένην κινδύνῳ ἐμῷ 

    <lb n="20"/>καὶ τῶν ἐμῶν κληρονόμων <app type="editorial"><lem resp="BL 1.341">κ<supplied reason="lost">αὶ διαδόχων πόρῳ τῆς πα</supplied>ντοίας ἡμῶν ὑποστάσεως</lem><rdg><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> πράσ</supplied>εωσ</rdg></app> 

    <lb n="21"/>τῆς καὶ <expan>ἐπικειμέ<ex>νης</ex></expan> ὑμῖν εἰς τοῦτο καθάπερ ἐκ δίκης· καὶ ἐπὶ τούτοις ἅπασιν 

    <lb n="22"/>ἐπερωτηθεὶς ὡμολόγησα <g type="stauros"/> <g type="stauros"/> <g type="stauros"/> <handShift new="m2"/> <g type="stauros"/> Αὐρήλιος Ἀρσένιος Καλλινίκου ὁ προγεγραμμένος 

    <lb n="23"/>ἐθέμην τὴν παροῦσαν <app type="editorial"><lem resp="BL 1.341">πρᾶσιν τοῦ</lem><rdg>πρᾶσιν <gap reason="illegible" quantity="4" unit="character"/> του</rdg></app> εἰρημένου μέρους τρίτου οἰκίας μετὰ παντὸς 

    <lb n="24"/>αὐτῆς τοῦ δικαίου καὶ <app type="editorial"><lem resp="BL 1.341"><expan>ἀπέσχ<ex>ον</ex></expan> <supplied reason="lost">π</supplied>αρὰ τῶν προκ<unclear>ει</unclear>μέν<supplied reason="lost">ων</supplied> <unclear>τὴν</unclear> τιμήν, τουτʼ ἔστιν</lem><rdg><gap reason="lost" extent="unknown" unit="character"/></rdg></app> 

    <lb n="25"/>χρυσοῦ <app type="editorial"><lem resp="BL 1.341">νομίσματα δύο παρὰ κεράτια <gap reason="illegible" quantity="2" unit="character"/>, <supplied reason="lost"><expan>νο<ex>μισμάτια</ex></expan> <num value="2">β</num> <expan>π<ex>αρὰ</ex></expan> <expan>κε<ex>ράτια</ex></expan> </supplied><gap reason="lost" quantity="2" unit="character"/> ζυγῷ</lem><rdg>νομί<supplied reason="lost">σματα </supplied><gap reason="lost" extent="unknown" unit="character"/> τῷ</rdg></app> Ἀλεξανδρείας <app type="editorial"><lem resp="BL 1.341">καὶ βεβαιώσω</lem><rdg><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="2" unit="character"/></rdg></app> 

    <lb n="26"/>ταύτην τὴν πρᾶσιν κατὰ πᾶσαν βεβαίωσιν· καὶ στοιχεῖ μοι πάντα ὡς <expan>πρόκ<ex>ειται</ex></expan>. 

    <lb n="27"/>ἔγραψα ὑπὲρ αὐτοῦ <app type="editorial"><lem resp="BL 1.341"><expan>ἀξιω<ex>θεὶς</ex></expan></lem><rdg/></app> <expan>Φλ<ex>άουιος</ex></expan> <app type="editorial"><lem resp="BL 1.341">Φ<gap reason="lost" quantity="2" unit="character"/><gap reason="illegible" quantity="1" unit="character"/>σι<gap reason="illegible" quantity="2" unit="character"/><unclear>αιος</unclear> <gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" quantity="3" unit="character"/>υ <gap reason="illegible" quantity="2" unit="character"/><abbr>κ<gap reason="illegible" quantity="2" unit="character"/>ομ</abbr> ἀπὸ Πανός</lem><rdg>Φ<gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="4" unit="character"/> ἀπὸ <gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="1" unit="character"/></rdg></app>. 

    <lb n="28"/><handShift new="m3"/> <g type="stauros"/> <expan>Φλ<ex>άουιος</ex></expan> Εὑρέσιος Βίκτορος Κτήτωρ ἀπὸ <supplied reason="lost">Παν</supplied>ὸς μαρτυρῶ τῇ παρούσῃ πράσει αἰτηθεὶς παρὰ τοῦ θεμένου. 

    <lb n="29"/><handShift new="m4"/> <g type="stauros"/> <expan>Φλ<ex>άουιος</ex></expan> Ἰωάννης <add place="above"><g type="stauros"/></add> Μεγάλου ἀπὸ Πανὸς μαρτυρῶ τῇ <supplied reason="lost">παρούσ</supplied>ῃ πράσει αἰτηθεὶς παρὰ τοῦ θεμένου. <g type="stauros"/> 

    <lb n="30"/><handShift new="m5"/> <g type="stauros"/> <expan>Φλ<ex>άουιος</ex></expan> Ζαχαρίας <app type="editorial"><lem resp="BL 1.341">Παννῆτος σὺν <expan>θ<ex>εῷ</ex></expan> <expan>γραμμ<ex>ατεὺς</ex></expan></lem><rdg><gap reason="lost" quantity="4" unit="character"/>νευτοσ</rdg></app> ἀπὸ Πανὸς μαρτυρῶ τῇ παρούσῃ πράσει αἰτηθεὶς παρὰ τοῦ 

    <lb n="31"/>θεμένου. <g type="stauros"/> 

    <lb n="32"/><handShift new="m6"/> <g type="stauros"/> ἐγράφη διὰ <expan>γρ<ex>αφείου</ex></expan> Μάρκου <app type="editorial"><lem resp="BL 1.341">σὺν θεῷ</lem><rdg><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="2" unit="character"/></rdg></app> <expan>συμβολαιογρά<ex>φου</ex></expan>. 

    <lb n="33"/><gap reason="illegible" extent="unknown" unit="character"><desc>vestiges</desc></gap> </ab></div>
