    <lb n="1"/><gap reason="lost" quantity="17" unit="character"/><gap reason="illegible" quantity="1" unit="character"/>αστω<gap reason="lost" quantity="3" unit="character"/><unclear>τ</unclear>ε του <unclear>πυ</unclear><gap reason="lost" quantity="10" unit="character"/><gap reason="illegible" quantity="1" unit="character"/> καὶ ἀπὸ 

