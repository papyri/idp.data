         <head n="13732" xml:lang="en">
            <date>AD 341</date>
            <placeName>?</placeName>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><ab>

    <lb n="1"/>Φλαουίοις Ποιμεν<gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> καὶ </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ἀπὸ</supplied> 

    <lb n="2"/>ἐπάρχων τοῦ <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="3"/>παρὰ Αὐρηλίου Πρωτᾶ <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="4"/><choice><reg>ἐπιμελητοῦ</reg><orig>ἐπειμελητοῦ</orig></choice> χρυσίο<supplied reason="lost">υ </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ὁμολογῶ ὀμνύων τὴν</supplied> 

    <lb n="5"/>τύχην καὶ <choice><reg>νίκην</reg><orig>νείκην</orig></choice> τῶ<supplied reason="lost">ν τὰ πάντα νικώντων δεσποτῶν ἡμῶν Αὐγούστων, ὑπατείας</supplied> 

    <lb n="6"/><hi rend="diaeresis">Ἰ</hi>ουλίου Κωνσταντίο<supplied reason="lost">υ πατρικίου ἀδελφοῦ Κωνσταντίνου Αὐγούστου καὶ</supplied> 

    <lb n="7"/>Ῥουφίνου τῶν λαμπ<supplied reason="lost">ροτάτων </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> παρειληφέναι παρὰ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="8"/>ἀπὸ λόγου προστίμο<supplied reason="lost">υ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="9"/>ἔπαρχον ἐξουσίαν δι<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="10"/>βαλανίου χρυσίου οὐγ<supplied reason="lost">κίας δύο <num value="2"/> </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="11"/>ὅνπερ χρυσὸν κατεκ<supplied reason="lost">όμισα </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="12"/>χρυσώνῃ ἀκολούθω<supplied reason="lost">ς </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="13"/>ὑπʼ ἐμοῦ γ<g type="apostrophe"/>εγενῆσθ<supplied reason="lost">αι </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> τοῦ</supplied> 

    <lb n="14"/>διασημοτάτου ἡγε<supplied reason="lost">μόνος </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="15"/><supplied reason="lost" cert="low">ἔστι δὲ τῆς ἀποχῆς τὸ ἀντίγραφον </supplied>· <gap reason="lost" extent="unknown" unit="character"/>

    <lb n="16"/>ὑπατ<supplied reason="lost">εί</supplied>ας Ἰουλίου Κω<supplied reason="lost">νσταντίου πατρικίου ἀδελφοῦ Κωνσταντίνου Αὐγούστου καὶ</supplied> 

    <lb n="17"/>Ῥουφίνου τῶν λαμ<supplied reason="lost">προτάτων </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="18"/>χρυσώνης Αὐρηλ<supplied reason="lost">ίῳ Πρωτᾷ ἐπιμελητῇ χρυσίου <expan>χα<ex>ίρειν</ex></expan>. παρείληφα παρὰ σοῦ</supplied> 

    <lb n="19"/>ἃς <choice><reg>κατεδίκασε</reg><orig>κατεδίκασαι</orig></choice> ὁ κύ<supplied reason="lost">ριος ἡμῶν ὁ διασημότατος ἡγεμὼν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="20"/>Παῦλον ὑπὲρ ὀνόμ<supplied reason="lost" cert="low">ατος </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> χρυσίου καθαροῦ</supplied> 

    <lb n="21"/>οὐγ<g type="apostrophe"/>κίας δύο. ἐρρῶ<supplied reason="lost">σθαί σε εὔχομαι.</supplied> 

    <lb n="22"/>ὑπατείας <choice><reg>Ἀντωνίου</reg><orig>Ἀντωνινοῦ</orig></choice> Μ<supplied reason="lost">αρκελλίνου καὶ Πετρωνίου Προβίνου τῶν λαμπροτά</supplied>

    <lb n="23" break="no"/>τ<supplied reason="lost">ω</supplied><unclear>ν</unclear> Ἐπεὶφ <num value="4">δ</num>. <gap reason="lost" extent="unknown" unit="character"/> 
