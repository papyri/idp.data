            <ref n="cpr;1;242" type="reprint-from">CPR1,242</ref>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><ab>
    <lb n="1"/>ἔτους πέμπτου Γαίου Καίσαρος Αὐτοκ<supplied reason="lost">ράτορος Σεβαστοῦ</supplied> 

    <lb n="2"/>μηνὸς <app type="editorial"><lem resp="BL 2.2.167">Σωτῆρ<supplied reason="lost">ος</supplied></lem><rdg>Σωτηρ<supplied reason="lost">ίου</supplied></rdg></app><gap reason="lost" extent="unknown" unit="character"/>δεκάτῃ ἐν Σεκ<supplied reason="lost">νεπαίου Νήσῳ</supplied> 

    <lb n="3"/>τῆς Ἡρακλείδου μερίδος τοῦ Ἀρσιν<supplied reason="lost">οίτου νομοῦ. ἐμίσ</supplied>

    <lb n="4" break="no"/>θωσεν Ἁρπαγάθης Ἐριέως ὡς ἐτ<supplied reason="lost">ῶν </supplied><gap reason="lost" quantity="5" unit="character"/> 

    <lb n="5"/>οὐλὴ <choice><reg>μετώπῳ</reg><orig>μετόπῳ</orig></choice> μέσῳ. Στοθοῆτις ὡς <supplied reason="lost">ἐτῶν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="6"/>οὐλὴ <choice><reg>μετώπῳ</reg><orig>μετόπῳ</orig></choice> ἐκ <choice><reg>δεξιῶν</reg><orig>δεξειῶ<unclear>ν</unclear></orig></choice> καὶ Ἀπ<supplied reason="lost">ῦγχις ὡς ἐτῶν εἴκοσι</supplied> 

    <lb n="7"/><choice><reg>τριῶν</reg><orig>τρις</orig></choice> <num value="23"/> οὐλὴ <choice><reg>πήχει</reg><orig>πήχι</orig></choice> ἐκ <choice><reg>δεξιῶν</reg><orig>δεξειῶν</orig></choice> ἀμφό<supplied reason="lost">τεροι Πανεφρέμμιος</supplied> 

    <lb n="8"/>Πέρσαι τῆς ἐπ<supplied reason="lost">ιγον</supplied>ῆς ἀλλήλων ἔν<supplied reason="lost">γυοι εἰς ἔκτισιν μεμισ</supplied>

    <lb n="9" break="no"/>θώμεθα παρʼ αὐτοῦ <choice><reg>τὸ</reg><orig>τωὶ</orig></choice> ὑπάρχον <supplied reason="lost">αὐτῷ ἐλαιουργεῖον</supplied> 

    <lb n="10"/>σὺν <choice><reg>χοίνιξι</reg><orig>χοινικεσιν</orig></choice> <choice><reg>δυσὶ</reg><orig>δοισὶ</orig></choice> <num value="2"/> καὶ <choice><reg>μηχανῇ</reg><orig>μηχανης</orig></choice> ἐ<unclear>λ</unclear><supplied reason="lost">αιουργικῇ ἀπὸ τῆς</supplied> 

    <lb n="11"/><choice><reg>ἐνεστώσης</reg><orig>ἐνεστόσης</orig></choice> ἡμέρας ἕως <choice><reg>μηνὸς</reg><orig>μηνι</orig></choice> <choice><reg>Καισαρείου</reg><orig>Καισαρείω</orig></choice>τοῦ 
<choice><reg>ἐνε<lb n="12" break="no"/>στῶτος</reg><orig>ἐνε<lb n="12" break="no"/>στοσης</orig></choice> ἔτους ἕως ἐπαγομένων πέμ<supplied reason="lost">πτης</supplied> 

    <lb n="13"/>φόρου τοῦ παντὸς ἀργυρίου δραχμὰς <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="14"/>δώσουσιν δὲ οἱ μεμισθωμένοι τ<supplied reason="lost">ῷ Ἁρπαγάθῃ ἀπὸ τοῦ</supplied> 

    <lb n="15"/>προκειμένου κεφαλαίου ἀργυρίου δρ<supplied reason="lost">αχμὰς εἴκοσι ὀκτὼ <num value="28"/></supplied> 

    <lb n="16"/>ἀπὸ μηνὸς Ἰουλιέως ἕως μηνὶ Δ<supplied reason="lost">ρουσιεῖ τριακάδι</supplied> 

    <lb n="17"/>τοῦ αὐτοῦ ἔτους κατὰ μῆνα ἐκτίν<supplied reason="lost">οντες ἀργυρίου δρα</supplied>

    <lb n="18" break="no"/>χμὰς <choice><reg>τέσσαρας</reg><orig>τέσσαρες</orig></choice> <num value="4"/>, τ<supplied reason="lost">ὰς</supplied> δὲ λοιπὰς ἄλλ<supplied reason="lost">ας δραχμὰς δέκα</supplied> 


    <lb n="19"/><choice><reg>δύο</reg><orig>δοίωι</orig></choice> <num value="12"/> δώσουσιν οἱ μεμισθωμέν<supplied reason="lost">οι </supplied><gap reason="lost" extent="unknown" unit="character"/> <choice><reg><supplied reason="lost">ἐλαι</supplied><lb n="20" break="no"/>ουργείου</reg><orig><supplied reason="lost"> ελαι</supplied><lb n="20" break="no"/>οργιουου</orig></choice> ἐν αὐτῶι τὸ ὄργανον πᾶν δ<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="21"/>ξύλων τοῦ <choice><reg>ἐλαιουργείου</reg><orig>ἐλεοργίου</orig></choice> εἶναι πρὸς το<supplied reason="lost">ὺς </supplied><choice><reg><supplied reason="lost">μεμισθωμέ</supplied><lb n="22" break="no"/>νους</reg><orig><supplied reason="lost">μεμισθωμε</supplied><lb n="22" break="no"/>νοις</orig></choice> <subst><add place="inline">σὺν</add><del rend="corrected">σο<unclear>ι</unclear>ν</del></subst> <choice><reg>μισθοῖς</reg><orig>μισθους</orig></choice> <choice><reg>τεκτόνων</reg><orig>τεκτωνες</orig></choice> ἐλ<supplied reason="lost">αιουργείου</supplied> 

    <lb n="23"/><choice><reg>εἶναι</reg><orig>εἰεῖναι</orig></choice> πρὸς τὸν Ἁρπαγάθην ο<unclear>μ</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="24"/><hi rend="diaeresis">ὑ</hi>πογραφεὺς τῶν μεμισθωμέ<supplied reason="lost">νων </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="25"/><supplied reason="lost">Τ</supplied><unclear>εσσεν</unclear>οῦφις ὡς <expan><ex>ἐτῶν</ex></expan> <num value="35">λε</num> <expan>οὐ<ex>λὴ</ex></expan> <choice><reg>μετώπῳ</reg><orig>μετόπῳ</orig></choice> ἐκ δ<supplied reason="lost">εξιῶν καὶ Σεν</supplied>

    <lb n="26" break="no"/>οῦφις <choice><reg>Σενούφιος</reg><orig>Σενουφις</orig></choice> ὡς <expan><ex>ἐτῶν</ex></expan> <num value="23">κγ</num> <expan>οὐ<ex>λὴ</ex></expan> μετ<supplied reason="lost">ώπῳ </supplied><gap reason="lost" quantity="4" unit="character"/> 

    <lb n="27"/>Στοτοῆτις καὶ Ἀπῦγχις <choice><reg>ἀμφό<supplied reason="lost">τεροι</supplied></reg><orig>ἀμφώ<supplied reason="lost">τεροι</supplied></orig></choice> <choice><reg><supplied reason="lost">Πανεφρέ</supplied><lb n="28" break="no"/>μμιος</reg><orig><supplied reason="lost"> πανεφρε</supplied><lb n="28" break="no"/>μμις</orig></choice> οἱ <choice><reg>δύο</reg><orig>δυω</orig></choice> <num value="2"/> Πέρσαι τῆς <choice><reg>ἐπιγον<supplied reason="lost">ῆς</supplied></reg><orig>ἐπικων<supplied reason="lost">ῆς</supplied></orig></choice> <supplied reason="lost">ὄντες ἀλλή</supplied>

    <lb n="29" break="no"/>λων <expan>ἔνγυ<ex>οι</ex></expan> εἰς ἔκτισιν ὁμολογ<supplied reason="lost">οῦμεν </supplied><choice><reg><supplied reason="lost">μεμισ</supplied><lb n="30" break="no"/>θῶσθαι</reg><orig><supplied reason="lost">μεμισ</supplied><lb n="30" break="no"/>θωμε</orig></choice> παρὰ Ἁρπαγάθου τοῦ Ἐριέ<supplied reason="lost">ως ἐλαιουργεῖον σὺν</supplied> 

    <lb n="31"/>μηχανῇ ἀπὸ τῆς <choice><reg>ἐνεστώσης</reg><orig>ἐνεστώσις</orig></choice> <supplied reason="lost">ἡμέρας ἕως μηνὶ</supplied> 

    <lb n="32"/><supplied reason="lost">Καισαρ</supplied><unclear>είω</unclear>ι <unclear>τοῦ</unclear> ἐν<unclear>ε</unclear><supplied reason="lost">σ</supplied><unclear>τῶτος</unclear> <supplied reason="lost">ἔτους <expan>ἐπαγομ<ex>ένων</ex></expan> <num value="5">ε</num> φόρου</supplied> 

    <lb n="33"/>τοῦ παντὸς ἀργυρίου δραχμὰς τεσσαράκον

    <lb n="34" break="no"/>τα <num value="40"/> ἃς καὶ διαγράψομεν ἀργυρί<unclear>ο</unclear><supplied reason="lost">υ δραχμὰς εἴκο</supplied>

    <lb n="35" break="no"/>σι ὀκτὼ <num value="28"/> ἀ<unclear>π</unclear><supplied reason="lost">ὸ τ</supplied>οῦ <choice><reg>προκειμένου</reg><orig>προκειμενη</orig></choice> ἀπ<supplied reason="lost">ὸ μηνὸς Ἰουλιέως</supplied> 

    <lb n="36"/>ἐν μηνὶ <choice><reg>Δρουσιεῖ</reg><orig>Δρουσιευς</orig></choice> κατὰ <choice><reg>μῆ<supplied reason="omitted">να</supplied></reg><orig>μη</orig></choice> <choice><reg>ἕκαστον</reg><orig>ἑξο<gap reason="illegible" quantity="1" unit="character"/>σο</orig></choice> <gap reason="lost" quantity="6" unit="character"/><supplied reason="lost"> δραχμὰς</supplied> 

    <lb n="37"/><choice><reg>τέσσαρας</reg><orig>τέσσαρος</orig></choice> <num value="4"/> ἃς καὶ <choice><reg>διαγράψομε<supplied reason="omitted">ν</supplied></reg><orig>διαγραψομε</orig></choice> τῷ μ<supplied reason="lost">ηνὶ </supplied><gap reason="lost" quantity="6" unit="character"/><supplied reason="lost"> δραχ</supplied>

    <lb n="38" break="no"/>μὰς δέκα <choice><reg>δύο</reg><orig>δυω</orig></choice> <num value="12"/> ἀ<supplied reason="lost">πὸ τ</supplied>οῦ προκειμέν<supplied reason="lost">ου κεφαλαίου</supplied> </ab></div>
