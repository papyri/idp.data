         <div xml:lang="grc" type="edition" xml:space="preserve">
<div n="r" type="textpart"><ab>
    <lb n="1"/>ἔτους <choice><reg>τρεισκαιδεκάτου</reg><orig>τρισκαι<supplied reason="lost">δεκά</supplied>του</orig></choice> Αὐτοκράτορο<supplied reason="lost">ς Καίσαρος Νέρουα</supplied> 

    <lb n="2"/>Τρα<hi rend="diaeresis">ϊ</hi>ανοῦ <expan>Σεβασ<ex>τοῦ</ex></expan> Γερμανικοῦ Δακι<supplied reason="lost">κοῦ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="3"/><num value="30">λ</num> ἐκ Ἀφροδείτῃ Βερνίκῃ <unclear>Ἡ</unclear><supplied reason="lost">ρακ</supplied><unclear>λε</unclear><supplied reason="lost">ίδου μερίδος</supplied> 

    <lb n="4"/>τοῦ Ἀρσινοείτου νομ<supplied reason="lost">ο</supplied>ῦ· ἐδάνεισεν Λ<supplied reason="lost">ούκιος Βελλῆνος</supplied> 

    <lb n="5"/>Γέμελλος διὰ Ἐπ<supplied reason="lost">α</supplied>γά<supplied reason="lost">θ</supplied>ου π<supplied reason="lost">αι</supplied>δαρί<supplied reason="lost">ου </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="6"/>οὐλὴ ἀντικνημί<supplied reason="lost">ω</supplied>ι ἀριστερῶ<supplied reason="lost">ι</supplied> Χα<supplied reason="lost">ιρᾷ </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ὡς ἐτῶν </supplied><gap reason="lost" extent="unknown" unit="character"/>  

    <lb n="7"/>κοντα πέντε οὐλὴ σιαγόνι δεξιᾷ <supplied reason="lost">καὶ Σοχώτῃ ὡς ἐτῶν</supplied> 

    <lb n="8"/>εἴκοσι πέντε <num value="25"/> οὐλὴ πήχει δεξιῶι ἀ<supplied reason="lost">μφοτέροις </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="9"/>νιος <choice><reg>Πέρσαις</reg><orig>Περσαι</orig></choice> τῆ<supplied reason="lost">ς ἐ</supplied>πιγονῆς <app type="editorial"><lem resp="BL 3.235">ἀ<supplied reason="lost">λληλ</supplied>έγγ<supplied reason="lost">υοι εἰς ἔκτι</supplied>

    <lb n="10" break="no"/>σιν</lem><rdg>κ<gap reason="lost" quantity="4" unit="character"/>ην<gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> χρῆ</supplied>σιν</rdg></app> ἀργυρίου <supplied reason="lost">ἐ</supplied>πισή<supplied reason="lost">μο</supplied><unclear>υ</unclear> σεβαστο<supplied reason="lost">ῦ νομίσματος κε</supplied>

    <lb n="11" break="no"/>φαλαί<supplied reason="lost">ο</supplied>υ δραχμὰς ἑκατ<supplied reason="lost">ὸ</supplied>ν τεσσ<supplied reason="lost">αράκοντα <num value="140"/> ἃς καὶ εἴλη</supplied>

    <lb n="12" break="no"/>φαν οἱ <choice><reg>δεδανεισμένοι</reg><orig>δεδανισμένοι</orig></choice> παρὰ τοῦ Ἐπα<supplied reason="lost">γάθου </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="13"/>κες ὧν καὶ τὴν ἀπόδοσιν <app type="editorial"><lem resp="V. Iannace, ZPE 233 (2025) 203">ποιησά<supplied reason="lost">σθωσαν οἱ</supplied> <choice><reg><supplied reason="lost">δεδα</supplied><lb n="14" break="no"/>νεισμένοι</reg><orig><supplied reason="lost">δεδα</supplied><lb n="14" break="no"/>νισμένοι</orig></choice></lem><rdg>ποιήσα<supplied reason="lost">σθαι ὁμολογοῦσι οἱ </supplied><choice><reg><supplied reason="lost">δεδα</supplied><lb n="14" break="no"/>νεισμένοι</reg><orig><supplied reason="lost">δεδα</supplied><lb n="14" break="no"/>νισμένοι</orig></choice></rdg></app> τῶι Λουκίωι Βελλήνωι Γ<supplied reason="lost">εμέλλῳ τῷ μηνὶ</supplied> 

    <lb n="15"/><supplied reason="lost">Πα</supplied>ῦν<supplied reason="lost">ι τ</supplied>οῦ <choice><reg>εἰσιόντος</reg><orig><hi rend="diaeresis">ἰ</hi>σιόντος</orig></choice> τεσσ<supplied reason="lost">αρεσκ</supplied>αιδ<supplied reason="lost">εκάτου ἔτους</supplied> 

    <lb n="16"/>Α<supplied reason="lost">ὐ</supplied>τοκ<supplied reason="lost">ρ</supplied>άτ<supplied reason="lost">ορ</supplied>ος Καί<supplied reason="lost">σα</supplied>ρος Νέρ<unclear>ου</unclear><supplied reason="lost">α Τ</supplied>ρα<hi rend="diaeresis">ϊ</hi>α<supplied reason="lost">νοῦ Σεβαστοῦ Γερμανικοῦ</supplied> 

    <lb n="17"/>Δ<supplied reason="lost">α</supplied>κικοῦ <choice><reg>ἀτοκί</reg><orig>ἀτοκ<supplied reason="lost">ε</supplied>ί</orig></choice>· τοῦ δὲ <app type="editorial"><lem resp="BL cf. 3.235"><unclear>ὑπ</unclear><supplied reason="lost">ε</supplied>ρ<unclear>π</unclear><supplied reason="lost">εσόντος </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> χρόνου</supplied></lem><rdg>ε<unclear>ν</unclear><supplied reason="lost">τ</supplied>ρ<unclear>ε</unclear><gap reason="lost" extent="unknown" unit="character"/></rdg></app> 

    <lb n="18"/>σ<supplied reason="lost">υν</supplied>εχώρ<supplied reason="lost">ου</supplied>ν οἱ <choice><reg>δεδανεισμένοι</reg><orig>δ<supplied reason="lost">εδ</supplied>ανισμέ<supplied reason="lost">ν</supplied>οι</orig></choice> <supplied reason="lost">καὶ</supplied> οἱ <app type="editorial"><lem resp="BL 3.235"><supplied reason="lost">παρʼ αὐτῶν ἐκτίσειν τόκον διδραχμιαῖον</supplied></lem><rdg>παρʼ αὐτῶν <gap reason="lost" extent="unknown" unit="character"/> </rdg></app> 

    <lb n="19"/><app type="editorial"><lem resp="BL 3.235">ἑ<supplied reason="lost">κά</supplied>στης <supplied reason="lost">μνᾶς</supplied> <unclear>κ</unclear>α<supplied reason="lost">τ</supplied><unclear>ὰ</unclear></lem><rdg>μ<gap reason="lost" quantity="2" unit="character"/>ς τῆς <gap reason="lost" quantity="2" unit="character"/><gap reason="illegible" quantity="1" unit="character"/>α<gap reason="lost" quantity="1" unit="character"/><gap reason="illegible" quantity="1" unit="character"/></rdg></app> μῆνα ἕκ<supplied reason="lost">αστον </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> τῆς πράξεως οὔσης τῷ</supplied> 

    <lb n="20"/>Λ<supplied reason="lost">ου</supplied>κίωι Β<supplied reason="lost">ε</supplied>λλ<supplied reason="lost">ήν</supplied>ωι Γεμέλλω<supplied reason="lost">ι </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="21"/>τῶ<supplied reason="lost">ν</supplied> <choice><reg>δεδανεισμένων</reg><orig>δεδ<supplied reason="lost">α</supplied>νισ<supplied reason="lost">μέν</supplied>ων</orig></choice> καὶ <supplied reason="lost">ἐξ</supplied> <app type="editorial"><lem resp="BL 3.235">ο<supplied reason="lost">ὗ ἐὰν αὐτῶν αἱρῆται</supplied></lem><rdg>ἑ<supplied reason="lost">νὸς <num value="1"/> ἑκάστου αὐτῶν</supplied></rdg></app> 

    <lb n="22"/><app type="editorial"><lem resp="BL 3.235">κ<supplied reason="lost">αὶ ἐκ τῶν</supplied> ὑπαρχόν<supplied reason="lost">τω</supplied><unclear>ν</unclear> αὐτοῖς π<supplied reason="lost">άντων καθάπερ ἐκ δίκης</supplied></lem><rdg>κ<supplied reason="lost">αὶ τοῦ</supplied> ὑπάρχον<supplied reason="lost">το</supplied>ς αὐτοῖς π<gap reason="lost" extent="unknown" unit="character"/></rdg></app> 

    <lb n="23"/>ἀ<supplied reason="lost">π</supplied>εσχηκότω<supplied reason="lost">ν</supplied> τῶν δεδαν<supplied reason="lost">εισμένων παρὰ Λουκίου</supplied> 

    <lb n="24"/>Βελλήνου Γεμέλλου τὰ <app type="editorial"><lem resp="BL 3.235">ἐν<supplied reason="lost">οίκ</supplied>ι<supplied reason="lost">α τοῦ ἐνεστῶτος</supplied></lem><rdg>ἐν<supplied reason="lost">οίκ</supplied>ι<supplied reason="lost">α </supplied><gap reason="lost" extent="unknown" unit="character"/></rdg></app> 

    <lb n="25"/><choice><reg>τρεισκαιδεκάτου</reg><orig>τρισκαιδεκάτου</orig></choice> ἔτους ὧν ἔχ<supplied reason="lost">ει</supplied> <app type="editorial"><lem resp="BL 3.235"><supplied reason="lost">π</supplied>α<unclear>ρ</unclear>ʼ <supplied reason="lost">αὐτῶν ἐν μισθώσει </supplied><gap reason="lost" extent="unknown" unit="character"/></lem><rdg><supplied reason="lost">τ</supplied>α<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/></rdg></app> 

    <lb n="26"/>ἐν ᾧ ἔχουσι οἱ <choice><reg>δεδανεισμένοι</reg><orig>δεδανισμένοι</orig></choice> ἥμ<supplied reason="lost">ισυ <num value="1/2"/> </supplied><gap reason="lost" extent="unknown" unit="character"/>  

    <lb n="27"/>ἐν κώμῃ Εὐημερ<supplied reason="lost">ε</supplied>ί<supplied reason="lost">ᾳ τ</supplied>ῆ<supplied reason="lost">ς Θεμίστου μερίδος </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ὑπογραφεὺς</supplied> 

    <lb n="28"/>τῶν δεδανισμέ<supplied reason="lost">νων</supplied> Ἥ<supplied reason="lost">ρων</supplied> <gap reason="illegible" quantity="1" unit="character"/> <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="29"/><handShift new="m2"/> Χαιρᾶς καὶ Σοχ<supplied reason="lost">ώ</supplied>της <gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ἀμφότεροι Πέρ</supplied>

    <lb n="30" break="no"/>σαι τῆς ἐπιγον<supplied reason="lost">ῆς</supplied> καὶ Α<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="1" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="31"/><choice><reg>δεδανείσμεθα</reg><orig>δεδανίσμεθα</orig></choice> π<supplied reason="lost">α</supplied>ρὰ Λο<supplied reason="lost">υκ</supplied>ί<supplied reason="lost">ου Βελλήνου Γεμμέλλου διὰ</supplied> 

    <lb n="32"/>Ἐπαγάθου παιδαρίου τ<supplied reason="lost">ὰς</supplied> τοῦ ἀ<supplied reason="lost">ργυρίου σεβαστοῦ νομίσματος δρα</supplied>

    <lb n="33" break="no"/>χμὰς ἑκατὸ<supplied reason="lost">ν τεσ</supplied>σαράκοντα <num value="140"/> <supplied reason="lost">ἃς καὶ ἀποδώσομεν τῷ</supplied> 

    <lb n="34"/>μηνὶ Παῦνι τοῦ <choice><reg>εἰσιόντος</reg><orig><hi rend="diaeresis">ἰ</hi><supplied reason="lost">σιόν</supplied>τος</orig></choice> τ<supplied reason="lost">εσσαρεσκαιδεκάτου</supplied> 

    <lb n="35"/>ἔτους <choice><reg>ἀτοκί</reg><orig>ἀτοκε<supplied reason="lost">ί</supplied></orig></choice>· τοῦ δὲ ἐν<unclear>τ</unclear>ρ<unclear>ε</unclear><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> τε</supplied>

    <lb n="36" break="no"/>λέσομεν τὸ ἴσ<supplied reason="lost">ο</supplied>ν <gap reason="lost" quantity="2" unit="character"/>ε<gap reason="lost" quantity="4" unit="character"/>ν<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="37"/>μῆνα ἕκα<supplied reason="lost">στο</supplied>ν καὶ ἀπε<supplied reason="lost">σχήκαμεν</supplied> πα<supplied reason="lost">ρὰ Λουκίου Βελλήνου</supplied> 

    <lb n="38"/>Γεμέλλου τὰ ἐ<unclear>ν</unclear>οίκ<unclear>ια</unclear> ἀπὸ <gap reason="lost" quantity="5" unit="character"/>ην<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="39"/>ἔτους ὧν <choice><reg>ἔχει</reg><orig><unclear>ἔ</unclear>χι</orig></choice> τα<gap reason="lost" quantity="3" unit="character"/><gap reason="illegible" quantity="1" unit="character"/>ο<gap reason="illegible" quantity="5" unit="character"/>ιη<gap reason="lost" extent="unknown" unit="character"/> 

