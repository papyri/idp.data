    <lb n="41"/><handShift new="m3" cert="low"/> <expan>πρόκ<ex cert="low">ειται</ex></expan> ἔγραψα <expan>ὑπ<ex>ὲρ</ex></expan> <supplied reason="omitted">αὐτῶν</supplied> Ἥρων Νειλ<gap reason="illegible" quantity="3" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="42"/><expan>ἀναγέγ<ex>ραπται</ex></expan> διʼ <app type="editorial"><lem resp="BL 1.408"><unclear>Ἀφ</unclear>ρ<unclear>οδ</unclear>ι<gap reason="lost" quantity="3" unit="character"/><gap reason="illegible" quantity="1" unit="character"/>ς</lem><rdg><abbr>Η</abbr> Εὐη<supplied reason="lost">μερί</supplied>ασ</rdg></app> <expan>γρ<ex>αφείου</ex></expan> 
