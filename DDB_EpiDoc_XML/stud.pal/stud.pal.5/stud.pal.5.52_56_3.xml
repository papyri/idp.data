    <lb n="16"/>καὶ αὐτοῦ <num value="14">ιδ</num> <expan><ex>ἔτους</ex></expan> <expan>μη<ex>νῶν</ex></expan> <num value="35">λε</num> <expan>ἡμερ<ex>ῶν</ex></expan> <num value="2">β</num><supplied reason="lost"><num value="5">ε</num></supplied> ὡς τοῦ 

    <lb n="17"/><expan>μη<ex>νὸς</ex></expan> <expan><ex>δραχμῶν</ex></expan> <num value="180">ρπ</num> <expan><ex>τάλαντον</ex></expan> <num value="1">α</num> <expan><ex>δραχμὰς</ex></expan> <num value="450">υν</num> <expan><ex>γίνεται</ex></expan> ἐπὶ τὸ αὐτὸ τῆς 

