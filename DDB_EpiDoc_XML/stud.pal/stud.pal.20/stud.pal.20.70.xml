         <head n="15027" xml:lang="en">
            <date>AD 261</date>
            <placeName>Arsinoite</placeName>
            <ref n="sb;1;5126" type="reprint-from">SB 1 5126</ref>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve">
<ab>
    <lb n="1"/>Α<supplied reason="lost">ὐ</supplied>ρηλίᾳ <app type="editorial"><lem resp="BL 12.276"><subst><add place="inline">Θερμουθαρίῳ</add><del rend="corrected">Θερμουθαριν</del></subst></lem><rdg>Θερμουθαρίῳ</rdg></app> <app type="editorial"><lem resp="BL 12.276"><choice><reg>Ἀντινοΐδι</reg><orig>Ἀ<unclear>ντινο</unclear>ΐς</orig></choice></lem><rdg><choice><reg>τῇ</reg><orig>ῇ</orig></choice> <supplied reason="lost">κα</supplied>ὶ <choice><reg>Ἡραίδι</reg><orig>Ἡραις</orig></choice></rdg></app> <choice><reg>μήτρι</reg><orig>μήτηρ</orig></choice>

    <lb n="2"/><app type="editorial"><lem resp="BL 12.276"><choice><reg>δικαίῳ</reg><orig>δικέων</orig></choice></lem><rdg><expan>δι<ex>καίῳ</ex></expan> τῶν</rdg></app> τέκνων παρὰ Αὐρηλίων Ἰουλίου 

    <lb n="3"/>υ<hi rend="diaeresis">ἱ</hi>οῦ Παρμένωνος <choice><reg>οὐ<supplied reason="omitted">ε</supplied>τρανοῦ</reg><orig>οὐτρανοῦ</orig></choice> τῶν ἐντίμως 

    <lb n="4"/>ἀπολελυμένων καὶ Ἀμμωνᾶ οἰκέτου <choice><reg>σου</reg><orig>μου</orig></choice> 

    <lb n="5"/>τῶν <num value="2">δύο</num>. βουλόμεθα <choice><reg>μισθώσασθαι</reg><orig>μισσθώσασθαι</orig></choice> παρὰ 

    <lb n="6"/>σοῦ τὰς <hi rend="diaeresis">ὑ</hi>παρχούσας σοι περὶ κώμην Διονυ

    <lb n="7" break="no"/>σιάδα <choice><reg>ἐλαιῶνος</reg><orig>ἐλεῶνος</orig></choice> ἐν <num value="3">τρισὶ</num> τόποις ἐν μὲν τό

    <lb n="8" break="no"/>πῳ Ἐπιχάρου <choice><reg>λεγομένῳ</reg><orig>λεγομένου</orig></choice> ἀρούρας <num value="5"><choice><reg>πέντε</reg><orig>πένται</orig></choice></num> καὶ 

    <lb n="9"/>ἐν τόπῳ Δαρίου <choice><reg>λεγομένῳ</reg><orig>λεγομένου</orig></choice> ἀρούρης <num value="1">μιᾶς</num> καὶ ὁμοί

    <lb n="10" break="no"/>ως ἐν τόπῳ Γεμήνεως <choice><reg>λεγομένῳ</reg><orig>λεγομένο<supplied reason="lost">υ</supplied></orig></choice> <supplied reason="lost">ἄ</supplied>λλης 

    <lb n="11"/>ἀρούρ<unclear>η</unclear>ς <num value="1">μι<unclear>ᾶ</unclear>ς</num> <unclear>ἢ</unclear> <choice><reg>ὅσαι</reg><orig>ὅσε</orig></choice> <choice><reg>ἐὰν</reg><orig>ἠὰν</orig></choice> ὦσιν καὶ ἐν τόπ<supplied reason="lost">ῳ</supplied> 

    <lb n="12"/><app type="editorial"><lem resp="BL 9.344">Θαλαα<supplied reason="lost" cert="low">ί</supplied><unclear>ρ</unclear>εως<certainty match=".." locus="value"/></lem><rdg>Θαλαα<supplied reason="lost">ύτ</supplied>εως</rdg></app> <choice><reg>λεγομένῳ</reg><orig><supplied reason="lost">λεγ</supplied>ο<supplied reason="lost">μ</supplied>ένου</orig></choice> φοινικῶνος καὶ <supplied reason="lost">τῶ</supplied><unclear>ν</unclear> 

    <lb n="13"/>ἐν τοῖς ἐλ<supplied reason="lost">αιων</supplied>ικοῖς φοίνικες ὄντων ἡμ<supplied reason="lost">ῶν</supplied> 

    <lb n="14"/>τῶν μισ<supplied reason="lost">θο</supplied>υμένων πάντων τῶν ἀρου<unclear>ρ</unclear>ῶν 

    <lb n="15"/>ἐπὶ χρόνον ἔτη <num value="3">τρία</num> ἀπὸ τοῦ ἐνεστῶτος πρώ

    <lb n="16" break="no"/>του ἔτους φόρου τοῦ φοινικῶνος <choice><reg>καθʼ</reg><orig>κάτʼ</orig></choice> <choice><reg>ἕκαστον</reg><orig>ἑκαστου</orig></choice> 

    <lb n="17"/><choice><reg>ἔτος</reg><orig>ἔτους</orig></choice> ἀργυρίου δραχμὰς <num value="100">ἑκατὸν</num> καὶ <choice><reg>ἐξαιρέτων</reg><orig>ἐξερέτων</orig></choice> ἀρ

    <lb n="18" break="no"/>τάβας <num value="3"><choice><reg>τρεῖς</reg><orig>τρῖς</orig></choice></num> καὶ Σύρων ἀρτάβης <num value="1/2"><choice><reg>ἥμισυ</reg><orig>οἵμυσου</orig></choice></num>, τῶν 

    <lb n="19"/>δὲ <choice><reg>ἐλαιώνων</reg><orig>ἐλεώνων</orig></choice> ἐπικάρπων <surplus>ι</surplus> τρίτον <choice><reg>μέρος</reg><orig>μερι</orig></choice> ἡμῖν 

    <lb n="20"/>τοῖς <choice><reg>μισθουμένοις</reg><orig>μισσθουμένοις</orig></choice>, τὸν δὲ <choice><reg>καθʼ</reg><orig>κάτʼ</orig></choice> <choice><reg>ἕκα<supplied reason="omitted">σ</supplied>τον</reg><orig>ἕκατον</orig></choice> <supplied reason="omitted">ἔτος</supplied> φόρον 

    <lb n="21"/><choice><reg>ἀποδώσομεν</reg><orig>ἀποδώσωμεν</orig></choice> ἐν μηνὶ Ἁθὺρ, τῶν δὲ <del rend="erasure">η</del> δημο

    <lb n="22" break="no"/>σίων <add place="above">πάντων</add> πρὸς <choice><reg>σὲ</reg><orig>εμὲ</orig></choice> τὴν κτήτορα, τῶν δὲ ἔργων 

    <lb n="23"/>πάντων τῶν <choice><reg>ἐλαιώνων</reg><orig>ἐλεώνων</orig></choice> καὶ φοινικῶνος χω

    <lb n="24" break="no"/>ματισμοῦ ποτισμοῦ <hi rend="diaeresis">ὑ</hi>ποσχισμούς, <app type="editorial"><lem resp="BL 12.27"><subst><add place="inline">σκαφητροῦ</add><del rend="corrected">σκαφητρυυ</del></subst></lem><rdg resp="BL 3.237">σκαφητούς</rdg><rdg>σκαφητού<del rend="erasure">ς</del></rdg></app> 

    <lb n="25"/>ξηρολογίας, κορμολογίας, <choice><reg>διωρύγων</reg><orig>ζωρύγων</orig></choice> καὶ <app type="editorial"><lem resp="BL 12.276"><choice><reg>ὑδραγωγῶν</reg><orig>ὑ<unclear>τρ</unclear>αγωγῶν</orig></choice></lem><rdg>ὑπαγωγῶν</rdg></app> 

    <lb n="26"/>ἀναβολὰς καὶ τὰ ἄλλα ὅσα <choice><reg>καθήκει</reg><orig>καθήκι</orig></choice> ἐκ τοῦ <choice><reg>ἰδίου</reg><orig>εἰδίου</orig></choice> τοῖς 

    <lb n="27"/>δέουσιν καιροῖς βλάβος μηδὲ <num value="1">ἓν</num> ποιοῦ<add place="above">ν</add>τες καὶ <choice><reg>κοπρί<supplied reason="omitted">ζοντες</supplied></reg><orig>κοπρι</orig></choice>, 

    <lb n="28"/><choice><reg>σοῦ</reg><orig>εμοῦ</orig></choice> παρα<unclear>σχ</unclear>ομένης τὰ κτήνη καὶ μετὰ τὸν χρόνον 

    <lb n="29"/><choice><reg>παραδώσομεν</reg><orig>παραδώσωμεν</orig></choice> <subst><add place="inline">ἀπὸ</add><del rend="corrected">αποδω</del></subst> <add place="above">συν</add>λογῆς καρποῦ καθὼς καὶ 

    <lb n="30"/><choice><reg>ἡμεῖς</reg><orig><hi rend="diaeresis">ὑ</hi>μῖς</orig></choice> <choice><reg>παρελάβαμεν</reg><orig>παραελάβαμεν</orig></choice> <choice><reg>ἀγρώστεως</reg><orig>ἀγρώσσθεως</orig></choice> <choice><reg>δείσης</reg><orig>δίσης</orig></choice> πάσης, 

    <lb n="31"/>ἂν <choice><reg>φαίνηται</reg><orig>φένηται</orig></choice> <choice><reg>μισθῶσαι</reg><orig>μισσθωσεως</orig></choice>. <app type="editorial"><lem resp="PN M.G. Elmaghrabi"><expan>Αὐρή<ex>λιοι</ex></expan></lem><rdg><expan>Αὐρή<ex>λιος</ex></expan></rdg></app> Ἰοῦλις καὶ <expan>Ἀ<supplied reason="lost">μ</supplied>μ<ex>ωνᾶς</ex></expan> <expan>μεμισθ<ex>ώμεθα</ex></expan> 

    <lb n="32"/>ὡς πρόκιται. 

    <lb n="33"/><expan><ex>ἔτους</ex></expan> <num value="1">α</num> τῶν κυρίων ἡμῶν Μακριανοῦ καὶ Κυήτου 

    <lb n="34"/>Σεβαστῶν Φαμενὼθ <num value="5">ε</num>. 
</ab></div>
