         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r" type="textpart"><!--milestone unit="4"--><ab>

    <lb n="1"/>Ἀντῶνις Λόνγος Νειλοῦτι 

    <lb n="2"/><supplied reason="lost">τ</supplied>ῇ μητρὶ <choice><reg>πλεῖστα</reg><orig>π<supplied reason="lost">λ</supplied>ιστα</orig></choice> χαίρειν. καὶ δι

    <lb n="3" type="inWord"/>ὰ πάντω<supplied reason="lost">ν</supplied> εὔχομαί <choice><reg>σε</reg><orig>σαι</orig></choice> <choice><reg>ὑγιαίνειν</reg><orig>υγειαινειν</orig></choice>. τὸ προσκύνη

    <lb n="4" type="inWord"/>μά σου <supplied reason="lost">ποι</supplied>ῶ <app type="editorial"><lem resp="bgu 3 p.6">κατʼ</lem><rdg>κατα<add place="above">ο</add></rdg></app> <choice><reg>ἑκάστην</reg><orig>αικαστην</orig></choice> <choice><reg>ἡμέραν</reg><orig>ημαιραν</orig></choice> παρὰ τῷ 


    <lb n="5"/>κυρίῳ <choice><reg>Σαράπιδι</reg><orig><supplied reason="lost">σαρ</supplied>απειδει</orig></choice>. <choice><reg>γιγνώσκειν</reg><orig>γεινωσκειν</orig></choice> <choice><reg>σε</reg><orig>σαι</orig></choice> θέλω, ὅ

    <lb n="6" type="inWord"/>τ<unclear>ι</unclear> οὐχ <app type="BL"><lem resp="1.72"><supplied reason="lost">ἤλπ</supplied><unclear>ι</unclear>ζον</lem><rdg><gap reason="lost" quantity="3" unit="character"/><unclear>ι</unclear>ζον</rdg></app>, ὅτι <app type="editorial"><lem resp="bgu 3 p.6"><choice><reg>ἀναβαίνεις</reg><orig>αναβενω</orig></choice></lem><rdg><choice><reg>ἀναβαίνω</reg><orig>αναβενω</orig></choice></rdg></app> εἰς τὴν μητρό

    <lb n="7" type="inWord"/>πολιν, <app type="editorial"><lem resp="bgu 3 p.6"><choice><reg>χάριν</reg><orig>χ<supplied reason="lost">α</supplied><unclear>ρ</unclear>ειν</orig></choice></lem><rdg><unclear>χ</unclear><gap reason="lost" quantity="2" unit="character"/><gap reason="illegible" quantity="1" unit="character"/>ειν</rdg></app> <choice><reg>τούτου</reg><orig>τουτο</orig></choice> οὐδʼ <choice><reg>ἐγὼ</reg><orig>εγο</orig></choice> <choice><reg>εἰσῆ<supplied reason="omitted">λ</supplied>θα</reg><orig>εισηθα</orig></choice> εἰς τὴν πό

    <lb n="8" type="inWord"/>λιν.  <gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="1" unit="character"/><gap reason="illegible" quantity="2" unit="character"/><unclear>οπ</unclear><gap reason="lost" quantity="2" unit="character"/>μην δὲ ἐλθεῖν εἰς <choice><reg>Καρανίδα</reg><orig>καρανιδα<add place="above">ν</add></orig></choice> 

    <lb n="9"/>ὅτι σαπρῶς <choice><reg>περιπατῶ</reg><orig>πα<unclear>ιρι</unclear>πατω</orig></choice>.  <choice><reg>ἔγραψά</reg><orig>αιγραψα</orig></choice> σοι, ὅτι γυμνός 

    <lb n="10"/><choice><reg>εἰμι</reg><orig>ειμει</orig></choice>. παρακα<supplied reason="lost">λ</supplied>ῶ <choice><reg>σε</reg><orig>σαι</orig></choice>, μήτηρ, <choice><reg>διαλ<supplied reason="omitted">λ</supplied>άγητί</reg><orig>δ<supplied reason="lost">ι</supplied>αλαγη<unclear>τ</unclear>ι</orig></choice> μοι. λοι

    <lb n="11" type="inWord"/>πὸν οἶδα <app type="BL"><lem resp="1.72">τί</lem><rdg><unclear>τι</unclear><gap reason="lost" quantity="3" unit="character"/></rdg></app> <choice><reg>ἐμαυτῷ</reg><orig>αιμαυτω</orig></choice> παρέσχημαι, <choice><reg>πεπαίδ

    <lb n="12" type="inWord"/>ευμαι</reg><orig>παιπαι<unclear>δ</unclear>δευμαι</orig></choice> καθʼ ὃν <app type="BL"><lem resp="1.72"><subst><add place="inline">δὴ</add><del rend="corrected">δι</del></subst></lem><rdg><unclear>δὲ</unclear></rdg></app> τρόπον, οἶδα ὅτι ἡμάρτηκα. 

    <lb n="13"/>ἤκο<unclear>υ</unclear>σα παρὰ το<supplied reason="lost">ῦ </supplied><app type="BL"><lem resp="1.72"><gap reason="lost" quantity="3" unit="character"/>ου</lem><rdg><supplied reason="lost">υἱ</supplied>οῦ</rdg></app> μου τὸν εὑρόντα <choice><reg>σε</reg><orig>σαι</orig></choice> 

    <lb n="14"/>ἐν τῷ <subst><add place="inline">Ἀρσαινοείτῃ</add><del rend="corrected">αρσαινοειτου</del></subst> καὶ <app type="BL"><lem resp="1.72"><choice><reg>ἀκεραίως</reg><orig>α<unclear>κα</unclear>ιρ<unclear>ι</unclear>ως</orig></choice></lem><rdg>ἀκ<unclear>αί</unclear>ρωσ</rdg></app> πάντα σοι δι

    <lb n="15" type="inWord"/>ήγηται. οὐκ οἶδες, ὅτι θέλω <app type="BL"><lem resp="1.72">π<unclear>η</unclear>ρ<unclear>ὸ</unclear>ς <choice><reg>γενέσθαι</reg><orig>γενεσται</orig></choice></lem><rdg><choice><reg>παραγενέσθαι</reg><orig>π<unclear>α</unclear>ρ<unclear>α</unclear>σγενεσται</orig></choice></rdg></app> 

    <lb n="16"/><choice><reg>ἢ</reg><orig>ει</orig></choice> <choice><reg>γνῶναι</reg><orig>γνουναι</orig></choice> <app type="BL"><lem resp="1.72">ὅπως</lem><rdg>τῶι</rdg></app> <choice><reg>ἀνθρώπωι</reg><orig>ανθροπωι</orig></choice>, <app type="BL"><lem resp="1.72"><supplied reason="lost">ἔ</supplied>τ<supplied reason="lost">ι</supplied></lem><rdg><supplied reason="lost">ὅ</supplied>τ<supplied reason="lost">ι</supplied></rdg></app> ὀφείλω ὄβολον 

    <lb n="17"/><gap reason="lost" quantity="6" unit="character"/><unclear>ο</unclear><gap reason="lost" quantity="11" unit="character"/> σὺ αὐτὴ ἐλθ<unclear>έ</unclear> <gap reason="illegible" quantity="1" unit="character"/> 

    <lb n="18"/><gap reason="lost" quantity="13" unit="character"/>χαν<unclear>κ</unclear><gap reason="lost" quantity="3" unit="character"/><unclear>ο</unclear>ν <choice><reg>ἤκουσα</reg><orig>ηγουσα</orig></choice> ὅτι <app type="BL"><lem resp="1.72">υ<g type="unintelligible"/></lem><rdg><gap reason="illegible" quantity="2" unit="character"/></rdg></app> 

    <lb n="19"/><gap reason="lost" quantity="15" unit="character"/><gap reason="illegible" quantity="1" unit="character"/>λησαι<gap reason="lost" quantity="2" unit="character"/> παρακαλῶ <choice><reg>σε</reg><orig>σαι</orig></choice> 

    <lb n="20"/><app type="BL"><lem resp="1.72"><gap reason="lost" quantity="20" unit="character"/><gap reason="illegible" quantity="1" unit="character"/>ε<gap reason="illegible" quantity="1" unit="character"/>α<gap reason="lost" quantity="2" unit="character"/>κ</lem><rdg><gap reason="lost" quantity="20" unit="character"/><gap reason="illegible" quantity="3" unit="character"/>α<gap reason="lost" quantity="1" unit="character"/><gap reason="illegible" quantity="1" unit="character"/></rdg></app> <choice><reg>ἐγὼ</reg><orig>αιγω</orig></choice> <app type="editorial"><lem resp="bgu 3 p.6"><choice><reg>σχεδ<supplied reason="omitted">ὸ</supplied>ν</reg><orig>σχεδν</orig></choice></lem><rdg>εχεδη</rdg></app> 

    <lb n="21"/><gap reason="lost" quantity="20" unit="character"/><unclear>ω</unclear> παρακαλῶ <choice><reg>σε</reg><orig><unclear>σαι</unclear></orig></choice> 

    <lb n="22"/><gap reason="lost" quantity="19" unit="character"/>ώνου θέλω <choice><reg>ἐγὼ</reg><orig>αιγω</orig></choice> 

    <lb n="23"/><gap reason="lost" quantity="19" unit="character"/>σει<gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="2" unit="character"/> οὐκ <unclear>ε</unclear><gap reason="illegible" quantity="1" unit="character"/> 

    <lb n="24"/><gap reason="lost" quantity="19" unit="character"/> <app type="BL"><lem resp="1.72">μὴ</lem><rdg><gap reason="illegible" quantity="4" unit="character"/></rdg></app> ἄλλως <app type="BL"><lem resp="1.73">ποι<supplied reason="lost">ή</supplied>

    <lb n="25" type="inWord"/><supplied reason="lost">σῃς</supplied></lem><rdg>ποι<gap reason="lost" extent="unknown" unit="character"/></rdg></app> <gap reason="lost" extent="unknown" unit="character"/> 

