    <lb n="12"/><app type="editorial"><lem resp="bgu 3 p.5"><supplied reason="lost">ποιήσῃ</supplied>ς</lem><rdg><gap reason="lost" quantity="4" unit="character"/><unclear>σ</unclear></rdg></app>. 

