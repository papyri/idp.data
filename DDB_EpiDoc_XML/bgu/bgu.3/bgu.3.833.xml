    <lb n="16"/>ἀπογραφήν, 

    <lb n="17"/>ἐμαυτὸν Μέλαν εἰς τὸ αὐ

    <lb n="18" break="no"/>τὸ <num value="14">ιδ</num> <expan><ex>ἔτος</ex></expan> <expan><ex>ἐτῶν</ex></expan> <num value="51">να</num>, 

    <lb n="19"/>κα<supplied reason="lost">ὶ</supplied> τοὺς γενομένου<supplied reason="lost">ς</supplied> ἐκ 

    <lb n="20"/>διαφόρων γυναι<supplied reason="lost">κ</supplied>ῶν 

    <lb n="21"/><choice><reg>υἱοὺς</reg><orig>ἡοὺς</orig></choice> <supplied reason="lost">ἐ</supplied>κ μὲν Τεσα<unclear>γ</unclear>ρίτ<supplied reason="lost">ος</supplied> 

    <lb n="22"/><choice><reg>Μεμφίτ<supplied reason="lost">ι</supplied>δος</reg><orig>Μεμφείτ<supplied reason="lost">ι</supplied>δος</orig></choice> Μέλα<supplied reason="lost">ν</supplied>α 

    <lb n="23"/><expan><ex>ἐτῶν</ex></expan> <num value="1">α</num> 

    <lb n="24"/>ἐκ δὲ Ἑριέω<supplied reason="lost">ς θυ</supplied>γατέρα Τνε

    <lb n="25" break="no"/>φρέμφι<supplied reason="lost">ν</supplied> <expan><ex>ἐτῶν</ex></expan> <num value="16">ιϛ</num> 

    <lb n="26"/>παρὼν δὲ ὁ <expan>προγεγρ<ex cert="low">αμμένος</ex></expan> <expan>σταθ<ex>μοῦχος</ex></expan> 

    <lb n="27"/><supplied reason="lost">Ἰ</supplied>σίδωρ<supplied reason="lost">ος</supplied> <choice><reg>ἐγγυᾶται</reg><orig>ἐνγυᾶται</orig></choice> ἡμ<supplied reason="lost">ᾶς</supplied> 

    <lb n="28"/>τῶ<supplied reason="lost">ν</supplied> ἐπικεφαλίων 

    <lb n="29"/>κ<supplied reason="lost">αὶ</supplied> ὀμνύω τὴν τοῦ κυρίου 

    <lb n="30"/><supplied reason="lost">Αὐ</supplied>ρηλίου Ἀντωνίνου Καίσαρος 

    <lb n="31"/><supplied reason="lost">τύ</supplied>χην ἀληθῆ εἶναι τὰ <expan>προκ<ex>είμενα</ex></expan>. 

    <lb n="32"/><expan><ex>ἔτους</ex></expan> <num value="15">ιε</num> Αὐτοκράτορος Καίσαρος 

    <lb n="33"/>Μάρκου Αὐρηλίου Ἀντωνίνου 

