    <lb n="9"/><expan><ex>ἔτους</ex></expan> <num value="7">ζ</num> Φαρμοῦθι <num value="27">κζ</num> <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="10"/>διέγραψε <app type="editorial"><lem resp="bgu 3 p.5">Κάστω<supplied reason="lost">ρι</supplied></lem><rdg>Κάστω<supplied reason="lost">ρ</supplied></rdg></app> 

