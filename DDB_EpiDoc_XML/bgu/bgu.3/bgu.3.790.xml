    <lb n="12"/><choice><reg><expan>ἀργυρ<ex>ι</ex></expan>κῶν</reg><orig>ἀργυρκον</orig></choice> <expan>κώ<ex>μης</ex></expan> Ἀ<gap reason="lost" quantity="5" unit="character"/> 

