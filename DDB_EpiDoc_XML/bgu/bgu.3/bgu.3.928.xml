    <lb n="13"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ἐπὶ</supplied> τοῦ αὐτοῦ ἀκρωμίου πελίωμα ω<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="14"/><gap reason="lost" extent="unknown" unit="character"/>ατου <choice><reg>ἐρευθουμένου</reg><orig>ἐρευθουμεν</orig></choice> <choice><reg>οἰδήματος</reg><orig>δήματος</orig></choice> και<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="15"/><gap reason="lost" extent="unknown" unit="character"/><gap reason="illegible" quantity="1" unit="character"/>ιου οἴδημα μετὰ πελιώματος <gap reason="lost" extent="unknown" unit="character"/> 

