    <lb n="11"/><gap reason="lost" extent="unknown" unit="character"/> αὐτοῦ συ<gap reason="illegible" quantity="1" unit="character"/>ηρη<gap reason="illegible" quantity="2" unit="character"/>υ<gap reason="illegible" quantity="3" unit="character"/> <note xml:lang="en">?</note> <choice><reg>ἐπιδειχθεν<gap reason="lost" extent="unknown" unit="character"/></reg><orig>ἐπιδιχθεν<gap reason="lost" extent="unknown" unit="character"/></orig></choice> 

