    <lb n="17"/><gap reason="lost" extent="unknown" unit="character"/> δεξιᾶς οἴδημα καὶ <subst><add place="inline">κάτ<add place="above">ω</add></add><del rend="corrected">κατο</del></subst>  θ τουδ<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="18"/><gap reason="lost" extent="unknown" unit="character"/>χας καὶ ἐπὶ τοῦ μηροῦ τοῦ δεξιοῦ πελίω<supplied reason="lost">μα </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="19"/><gap reason="lost" extent="unknown" unit="character"/> προσφωνῶ ἐπιτεταιωρηκέναι <subst><add place="inline">τ<add place="above">ὸ</add></add><del rend="corrected">τω</del></subst> ἐπιδ<supplied reason="lost">ειχθὲν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="20"/><gap reason="lost" extent="unknown" unit="character"/>ισικων <choice><reg>τριβόνιων</reg><orig>τριβοων</orig></choice> ἐσχισμένον <gap reason="lost" extent="unknown" unit="character"/> 

