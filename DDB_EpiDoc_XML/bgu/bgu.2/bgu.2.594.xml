    <lb n="3"/><choice><reg>θέλω</reg><orig><supplied reason="lost">θ</supplied>έλωι</orig></choice>, ὅτι ο πατικουρας οὐδὲν <gap reason="lost" quantity="4" unit="character"/> 

    <lb n="4"/><app type="editorial"><lem resp="BL 1.55"><gap reason="lost" quantity="1" unit="character"/>ε</lem><rdg><gap reason="lost" quantity="1" unit="character"/><gap reason="illegible" quantity="2" unit="character"/></rdg></app> χάριν τοῦ καλάμου τοῦ θρύου <gap reason="illegible" quantity="2" unit="character"/>τ<gap reason="lost" quantity="1" unit="character"/><gap reason="illegible" quantity="2" unit="character"/> 

    <lb n="5"/><supplied reason="lost">λ</supplied>έγων ὅτι μετὰ τὸν θερισμὸ<supplied reason="lost">ν ἐργο</supplied>

    <lb n="6" break="no"/><supplied reason="lost">λ</supplied>αβήσομα<supplied reason="lost">ι</supplied>, ἄρτι γὰρ ἀσθενῶ<unclear>ι</unclear> <gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="4" unit="character"/> 

    <lb n="7"/><choice><reg>θέλεις</reg><orig><supplied reason="lost">θ</supplied><unclear>έλ</unclear>ις</orig></choice> δὲ πέμψαι τινὰ ε<gap reason="lost" quantity="7" unit="character"/> 

    <lb n="8"/><gap reason="lost" quantity="2" unit="character"/>υ<unclear>ρ</unclear>ιο<unclear>υ</unclear> <gap reason="illegible" quantity="2" unit="character"/>. ἔπεμψα <gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" quantity="4" unit="character"/><supplied reason="lost"> διὰ</supplied> 

    <lb n="9"/><supplied reason="lost" cert="low">Σα</supplied>βίν<supplied reason="lost" cert="low">ο</supplied>υ περιστερίδας εἴκοσι <choice><reg>δύο</reg><orig>δυω</orig></choice> <num value="22"/> <gap reason="lost" quantity="3" unit="character"/> 

