    <lb n="8"/>πυξίδας μέ<unclear>σ</unclear><supplied reason="lost">α</supplied><unclear>ς</unclear> <num value="2"><unclear>β</unclear></num> καὶ ἐλάσσους <num value="3"><unclear>γ</unclear></num> 

