    <lb n="11" break="no"/>δείας</reg><orig>αὐ<unclear>θ</unclear>α<lb n="11" break="no"/>δίας</orig></choice> λ<surplus>ε</surplus>ίαν δὲ ε<gap reason="lost" quantity="6" unit="character"/><gap reason="illegible" quantity="1" unit="character"/> εἰς ἡμᾶς ταυ<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="2" unit="character"/>

    <lb n="12"/>διαπεπραχθέ<unclear>ν</unclear><supplied reason="lost">τα </supplied><gap reason="lost" quantity="4" unit="character"/>ε ἀφʼ ὧν εχ<gap reason="lost" quantity="1" unit="character"/><gap reason="illegible" quantity="1" unit="character"/>ε<gap reason="illegible" quantity="2" unit="character"/>

    <lb n="13"/>ἐν τῆι φασ<gap reason="lost" extent="unknown" unit="character"/>ιπ<unclear>π</unclear><gap reason="lost" quantity="5" unit="character"/><gap reason="illegible" quantity="6" unit="character"/>σεα<unclear>π</unclear><gap reason="lost" quantity="4" unit="character"/>

