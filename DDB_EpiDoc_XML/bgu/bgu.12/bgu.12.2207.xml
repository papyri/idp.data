         <head n="16143" xml:lang="en">
            <date>AD 606</date>
            <placeName>Hermoupolis Magna</placeName>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r" type="textpart"><ab>

    <lb n="1"/><g type="stauros"/> ἐν ὀνόματι τῆς ἁγίας καὶ ζωοποιοῦ τριάδος πατρὸς καὶ υ<hi rend="diaeresis">ἱ</hi>οῦ καὶ ἁγίου 

    <lb n="2"/>πνεύματο<add place="above">ς</add>, βασιλείας τοῦ θειοτάτου ἡμῶν δεσπότου <expan>Φλ<ex>αυίου</ex></expan> Φωκᾶ τοῦ αἰωνίο<add place="above">υ</add> 

    <lb n="3"/>Αὐγού<supplied reason="lost">σ</supplied>του καὶ <expan>Αὐ<unclear>το</unclear>κ<ex>ράτορος</ex></expan> <unclear>ἔ</unclear>τους τετάρ<supplied reason="lost">του</supplied>, Φαῶφι πεντεκαιδεκάτῃ, δεκάτη<unclear>ς</unclear> <expan><unclear>ἰν</unclear>δ<ex>ικτίονος</ex></expan>. 

    <lb n="4"/><g type="stauros"/> Αὐρήλιος Ταυρῖνος υ<hi rend="diaeresis">ἱ</hi><unclear>ὸ</unclear><supplied reason="lost">ς </supplied><gap reason="lost" quantity="1" unit="character"/><gap reason="illegible" quantity="1" unit="character"/>αλωτος ἐκ μητρὸς Θέκλ<supplied reason="lost">ας</supplied> 

    <lb n="5"/>ἀμπελουργὸς ἀπὸ <choice><reg>ἐποικίου</reg><orig>ἐποικείου</orig></choice> Ταν<unclear>α</unclear>μη<unclear>υ</unclear> τοῦ <expan>Ἑρμ<ex>ουπολίτου</ex></expan> νομ<unclear>οῦ</unclear> 

    <lb n="6"/>διʼ ἐμοῦ ἐγγυωμένο<unclear>υ</unclear> αὐτὸ<unclear>ν</unclear> <unclear>καὶ</unclear> ἀ<supplied reason="lost">να</supplied>δ<unclear>εχο</unclear>μένου εἰς <unclear>τὴ</unclear><supplied reason="lost">ν</supplied> 

    <lb n="7"/>ἀπόδοσιν τοῦ <gap reason="lost" quantity="1" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="2" unit="character"/><unclear>ου</unclear> <hi rend="diaeresis">ἰ</hi><unclear>δί</unclear><supplied reason="lost">ῳ</supplied> <unclear>μου</unclear> κινδύνῳ καὶ <supplied reason="omitted">πόρῳ</supplied> τῆς <unclear>ἐμ</unclear><supplied reason="lost">ῆς</supplied> 

    <lb n="8"/>ὑποστάσεως <g type="stauros"/> <choice><reg>Αὐρηλίου</reg><orig><unclear>Αὐρη</unclear>λ<unclear>ιος</unclear></orig></choice> <choice><reg>Πέτρου</reg><orig><unclear>Πέτρ</unclear>ο<unclear>ς</unclear></orig></choice> <choice><reg>υἱοῦ</reg><orig>υἱος</orig></choice> Παύ<unclear>λου</unclear> ἐκ μητρ<unclear>ὸς</unclear> 

    <lb n="9"/>Ἀγνοίας ἀπὸ τ<unclear>οῦ</unclear> <unclear>αυ</unclear><gap reason="lost" quantity="1" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="3" unit="character"/> <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="10"/>Φ<unclear>οι</unclear>βάμμων<supplied reason="lost">ος ἀπ</supplied>ὸ τῆς <expan>Ἑρμ<ex>ουπολιτῶν</ex></expan> χαίρ<supplied reason="lost">ειν. ὁμολογῶ ἐσχηκέναι</supplied> 

    <lb n="11"/>καὶ πεπληρῶσ<unclear>θαι</unclear> παρά σου τῆς τελ<supplied reason="lost">είας καὶ ἀξίας τιμῆς</supplied> 

    <lb n="12"/>οἴνου μέτρων διακοσίων <supplied reason="lost">τεσσαράκοντα </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="13"/><expan>γί<ex>νεται</ex></expan> <expan>οἴ<ex>νου</ex></expan> <expan>μέτρ<ex>α</ex></expan> <num value="24">κδ</num><gap reason="illegible" quantity="1" unit="character"/>, ἅπερ παρέ<supplied reason="lost">ξω σοι ἐν τῷ Μεσορὴ μηνὶ</supplied> 

    <lb n="14"/>τῆς σὺν <expan>θ<ex>εῷ</ex></expan> ἑνδεκάτης <expan><unclear>ἰνδ</unclear><ex>ικτίονος</ex></expan> <supplied reason="lost">ἐν οἴνῳ νέῳ καλλίστῳ καὶ</supplied> 

    <lb n="15"/>εὐαρέ<supplied reason="omitted">σ</supplied>τῳ, μέτρῳ τοῦ <gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="16"/>διοικητοῦ· καὶ εἰ <choice><reg>εὑρε<supplied reason="lost">θείη</supplied></reg><orig>εὑρη<supplied reason="lost">θείη</supplied></orig></choice><supplied reason="lost"> ὄξος ἢ ἀποίητος ἢ ὀζόμενος</supplied> 

    <lb n="17"/>ἐξ αὐτῶν μέ<supplied reason="lost">χρι τοῦ Τῦβι μηνὸς τῆς αὐτῆς <expan>ἰνδ<ex>ικτίονος</ex></expan>, ὁμολογῶ</supplied> 

    <lb n="18"/>ἀλλάξαι σοι ἀν<supplied reason="lost">υπερθέτως </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="19"/><gap reason="illegible" quantity="1" unit="line"/> 

    <lb n="20"/><gap reason="lost" extent="unknown" unit="line"/> 
</ab></div><div n="v" type="textpart"><ab>

