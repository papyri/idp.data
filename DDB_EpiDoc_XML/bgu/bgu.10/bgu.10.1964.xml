    <lb n="1"/>κύριος ἔστω <gap reason="lost" extent="unknown" unit="character"/> χρόνον <supplied reason="lost">ἐ</supplied>ν<supplied reason="lost">ε</supplied>γκὼν <gap reason="lost" quantity="2" unit="character"/>υ <gap reason="lost" quantity="8" unit="character"/> <unclear>τ</unclear><gap reason="lost" quantity="10" unit="character"/> <gap reason="illegible" quantity="3" unit="character"/> <gap reason="lost" quantity="2" unit="character"/> <gap reason="illegible" quantity="2" unit="character"/> <supplied reason="lost">τὰς πεν</supplied>

