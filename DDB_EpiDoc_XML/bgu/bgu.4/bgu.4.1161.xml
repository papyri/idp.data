    <lb n="1"/><gap reason="illegible" quantity="1" unit="line"/> 

    <lb n="2"/><supplied reason="lost">π</supplied>αρὰ Πτολεμαίου τοῦ <unclear>Πτο</unclear><supplied reason="lost">λεμαίου </supplied><gap reason="lost" quantity="8" unit="character"/> 

    <lb n="3"/><supplied reason="lost">κ</supplied>αὶ παρὰ Ἑρμαφίλου τ<supplied reason="lost">οῦ </supplied><gap reason="lost" quantity="8" unit="character"/><supplied reason="lost"> Πέρσου</supplied> 

    <lb n="4"/>τῆς ἐπιγονῆς καὶ τ<supplied reason="lost">ῆς τούτου γυναι</supplied>

