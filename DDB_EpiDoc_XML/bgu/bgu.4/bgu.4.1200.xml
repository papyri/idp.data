         <head n="18650" xml:lang="en">
            <date>2/1 BC</date>
            <placeName>Herakleopolite?</placeName>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><ab>
    <lb n="1"/>Ποπλίωι Ὀκταίωι 

    <lb n="2"/>παρὰ Ὥρου τοῦ Τεῶτος καὶ τῶν <supplied reason="lost">ἄλλων ἱ</supplied><unclear>ερέων</unclear> <gap reason="lost" quantity="12" unit="character"/><supplied reason="lost"> καὶ</supplied> 

    <lb n="3"/>Ἀπόλλωνος θεῶν μεγίστων <supplied reason="lost">ὄν</supplied><unclear>τ</unclear><supplied reason="lost">ων ἀπ</supplied><unclear>ὸ</unclear> <unclear>κ</unclear><supplied reason="lost">ώμης Βουσίρεως</supplied> 

    <lb n="4"/>τοῦ ὑπὲρ Μέμφιν Ἡρακλεοπο<supplied reason="lost">λίτο</supplied>υ. <supplied reason="lost">περὶ κώμην </supplied><gap reason="lost" quantity="5" unit="character"/><supplied reason="lost"> ἐτύχομεν</supplied> 

    <lb n="5"/>ἐδάφ<unclear>η</unclear> ἔχοντες ἀνιερ<unclear>ωμ</unclear><supplied reason="lost">ένα καὶ πρόσοδον κατὰ διαδοχὴν τῶν</supplied> 

    <lb n="6"/>γον<supplied reason="lost">έ</supplied>ων ἡμῶν πυροῦ ἀρτάβας <supplied reason="lost">ἑκατόν <num value="100"/>. τῶι δὲ  <expan><ex>ἔτει</ex></expan> ἀνειλημμένων</supplied> 

    <lb n="7"/>εἰς τὸ <unclear>δημ</unclear><supplied reason="lost">ό</supplied>σιον τῶν ἐδαφῶν <unclear>διετα</unclear><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="15" unit="character"/><supplied reason="lost"> σύνταξιν</supplied> 

    <lb n="8"/>πυροῦ ἀρτάβας ἑκατὸν <num value="100"/> καὶ ἀπὸ <subst><add place="inline"><add place="above"><unclear>τῶν</unclear></add></add><del rend="corrected"><unclear>της</unclear></del></subst> <supplied reason="lost">προσόδου </supplied><gap reason="lost" quantity="15" unit="character"/> 

    <lb n="9"/>πυροῦ ἀρτάβας ἑκατόν <num value="100"/>, ὥστε εἶνα<supplied reason="lost">ι</supplied> τὰ<supplied reason="lost">ς πάσας διακοσίας <num value="200"/> κατʼ ἔτος</supplied> 

    <lb n="10"/>ἀπὸ τοῦ <num value="11"><supplied reason="lost">ι</supplied><unclear>α</unclear></num> <supplied reason="lost"><expan><ex>ἔτους</ex></expan></supplied> Καίσαρος μέχρι τοῦ <num value="19"><unclear>ι</unclear><supplied reason="lost">θ</supplied></num><supplied reason="lost"> <expan><ex>ἔτους</ex></expan> </supplied><gap reason="lost" quantity="10" unit="character"/><supplied reason="lost"> καὶ εἰς τὰς</supplied> 

    <lb n="11"/>ὑπὲρ τοῦ θ<unclear>ε</unclear><supplied reason="lost">οῦ</supplied> καὶ κυρίου Αὐτοκράτορος <unclear>Κα</unclear><supplied reason="lost">ίσαρος καθηκούσας</supplied> 

    <lb n="12"/>θυσίας καὶ σπονδὰς καὶ εἰς τὰς <supplied reason="lost">συν</supplied>τελουμένας <unclear>ὑφ</unclear>ʼ <supplied reason="lost">ἡμῶν λειτουργίας</supplied> 

    <lb n="13"/>κατʼ ἔτος ἀργυρίου δραχμὰς διακοσ<supplied reason="lost">ί</supplied>ας ὀγδοήκον<supplied reason="lost">τα</supplied> <num value="280"/>. <unclear>ἀπ</unclear><supplied reason="lost">ὸ δὲ τοῦ <num value="19">ιθ</num> <expan><ex>ἔτους</ex></expan></supplied> 

    <lb n="14"/>Ἀσκληπιάδου τοῦ τὴν πρόσοδον ἀν<unclear>ηγ</unclear>μένου εἰς <unclear>Ἰτ</unclear>αλίαν <supplied reason="lost">ἐγδημήσαντος οἱ</supplied> 

    <lb n="15"/>προστάται καὶ σιτολόγοι Ἡρακλείδης καὶ Διονύσιος ἀμφότ<supplied reason="lost">εροι ὄντες</supplied> 

    <lb n="16"/>τοῦ ἀδελφοῦ τοῦ Ἀσκληπιάδου υἱοὶ, <choice><reg>ὄντες</reg><orig><unclear>ὀ</unclear>ντω<unclear>ν</unclear></orig></choice> <unclear>δὲ</unclear> <unclear>ἀπὸ</unclear> <supplied reason="lost">κ</supplied>ώ<supplied reason="lost">μης Λινῆ πεισθέντες</supplied> 

    <lb n="17"/>οὐ μικρῶι κεφαλαίωι <subst><add place="inline"><add place="above">παρὰ τῶν</add></add><del rend="corrected"><hi rend="supraline">υπο των</hi></del></subst> ἐκ τῆς κώμης αὐτῶν <unclear>ἀπ</unclear><supplied reason="lost">εστέρησαν</supplied> 

    <lb n="18"/>ἡμᾶς ὧν ἔχομεν ἐκ τῆς προσόδου πυροῦ <subst><add place="inline">ἀρταβῶν</add><del rend="corrected">αρταβας</del></subst> ἑκατὸν <num value="100"/> <gap reason="lost" quantity="6" unit="character"/><supplied reason="lost"> μέχρι</supplied> 

    <lb n="19"/>τοῦ <num value="19">ιθ</num> <expan><ex>ἔτους</ex></expan> Καίσαρος. ὁ προγεγραμμένος <add place="above"><unclear>δὲ</unclear></add> Ἡρακλείδης καὶ Διονύ<supplied reason="lost">σιος </supplied><gap reason="lost" quantity="6" unit="character"/> 

    <lb n="20"/>ἐξουσίαν ἔχοντες τῶν Ἀσκληπιάδου ἀνόμως ἀποδέδωκαν <unclear>τοῖ</unclear><supplied reason="lost">ς ἀπὸ Λινῆ</supplied> 

    <lb n="21"/><add place="above">μηδέπω ἐσχηκότες ἐκ τῆς προσόδου <expan><ex>πυροῦ</ex></expan> πλ<unclear>ω</unclear><gap reason="illegible" quantity="4" unit="character"/>τας ἡμῶν εἰς πλήρωσιν <supplied reason="lost">τ</supplied><unclear>ῶν</unclear> <num value="175">ροε</num> <expan><ex>πυροῦ</ex></expan> <num value="100">ρ</num></add> τὰς ἡμῶν <del rend="erasure">πυροῦ ἀρτάβας ἑκατὸν <num value="100"/></del>  καὶ μέχρι τοῦ νῦν 
διεμώ<unclear>λυσαν</unclear> <app type="editorial"><lem resp="P. van Minnen, Tyche 30 (2015) 237 f. (KorrTyche 816)"><app type="alternative"><lem><gap reason="lost" extent="unknown" unit="character"/> <supplied reason="lost">δυνα</supplied><lb n="22" break="no"/>στευόμενοι</lem><rdg><gap reason="lost" extent="unknown" unit="character"/> <supplied reason="lost">καταδυνα</supplied><lb n="22" break="no"/>στευόμενοι</rdg><rdg><supplied reason="lost">τὸν χρόνον</supplied> <supplied reason="lost">δυνα</supplied><lb n="22" break="no"/>στευόμενοι</rdg></app></lem><rdg><supplied reason="lost">ἡμᾶς </supplied><gap reason="lost" quantity="6" unit="character"/> 

    <lb n="22"/>στευόμενοι</rdg></app> ὑπʼ αὐτῶν. <g type="parens-deletion-opening"/><choice><reg>νυνὶ</reg><orig>νυνεὶ</orig></choice> δὲ ἐπισκεψάμενοι τὸ δημόσιο<supplied reason="lost">ν καὶ εὑρόντες</supplied> 

    <lb n="23"/><g type="parens-deletion-opening"/>διεγβαλλομένας εἰς τὸ ἡμῶν ἱερὸν τὰς προκειμένας πυροῦ <supplied reason="lost">ἀ</supplied>ρτάβας ἑκατ<supplied reason="lost">ὸν <num value="100"/> καὶ</supplied> 

    <lb n="24"/><g type="parens-deletion-opening"/>αὐτῶν τῶν ἀπὸ Λινῆ πυροῦ ἀρτάβας ἑβδομήκοντα πέντε <num value="75"/><g type="parens-deletion-closing"/> <del rend="erasure">δι</del> ἐπὶ σὲ κα<supplied reason="lost">ταπεφεύ</supplied>

    <lb n="25" break="no"/>γαμεν <del rend="erasure">τὸν πάντων</del> <g type="parens-deletion-closing"/>, <add place="above">ἀξιοῦμέν σε τὸν πάντ<supplied reason="lost">ων</supplied></add> σωτῆρα καὶ <add place="above">εὐεργέτην</add> <del rend="erasure">ἀντιλήμπτορα, ὅπως</del> <subst><add place="inline">συντάξ<add place="above">αι</add></add><del rend="corrected">συνταξης</del></subst> κλ<supplied reason="lost">ηθῆναι αὐτοὺς</supplied> 

    <lb n="26"/><supplied reason="lost">ἐ</supplied>πὶ σὲ καὶ <subst><add place="inline">διαλαβ<add place="above">εῖν</add></add><del rend="corrected">διαλαβης</del></subst> περὶ τῆς ἀληθε<supplied reason="lost">ίας, ὅτι</supplied> <subst><add place="inline">ἀ<add place="above">πεστέρεσαν</add></add><del rend="corrected">α<del rend="erasure">φηρπακασιν</del></del></subst> <subst><add place="inline">ἡ<supplied reason="lost">μ</supplied><add place="above">ᾶς</add></add><del rend="corrected">η<supplied reason="lost">μων</supplied></del></subst> <gap reason="lost" quantity="5" unit="character"/> 

    <lb n="27"/><supplied reason="lost">μ</supplied>έχρι τοῦ <num value="28">κη</num> <expan><ex>ἔτους</ex></expan> πυροῦ ἀρταβῶν ἐνακοσ<supplied reason="lost">ίων <num value="900"/> καὶ</supplied> <unclear>τοῦ</unclear>δε τοῦ <num value="29">κθ</num> <expan><ex>ἔτους</ex></expan> <supplied reason="lost">Καί</supplied>σαρος <expan><ex>πυροῦ</ex></expan> <unclear>ἀ</unclear><supplied reason="lost">ρτάβας <num value="100">ρ</num></supplied> 

