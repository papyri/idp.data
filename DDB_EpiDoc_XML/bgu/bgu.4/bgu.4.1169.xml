    <lb n="1"/><expan>Πρωτάρχ<ex>ωι</ex></expan> 

    <lb n="2"/>παρὰ <handShift new="m2"/> Τ<unclear>η</unclear>θοήους τοῦ Ἀ<gap reason="illegible" quantity="1" unit="character"/>

