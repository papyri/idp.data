         <head n="18626b" xml:lang="en">
            <date>10 BC</date>
            <placeName>Alexandria</placeName>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><ab>

    <lb n="50"/><expan>Πρωτάρχ<ex>ωι</ex></expan> 

    <lb n="51"/>παρὰ Ἀμμωνίου τοῦ Ἀμενούθου καὶ <expan>π<ex>αρὰ</ex></expan> Ψενανούφιος τοῦ Ἁρπαήσιος. <expan>συγχ<ex>ωρεῖ</ex></expan> ὁ Ψενα

    <lb n="52" break="no"/>νοῦφις <expan>παραχωρ<unclear>ή</unclear><ex>σειν</ex></expan> τῷ Ἀμμωνίῳ <expan>τὴ<ex>ν</ex></expan> <app type="alternative"><lem><expan>πρᾶ<ex>ξιν</ex></expan></lem><rdg>πρᾶξιν</rdg></app> τῶν <choice><reg><expan>ὀφειλομ<ex>ένων</ex></expan></reg><orig><expan>ὀφιλομ<ex>ένων</ex></expan></orig></choice> τῷ πατρὶ αὐτοῦ Ἁρπαήσει Ἀ<unclear>νουβί</unclear>

    <lb n="53" break="no"/>ωνος ὑπὸ Ἀμμωνοῦτος τῆς καὶ Σε<unclear>μ</unclear>νούφιος τῆς Πετεχνούφιος <expan>Περσ<ex>ίνης</ex></expan> κατὰ <expan><supplied reason="lost">τ</supplied>ὴ<ex>ν</ex></expan> 

    <lb n="54"/><expan><unclear>γεγο</unclear><ex>νυῖαν</ex></expan> ἐν <expan>Ὑψηλ<ex>ῇ</ex></expan> τοῦ <expan>Λυκοπ<ex>ολίτου</ex></expan> ἀσφάλειαν τῷ <num value="12">ι<unclear>β</unclear></num> <expan><ex>ἔτει</ex></expan> <expan>Καίσ<ex>αρος</ex></expan> <expan>Φαῶ<ex>φι</ex></expan> <expan>ἀργυ<ex>ρίου</ex></expan> <expan><ex>δραχμὰς</ex></expan> 
<app type="alternative"><lem><num value="26">κϛ</num></lem><rdg>εἴκοσι ἓξ</rdg></app>, καὶ ἐ<unclear>ξεῖναι</unclear> Ἀμμωνίῳ 

    <lb n="55"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="2" unit="character"/> <expan><unclear>ὑπό</unclear>χρ<unclear>ε</unclear><ex>ον</ex></expan> ταύτας τε <unclear>καὶ</unclear> <expan>τ<ex>οὺς</ex></expan> <expan>τ<unclear>όκ</unclear><ex>ους</ex></expan> ἀπὸ Ἁθὺρ <num value="19">ιθ</num> <expan><ex>ἔτους</ex></expan> Καίσαρος <del rend="erasure">τὰ<unclear>ς</unclear> <unclear>δὲ</unclear> <expan><unclear>ἀνειλ</unclear><ex>ημμένας</ex></expan></del> 

    <lb n="56"/><del rend="erasure"><unclear>εἰς</unclear> <unclear>τὸ</unclear> <expan><unclear>ἴδιο</unclear><ex>ν</ex></expan> <expan>κομίζ<ex>εσθαι</ex></expan></del> καὶ <expan>παραδιδό<ex>ναι</ex></expan> καὶ <expan>ἐνεχυράζ<ex>ειν</ex></expan> καὶ <unclear>ἐπι</unclear><gap reason="illegible" quantity="3" unit="character"/>ειν καὶ τὰ <expan><unclear>γενόμ</unclear><ex>ενα</ex></expan> <expan>ἀποφέ<ex>ρεσθαι</ex></expan> εἰς τὸ <expan>ἴ<ex>διον</ex></expan> 

    <lb n="57"/>καὶ ὅτι ἐὰν <expan>θέλ<ex>ῃ</ex></expan> <expan>ἐπιτελ<ex>εῖν</ex></expan> <add place="above">ὡς καὶ <expan>αὐ<ex>τὸν</ex></expan> <expan>Πετεφνο<ex>ῦθιν</ex></expan> <expan>ἀπεσχ<ex>ηκότα</ex></expan> παρʼ Ἀμμωνίου διὰ <expan>χ<ex>ειρὸς</ex></expan> ἐξ <expan>οἴκ<ex>ου</ex></expan> τὰ ἴσα,</add> <supplied reason="lost">τ</supplied>ὸν δὲ Πετεφνοῦθιν 
μὴ <expan><unclear>ἐπελεύ</unclear><ex>σεσθαι</ex></expan> ἐπὶ <expan>τὴ<ex>ν</ex></expan> <expan>συγχώρη<ex>σιν</ex></expan> μη<supplied reason="lost">δʼ</supplied> <expan>ἄλ<ex>λον</ex></expan> 

    <lb n="58"/>μηδένα, παρέχεσθαι δὲ τ<supplied reason="lost">ὸ </supplied><choice><reg><supplied reason="lost">δ</supplied>άνει</reg><orig><supplied reason="lost">δ</supplied>άνη</orig></choice><expan><ex>ον</ex></expan> <expan>ἀπερίλ<ex>υτον</ex></expan> <unclear>καὶ</unclear> <unclear>ἔτι</unclear> τὸν <expan><unclear>ἐ</unclear><supplied reason="lost">πε</supplied>λ<ex>ευσόμενον</ex></expan> <expan>ἀποσ<ex>τήσειν</ex></expan> <expan>παραχ<ex>ρῆμα</ex></expan> τοῖς <expan>ἰδί<ex>οις</ex></expan> <expan><supplied reason="lost">δ</supplied>απ<ex>ανήμασιν</ex></expan> ἢ χω

    <lb n="59" break="no"/>ρὶς <expan>τ<ex>οῦ κύρια εἶ</ex>ναι</expan> <expan>τ<ex>ὰ</ex></expan> <expan>σ<ex>υνκεχωρημένα</ex></expan> <expan>ἐκτίν<ex>ειν</ex></expan> τὸν <expan>Πετε<supplied reason="lost">φνο<ex>ῦθιν</ex></supplied></expan><supplied reason="lost"> τὸ </supplied><expan><supplied reason="lost">κ</supplied>εφάλ<ex>αιον</ex></expan> <unclear>σὺν</unclear> <unclear>τοῖς</unclear> <expan>τόκ<ex>οις</ex></expan> 
<expan>καθ<ex>άπερ</ex></expan> <choice><reg>ἐκ</reg><orig>ἐγ</orig></choice> <expan>δίκ<ex>ης</ex></expan>, <expan>ἀναδέδωκ<ex>ε</ex></expan> δὲ τῷ Ἀμμωνίῳ 

    <lb n="60"/>καὶ τὰς περὶ τῆς <choice><reg><expan>ὀφειλ<ex>ῆς</ex></expan></reg><orig><expan>ὀφιλ<ex>ῆς</ex></expan></orig></choice> <expan>ἀσφαλ<ex>είας</ex></expan>. <expan><ex>ἔτους</ex></expan> <num value="20">κ</num> Καίσαρος <expan>Φαρμο<ex>ῦθι</ex></expan> <num value="22">κβ</num>. 
<milestone rend="paragraphos" unit="undefined"/>
