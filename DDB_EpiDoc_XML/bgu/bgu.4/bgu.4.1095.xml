    <lb n="1"/><gap reason="illegible" quantity="1" unit="line"/> 

    <lb n="1"/><gap reason="lost" quantity="2" unit="character"/>ς τὰ ὄντα παράσ<supplied reason="lost">χ</supplied>ου <gap reason="lost" quantity="3" unit="character"/> 

    <lb n="2"/><supplied reason="lost">ἐ</supplied>ξέβημεν τῆς ἐπισκ<supplied reason="lost">έψεως</supplied>. 

    <lb n="3"/>ἐπέμψαμεν δέ σοι δι<supplied reason="lost">ὰ μαχαι</supplied>

