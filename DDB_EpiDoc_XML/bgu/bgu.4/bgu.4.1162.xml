         <head n="18612" xml:lang="en">
            <date>17/16 BC</date>
            <placeName>Alexandria</placeName>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><ab>
    <lb n="1"/><app type="editorial"><lem resp="P. van Minnen, Tyche 36 (2021) 207 (KorrTyche 1082)"><supplied reason="lost">Ἀχαίωι </supplied><gap reason="lost" extent="unknown" unit="character"><certainty match=".." locus="name"/></gap> <supplied reason="lost">παρὰ</supplied> </lem><rdg><supplied reason="lost">Πρωτάρχωι παρὰ</supplied></rdg></app> <supplied reason="lost">Σωσιπάτρου τοῦ </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> καὶ παρὰ Ἀριστοκλήας τῆς </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> Περσίνης μετὰ κυρίου τοῦ </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> καὶ Τρυφαίνης τῆς </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> Περσίνης μετὰ κυρίου 
τοῦ </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost">. συνχωροῦσιν ἡ Ἀριστοκλήα καὶ Τρύφαινα ἔχειν παρὰ τοῦ</supplied> 

    <lb n="1"/><supplied reason="lost">Σωσιπάτρου</supplied> <choice><reg>δάνειον</reg><orig><unclear>δάνηον</unclear></orig></choice> διὰ <choice><reg>χειρ<supplied reason="lost">ὸς</supplied></reg><orig>χιρ<supplied reason="lost">ὸς</supplied></orig></choice> <supplied reason="lost">ἐξ οἴκου <expan>ἀργυ<ex>ρίου</ex></expan> Πτολε</supplied>

    <lb n="2" break="no"/>μαικοῦ δραχμὰς χιλίας ὀγδοήκοντα <num value="1080"/> <unclear>ἄτ</unclear><supplied reason="lost">οκον,</supplied> 

    <lb n="3"/>ὃ καὶ <choice><reg>ἀποδώσειν</reg><orig>ἀποδώσιν</orig></choice> ἐν μησὶν δέκα <num value="10"/> οὖσιν ἀ<unclear>πὸ</unclear> <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="4"/>τοῦ ἐνεστῶτος <choice><reg>τεσσαρεσκαιδεκ<supplied reason="omitted">άτ</supplied>ου</reg><orig>τεσσαρεσκαιδεκου</orig></choice> ἔτους <supplied reason="lost">Καίσαρος</supplied> 

    <lb n="5"/>ἄνευ πάσης ὑπερθέσεως, εἰ δὲ <supplied reason="omitted">μὴ</supplied> εἶναι αὐ<supplied reason="lost">τὰς ἀγω</supplied>

    <lb n="6" break="no"/>γίμας καὶ <choice><reg>συνέχ<supplied reason="omitted">εσ</supplied>θαι</reg><orig>συνεχθαι</orig></choice> μέχρι τοῦ <choice><reg>ἐ<supplied reason="omitted">κ</supplied>τεῖσα<supplied reason="lost">ι</supplied></reg><orig>ἐτῖσα<supplied reason="lost">ι</supplied></orig></choice> <supplied reason="lost">τὸ μὲν </supplied><choice><reg><supplied reason="lost">δά</supplied>

    <lb n="7" break="no"/>νειον</reg><orig><supplied reason="lost">δά</supplied><lb n="7" break="no"/>νηον</orig></choice> <surplus>συ</surplus> τὰς τοῦ <expan>ἀργυ<ex>ρίου</ex></expan> δραχμὰς χιλίας ὀ<supplied reason="lost">γδοήκον</supplied>

    <lb n="8" break="no"/>τα <num value="1080"/> σὺν ἡμιολίᾳ παραχρῆμα, τοῦ δʼ ὑπερ<supplied reason="lost">πεσόντος</supplied> 

    <lb n="9"/>χρόνου τοὺς κατὰ τὸ <choice><reg>διάγραμ<supplied reason="omitted">μα</supplied></reg><orig>διαγραμ</orig></choice> τόκου<supplied reason="lost">ς <expan><ex>διδράχμους</ex></expan> τῆς</supplied> 

    <lb n="10"/>πράξεως γεινομένης τῷ Σωσι<unclear>πάτρ</unclear><supplied reason="lost">ῳ ἔκ τε</supplied> 

    <lb n="11"/>τῶν <choice><reg>ὑποχρέ<supplied reason="omitted">ω</supplied>ν</reg><orig>ὑποχρεν</orig></choice> <choice><reg><expan>ἀλλη<supplied reason="omitted">λ</supplied>εγ<ex>γύων</ex></expan></reg><orig>ἀλληεν</orig></choice> οὐσῶν <unclear>εἰς</unclear> <choice><reg>ἔ<supplied reason="omitted">κ</supplied>τι<supplied reason="lost">σιν</supplied></reg><orig><unclear>ἐ</unclear>τ<unclear>ι</unclear><supplied reason="lost">σιν</supplied></orig></choice> <supplied reason="lost">καὶ </supplied><choice><reg><supplied reason="lost">ἐ</supplied>

    <lb n="12" break="no"/>κ</reg><orig><supplied reason="lost">ἐ</supplied><lb n="12" break="no"/>γ</orig></choice> μιᾶς <num value="1"/> ἧς ἐὰν αὐτῶν <choice><reg>α<supplied reason="omitted">ἱ</supplied>ρῆται</reg><orig>ἁρ<unclear>ῆται</unclear></orig></choice> καὶ ἐκ <unclear>τῶν</unclear> <supplied reason="lost">ὑπαρχόντων</supplied> 

    <lb n="13"/>αὐταῖς <expan>πάντω<ex>ν</ex></expan> <choice><reg>κα<supplied reason="omitted">θά</supplied>περ</reg><orig>κάπερ</orig></choice> <choice><reg>ἐκ</reg><orig>ἐγ</orig></choice> <choice><reg>δί<supplied reason="omitted">κ</supplied>ης</reg><orig>διης</orig></choice> <choice><reg><expan>ἀκ<supplied reason="omitted">ύ</supplied>ρω<ex>ν</ex></expan></reg><orig>ἀκρω</orig></choice> οὐσῶ<supplied reason="lost">ν καὶ ὧν</supplied> 

    <lb n="14"/>ἐὰν <choice><reg><expan>ἐπενέγκω<ex>σι</ex></expan></reg><orig><unclear>ἐπεν</unclear>ενκω</orig></choice> <expan>πίστε<ex>ων</ex></expan> <expan>πασῶ<ex>ν</ex></expan> σκέπης <supplied reason="lost">πάσης.</supplied> 

    <lb n="15"/>τοῦτο δʼ <unclear>ἔστιν</unclear> τὸ <choice><reg>δάνειον</reg><orig>δάνηον</orig></choice> ἐκτὸς οὗ <supplied reason="lost">ὀφείλου</supplied>

    <lb n="16" break="no"/>σιν αἱ αὐταὶ Ἀριστοκλήα καὶ Τρύφαινα <supplied reason="lost">Σωσιπά</supplied>

    <lb n="17" break="no"/>τρῳ καθʼ ἑτέραν συνχώρησιν διὰ τοῦ <supplied reason="lost">αὐτοῦ</supplied> 

    <lb n="18"/>κριτηρίου ἄλλων ἀργυρίου ταλάντων τριῶ<supplied reason="lost">ν</supplied> <num value="3"/>. 
<milestone rend="paragraphos" unit="undefined"/>
    <lb n="19"/></ab></div>
