    <lb n="17"/>αὐτῷ· <abbr><unclear>ει</unclear><gap reason="illegible" quantity="1" unit="character"/><unclear>λ</unclear>εια</abbr> <abbr><unclear>πλ</unclear>ε<unclear>η</unclear>ο</abbr> <unclear>α</unclear><gap reason="illegible" quantity="2" unit="character"/><unclear>π</unclear><gap reason="illegible" quantity="3" unit="character"/>ι<unclear>σαι</unclear> <abbr><unclear>του</unclear><gap reason="illegible" quantity="1" unit="character"/></abbr> ἀπολ<unclear>εί</unclear>

