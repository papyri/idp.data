    <lb n="18"/><choice><reg>μέτρῳ</reg><orig>μετρο</orig></choice> <choice><reg>χαλκῷ</reg><orig>χαλκο</orig></choice> Ἀβουτᾶ<unclear>τος</unclear> <supplied reason="lost">τῆς</supplied> 

