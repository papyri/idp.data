    <lb n="1"/><expan><ex>ἔτους</ex></expan> <num value="16"><unclear>ι</unclear>ϛ</num> <unclear>Καίσαρ</unclear><supplied reason="lost">ο</supplied>ς Τῦβ<supplied reason="lost">ι </supplied><gap reason="lost" quantity="1" unit="character"/><num value="6">ϛ</num> 

