    <lb n="1"/><handShift new="m2"/> <num value="63">ξγ</num> 

    <lb n="2"/><handShift new="m1"/> Ἁρποκρατίωνι τῷ καὶ <hi rend="diaeresis">Ἱ</hi>έρα<supplied reason="lost">κι</supplied> 

