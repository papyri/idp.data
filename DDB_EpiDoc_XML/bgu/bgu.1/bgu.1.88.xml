    <lb n="1"/><expan><ex>ἔτους</ex></expan> <num value="10">ι</num> Ἀντωνίνου Κ<supplied reason="lost">α</supplied>ί<supplied reason="lost">σαρο</supplied>ς τ<supplied reason="lost">οῦ κυρ</supplied>ί<supplied reason="lost">ου</supplied> 

