         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r" type="textpart"><!--milestone unit="4"--><ab>

    <lb n="1"/><gap reason="lost" extent="unknown" unit="character"/><gap reason="illegible" quantity="2" unit="character"/><unclear>ω</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="2"/><gap reason="lost" quantity="14" unit="character"/><supplied reason="lost"> Κ</supplied>άστορ<gap reason="lost" quantity="13" unit="character"/><supplied reason="lost"> λαογρα</supplied>φουμε<supplied reason="lost">ν </supplied><gap reason="lost" quantity="5" unit="character"/> 

    <lb n="3"/><gap reason="lost" quantity="16" unit="character"/> τοῦ αὐτοῦ Ἀρσι<supplied reason="lost">νο</supplied>είτου ἀναγραφομένου α<gap reason="lost" quantity="6" unit="character"/> 

    <lb n="4"/><gap reason="lost" quantity="16" unit="character"/> <app type="editorial"><lem resp="bgu 3 p.2"><choice><corr>συνχωρεῖ</corr><sic>συνχωρι</sic></choice></lem><rdg>συνχωρη</rdg></app> <gap reason="illegible" quantity="4" unit="character"/><gap reason="lost" quantity="2" unit="character"/> <gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="1" unit="character"/> <app type="editorial"><lem resp="BGU 2 p.355"><supplied reason="lost">πεπρακέ</supplied>

    <lb n="5" type="inWord"/><unclear>ν</unclear><supplied reason="lost">αι καὶ παρακεχωρηκ</supplied>έναι</lem><rdg><gap reason="lost" quantity="6" unit="character"/>ν<gap reason="lost" quantity="6" unit="character"/><supplied reason="lost"> πεπρακ</supplied>έναι</rdg></app> τῇ Θερμού<unclear>θι</unclear> ἀπὸ τοῦ νῦν εἰς τὸν ἀεὶ χρόνον 

    <lb n="6"/><unclear>π</unclear><gap reason="lost" quantity="11" unit="character"/> τῇ προτέρᾳ κ<gap reason="lost" quantity="6" unit="character"/> κώμῃ <gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" quantity="9" unit="character"/> 

    <lb n="7"/><unclear>ο</unclear><supplied reason="lost">ἰκίαν </supplied><choice><corr><supplied reason="lost">καταπεπτ</supplied>ωκυῖαν</corr><sic><supplied reason="lost">καταπεπτ</supplied>ωκυειαν</sic></choice>, ἧς γείτο<supplied reason="lost">νες </supplied><gap reason="lost" quantity="5" unit="character"/><gap reason="illegible" quantity="2" unit="character"/>νεως Ἀπολ<unclear>ι</unclear><supplied reason="lost">ναρίου </supplied><gap reason="lost" quantity="2" unit="character"/> 

    <lb n="8"/>κ<gap reason="lost" quantity="10" unit="character"/><gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" quantity="1" unit="character"/><supplied reason="lost"> Ο</supplied>ὐαλερίου οὐετρα<supplied reason="lost">νοῦ Ἀντινοέ</supplied>ως, ἀπηλιώτ<supplied reason="lost">ῃ </supplied><gap reason="lost" quantity="6" unit="character"/> 

    <lb n="9"/><unclear>μ</unclear><gap reason="lost" quantity="5" unit="character"/><supplied reason="lost"> Ἀπολι</supplied>ναρίου Ἀντινοέως καὶ περ<supplied reason="lost">ὶ τ</supplied>ὴν <supplied reason="lost">α</supplied>ὐτὴν κώμην ἐν πε<supplied reason="lost">δίῳ Ψεναρ</supplied>

    <lb n="10" type="inWord"/>ψενήσε<supplied reason="lost">ως </supplied><app type="editorial"><lem><supplied reason="lost">ἐ</supplied>ν μὲν</lem><rdg><gap reason="lost" quantity="2" unit="character"/>μ ἐν</rdg></app> τόπῳ Πολύδᾳ λεγομένῳ ἐλαιῶνος ἐν δυσὶ <num value="2"/> σφραγ<supplied reason="lost">ῖσι</supplied> 

    <lb n="11"/>ἀρούρης ἥμισυ <num value="1/2"/>, ὂν ἐν κατοικικῇ τάξει, οὗ γείτονες τῆς μὲν πρώτης <choice><corr>σφρα

    <lb n="12" type="inWord"/>γῖδος</corr><sic>σφραγειδος</sic></choice> νότῳ καὶ λιβὶ <supplied reason="lost">Λ</supplied>ονγεινίας Γεμέλλας, βορρᾶ διῶρυξ Θελαμοῦι λεγο

    <lb n="13" type="inWord"/>μένη, ἀπηλιώτῃ <unclear>τῶν</unclear> τοῦ Καστὼλ ἐλαιών, ἐν οἷς πυργομαγδώλ, τῆς δὲ δ<supplied reason="lost">ευ</supplied>

    <lb n="14" type="inWord"/>τέρας <choice><corr>σφραγῖδος</corr><sic>σφραγειδος</sic></choice> νότῳ τῆς ἀποδιδομένης Διδυμαρίου ἐλαιών, βορρᾶ ἡ προγε

    <lb n="15" type="inWord"/>γραμμένη διῶρυξ, ἀπηλιώτῃ καὶ λιβὶ τῆς προκειμένης Σεμπρωνίας Γε

    <lb n="16" type="inWord"/>μέλλας ἐλαιὼν καὶ ἐν τῷ αὐτῷ πεδίῳ ἐν τόπῳ Χαλκολόγῳ λεγομένῳ ἐλ<unclear>αιῶ</unclear>

    <lb n="17" type="inWord"/>νος ἐν κατοικικῇ τάξει ἥμισυ μέρος ἀρούρης μιᾶς <num value="1"/> ἐκ τοῦ πρὸς ἀπηλιώτην μέ

    <lb n="18" type="inWord"/>ρους <choice><corr>διατεῖνον</corr><sic>διατινον</sic></choice> νότον ἐπὶ βορρᾶν, οὗ ὅλου γείτονες νότῳ <app type="editorial"><lem resp="bgu 1 p.358">φάλαγξ</lem><rdg>Φάλαγξ</rdg></app>, βορρᾷ καὶ λιβὶ 

    <lb n="19"/>γενηματογραφούμ<supplied reason="lost">ε</supplied>νος ἐλαιών, πρότερον Λογγείνου Γεμέλλου, ἀπηλιώ

    <lb n="20" type="inWord"/>τῃ Σεμπρωνίου Λον<supplied reason="lost">γ</supplied>είνου Ἀντινοέως ἐλαιών, καὶ περὶ κώμην <app type="BL"><lem resp="1.35">Κερκε<unclear>σουχ</unclear>α</lem><rdg>Κερκε<unclear>σῆ</unclear><supplied reason="lost">φ</supplied><unclear>ι</unclear><supplied reason="lost">ν</supplied></rdg></app> 

    <lb n="21"/>ἐν τόπῳ οἰκίας Κανν<gap reason="lost" quantity="1" unit="character"/><supplied reason="lost"> λ</supplied>εγομένου δίμοιρον μέρος κλήρου κατοικικοῦ ἀρ<supplied reason="lost">ου</supplied><unclear>ρῶν</unclear> 

    <lb n="22"/>τριῶν <num value="3"/>, ὧν ὅλων γείτο<supplied reason="lost">νε</supplied>ς νότῳ καὶ λιβὶ τοῦ προγεγραμμένου Σεμπρωνίου Λον

    <lb n="23" type="inWord"/>γείν<supplied reason="lost">ου, β</supplied>ορρᾶ κληρονόμων Μάγνας ἀδελφῆς αὐτοῦ, ἀπηλιώτῃ ὑδραγωγός, ἐξ <unclear>οὗ</unclear> <unclear>π</unclear><supplied reason="lost">οτί</supplied>

    <lb n="24" type="inWord"/>ζε<supplied reason="lost">ται ὁ κ</supplied>λῆρος μ<gap reason="lost" quantity="3" unit="character"/><supplied reason="lost"> δ</supplied>ημοσία γῆ, καὶ ἐν τῷ αὐτῷ πεδίῳ ἐν τόπῳ οἰκίας Σα<gap reason="lost" quantity="5" unit="character"/> 

    <lb n="25"/>λο<unclear>χ</unclear> <supplied reason="lost">λεγο</supplied>μένου ἥμ<supplied reason="lost">ισυ</supplied> μέρος κλήρου κατοικικοῦ ἀρουρῶν δυῶν <num value="2"/>, ὧν ὅλων <supplied reason="lost">γείτο</supplied>

    <lb n="26" type="inWord"/>νες νότῳ Λονγείνου <supplied reason="lost">Π</supplied>ρείσκου στρατιώτου <choice><corr>σιτικὰ</corr><sic>σειτικα</sic></choice> ἐδάφη, βορρᾶ Σεμπρωνίας 

    <lb n="27"/>Λονγείνας <choice><corr>σιτικὰ</corr><sic>σειτικα</sic></choice> ἐ<supplied reason="lost">δάφ</supplied>η, ἀπηλιώτῃ βασιλικὴ γῆ, λιβὶ φάλαγξ, ἐπὶ δὲ παντὸς τοῦ 

    <lb n="28"/><app type="editorial"><lem resp="BGU 1 p.396">ἀρου<supplied reason="lost">ρ</supplied>ηδοῦ</lem><rdg>ἀρου<supplied reason="lost">ρ</supplied><unclear>ο</unclear>δοῦ</rdg></app> κατʼ <app type="BL"><lem resp="1.36">ἐπιβ<supplied reason="lost">ολὴ</supplied>ν</lem><rdg>ἐπίκ<supplied reason="lost">λησι</supplied>ν</rdg></app> τοῦ ἐνόντος παντὸς ἐδάφους ἢ ὅσοι ἐὰν ὦσι ἐπὶ <unclear>τὸ</unclear> 

    <lb n="29"/>πλεῖον ἢ ἔλασσον <gap reason="lost" quantity="4" unit="character"/>ούσῃ ἐπιβολῇ <choice><corr>τιμῆς</corr><sic>τειμης</sic></choice> καὶ παραχωρητικοῦ ἀργυρίου 

    <lb n="30"/>δραχμῶν <choice><corr>δισχιλίων</corr><sic>δισχειλ<supplied reason="lost">ιων</supplied></sic></choice> ἑκατόν <num value="2100"/>, ἃς καὶ ἀπέσχεν ἡ Διδυμάριον παρὰ τῆς Θερμού

    <lb n="31" type="inWord"/>θιος ἐντεῦθεν διὰ χ<supplied reason="lost">ειρὸ</supplied>ς ἀφʼ ὧν εἰσι τῆς μὲν οἰκίας, ἐν ᾗ αὐλή, ἀργυρίου δραχμαὶ 

    <lb n="32"/>ἑκατόν <num value="100"/>, τῶν δʼ ἄλλων <supplied reason="lost">αἱ λ</supplied>οιπαὶ ἀργυρίου δραχμαὶ <choice><corr>δισχιλίαι</corr><sic>δισχειλιαι</sic></choice> <num value="2000"/>, καὶ ἀπὸ τοῦ νῦν τὴν 

    <lb n="33"/>Θερμοῦθιν κρατεῖν <supplied reason="lost">καὶ κ</supplied>υριεύειν τῶν πεπραμένων καὶ παρακεχωρημέ

    <lb n="34" type="inWord"/>νων αὐτῇ ὡς πρόκ<supplied reason="lost">ιται</supplied> καὶ τὰ ἐξ αὐτῶν περιγεινόμενα ἀποφέρεσθαι <choice><corr>εἰς</corr><sic>ις</sic></choice> τὸ ἴδιον 

    <lb n="35"/>καὶ ἐξουσίαν ἔχειν ἑτέρ<supplied reason="lost">ο</supplied>ις <choice><corr>πωλεῖν</corr><sic>πολειν</sic></choice> καὶ διοικεῖν καὶ ἐπιτελεῖν περὶ αὐτῶν 

    <lb n="36"/>ὡς ἐὰν αἱρῆται, τὴν δὲ <supplied reason="lost">Δ</supplied>ιδυμάριον μὴ ἐπιπορεύεσθαι ἐπὶ ταῦτα μηδʼ ἄλλον 

    <lb n="37"/>ὑπὲρ αὐτῆς μηδένα κατὰ μηδένα τρόπον, τὸν δὲ καὶ ἐπελευσόμενον <choice><corr>ἀ

    <lb n="38" type="inWord"/>ποστήσειν</corr><sic>απο<supplied reason="lost">σ</supplied><unclear>τ</unclear>ησιν</sic></choice> παραχρῆμα τοῖς ἰδίοις δαπανήμασι, τῆς βεβαιώσεως ἐξακο

    <lb n="39" type="inWord"/>λο<supplied reason="lost">υθο</supplied>ύσης αὐτῇ κατὰ πᾶσαν βεβαίωσιν διὰ παντὸς ἢ χωρὶς <app type="BL"><lem resp="1.36"><unclear>τοῦ</unclear> <supplied reason="lost">π</supplied><unclear>ο</unclear>ιεῖν</lem><rdg>η<gap reason="lost" quantity="2" unit="character"/><unclear>ι</unclear>ειν</rdg></app> 

    <lb n="40"/>α<gap reason="lost" quantity="4" unit="character"/> αὐτὰ ἔτι καὶ <choice><corr>ἐκτίνειν</corr><sic>εκτιννιν</sic></choice> τὴν <choice><corr>τιμὴν</corr><sic>τειμην</sic></choice> σὺν ἡμιολίᾳ καὶ <unclear>τὰ</unclear> <app type="editorial"><lem resp="BGU 1 p.396">β<unclear>λ</unclear><supplied reason="lost">α</supplied><unclear>β</unclear>η καὶ δα

    <lb n="41" type="inWord"/><supplied reason="lost">πανή</supplied>ματα</lem><rdg>β<unclear>λ</unclear><gap reason="lost" quantity="1" unit="character"/><gap reason="illegible" quantity="1" unit="character"/>η καὶ α<gap reason="lost" quantity="4" unit="character"/><unclear>θετ</unclear>α</rdg></app> καὶ τὸ ὡρισμένον <choice><corr>πρόστιμον</corr><sic>προστειμον</sic></choice> καθάπερ <choice><corr>ἐκ</corr><sic>εγ</sic></choice> δίκης <app type="editorial"><lem resp="bgu 3 p.2"><unclear>καὶ</unclear> <choice><corr>ἀξιῶ</corr><sic><unclear>α</unclear>ξει<unclear>ω</unclear></sic></choice></lem><rdg><gap reason="lost" quantity="2" unit="character"/><gap reason="illegible" quantity="3" unit="character"/></rdg></app> 

    <lb n="42"/><supplied reason="lost"><expan><ex>ἔτους</ex></expan> </supplied><gap reason="lost" quantity="2" unit="character"/><supplied reason="lost">δ</supplied><unclear>εκ</unclear>άτου Αὐτοκράτορος Καίσαρος Μάρκου Αὐρηλίου Ἀντω<supplied reason="lost">νίνου</supplied> 

    <lb n="43"/><supplied reason="lost">Ἀρμενια</supplied>κοῦ Μηδικοῦ <supplied reason="lost">Παρθικ</supplied>οῦ Γερμαν<supplied reason="lost">ικοῦ</supplied> <unclear>Σαρμ</unclear>ατικ<supplied reason="lost">οῦ Με</supplied><unclear>γ</unclear><supplied reason="lost">ίστου</supplied> 

