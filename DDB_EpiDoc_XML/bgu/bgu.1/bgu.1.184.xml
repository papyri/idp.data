    <lb n="1"/><handShift new="m2"/> <expan>πώλ<ex>ησις</ex></expan> 

    <lb n="2"/><handShift new="m1"/> Ἀπολλωνίωι καὶ <app type="editorial"><lem resp="BL 1.434">Θέ<supplied reason="lost">ωνι</supplied></lem><rdg>Θε<gap reason="lost" quantity="6" unit="character"/></rdg></app> 

