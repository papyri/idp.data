         <head n="4826" xml:lang="en">
            <date>64/63 BC</date>
            <placeName>Herakleopolite</placeName>
            <ref n="sb;4;7408" type="reprint-from">SB4,7408</ref>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><ab>
    <lb n="1"/><supplied reason="lost">Διονύσιος Πανίσκωι χαίρειν. τοῦ πρὸς Ἡρακλείδην τὸν σιτολόγον χρηματισμοῦ ἀντίγραφον ὑπόκειται. ἔρρωσο. <expan><ex>ἔτους</ex></expan> <num value="18">ιη</num> </supplied><gap reason="lost" quantity="6" unit="character"/> 

    <lb n="2"/><supplied reason="lost">Ἡρακλείδῃ</supplied> 

    <lb n="3"/><handShift new="m4"/> τοῦ παρʼ Ἀθηναίου τοῦ συγγενο<supplied reason="lost">ῦς καὶ διοικητοῦ χρηματισμοῦ ἀντίγραφον ὑπόκειται. κατακολουθήσας οὖν μέτρησον συνεπιστέλλοντος</supplied> 

    <lb n="4"/>Πανίσκου τοῦ βασιλικοῦ γραμ<unclear>μ</unclear><supplied reason="lost">ατέως τοῖς </supplied><gap reason="lost" quantity="45" unit="character"/><supplied reason="lost"> τῶν λειτουργῶν τῶν τεταγμένων</supplied> 

    <lb n="5"/>τῶι διοικητῆι ἐκ τοῦ θαλασσίου <supplied reason="lost">στόλου πληρωμάτων τὴν καθήκουσαν αὐτοῖς τροφὴν τῆς ἀπὸ </supplied><gap reason="lost" quantity="9" unit="character"/><supplied reason="lost"> τοῦ <num value="18">ιη</num> <expan><ex cert="low">ἔτους</ex></expan> ἕως </supplied><gap reason="lost" quantity="7" unit="character"/><supplied reason="lost"> τοῦ <num value="19">ιθ</num> <expan><ex>ἔτους</ex></expan> <expan><ex>ἑνδεκα</ex>μή<ex>νου</ex></expan></supplied> 

    <lb n="6"/>τοῦ μηνὸς πυροῦ <expan>ἀρτάβ<ex>ας</ex></expan> <num value="424">υκδ</num><gap reason="lost" quantity="1" unit="character"/><supplied reason="lost"> <expan><ex>ἑνδεκα</ex>μή<ex>νου</ex></expan> τὰς συναγομένας <expan><ex>πυροῦ ἀρταβῶν</ex></expan> <expan>μ<ex>υριάδας</ex></expan> <num value="4">δ</num>  <num value="6740">Ϛψμ</num> <num value="46740"/> </supplied><gap reason="lost" quantity="50" unit="character"/> 

    <lb n="7"/>καὶ <expan>σύ<ex>μβολα</ex></expan> καὶ <expan>ἀντισύ<ex>μβολα</ex></expan> <expan>ποίη<ex>σαι</ex></expan> πρὸς <expan>αὐ<ex>τοὺς</ex></expan> ὡς <expan>καθή<ex>κει</ex></expan>. 

    <lb n="8"/>Ἀθήναιος Διονυσίωι χαίρειν. <unclear>ἐ</unclear><gap reason="lost" quantity="40" unit="character"/><supplied reason="lost"> τῶν λειτουργῶν τῶν τεταγμένων μοι ἐκ τοῦ θαλασσίου</supplied> 

    <lb n="9"/>στόλου πληρωμάτων, ὧν τὸ κατὰ <supplied reason="lost">πλοῖον ὑπόκειται, </supplied><gap reason="lost" quantity="10" unit="character"/><supplied reason="lost"> καθήκειν αὐτοῖς ἀπὸ </supplied><gap reason="lost" quantity="8" unit="character"/><supplied reason="lost"> τοῦ <num value="18">ιη</num> <expan><ex>ἔτους</ex></expan> ἕως </supplied><gap reason="lost" quantity="8" unit="character"/>ο 

    <lb n="10"/>τοῦ <num value="19">ιθ</num> <expan><ex>ἔτους</ex></expan> <expan>ἑνδεκαμήν<ex>ου</ex></expan> τοῦ <expan>μην<ex>ὸς</ex></expan> <expan><ex>πυροῦ</ex></expan> <expan>ἀνη<ex>λωτικῷ</ex></expan> <expan><ex>ἀρτάβας</ex></expan> <num value="424">υκδ</num><gap reason="illegible" quantity="1" unit="character"/> <supplied reason="lost">τὰ συναγόμενα <expan><ex>πυροῦ ἀρταβῶν</ex></expan> <expan>μ<ex>υριάδας</ex></expan> <num value="4">δ</num> <num value="6700">Ϛψ</num> <num value="46700"/> </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="11"/><choice><reg>ἀφράκτων</reg><orig>ἀφρακτου</orig></choice>· τριηράρχου Χαρι<supplied reason="lost">σ</supplied>ανδραγά<supplied reason="lost">θου καὶ Εὐνόμου Ναυκρατιτῶν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="12"/>Σαραπίωνος καὶ Μηνοδότου Κανωπ<unclear>ι</unclear><supplied reason="lost" cert="low">τῶν </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="low"> Δικρότων πληρωμάτων </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="13"/>Ἑρμίου <num value="120">ρκ</num>, Ἱέρακος <num value="170">ρο</num> Δη<unclear>μ</unclear>ο<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="14"/>τοῦ Σαραπίωνος <num value="142">ρμβ</num>, Θε<unclear>ο</unclear>δέκτου καὶ Α<unclear>ἰτι</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="15"/>Νουμηνίου <num value="150">ρν</num>, Ἀθηναγόρου <num value="128">ρκη</num>, Ἀφροδισίου καὶ ἄλλων <num value="148">ρμη</num>, Ἀρτέμωνος <unclear>ο</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="16"/>Ζηνίωνος τοῦ Πάπου <num value="138">ρλη</num> <num value="1/2">𐅵</num> μονοκρότων· Ποσειδωνίου καὶ Ζήν<supplied reason="lost">ων</supplied><unclear>ος</unclear> <num value="6"><unclear>ϛ</unclear></num><gap reason="illegible" quantity="1" unit="character"/> <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="17"/>Μεννέου καὶ Ἑρμαίου <num value="32">λβ</num>, Ἐπιγένου <num value="38">λη</num>, Ἱέρακος <num value="35">λε</num>, Θεογείτονος <num value="22">κβ</num>, Ζήν<unclear>ω</unclear><supplied reason="lost">νος </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="18"/>Διονυσίου καὶ Μώζου <num value="44">μδ</num>, Κρατέρου καὶ Νικολάου <num value="40">μ</num>, Πρωτάρχου καὶ Φιλοξένου <num value="3"><unclear>γ</unclear></num><gap reason="lost" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="19"/>Διογένου καὶ Μενελάου <num value="34">λδ</num>, Ἀθηναγόρου καὶ Ἀγαθοκλέους <num value="94">ϙδ</num> Θρακίδου <num value="800">ω</num>, Ἀπολλωνίου <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="20"/><gap reason="illegible" quantity="1" unit="line"/> 

