         <head n="4833a 4833b" xml:lang="en">
            <date>64/63 BC</date>
            <placeName>Herakleopolite</placeName>
            <ref n="sb;4;7414" type="reprint-from">SB 4 7414</ref>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="1,ctr" type="textpart"><ab>

    <lb n="1"/><handShift new="m3"/> Διονύσιος Πανίσκωι χαίρειν. 

    <lb n="2"/>τοῦ πρὸς Φαμῆν τὸν τραπεζίτην 

    <lb n="3"/>χρηματισμοῦ ἀντίγραφον ὑπόκειται. 

    <lb n="4"/><handShift new="m10"/>    ἔρρωσο <expan><ex>ἔτους</ex></expan> <num value="18">ιη</num> Ἐπεὶφ <num value="12">ιβ</num>. 

    <lb n="5"/><handShift new="m3"/> Φαμῆι. 

    <lb n="6"/>χρημάτισον συνεπιστέλλοντος 

    <lb n="7"/>Πανίσκου τοῦ βασιλικοῦ γραμματέως 

    <lb n="8"/>τοῖς περὶ τὴν διοίκησιν παρατρέχ<unclear>ουσιν</unclear> 

    <lb n="9"/>τὰ ὑποκείμενα καὶ ἕως τοῦ <num value="17">ιζ</num> <expan><ex>ἔτους</ex></expan> 

    <lb n="10"/><choice><reg>προειμένα</reg><orig>προιμένα</orig></choice> ἐν <expan>χα<ex>λκοῦ</ex></expan> <expan><ex>ταλάντοις</ex></expan> <num value="190">ρϙ</num>, <expan>χα<ex>λκοῦ</ex></expan> <expan><ex>ταλάντοις</ex></expan> δέκα <expan><ex>γίνεται</ex></expan> <expan><ex>τάλαντα</ex></expan> <num value="10">ι</num> 

    <lb n="11"/>καὶ <expan>σύ<ex>μβολα</ex></expan> καὶ <expan>ἀντισύ<ex>μβολα</ex></expan> <expan>ποίη<ex>σαι</ex></expan> πρὸς <expan><unclear>αὐ</unclear><ex>τοὺς</ex></expan> ὡς <expan>καθή<ex>κει</ex></expan>. 
<milestone rend="paragraphos" unit="undefined"/>
    <lb n="12"/><handShift new="m8"/> τοῖς <expan>γρ<ex>αμματεῦσι</ex></expan>. <expan>ἐπιστ<unclear>α</unclear><ex>λῆναι</ex></expan> πρὸς <abbr>μεθ<unclear>α</unclear></abbr> 

    <lb n="13"/>τῶι πρ<unclear>ο</unclear><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><expan><ex>  </ex></expan> περὶ τῶ<unclear>ν</unclear> 

    <lb n="14"/><abbr>α</abbr> <expan>προσχρῆ<ex>σθαι</ex></expan> 

    <lb n="15"/><expan><ex>ἔτους</ex></expan> <num value="18">ιη</num> Ἐπὶφ <num value="13">ιγ</num> 
</ab></div><div n="1,ms" type="textpart"><ab>

    <lb n="16"/><handShift new="m9"/> <expan><unclear>Ἡ</unclear>ρακ<unclear>λεο</unclear>δώ<ex cert="low">ρῳ</ex></expan> 

    <lb n="17"/><expan><ex>ἔτους</ex></expan> <num value="18">ιη</num> Ἐπὶφ <num value="14">ιδ</num> 
</ab></div><div n="2" type="textpart"><ab>

    <lb n="18"/><handShift new="m6" cert="low"/> απειοις καὶ παρα<gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="19"/>ρε<gap reason="illegible" quantity="4" unit="character"/>τουσ<unclear>ηλ</unclear>αχ<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="20"/>ἀρτάβας χιλίας <gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> <expan><ex>γίνονται</ex></expan> <expan><ex>πυροῦ</ex></expan> <num value="1">α</num></supplied><gap reason="lost" quantity="3" unit="character"/><supplied reason="lost"> καὶ <expan>σύ<ex>μβολα</ex></expan> καὶ <expan>ἀντισύ<ex>μβολα</ex></expan> <expan>ποίη<ex>σαι</ex></expan></supplied> 

    <lb n="21"/>πρὸς <expan>αὐ<ex>τοὺς</ex></expan> ὡς <expan>καθ<ex>ήκει</ex></expan>. 
<milestone rend="paragraphos" unit="undefined"/>
    <lb n="22"/><handShift new="m9"/> <app type="editorial"><lem resp="BL 12.21">εἰς <expan>ἀ<unclear>ν</unclear><ex cert="low">τιγραφήν</ex></expan></lem><rdg>εἰς <abbr>α<gap reason="illegible" quantity="1" unit="character"/></abbr></rdg></app> <expan><ex>ἔτους</ex></expan> <num value="18">ιη</num> Ἐπὶφ <num value="14">ιδ</num> </ab></div></div>
