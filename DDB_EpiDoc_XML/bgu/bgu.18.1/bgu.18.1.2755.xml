         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r" type="textpart"><ab>

    <lb n="1"/><supplied reason="lost">Ἀνδρόμαχος Ἁρχ</supplied>ήβει <supplied reason="lost">χαίρειν· τοῦ πρὸς Λεωνίδην τὸν σιτολόγον χρημα</supplied>

    <lb n="2" break="no"/><supplied reason="lost">τισμοῦ ἀντίγραφο</supplied><unclear>ν</unclear> ὑπόκειται· <gap reason="lost" extent="unknown" unit="character"/>

    <lb n="3"/><gap reason="lost" extent="unknown" unit="character"/> <space extent="unknown" unit="character"/> <supplied reason="lost">ἔρρωσο· <expan><ex>ἔτους</ex></expan> </supplied><gap reason="lost" extent="unknown" unit="character"/>

    <lb n="4"/><handShift new="m2"/><supplied reason="lost">Λεωνίδῃ· τοῦ π</supplied>αρὰ Διονυσίου καὶ <unclear>Ἡρ</unclear><supplied reason="lost">ωΐδου τῶν πρὸς τῇ ναυλώσει χρη</supplied>

    <lb n="5" break="no"/><supplied reason="lost">ματισμοῦ ἀντίγρ</supplied>αφον ὑπόκειται· <unclear>κα</unclear><supplied reason="lost">τακολουθήσας οὖν ἐμβαλοῦ</supplied>

    <lb n="6"/><supplied reason="lost">συνεπιστέλλοντος</supplied> <unclear>Ἁ</unclear>ρχήβιος τοῦ βασιλι<supplied reason="lost">κοῦ γραμματέως εἰς <expan>σκά<ex>φην</ex></expan> </supplied><gap reason="lost" quantity="3" unit="character"/>

    <lb n="7"/><gap reason="lost" quantity="7" unit="character"/><supplied reason="lost"> ἀγωγῆς </supplied><gap reason="lost" quantity="1" unit="character"/><num value="20"><unclear>κ</unclear></num>, πυροῦ καθαροῦ ἀδό<supplied reason="lost">λου κεκοσκινευμένου μέτρῳ</supplied>

    <lb n="8"/><supplied reason="lost">δοχικῷ τῷ πρὸς τ</supplied><unclear>ὸ</unclear> χαλκοῦν καὶ σκυτ<unclear>άλῃ</unclear> <supplied reason="lost">δικαίᾳ, </supplied><gap reason="lost" quantity="1" unit="character"/><supplied reason="lost"> ἐπιβι</supplied>

    <lb n="9" break="no"/><supplied reason="lost">βασθέντων φυλακιτῶν κ</supplied><unclear>ε</unclear>κ<unclear>λη</unclear>ρουχημ<unclear>έν</unclear><supplied reason="lost">ων τῶν μάλιστα πίστιν</supplied>

    <lb n="10"/><supplied reason="lost">ἐχόντων, οἷ</supplied>ς καὶ τὸ δεῖγμα κατεσφραγι<unclear>σ</unclear><supplied reason="lost">μένον ἐπιτεθήσεται</supplied>

    <lb n="11"/><supplied reason="lost">ἐν γεΐνοις</supplied> <unclear>ὠμ</unclear>οῖς ἀγγείοις καὶ <unclear>π</unclear>αρακο<unclear>μι</unclear><supplied reason="lost">οῦσι παρὰ τοῦ πρὸς</supplied>

    <lb n="12"/><supplied reason="lost">τῇ σιτηρᾷ</supplied> <unclear>τ</unclear>ῆς ἐξαιρέσεως <supplied reason="omitted">τοὺς</supplied> καθήκο<unclear>ν</unclear><supplied reason="lost">τας χρηματισμοὺς ληφθεί</supplied>

    <lb n="13" break="no"/><supplied reason="lost">σης καὶ τοῦ</supplied> ν<supplied reason="lost">αυκλή</supplied>ρου χειρογραφίας ὅρκ<supplied reason="lost">ου βασιλικοῦ περὶ</supplied>

    <lb n="14"/><supplied reason="lost">τοῦ ἀκ</supplied>ακ<unclear>οπ</unclear>ο<supplied reason="lost">ίητον</supplied> ἀποκαταστή<unclear>σει</unclear><supplied reason="lost">ν εἰς τὴν πόλιν τὸν γόμον</supplied>

    <lb n="15"/><supplied reason="lost">οὐ στραγ</supplied><unclear>ε</unclear>υσ<unclear>άμεν</unclear><supplied reason="lost">ον</supplied> ἐν τῶι π<unclear>ό</unclear>ρωι <unclear>με</unclear><supplied reason="lost">τὰ τῆς Παγκράτου καὶ Δημη</supplied>

    <lb n="16" break="no"/><supplied reason="lost">τρίου τῶν π</supplied>ερὶ τὴν δι<unclear>ο</unclear>ίκησιν μαχαιρο<supplied reason="lost">φ</supplied>ό<supplied reason="lost">ρων καὶ ἐπίπλων γνώμης,</supplied>

