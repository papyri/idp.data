    <lb n="1"/>κα<unclear>τ</unclear><gap reason="lost" quantity="3" unit="character"/><gap reason="illegible" quantity="1" unit="character"/>αιου<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="3" unit="character"/><unclear>τ</unclear>ε γράψα<unclear>ς</unclear> μο<supplied reason="lost">ι</supplied>, τί <choice><reg>θέλεις</reg><orig>θελις</orig></choice> με ποιῆ

