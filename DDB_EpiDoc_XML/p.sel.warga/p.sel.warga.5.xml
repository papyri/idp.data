    <lb n="11"/><supplied reason="lost">μετώ</supplied><unclear>π</unclear>ωι μέσῳ <choice><reg>ἀπέχειν</reg><orig>ἀπέχιν</orig></choice> παρʼ αὐτοῦ 

    <lb n="12"/><supplied reason="lost">ἀργυρ</supplied>ίου δραχμὰς ἑκατὸν εἴκοσι <num value="120"/> 

    <lb n="13"/><supplied reason="lost">ἃς</supplied> ὤφειλεν ὁ Ἑρμᾶς τῶι Διονυ

    <lb n="14" break="no"/><supplied reason="lost">σάμ</supplied>μων<unclear>ι</unclear> καθʼ ὁμολογίαν <expan>τελειωθεῖ<ex>σαν</ex></expan> 

    <lb n="15"/><supplied reason="lost">διὰ</supplied> τοῦ ἐν Μαρ<unclear>έ</unclear>ψει τῆς αὐτῆς <expan>μερίδο<ex>ς</ex></expan> 

    <lb n="16"/><supplied reason="lost">γρα</supplied>φείου τῶι <expan>τεσσαρεσκαιδεκά<ex>τῳ</ex></expan> <expan><ex>ἔτει</ex></expan> 

    <lb n="17"/><supplied reason="lost">Ἁδριαν</supplied>οῦ Καίσαρος τοῦ κυρίου μηνὶ 

    <lb n="18"/><gap reason="lost" extent="unknown" unit="character"/> τρίτῃ καὶ εἰκάδι. ἣν καὶ ἀνα

    <lb n="19" break="no"/><supplied reason="lost">δέδ</supplied>ωκεν αὐτῷ εἰς ἀθέτησιν <unclear>κα</unclear>ὶ 

    <lb n="20"/><supplied reason="lost">ἀκύ</supplied><unclear>ρ</unclear><supplied reason="lost">ωσ</supplied>ιν μηδὲν τὸν <expan>Διονυ<unclear>σάμ</unclear><ex cert="low">μωνα</ex></expan>

    <lb n="21"/><supplied reason="lost">μηδὲ τοὺς</supplied> παρʼ αὐτοῦ <choice><reg>ἐνκαλεῖν</reg><orig>ἐνκαλῖν</orig></choice> μηδʼ ἐν

    <lb n="22" break="no"/><supplied reason="lost">καλέσειν</supplied> <unclear>τ</unclear>ῶι Ἑρμᾷ μηδὲ τοῖς παρὰ τοῦ 

    <lb n="23"/><supplied reason="lost">Ἑρμᾶ π</supplied>ερὶ ὧν ἀπέσχηκεν <unclear>κα</unclear>θότι 

    <lb n="24"/><supplied reason="lost">πρόκ</supplied>ειται μηδὲ <unclear>π</unclear><supplied reason="lost">ερὶ ἑ</supplied>τέρου ἁπλῶς <expan>πράγ<unclear>μ</unclear><ex>ατος</ex></expan> 

    <lb n="25"/><supplied reason="lost">ἐνγρ</supplied>άπτου ἀγρ<unclear>ά</unclear><supplied reason="lost">φο</supplied>υ μέχρι τῆς <expan>ἐνεστ<ex>ώσης</ex></expan> 

    <lb n="26"/><supplied reason="lost">ἡμ</supplied>έρας τρόπῳ μηδενί. τὰ <expan>δι<unclear>ο</unclear>μολ<ex>ογούμενα</ex></expan> <expan>κ<unclear>ύρι</unclear><ex>α</ex></expan> 

    <lb n="27"/><handShift new="m2"/> <supplied reason="lost">Διο</supplied>νυσάμμων Ἰσίωνος ἀπέχω 

    <lb n="28"/><supplied reason="lost">παρ</supplied><unclear>ὰ</unclear> τοῦ <choice><reg>Ἑρμᾶτος</reg><orig>Ἑρμᾶτας</orig></choice> το<unclear>ῦ</unclear> <unclear>Ἄ</unclear>ρ<supplied reason="omitted">ωνος</supplied> 

    <lb n="29"/><supplied reason="lost">ἀργυ</supplied>ρίου δ<unclear>ρ</unclear><supplied reason="lost">αχμὰς ἑκα</supplied>

    <lb n="30" break="no"/><supplied reason="lost">τὸ</supplied><unclear>ν</unclear> <choice><reg>ἔικοσι</reg><orig>ἴκοσ<unclear>ι</unclear></orig></choice> <supplied reason="lost">καὶ μηδὲν</supplied> 

    <lb n="31"/><supplied reason="lost">ἐνκ</supplied>αλῶ καθ<supplied reason="lost">ὼς πρόκιται.</supplied> </ab></div>
