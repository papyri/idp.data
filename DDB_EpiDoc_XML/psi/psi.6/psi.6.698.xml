    <lb n="9"/><gap reason="lost" extent="unknown" unit="character"/>κα<gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><unclear>οι</unclear> ἐπὶ τοῦ <app type="editorial"><lem resp="BL 8.400">συμποσίου</lem><rdg><choice><reg>γυμνασίου</reg><orig>γυμνοσίου</orig></choice></rdg></app> καὶ τὸν ἐντὸς <unclear>φα</unclear><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><unclear>ω</unclear>να καὶ τὸ <app type="editorial"><lem resp="BL 8.400">συμπόσιον</lem><rdg><choice><reg>γυμνάσιον</reg><orig>γυμνόσιον</orig></choice></rdg></app> <gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="5" unit="character"/><unclear>ατης</unclear> <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="10"/><gap reason="lost" quantity="22" unit="character"/><gap reason="illegible" quantity="1" unit="character"/> ἄχρι <unclear>παν</unclear>τὸ<unclear>ς</unclear> <gap reason="lost" quantity="6" unit="character"/><supplied reason="lost" cert="low">ὁλό</supplied>κληρον τὴν ἁψῖδα τὴν ἐν τῇ νοτί<supplied reason="lost">νῃ μερίδι </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="11"/><gap reason="lost" quantity="22" unit="character"/><gap reason="illegible" quantity="1" unit="character"/>αλλη<gap reason="illegible" quantity="4" unit="character"/><gap reason="lost" quantity="12" unit="character"/> τῆς ἁψῖδος τὴν <choice><reg>ἀπηλιωτικὴν</reg><orig>ἀπιλιωτικὴν</orig></choice> καμ<supplied reason="lost" cert="low">άραν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="12"/><gap reason="lost" quantity="22" unit="character"/>τος ἀπο<gap reason="lost" quantity="20" unit="character"/><gap reason="illegible" quantity="1" unit="character"/> βασιλείου ὕψος ἄχρι παντὸς μ<supplied reason="lost">έρος </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="13"/><gap reason="lost" quantity="50" unit="character"/><gap reason="illegible" quantity="2" unit="character"/><supplied reason="lost">τ</supplied>ης κληρώσεως ἐγὼ ὁ Ἰωάνν<supplied reason="lost">ης </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="14"/><gap reason="lost" quantity="50" unit="character"/> Θεώνιλλα εἰς τὸ καθʼ ἡμᾶς ἥμι<supplied reason="lost">συ μέρος </supplied><gap reason="lost" extent="unknown" unit="character"/> 

