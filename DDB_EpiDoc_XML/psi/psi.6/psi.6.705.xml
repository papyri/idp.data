         <head n="31098" xml:lang="en">
            <date>III spc</date>
            <placeName>Oxyrhynchus</placeName>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><ab>
    <lb n="1"/><num value="24">κδ</num> 

    <lb n="2"/><gap reason="lost" extent="unknown" unit="character"/> εὐθενιαρχήσας ἐξηγητεύσας τῆς λαμπροτάτης πόλ<supplied reason="lost">ε</supplied>ως <unclear>τῶν</unclear> <unclear>Ἀλ</unclear><supplied reason="lost">εξανδρέων </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="3"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> Ὀ</supplied>ξυρυγχιτῶν πόλεως Αὐρηλίῳ Ἀπολλωνίῳ τῷ καὶ Διονυσί<supplied reason="lost">ῳ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="4"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> τῶν Ἀλεξ</supplied>ανδρέων γυμνασιάρχῳ πρυτανεύσαντι βουλευτῇ τῆ<supplied reason="lost">ς </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="5"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> χαίρειν. ὁμολ</supplied>ογῶ πεπρακέναι σοι ἀ<supplied reason="lost">π</supplied>ὸ τοῦ νῦν εἰς τὸν ἅπαντα χρόνον τὸ ὑπάρ<supplied reason="lost">χον μοι </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="6"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ἀκολο</supplied>ύθως τοῖς εἰς ἐμ<unclear>ὲ</unclear> <supplied reason="lost">καταντ</supplied>ήμασι ἐ<unclear>ν</unclear> τῇ αὐτῇ <choice><reg>πόλει</reg><orig><supplied reason="lost">π</supplied>όλι</orig></choice> ἐπʼ ἀμφόδου <gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="7"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ἕκτο</supplied>ν μέρος ἱππῶνος ἤτοι καμηλῶνος καὶ τῶν ἐν αὐτῷ <gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="8"/><gap reason="lost" extent="unknown" unit="character"/> ἀνηκόντων καὶ συνκυρούντων πάντων κοινωνικῶ<supplied reason="lost">ν </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> πρὸς τὸν δεῖνα </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> γείτονες </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="9"/><gap reason="lost" extent="unknown" unit="character"/><gap reason="illegible" quantity="1" unit="character"/> Ἀπολλωνίας τῆς καὶ Ἡλιοδώρας, βορρᾶ καὶ λιβὸς <unclear>ἑ</unclear>τ<unclear>έ</unclear>ρων ὀνομ<supplied reason="lost">άτων </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="10"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> τιμῆς τῆς συμπεφωνημένης</supplied> πρὸς ἀλλήλους τοῦ ἕκτου μέ<supplied reason="lost">ρ</supplied>ους τοῦ ἱππῶνος <unclear>ἤτοι</unclear> κ<supplied reason="lost">αμηλῶνος </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="11"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ἀργυρίου σεβαστ</supplied><gap reason="lost" quantity="2" unit="character"/><supplied reason="lost"> νο</supplied>μίσματος δραχμῶν τρισχιλίων <num value="3000"/>, ἅσπερ ἀ<unclear>πέσχο</unclear>ν <gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="12"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> καὶ ἐπερωτηθεὶς</supplied> ὡμολόγησα. διὸ ἀπὸ τοῦ νῦν κ<supplied reason="lost">ρα</supplied><unclear>τεῖν</unclear> σε καὶ κυ<supplied reason="lost">ριεύειν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="13"/><gap reason="illegible" extent="unknown" unit="character"><desc>vestiges</desc></gap> </ab></div>
