         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r" type="textpart"><ab>

    <lb n="1"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> Πανακέ</supplied>στορι χαίρε<unclear>ι</unclear>ν. <unclear>Ἐ</unclear>τεάρχωι καὶ Σωστράτω<supplied reason="lost">ι </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="2"/><gap reason="lost" extent="unknown" unit="character"/>ω<unclear>ν</unclear> αὐτῶν προχρῆσαι <expan><ex>δραχμὰς</ex></expan> <num value="137">ρλζ</num> <num value="3"><expan><ex>τριώβολον</ex></expan></num>. τὸ μὲ<supplied reason="lost" cert="low">ν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="3"/><gap reason="lost" extent="unknown" unit="character"/>εσθαι δὲ τ<supplied reason="lost">ὰς</supplied> <expan><ex>δραχμὰς</ex></expan> <num value="37">λζ</num> <num value="3"><expan><ex>τριώβολον</ex></expan></num> καὶ ἕλκειν αὐτοὺς α<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="4"/><gap reason="lost" extent="unknown" unit="character"/>αι σύμβολα δὲ ἐγράφη ἐν Κροκοδίλων πόλε<supplied reason="lost">ι </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="5"/><app type="editorial"><lem resp="BL 3.224"><gap reason="lost" extent="unknown" unit="character"/>κέλητος</lem><rdg><gap reason="illegible" quantity="1" unit="character"/>ελητος</rdg></app> μετρῆσαι πυρῶν <expan><ex>ἀρτάβας</ex></expan> <num value="20">κ</num> <gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="6"/><gap reason="lost" extent="unknown" unit="character"/>ν ἀλλὰ ἀξιῶσαι τοὺς ναύτας δοθῆναι α<unclear>υ</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="7"/><gap reason="lost" extent="unknown" unit="character"/><gap reason="illegible" quantity="1" unit="character"/> τὰ δὲ σύμβολα ἡμᾶς λαβεῖν καὶ κομίσα<unclear>σ</unclear><supplied reason="lost" cert="low">θαι </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="8"/><gap reason="lost" extent="unknown" unit="character"/> συνεθήκαμεν αὐτὰ Μάρωνι ὥστε ἀποδ<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="9"/><gap reason="lost" extent="unknown" unit="character"/> ἀντιγράφει καί φησιν συσταθῆναι Δᾶμιν <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="10"/><gap reason="lost" extent="unknown" unit="character"/>των καὶ ἔτι καὶ νῦν οὐ κεκομίσθαι ἐπι<unclear>μ</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="11"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="low">ἀργ</supplied><unclear>ύ</unclear>ριον ληφθῆι παρʼ αὐτῶν ἢ τιμὴν τῆς <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="12"/><gap reason="lost" extent="unknown" unit="character"/><unclear>τ</unclear>ελοῦμεν ἡμῖν αὐτοῖς. <space extent="unknown" unit="character"/> καὶ το<gap reason="lost" extent="unknown" unit="character"/> 

