         <head n="2121" xml:lang="en">
            <date>258/257 BC</date>
            <placeName>Philadelphia</placeName>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r" type="textpart"><ab>

    <lb n="1"/>Ἀγρεοφῶν Ζήνωνι χαίρειν. ὑπογέγραφά <unclear>σ</unclear><supplied reason="lost" cert="low">οι τῆς παρὰ Ἐφαρμόστου πρὸς ἡμᾶς ἐπιστολῆς</supplied> 

    <lb n="2"/>τὸ ἀντίγραφο<supplied reason="lost">ν</supplied>. καλῶς οὖν ποιήσεις τάς τʼ ἐπιστ<supplied reason="lost">ολὰς </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="3"/>καὶ πυθόμενος δὲ τίνα ἐστὶν ἃ ἀντιγράφει Ἀπο<supplied reason="lost" cert="low">λλώνιος</supplied><gap reason="lost" quantity="4" unit="character"/>απο<gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ἀ</supplied>

    <lb n="4" break="no"/><unclear>πορεῖ</unclear> γὰ<supplied reason="lost">ρ </supplied><gap reason="lost" quantity="1" unit="character"/>μοι<unclear>α</unclear><gap reason="lost" quantity="12" unit="character"/><unclear>γ</unclear>ραφετ<gap reason="lost" quantity="1" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="3" unit="character"/><gap reason="lost" quantity="5" unit="character"/> τα<gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="5"/>παρεχομένου εἰς τὰ πράγματα· πρὸς γὰρ τοὺς πολλ<unclear>οὺ</unclear><supplied reason="lost">ς</supplied> <unclear>ὁ</unclear> φόβος χρησι<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="6"/>μάλιστα ἑαυτὸν ἐντείνειν εἴωθεν ὅταν <choice><reg>φιλανθρωπότατ<supplied reason="omitted">α</supplied></reg><orig>φιλανθρωποτατ</orig></choice> αὐτῶι <unclear>μο</unclear><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="7"/><add place="above">ἐὰν δή τις λό<supplied reason="lost">γο</supplied><unclear>ς</unclear> γίνηται, καλῶς ποιήσεις</add> <del rend="erasure">καλῶς ποιήσεις ἐάν τε ὁ καιρὸς γένηται μν<unclear>η</unclear>σθεὶς <unclear>αὐ</unclear>τῶι καὶ σοί, δέομαι δὲ</del> <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="8"/>αὐτοῦ καθὰ καὶ μέχρι τοῦ νῦν.  ἔρρωσο. <expan><ex>ἔτους</ex></expan> <num value="28"><unclear>κ</unclear>η</num>, Δύστρου <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="9"/>Ἐφάρμοστος Ἀγρεοφῶντι. λαβὼν τὰς ἐπιστολὰς ἃς κομίζει Πα<unclear>ι</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="10"/>οὔσας τὰς πάσας ὀκτὼ <num value="8"/> σὺν τῆι παρʼ Ἱππονίκου καὶ Ζωίλου τοῦ τραπ<supplied reason="lost">εζίτου </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="11"/>λόγους ἀπόδος διὰ Ζήνωνος Ἀπολλωνίωι τῶι διοικητῆι καὶ τουτ<gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> καὶ μὴ ἄλλως </supplied><choice><reg><supplied reason="lost">ποι</supplied><lb n="12" break="no"/>ήσῃς</reg><orig><supplied reason="lost">ποι</supplied><lb n="12" break="no"/>ησηις</orig></choice> <foreign xml:lang="la">?.</foreign> ἐπιμελὲς δέ σοι γενέσθω τοῦ Ζήνωνος πυθέσθαι τίνα ἐσ<supplied reason="lost" cert="low">τὶν ἃ ἀντιγράφει Ἀπολλώνιος</supplied> 

    <lb n="13"/>πρὸς τὰς παρʼ ἡμῶν ἐπιστολάς· πάλιν γὰρ παραπλήσια γέγραφεν Ἱ<unclear>π</unclear><supplied reason="lost" cert="low">πόνικος </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> πρό</supplied>

    <lb n="14" break="no"/>τερον ἐπέστειλεν Ἀθηνάδου παραγενομένου πρὸς ἡμᾶς. 

    <lb n="15"/>ἔρρωσο. <expan><ex>ἔτους</ex></expan> <num value="28">κη</num>, Μεχὶρ <num value="13">ιγ</num>. 
</ab></div><div n="v" type="textpart"><ab>

    <lb n="16"/>Ἀγρεοφῶν. ἀντίγραφον τῆς 

    <lb n="17"/>παρὰ Ἐφαρμόστου ἐπισ<supplied reason="lost">τολῆ</supplied>ς. 

    <lb n="18"/><expan><ex>ἔτους</ex></expan> <num value="28">κη</num>, Δύστρου <num value="16">ιϛ</num>, ἐν Μέν<supplied reason="lost">δ</supplied><unclear>η</unclear>τι. </ab></div></div>
