    <lb n="1"/><gap reason="lost" extent="unknown" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><unclear>τ</unclear>η<gap reason="lost" quantity="3" unit="character"/> <unclear>Φλα</unclear>βιανῷ τῷ λαμ<unclear>π</unclear><supplied reason="lost">ροτάτῳ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="2"/><gap reason="lost" extent="unknown" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="2" unit="character"/> <unclear>Σ</unclear>εύθους οὐετρανοῦ τῶν <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="3"/><supplied reason="lost">ἀ</supplied><unclear>π</unclear>λεονέκτητον <choice><reg>παρέχει</reg><orig>παρέχι</orig></choice> ἡ σὴ ἀρετή, ἡγεμὼν κύρι<supplied reason="lost">ε, </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="4"/><supplied reason="lost">ἐγ</supplied><unclear>ὼ</unclear> <unclear>τ</unclear>οίνυν συνήφθην ἀνδρὶ <expan>βουλ<ex cert="low">ευτῇ</ex></expan> Σαρπαστο<unclear>υ</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="5"/>ἐμῇ προικὶ ἐδάφη καὶ ψυκτῆρας λευκαν<unclear>ι</unclear>ων. <choice><reg cert="low">ἐπεὶ</reg><orig>ἐπὶ</orig></choice> τοίν<unclear>υ</unclear><supplied reason="lost">ν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="6"/>βουλευτικῆς καταλαβούσης τὸν αὐτόν μου σύνβιον ενχ<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="7"/>διε<gap reason="illegible" quantity="2" unit="character"/>ασατο τὰ αὐτοῦ <hi rend="diaeresis">ὑ</hi>παλλάγματα κατʼ <g type="apostrophe"/> ἐμὴν ἄγνοιαν ὧν ενς<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="8"/>καὶ ἔστιν πολλὰ ἃ <choice><reg>ὀφείλει</reg><orig>ὀφίλι</orig></choice> διὰ τὰ <choice><reg>λειτουργήματα</reg><orig>λιτουργήματα</orig></choice>. <choice><reg>καὶ ἐγὼ</reg><orig>κἀγὼ</orig></choice> περιπν<unclear>ε</unclear><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ἀξι</supplied>

    <lb n="9" break="no"/><unclear>ῶ</unclear> καὶ δέομαι τῆς σῆς δικαιοκρισίας ὡς εἰδὼς τὸ <choice><reg>γυναικεῖον</reg><orig>γυνεον</orig></choice> γένος <gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ἀγο</supplied>

    <lb n="10" break="no"/><unclear>ραστὴν</unclear> προνοίᾳ Μακροβίου τοῦ <choice><reg>προπολιτευομένου</reg><orig>προπολιτευομαίνου</orig></choice> τὴν του<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="11"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="2" unit="character"/> καὶ τοὺς φόρους καὶ τὰ ἐνοίκια ἐμοῦ παρεχούση<unclear>ς</unclear> <unclear>α</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="12"/>τοὺς ν<supplied reason="lost">όμ</supplied>ους, <subst><add place="inline">ἵνα</add><del rend="corrected"><hi rend="diaeresis">ι</hi>νας</del></subst> <choice><reg>καὶ ἐγὼ</reg><orig>κἀγὼ</orig></choice> δυνηθῶ πληρῶσαι τοὺς λοιποὺς <unclear>δα</unclear>ν<supplied reason="lost">ειστάς </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="13"/><gap reason="illegible" quantity="2" unit="character"/><unclear>τ</unclear>ω<unclear>ν</unclear> <choice><reg>παρέχειν</reg><orig>παρέχιν</orig></choice> αὐτὸν τὴν προῖκαν ἣν <choice><reg>κατανήλωσεν</reg><orig>κατηνήλωσεν</orig></choice> ὁ αὐ<supplied reason="lost" cert="low">τός μου σύμβιος </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="14"/>τῶν <choice><reg>λειτουργημάτων</reg><orig>λιτουργημάτων</orig></choice> ἅτινα ἐγὼ <subst><add place="inline"><unclear>κ</unclear>ρατοῦμαι</add><del rend="corrected"><unclear>κυ</unclear>ρατουμαι</del></subst> περὶ τούτων ω<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="15"/>εὐπορίαν, καὶ <choice><reg>ἐμὲ</reg><orig>καμαι</orig></choice> δὲ τὸ ἀ<unclear>ν</unclear><supplied reason="lost">ε</supplied><unclear>πηρέα</unclear>στον <choice><reg>ἔχειν</reg><orig>ἔχιν</orig></choice> <hi rend="diaeresis">ὑ</hi>πὸ τῶν <choice><reg>δανειστῶν</reg><orig>δανιστῶν</orig></choice> <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="16"/>φρι<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/><unclear>ο</unclear>τηλα<unclear>β</unclear><gap reason="lost" extent="unknown" unit="character"/> 

