    <lb n="5" type="inWord"/>μίσματος τάλαντα 

    <lb n="6"/>δύο <num value="2"/> <expan>δρα<ex>χμὰς</ex></expan> ὀκτακοσίας <num value="800"/> 

    <lb n="7"/>ἐκ τοῦ δανείου τῶν 

