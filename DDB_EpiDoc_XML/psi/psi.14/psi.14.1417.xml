    <lb n="10" break="no"/>νημένης πρὸς ἀλλήλους ἀργυ<supplied reason="lost">ρ</supplied>ί<supplied reason="lost">ου σε</supplied>βα

    <lb n="11" break="no"/>στῶν νομίσματος τ<supplied reason="lost">άλα</supplied><unclear>ν</unclear><supplied reason="lost">τ</supplied>ον <supplied reason="lost">ἓ</supplied><unclear>ν</unclear> <expan><unclear>γ</unclear><supplied reason="lost">ί<ex>νεται</ex></supplied></expan> <expan><ex cert="low">τάλαντον</ex></expan> <num value="1"><unclear>α</unclear></num> 

    <lb n="12"/>ὅπερ αὐτόθι ἀπέσχον <unclear>π</unclear><supplied reason="lost">α</supplied>ρὰ σοῦ <supplied reason="lost">ἐκ</supplied> <unclear>π</unclear>λή

    <lb n="13" break="no"/>ρους διὰ <choice><reg>χειρός</reg><orig>χιρός</orig></choice>, τὴ<supplied reason="lost">ν δʼ α</supplied><unclear>ὐτὴ</unclear><supplied reason="lost">ν</supplied> <unclear>ὄνον</unclear> ἐν

    <lb n="14" break="no"/>τεῦθεν <choice><reg>παρείληφ<supplied reason="lost">ας</supplied></reg><orig>παρίληφ<supplied reason="lost">ας</supplied></orig></choice> <supplied reason="lost">παρʼ</supplied> ἐμοῦ τ<unclear>αύ</unclear>τ<unclear>η</unclear>ν 

    <lb n="15"/>τοιαύτην ἀνα<supplied reason="lost">π</supplied>ό<supplied reason="lost">ρριφον, ἣν καὶ</supplied> <unclear>βεβ</unclear><supplied reason="lost">αιώ</supplied>

    <lb n="16" break="no"/>σω σοι πάσῃ <choice><reg>βεβ<supplied reason="lost">αι</supplied>ώσει</reg><orig>βεβ<supplied reason="lost">αι</supplied>ώσι</orig></choice> καὶ τ<supplied reason="lost">ὸν ἐπελευ</supplied>

    <lb n="17" break="no"/>σόμενόν σο<supplied reason="lost">ι</supplied> <unclear>τα</unclear><supplied reason="lost">ύ</supplied><unclear>τη</unclear><supplied reason="lost">ς χά</supplied><unclear>ρ</unclear><supplied reason="lost">ιν ἀποστήσω</supplied> 

