         <head n="20136" xml:lang="en">
            <date>AD 609</date>
            <placeName>Oxyrhynchus</placeName>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r" type="textpart"><ab>

    <lb n="1"/><app type="editorial"><lem resp="BL 10.235"><supplied reason="lost"><g type="stauros"/></supplied> <supplied reason="lost">ἐ</supplied>ν ὀνόμα<unclear>τι</unclear> <unclear>τῆς</unclear> ἁ<unclear>γίας</unclear> <unclear>ἀ</unclear>χράντου</lem><rdg><supplied reason="lost">ἐ</supplied>ν <supplied reason="lost">ὀ</supplied>νόμ<unclear>α</unclear><supplied reason="lost">τι τῆς ἁγίας καὶ ζ</supplied><unclear>ωοποι</unclear>οῦ</rdg></app> καὶ 

    <lb n="2"/><supplied reason="lost">ὁμο</supplied>ουσίου τριά<unclear>δ</unclear>ος <expan><supplied reason="lost">π</supplied>ατρ<ex>ὸς</ex></expan> καὶ υἱοῦ καὶ <expan>ἁγί<ex>ου</ex></expan> <expan>πνεύμα<ex>τος</ex></expan>, 

    <lb n="3"/>β<supplied reason="lost">ασι</supplied>λείας τοῦ θειο<unclear>τά</unclear>του καὶ <expan>εὐσεβ<ex>εστάτου</ex></expan> ἡμῶν 

    <lb n="4"/><expan>δεσπ<ex>ότου</ex></expan> ἔτους <num value="7">ζ</num> Παχὼν <num value="13">ιγ</num> <expan><hi rend="diaeresis">ἰ</hi>νδ<ex>ικτίονος</ex></expan> <num value="12">ιβ</num>. 

    <lb n="5"/><expan>Φλ<ex>αουίῳ</ex></expan> Ἀπίωνι τῷ πανευφήμῳ καὶ <expan><hi rend="diaeresis">ὑ</hi>περφ<ex>υεστάτῳ</ex></expan> 

    <lb n="6"/>ἀπὸ <choice><reg>ὑπάτων</reg><orig>ὑπατο</orig></choice> καὶ πατ<unclear>ρ</unclear>ικίῳ γεουχοῦντι καὶ 


    <lb n="7"/>ἐνταῦθα τῇ λαμπρᾷ <expan><supplied reason="lost">Ὀξυρ</supplied>υγχ<ex>ιτῶν</ex></expan> πόλει, διὰ 

    <lb n="8"/>Μηνᾶ οἰκέτου τοῦ ἐπερωτῶν<supplied reason="lost">τος κ</supplied>αὶ προσ

    <lb n="9" break="no"/>πορίζοντος τῷ ἰδίῳ <expan>δ<supplied reason="lost">εσπ<ex>ότῃ</ex></supplied></expan><supplied reason="lost"> τῷ</supplied> αὐτῷ <expan>πανευφ<ex>ήμῳ</ex></expan> 

    <lb n="10"/>ἀνδρὶ τὴν ἀγωγὴν καὶ ἐν<supplied reason="lost">οχή</supplied>ν, ἐγὼ Ἰερημίας 

    <lb n="11"/><supplied reason="lost">υ</supplied><hi rend="diaeresis">ἱ</hi>ὸς Ἰωσὴφ <expan>μητρ<ex>ὸς</ex></expan> <app type="editorial"><lem resp="BL 11.243">Θαή<supplied reason="lost">σί</supplied>ας</lem><rdg>Θαή<supplied reason="lost" cert="low">σι</supplied>ος</rdg></app> ἀπὸ <expan>ἐποικ<ex>ίου</ex></expan> 

    <lb n="12"/>Παγγουλεείου τοῦ <expan>Ὀξυρ<supplied reason="lost">υγχ<ex>ίτου</ex></supplied></expan><supplied reason="lost"> νομ</supplied>οῦ <expan>διαφέρ<ex>οντος</ex></expan> 

    <lb n="13"/>τῇ <expan><hi rend="diaeresis">ὑ</hi>μετέρ<ex>ᾳ</ex></expan> <expan><hi rend="diaeresis">ὑ</hi>περφ<ex>υείᾳ</ex></expan>, <expan>ἐν<supplied reason="lost">απόγρ</supplied>αφ<ex>ος</ex></expan> αὐτῆς 

    <lb n="14"/>γεωργός, ὁμολογῶ ἑκ<supplied reason="lost">ουσίᾳ γνώ</supplied>μῃ καὶ 

    <lb n="15"/>αὐθαιρέτῳ προαιρέσει, <choice><reg>ἐπομνύμενος</reg><orig>ἐπωμνύμενος</orig></choice> τὸ<supplied reason="lost">ν</supplied> <choice><reg>θεῖον</reg><orig>θῖον</orig></choice> 

    <lb n="16"/>καὶ σεβάσμιον ὅρκον, <supplied reason="lost">ἐγγυᾶ</supplied>σθαι <supplied reason="lost">κα</supplied>ὶ 

    <lb n="17"/>ἀναδέχεσθαι πα<unclear>ρ</unclear><supplied reason="lost">ὰ </supplied><app type="editorial"><lem resp="BL 8.391"><supplied reason="lost">τῇ ὑμετ</supplied><unclear>έ</unclear>ρᾳ <expan>ὑ<unclear>π</unclear>ερ<ex>φυείᾳ</ex></expan></lem><rdg><supplied reason="lost">τῆς ὑμετ</supplied>έρας <expan><supplied reason="lost">ὑ</supplied>περφ<ex>υείας</ex></expan></rdg></app> 

    <lb n="18"/>διὰ τῶν αὐτῇ προση<unclear>κ</unclear><supplied reason="lost">όντων Α</supplied>ὐρήλ<unclear>ιον</unclear> 

    <lb n="19"/>Παμοῦν υ<hi rend="diaeresis">ἱ</hi>ὸν Ἐνὼχ μη<supplied reason="lost">τρὸς</supplied> <gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="4" unit="character"/> 

    <lb n="20"/><expan>ὁρμώμε<ex>νον</ex></expan> καὶ αὐτὸν ἀπὸ <supplied reason="lost">τοῦ αὐτοῦ</supplied> ἐποικίου, 

    <lb n="21"/>ἐναπόγραφον αὐτῆς γε<supplied reason="lost">ωργόν, ἐ</supplied>φʼ ᾧ τε αὐτὸν 

    <lb n="22"/>ἀδιαλείπτως παραμεῖ<supplied reason="lost">ναι καὶ</supplied> διάγειν 

    <lb n="23"/>ἐν τῷ αὐτῷ <expan>κτήμα<ex>τι</ex></expan> κα<supplied reason="lost">ὶ μηδαμῶ</supplied>ς αὐτὸν 

    <lb n="24"/><choice><reg>ἀπολιμπά<supplied reason="lost">ν</supplied>εσθαι</reg><orig>ἀπολειμπά<supplied reason="lost">ν</supplied>εσθαι</orig></choice> <app type="editorial"><lem resp="N. Gonis, Tyche 30 (2015) 226 (KorrTyche 798)"><supplied reason="lost">μήτε μ</supplied><unclear>ὴ</unclear>ν</lem><rdg><supplied reason="lost" cert="low">τὸ αὐτὸ μή</supplied>τε</rdg></app> <choice><reg><expan>μεθίστασ<ex>θαι</ex></expan></reg><orig><expan>μεθ<unclear>εί</unclear>στασ<ex>θαι</ex></expan></orig></choice> 

    <lb n="25"/>εἰς ἕτερο<unclear>ν</unclear> <choice><reg><supplied reason="omitted">τό</supplied>πον</reg><orig>πο<unclear>ν</unclear></orig></choice>, ἀλλὰ καὶ <expan><supplied reason="lost">ἐπιζητού</supplied>μ<supplied reason="lost">ε</supplied><ex>νον</ex></expan> αὐτὸν 

    <lb n="26"/>πρὸς ἐμὲ ἐν οἱᾳδήπο<supplied reason="lost">τε ἡ</supplied>μέρᾳ οἱασ

    <lb n="27" break="no"/>δηποτοῦν ἕνεκεν προ<supplied reason="lost">φάσεως</supplied> <supplied reason="omitted">τοῦ</supplied>τον 

    <lb n="28"/><expan>παραφέρ<ex>ω</ex></expan> καὶ παραδώ<unclear>σ</unclear><supplied reason="lost">ω δίχ</supplied>α παντὸς 

    <lb n="29"/>τόπου προσφυγῆς ἔν<supplied reason="lost">θα αὐ</supplied>τὸν καὶ <expan>παρείληφ<ex>α</ex></expan>, 

    <lb n="30"/>ἐν τῇ φυλακῇ τοῦ ἐ<supplied reason="lost">νδόξου ὑ</supplied>μῶν οἴκου. 

    <lb n="31"/>εἰ δὲ μὴ τοῦτο ποιήσω, <supplied reason="lost">ὁμολ</supplied>ογῶ <hi rend="diaeresis">ὑ</hi><supplied reason="lost">π</supplied>εύθυνος 

    <lb n="32"/>εἶναι πᾶσιν τοῖς πρὸ<supplied reason="lost">ς αὐτὸν</supplied> <expan>ἐπιζητουμέ<ex>νοις</ex></expan> 

    <lb n="33"/>ἀποκρίνασθαι. <expan>κυρ<ex>ία</ex></expan> ἡ <supplied reason="lost">ἐγγύ</supplied>η <expan>ἁπλ<ex>ῆ</ex></expan> <expan>γραφ<ex>εῖσα</ex></expan> 

    <lb n="34"/>καὶ <expan>ἐπερ<ex>ωτηθεὶς</ex></expan> <expan>ὡμολ<ex>όγησα</ex></expan>. <handShift new="m2"/> Ἰ<supplied reason="lost">ερημ</supplied>ίας υἱὸς 

    <lb n="35"/>Ἰωσὴφ στοιχεῖ μοι ὡς <expan>πρόκ<ex>ειται</ex></expan>. 

    <lb n="36"/>Παπνούθιος <expan>ἔγρ<ex>αψα</ex></expan> <expan><ex>ὑπὲρ</ex></expan> 

    <lb n="37"/><expan>ἀγρ<ex>αμμάτου</ex></expan> ὄντος. 

    <lb n="38"/><app type="editorial"><lem resp="BL 8.391"><g type="chirho"/> <foreign xml:lang="la">di e<unclear>mu</unclear> <expan>Pap<ex>nutiu</ex></expan> <expan><unclear>eteli</unclear><ex>o</ex></expan><unclear>t</unclear>h<hi rend="supraline">h</hi></foreign></lem><rdg><foreign xml:lang="la">di e<unclear>mu</unclear></foreign><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="3" unit="character"/></rdg></app>  
</ab></div><div n="v" type="textpart"><ab>

    <lb n="1"/><g type="stauros"/> <expan>ἐγγ<ex>ύη</ex></expan> Ἰερημίου υἱοῦ Ἰωσὴφ <expan>ἀναδεχομέ<ex>νου</ex></expan> Παμοῦν υἱὸν Ἐνὼχ 

    <lb n="2"/>ἀπὸ <expan>ἐποικ<ex>ίου</ex></expan> Παγγουλεείου </ab></div></div>
      </body>
