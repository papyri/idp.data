         <head n="37096" xml:lang="en">
            <date>VI spc</date>
            <placeName>Oxyrhynchus</placeName>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><ab>
    <lb n="1"/><g type="stauros"/> ἐν ὀνόματι τοῦ κυρίου 

    <lb n="2"/>καὶ <expan>δεσπό<ex>του</ex></expan> Ἰησοῦ Χριστοῦ 

    <lb n="3"/>τοῦ <expan>θε<ex>ο</ex>ῦ</expan> καὶ σωτῆρος 

    <lb n="4"/>ἡμῶν, μηνὶ Φαῶφι <num value="29">κθ</num> 

    <lb n="5"/><expan><hi rend="diaeresis">ἰ</hi>νδ<ex>ικτίονος</ex></expan> τρεισκαιδεκάτης <g type="stauros"/> 

    <lb n="6"/>ἐγὼ <hi rend="diaeresis">Ἰ</hi>οῦστος οἰνοπράτης 

    <lb n="7"/>υ<hi rend="diaeresis">ἱ</hi>ὸς Σερήνου μητρὸς 

    <lb n="8"/>Φιλοξένας ἀπὸ τῆς 

    <lb n="9"/><expan>Ὀξυρυγχ<ex>ιτῶν</ex></expan> πόλεως <hi rend="diaeresis">ὑ</hi>μῖν 

    <lb n="10"/>Ἀπίου υ<hi rend="diaeresis">ἱ</hi>ῷ Φοιβάμμωνος 

    <lb n="11"/>καὶ Ἀ<unclear>βρα</unclear>αμίῳ υ<hi rend="diaeresis">ἱ</hi>ῷ 

    <lb n="12"/><unclear>Ἀ</unclear>λλ<unclear>ᾶ</unclear> ὁρμωμένοις 

    <lb n="13"/>ἀπὸ <expan>κώμ<ex>ης</ex></expan> Μικτόλεως 

    <lb n="14"/>τοῦ Ἀρσινοείτου νομοῦ, 

    <lb n="15"/>ὁμολ<supplied reason="lost">ογ</supplied><unclear>ῶ</unclear> ἐσχ<unclear>ηκέν</unclear><supplied reason="lost">αι</supplied> 

    <lb n="16"/><unclear>παρ</unclear>ʼ <hi rend="diaeresis">ὑ</hi>μῶν ἐντεῦθεν 

    <lb n="17"/>ἤδη ἐν χρήσει ἐις 

    <lb n="18"/><hi rend="diaeresis">ἰ</hi>δίας μου καὶ ἀναγκαίας 

    <lb n="19"/>χρείας χρυσοῦ νόμισμα 

    <lb n="20"/>ἓν εἰς κεράτια δεκα

    <lb n="21" break="no"/>επτὰ <hi rend="diaeresis">ἰ</hi>διωτικῷ ζυγῷ, 

    <lb n="22"/><expan>γί<ex>νεται</ex></expan> <expan>χρ<ex>υσοῦ</ex></expan> <expan>νό<ex>μισμα</ex></expan> <num value="1">α</num> <expan>κερ<ex>άτια</ex></expan> <num value="17">ιζ</num> <expan>ἰδ<ex>ιωτικῷ</ex></expan> 

    <lb n="23"/><expan>ζυγ<ex>ῷ</ex></expan>, καὶ τοῦτο <expan>ὁμολ<ex>ογῶ</ex></expan> 

    <lb n="24"/>παρασχεῖν <hi rend="diaeresis">ὑ</hi>μῖν ἐν τῇ 

    <lb n="25"/>ὀγδ<supplied reason="lost">ό</supplied>ῃ ἡμέρᾳ τῆς 

    <lb n="26"/>ἑορτῆς τοῦ ἀρχαγ<g type="apostrophe"/>γέλου 

    <lb n="27"/><unclear>Μ</unclear><supplied reason="lost">ιχ</supplied>αὴλ τοῦ Ἁθὺρ 

    <lb n="28"/><supplied reason="lost">μην</supplied>ὸς τῆς παρούσης 

    <lb n="29"/><supplied reason="lost">τρεισ</supplied>καιδεκάτης 

    <lb n="30"/><supplied reason="lost"><expan>ἰνδ<ex>ικτίονος</ex></expan></supplied> <surplus>καὶ <expan>ἐπερ<ex>ωτηθείς</ex></expan></surplus>. <expan>κύρ<ex>ιον</ex></expan> τὸ 

    <lb n="31"/><expan><supplied reason="lost">γρα</supplied>μμ<ex>άτιον</ex></expan> <expan>ἁπλ<ex>οῦν</ex></expan> <expan>γραφ<ex>ὲν</ex></expan> καὶ 

    <lb n="32"/><expan><supplied reason="lost">ἐπε</supplied>ρ<ex>ωτηθεὶς</ex></expan> <expan>ὡμολόγ<ex>ησα</ex></expan> <g type="stauros"/> 

    <lb n="33"/><g type="stauros"/> <hi rend="diaeresis">Ἰ</hi>οῦστος υ<hi rend="diaeresis">ἱ</hi>ὸς Σερήνου 

    <lb n="34"/>στοιχεῖ μοι τοῦτο τὸ 

    <lb n="35"/><expan><unclear>γραμμ</unclear><ex>άτιον</ex></expan> ὡς <expan>πρόκ<ex>ειται</ex></expan>. 

    <lb n="36"/><unclear>Ἰοῦ</unclear>στος ἔγραψα <expan><ex>ὑπὲρ</ex></expan> αὐτοῦ 

    <lb n="37"/><expan><unclear>ἀγρα</unclear>μμ<ex>άτου</ex></expan> ὄντος <g type="stauros"/> 

    <lb n="38"/><g type="xs"/> <foreign xml:lang="la">di emu Ius<unclear>tu</unclear></foreign> <app type="editorial"><lem resp="BL 8.391"><foreign xml:lang="la"><expan>e<unclear>t</unclear><ex>elio</ex></expan><unclear>t</unclear>h<hi rend="supraline">h</hi></foreign></lem><rdg><abbr>ἐτελ</abbr></rdg></app> </ab></div>
