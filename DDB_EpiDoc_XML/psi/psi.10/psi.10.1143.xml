    <lb n="2"/>Ψῦφιν καὶ Κρονίωνα <expan><ex>ἀρουρῶν</ex></expan> <num value="3">γ</num> <num value="1/2">𐅵</num> <num value="1/4">δ</num></lem><rdg/></app>
</ab></div></div>
