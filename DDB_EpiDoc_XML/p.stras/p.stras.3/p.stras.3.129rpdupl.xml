    <lb n="8" type="inWord"/><unclear>μ</unclear><supplied reason="lost" cert="low">έ</supplied>νῳ πάγῳ χοιρεμπόρους καὶ ὀρνιθο<supplied reason="lost">τ</supplied><unclear>ρόπον</unclear> <app type="editorial"><lem><unclear>κατὰ</unclear></lem><rdg>ορνιθ<unclear>ο</unclear><supplied reason="lost">τ</supplied><unclear>ροπ</unclear><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="2" unit="character"/></rdg></app> 

