         <head n="18758" xml:lang="en">
            <date>ca. AD 18</date>
            <placeName>Apollonopolite Magna</placeName>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><ab>
    <lb n="1"/>ἱέρακι Πανίσκου <choice><reg>βασιλικῶ<supplied reason="lost">ι</supplied></reg><orig>βασιλεικῶ<supplied reason="lost">ι</supplied></orig></choice> <supplied reason="lost">γραμματεῖ</supplied> 

    <lb n="2"/><choice><reg>Ἀπολλωνοπολίτου</reg><orig>Ἀπαλλωνοπολίτου</orig></choice> καὶ Εἰλιθ<supplied reason="lost">υιοπολίτου</supplied> 

    <lb n="3"/><del rend="erasure">παρὰ Ὥρου Βήκιος Ἀπολλων<supplied reason="lost">οπολίτου</supplied><gap reason="illegible" quantity="1" unit="character"/></del> 

    <lb n="4"/>ἐν τῷ <num value="4">δ</num> <expan><ex>ἔτει</ex></expan> Τιβερίου Καίσαρος Σ<supplied reason="lost">εβαστοῦ </supplied><gap reason="lost" quantity="3" unit="character"/> 

    <lb n="5"/><del rend="erasure">Π<supplied reason="lost">α</supplied>ῦνι <gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="1" unit="character"/></del>νησον κα<gap reason="lost" quantity="8" unit="character"/> 

    <lb n="6"/>καλουμένη πρὸς γενημα<gap reason="lost" quantity="10" unit="character"/> 

    <lb n="7"/>ὑπόστασιν φόρου παρακελε<supplied reason="lost" cert="low">υομένου </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="8"/>τὸν ἐμὸν υἱὸν ἐπὶ μισθῶι ἐ<supplied reason="lost" cert="low">κομίσατο </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="9"/>ἐφʼ ᾧ χορηγεῖν αὐτῶι μισθ<supplied reason="lost">οῦ τρίτον</supplied> 

    <lb n="10"/>μέρος καὶ ἔχειν <choice><reg>μετοχικ<supplied reason="lost">ὸν</supplied></reg><orig>μετοχεικ<supplied reason="lost">ὸν</supplied></orig></choice> <gap reason="lost" quantity="8" unit="character"/> 

    <lb n="11"/>εἰς συνπλήρωσιν διμοίρου <gap reason="lost" quantity="9" unit="character"/> 

    <lb n="12"/>ηστήσας πρὸς αὐτὸν δια<gap reason="lost" quantity="9" unit="character"/> 

    <lb n="13"/>τρίτων παρηρίθμησεν αὐτ<supplied reason="lost">ῶι τοὺς κα</supplied>

    <lb n="14" break="no"/>θήκοντας μισθοὺς τὰ συν<gap reason="lost" quantity="9" unit="character"/> 

    <lb n="15"/><gap reason="lost" quantity="8" unit="character"/>ε καιροῦ π<gap reason="illegible" quantity="3" unit="character"/><gap reason="lost" quantity="9" unit="character"/> 

    <lb n="16"/><gap reason="lost" extent="unknown" unit="line"/> </ab></div>
