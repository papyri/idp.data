         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="a" type="textpart"><ab>

    <lb n="1"/><gap reason="lost" extent="unknown" unit="character"/> Πυθονίκωι χαί<supplied reason="lost">ρ</supplied>ειν.  τρίτην η<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="2"/><gap reason="lost" extent="unknown" unit="character"/><gap reason="illegible" quantity="2" unit="character"/>τε αὐτῶι τε καὶ <gap reason="illegible" quantity="2" unit="character"/>πιν καὶ Ζηνό<supplied reason="lost">βιον </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="3"/><gap reason="lost" extent="unknown" unit="character"/>του τὸν <expan>προσγρ<ex>α</ex>φόμενον</expan> προστιθεν<supplied reason="lost">τ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="4"/><supplied reason="lost">καὶ ὅπως μὴ </supplied> ἄλλως ἔσται ἀλλὰ καθότι ἐπέσταλκ<supplied reason="lost">α </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="5"/><gap reason="lost" extent="unknown" unit="character"/><gap reason="illegible" quantity="4" unit="character"/>τε δὲ <abbr>γρ</abbr> <add place="above">σοι</add> τ<gap reason="lost" quantity="2" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="5" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="2" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="6"/><gap reason="lost" extent="unknown" unit="character"/><gap reason="illegible" quantity="3" unit="character"/>ειστρει <space extent="unknown" unit="character"/> 
</ab></div><div n="b" type="textpart"><ab>

    <lb n="7"/>Ἡλιόδωρ<supplied reason="lost">ο</supplied>ς Πυθονίκ<supplied reason="lost">ωι χαίρειν.</supplied> 

    <lb n="8"/>κ<supplied reason="lost">α</supplied>ὶ αὐτὸ<supplied reason="lost">ς</supplied> δὲ ὑγίαι<supplied reason="lost">νον </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="9"/><abbr>π<gap reason="lost" quantity="9" unit="character"/>ο</abbr> ὅπω<supplied reason="lost">ς</supplied> ὁ <unclear>ο</unclear><gap reason="lost" quantity="2" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="10"/>δεμ<gap reason="lost" quantity="6" unit="character"/> σοι καὶ ἔμπροσ<supplied reason="lost">θεν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="11"/>τῶ<supplied reason="lost">ν κατοίκ</supplied>ων ἱππέων τω<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="12"/>ἀνα<gap reason="lost" quantity="6" unit="character"/>σι προπέμποντ<supplied reason="lost">ες </supplied><gap reason="lost" extent="unknown" unit="character"/> 

