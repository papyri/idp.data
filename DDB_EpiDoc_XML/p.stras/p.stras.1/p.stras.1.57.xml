         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r" type="textpart"><ab>

    <lb n="1"/>Αἰλίωι <choice><reg>Μαμερτίνωι</reg><orig>Μαμερτείνωι</orig></choice> τῷ κρατίστωι <app type="editorial"><lem resp="BL 8.414"><supplied reason="lost">διο</supplied><unclear>ι</unclear><supplied reason="lost">κ</supplied><unclear>ητῆ</unclear>ι</lem><rdg><gap reason="lost" extent="unknown" unit="character"/></rdg></app> 

    <lb n="2"/>παρὰ Ἀπωνίου Ἡρακλᾶ τοῦ <app type="editorial"><lem resp="BL 6.190">Μυσθαρίωνος</lem><rdg><expan>Μύσθο<ex>υ</ex></expan> Δίωνος</rdg></app> ἀπὸ <unclear>τ</unclear><supplied reason="lost">οῦ Ἀ</supplied><unclear>ρ</unclear>σι<unclear>ν</unclear>οείτου <unclear>νο</unclear>μ<unclear>ο</unclear><supplied reason="lost">ῦ</supplied>. 

    <lb n="3"/><unclear>δ</unclear>ημόσιος γεωργὸς τυνχάνων, κύριε, <unclear>ἐ</unclear><supplied reason="lost">δόθην</supplied> <unclear>εἰ</unclear><supplied reason="lost">ς</supplied> <unclear>κω</unclear>μο<unclear>γ</unclear>ρ<supplied reason="lost">αμ</supplied>

    <lb n="4" break="no"/><unclear>μ</unclear>ατείαν κώμης Εὐημερίας γεουχῶν <unclear>ἐν</unclear> <unclear>κ</unclear><supplied reason="lost">ώμῃ</supplied> <unclear>Θεα</unclear><supplied reason="lost">δε</supplied><unclear>λ</unclear>φεί<supplied reason="lost">ᾳ</supplied> 

    <lb n="5"/><choice><reg>ἔγγιστα</reg><orig>ἔνγιστα</orig></choice> οὔσης τῆς προκειμέ<unclear>ν</unclear>η<unclear>ς</unclear> <supplied reason="lost">κώμης</supplied> <unclear>Εὐ</unclear>ημε<supplied reason="lost">ρ</supplied>ία<unclear>ς</unclear> 

    <lb n="6"/>μηδὲ μείλιον ἀπεχουσῶν ἀλλ<unclear>ήλ</unclear><supplied reason="lost">ων. διὸ π</supplied><unclear>ρ</unclear><supplied reason="lost">ό</supplied><unclear>σει</unclear>μι σοὶ τ<supplied reason="lost">ῷ</supplied> 

    <lb n="7"/>κυρίῳ ἀπαλλάξαι με τῆς κωμογρ<unclear>α</unclear><supplied reason="lost">μματείας, τυγχάνω γὰρ ἐν τῇ</supplied> 

    <lb n="8"/>προγεγραμμένῃ Θεαδελφείᾳ δημόσιος γεωρ<supplied reason="lost">γ</supplied>ός. εἰμ<supplied reason="lost">ὶ</supplied> <unclear>δὲ</unclear> <unclear>σ</unclear><supplied reason="lost">ι</supplied>

    <lb n="9" break="no"/><supplied reason="lost">τ</supplied>ολόγος τῆς αὐτῆς κώμης καὶ παρ<supplied reason="lost">έ</supplied><unclear>λαβ</unclear><supplied reason="lost">ον </supplied><gap reason="lost" quantity="3" unit="character"/><unclear>ρ</unclear>ον τῆς 

    <lb n="10"/>προγεγραμμένης κώμης Θεαδελφε<unclear>ί</unclear>ας <gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" quantity="2" unit="character"/><unclear>η</unclear> <unclear>α</unclear><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="4" unit="character"/> 

    <lb n="11"/>οὐκ ἐξ<unclear>αρ</unclear>κῶ δὲ πρὸς τὰς δύο <choice><reg>λειτουργίας</reg><orig>λιτουργίας</orig></choice>. ἀξ<unclear>ι</unclear>ῶ <app type="editorial"><lem resp="BL 1.406"><supplied reason="lost">γράψ</supplied><unclear>αι</unclear> <unclear>τ</unclear><supplied reason="lost">ῷ τῶν</supplied></lem><rdg><gap reason="lost" quantity="4" unit="character"/><gap reason="illegible" quantity="3" unit="character"/><gap reason="lost" quantity="4" unit="character"/></rdg></app> 

    <lb n="12"/><unclear>Θ</unclear>εμ<unclear>ίστ</unclear>ου καὶ Πολέμωνος μερίδων <app type="editorial"><lem resp="BL 1.406">το<supplied reason="lost">ῦ</supplied> Ἀ<supplied reason="lost">ρσιν</supplied>οείτ<supplied reason="lost">ου</supplied> 

    <lb n="13"/>στρατηγῷ</lem><rdg>το<supplied reason="lost">π</supplied>ά<supplied reason="lost">ρχαις</supplied> <unclear>καὶ</unclear> τ<supplied reason="lost">ῷ ἐπι</supplied>στρ<unclear>ατ</unclear>ήγῳ</rdg></app> ἀπαλλάξαι με τῆς κωμ<unclear>ογ</unclear>ραμ<unclear>μ</unclear><supplied reason="lost">ατείας, ἵ</supplied><unclear>ν</unclear>α δ<unclear>υ</unclear><supplied reason="lost">νη</supplied>

    <lb n="14" break="no"/><unclear>θ</unclear>ῶ ἐ<supplied reason="lost">ξα</supplied><unclear>ρ</unclear>κεῖν πρὸς τὴν δημοσία<unclear>ν</unclear> <supplied reason="lost">γεωργίαν καὶ τὴν σιτολογί</supplied>

    <lb n="15" break="no"/>αν, ἵ<supplied reason="lost">νʼ ὦ</supplied> εὐ<unclear>ε</unclear>ργ<supplied reason="lost">ε</supplied><unclear>τ</unclear>ημένος. 

    <lb n="16"/><handShift new="m2"/> Ἀ<unclear>π</unclear><supplied reason="lost">ώνι</supplied><unclear>ο</unclear>ς δι<unclear>ὰ</unclear> Ἀ<unclear>ρ</unclear><supplied reason="lost">εί</supplied><unclear>ου</unclear> ἐπιδέδωκα. 

    <lb n="17"/><app type="editorial"><lem resp="BL 8.414"><expan><ex>ἔτους</ex></expan> <num value="16">ιϛ</num></lem><rdg><num value="17">ι<unclear>ζ</unclear></num></rdg></app>, <unclear>Τ</unclear>ῦ<supplied reason="lost">β</supplied>ι <num value="4">δ</num>. 

    <lb n="18"/><handShift new="m3"/> <app type="editorial"><lem resp="BL 1.406"><gap reason="illegible" quantity="4" unit="character"/> <unclear>μ</unclear><supplied reason="lost">ηδε</supplied><unclear>ν</unclear>ὸς ἐπεχομένου</lem><rdg><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="3" unit="character"/><gap reason="illegible" quantity="1" unit="character"/>ος ἐπέχω. <expan>ἐγρά<ex>φη</ex></expan></rdg></app> <app type="editorial"><lem resp="BL 8.414">πρὸ β<unclear>ήμ</unclear><supplied reason="lost">ατός μοι ἔντυχε</supplied></lem><rdg>πρὸ <num value="2">β</num> ἰ<unclear>δῶ</unclear><supplied reason="lost">ν </supplied><gap reason="lost" quantity="6" unit="character"/></rdg></app>. 
