    <lb n="1"/><g type="stauros"/> τῷ τ<gap reason="illegible" quantity="3" unit="character"/><gap reason="lost" quantity="25" unit="character"/><unclear>ίῳ</unclear> Ἡσ<unclear>υ</unclear>χ<unclear>ε</unclear><supplied reason="lost">ίου</supplied> 

