         <head n="16989" xml:lang="en">
            <date>ca. AD 100</date>
            <placeName>Oxyrhynchus</placeName>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><ab>
    <lb n="1"/><gap reason="lost" extent="unknown" unit="line"/> 

    <lb n="1"/><supplied reason="lost">μηδὲ κακοτε</supplied>χνήσει<supplied reason="lost">ν τὴν ὁμολογοῦσαν πρὸς τὴν</supplied> 

    <lb n="2"/><supplied reason="lost">παραχώρησιν</supplied> ταύτην τρόπῳ <supplied reason="lost">μηδενὶ ἀλλὰ καὶ ἐπάναγκον</supplied> 

    <lb n="3"/><supplied reason="lost">αὐτὴν παρ</supplied>έξασθαι τῶι Παυσαν<supplied reason="lost">ίᾳ καὶ τοῖς παρʼ αὐτοῦ τὰς αὐ</supplied>

    <lb n="4" break="no"/><supplied reason="lost">τὰς ἀρο</supplied>ύρας βεβαίας διὰ πα<supplied reason="lost">ντὸς ἀπὸ πάντων πάσῃ βεβαιώσει</supplied> 

    <lb n="5"/><supplied reason="lost">καὶ καθ</supplied>αρὰς ἀπὸ μὲν δη<supplied reason="lost">μοσίων καὶ τῶν ἄλλων τελεσμάτων</supplied> 

    <lb n="6"/><supplied reason="lost">πάντ</supplied>ων τῶν ἕως τῶ<supplied reason="lost">ν ἐπαγομένων τοῦ διελθόντος ἔτους καὶ</supplied> 

    <lb n="7"/><supplied reason="lost">αὐτοῦ</supplied> τοῦ διελθόντος <gap reason="lost" quantity="6" unit="character"/><supplied reason="lost"> ἔτους ἀπὸ δὲ πάσης γεωργίας βα</supplied>

    <lb n="8" break="no"/><supplied reason="lost">σιλικ</supplied>ῆς καὶ οὐσιακῆς κ<supplied reason="lost">αὶ ἱερᾶς καὶ ἑτέρου τινος εἴδους·</supplied> 

    <lb n="9"/><supplied reason="lost">ἐὰν δέ</supplied> τι τούτων ἡ ὁμ<supplied reason="lost">ολογοῦσα παρασυνγράφῃ ἄκυρον ἔστω</supplied> 

    <lb n="10"/><supplied reason="lost">καὶ </supplied><choice><reg><supplied reason="lost">πρ</supplied>οσαποτεισάτω</reg><orig><supplied reason="lost">πρ</supplied>οσαποτισάτωι</orig></choice> <supplied reason="lost">Παυσανίᾳ ἢ τοῖς παρʼ αὐτοῦ καθʼ ὃ ἐὰν παρα</supplied>

    <lb n="11" break="no"/><supplied reason="lost">συγγρ</supplied>άφῃ εἶδο<supplied reason="lost">ς τό τε βλάβος καὶ ἐπίτιμον ἀργυρίου δραχμὰς</supplied> 

    <lb n="12"/><supplied reason="lost">πεντα</supplied>κοσίας <num value="500"/> καὶ εἰς τὸ δημ<supplied reason="lost">όσιον τὰς ἴσας καὶ μηδὲν ἦσσον τὰ</supplied> 

    <lb n="13"/><supplied reason="lost">διωμ</supplied>ολογήμενα κύρι<supplied reason="lost">α ἔστω καθʼ ὅ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="14"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> σ</supplied>αν. <handShift new="m2"/> Σινθῶνις Ὡρ <gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> τοῦ </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> παρακεχώ</supplied>

    <lb n="15" break="no"/><supplied reason="lost">ρηκ</supplied>α Παυσανίᾳ τὰς ὑ<supplied reason="lost">παρχούσας μοι περὶ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="16"/><supplied reason="lost">ἐπο</supplied>ίκιον ἐκ τοῦ Σωκ<supplied reason="lost">ράτους τοῦ </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> κλή</supplied>

    <lb n="17" break="no"/><supplied reason="lost">ρου</supplied> κατοικικῆς γῆς ἀρ<supplied reason="lost">ούρας </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="18"/><gap reason="lost" extent="unknown" unit="character"/> καὶ ἀπέχω τὸ παραχω<supplied reason="lost">ρητικὸν τὰς τοῦ ἀργυρίου δραχ</supplied>

    <lb n="19" break="no"/><supplied reason="lost">μὰς π</supplied>εντακοσίας <num value="500"/> καὶ βεβα<supplied reason="lost">ιώσω </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="20"/><gap reason="lost" extent="unknown" unit="character"/>ια βασιλικῆς κα<supplied reason="lost">ὶ οὐσιακῆς καὶ ἑτέρου τινος εἴ</supplied>

    <lb n="21" break="no"/><supplied reason="lost">δους</supplied> Ἀσκληπιάδης Ἀσκλ<supplied reason="lost">ηπιάδου τοῦ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="22"/><supplied reason="lost">ἐπι</supplied>γέγραμμαι τῆς μητρός μου κύρ<supplied reason="lost">ιος καὶ ἔγραψα ὑπὲρ αὐ</supplied>

    <lb n="23" break="no"/><supplied reason="lost">τῆς μ</supplied>ὴ <choice><reg>εἰδυίης</reg><orig><hi rend="diaeresis">ἰ</hi>δυ<hi rend="diaeresis">ί</hi>ης</orig></choice> γράμματα. <handShift new="m3"/> Παυσαν<supplied reason="lost">ίας </supplied><gap reason="lost" quantity="4" unit="character"/><supplied reason="lost"> ὁ προγεγρα</supplied>

    <lb n="24" break="no"/><supplied reason="lost">μμέ</supplied>νος παρακεχώρημαι ὡς πρόκειτ<supplied reason="lost">αι </supplied><gap reason="lost" extent="unknown" unit="character"/>· 

    <lb n="25"/><supplied reason="lost">Σίνθ</supplied>ωνις <expan>ὡ<ex>ς</ex></expan> <expan><ex>ἐτῶν</ex></expan> <num value="47">μζ</num> <expan><unclear>οὐλ</unclear><ex>ὴ</ex></expan> <expan>ὀφρ<ex>ύι</ex></expan> <expan>ἀριστ<ex>ερᾷ</ex></expan>. 

    <lb n="26"/><supplied reason="lost">Ἀσ</supplied>κ<supplied reason="lost">λη</supplied>π<supplied reason="lost">ιάδης ὡς <expan><ex>ἐτῶν</ex></expan> </supplied><gap reason="lost" quantity="2" unit="character"/> <expan><unclear>οὐλ</unclear><ex>ὴ</ex></expan> παρὰ π<gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" extent="unknown" unit="character"/>. 

    <lb n="27"/><supplied reason="lost">Παυσανίας ὡς <expan><ex>ἐτῶν</ex></expan> </supplied><gap reason="lost" quantity="2" unit="character"/><supplied reason="lost"> οὐλὴ </supplied><abbr><supplied reason="lost">πη</supplied>χ</abbr>. </ab></div>
