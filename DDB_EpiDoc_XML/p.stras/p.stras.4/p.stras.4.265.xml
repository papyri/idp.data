    <lb n="1"/><supplied reason="lost">ἔτους </supplied><gap reason="lost" quantity="7" unit="character"/><supplied reason="lost"> Τιβερίου Κλαυδί</supplied>ου Καίσαρ<supplied reason="lost">ος Σεβαστοῦ</supplied> 

    <lb n="2"/><supplied reason="lost">Γερμανικοῦ Αὐτοκράτορος μηνὸ</supplied>ς Γορπιαίου<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="3"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> Ἐπεὶφ </supplied><gap reason="lost" quantity="2" unit="character"/><supplied reason="lost"> ἐν τῇ Σοκνοπαίου</supplied> Νήσου τῆς Ἡ<unclear>ρ</unclear><supplied reason="lost">ακλείδου μέ</supplied>

    <lb n="4" break="no"/><supplied reason="lost">ριδος τοῦ Ἀρσινοίτου νομοῦ. ὁμολογ</supplied>εῖ Ἐριεῦς νε<supplied reason="lost">ώτερος</supplied> 

    <lb n="5"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ὡς <expan><ex>ἐτῶν</ex></expan> </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> πέν</supplied>τε οὐλὴ πα<supplied reason="lost">ρὰ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="6"/><supplied reason="lost">Ἀπύνχι </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ὡς <expan><ex>ἐτῶν</ex></expan> τρ</supplied>ιάκοντα δ<supplied reason="lost">ύο <num value="32"/> οὐλὴ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="7"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ίσ</supplied>τασθαι αὐτῶι τὸν ὁμολο<supplied reason="lost">γοῦντα </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="8"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> τοῦ</supplied> ἐπιβάλλοντος αὐτῷ μέρο<supplied reason="lost">υς </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="9"/><gap reason="lost" extent="unknown" unit="character"/><gap reason="illegible" quantity="1" unit="character"/>τι Τιβερίου Κ<supplied reason="lost">λα</supplied>υδίου Καίσαρ<supplied reason="lost">ος Σεβαστοῦ</supplied> 

    <lb n="10"/><supplied reason="lost">Γερμανικοῦ Αὐτοκρά</supplied>τορος διὰ τοῦ τῆς αὐτῆς κ<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="11"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> προγε</supplied>γραμμένου Ἀπύνχεως ομου<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="12"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> Στοτ</supplied>οήτιος ἐνάτου μέρος <supplied reason="lost">ο</supplied>ἰκί<supplied reason="lost">ας καὶ αὐλῆς</supplied> 

    <lb n="13"/><gap reason="lost" extent="unknown" unit="character"/><gap reason="illegible" quantity="1" unit="character"/>οτου μέρος κοινὸν καὶ <choice><reg>ἀδιαίρετον</reg><orig>ἀδιέ<supplied reason="lost">ρετον</supplied></orig></choice> <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="14"/><gap reason="lost" extent="unknown" unit="character"/>αντων ἐν τῇ προκειμέ<supplied reason="lost">νῃ κώμῃ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="15"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> π</supplied>ρότερον Ταυσέως <choice><reg>νυνὶ</reg><orig>νυνεὶ</orig></choice> <gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="8" unit="character"/><unclear>υ</unclear> 

    <lb n="16"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> βορρᾶ ῥύ</supplied>μη βασιλικὴ λιβὸς Θασῆτος καὶ τα

    <lb n="17" break="no"/><gap reason="lost" extent="unknown" unit="character"/> <choice><reg>ἀπηλιώτου</reg><orig><supplied reason="lost">ἀ</supplied>πηλιότου</orig></choice> πρότερον Πανεφρύμμιος 

    <lb n="18"/><supplied reason="lost">καὶ τῶν ἀδελφῶν νυνὶ καὶ</supplied> τῶν τέκνων οἰκία καὶ ἀπέχειν <unclear>πα</unclear>

    <lb n="19" break="no"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> Ἀπύν</supplied>χιος παραχρῆμα διὰ χειρὸς ἐξ οἴκου 

    <lb n="20"/><supplied reason="lost">τὴν </supplied><gap reason="lost" extent="unknown" unit="character"/><gap reason="illegible" quantity="1" unit="character"/>ους τοῦ ἐνάτου μέρους τι<unclear>μὴν</unclear> καὶ 

    <lb n="21"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> τῷ Ἀ</supplied>πύνχι καὶ τοῖς παρʼ αὐτοῦ πάσῃ βεβαι

    <lb n="22" break="no"/><supplied reason="lost">ώσει ἀπὸ τῆς ἐνεστ</supplied>ώσης ἡμέρας <supplied reason="lost">ἐ</supplied>πὶ τὸν ἅπαντα χρόνον. 

    <lb n="23"/><supplied reason="lost">ἐὰν δὲ μὴ βεβαιοῖ καθ</supplied>ὰ γέγραπται ἀποτεισάτω ὁ ὁμολογῶν 

    <lb n="24"/><supplied reason="lost">τῷ Ἀπύνχι ἢ τοῖς παρʼ αὐ</supplied>τοῦ τά τε βλάβη καὶ τὰ δαπανήματα 

    <lb n="25"/><supplied reason="lost">διπλᾶ </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> μ</supplied><unclear>ά</unclear>των καὶ ἐπίτ<supplied reason="lost">ι</supplied>μον ἀργυρίου δραχμὰς 

    <lb n="26"/><supplied reason="lost">καὶ εἰς τὸ δημόσιον τὰς</supplied> <unclear>ἴ</unclear><supplied reason="lost">σας</supplied> καὶ μηδὲ<supplied reason="lost">ν ἧσσον </supplied><gap reason="lost" extent="unknown" unit="character"/> 

