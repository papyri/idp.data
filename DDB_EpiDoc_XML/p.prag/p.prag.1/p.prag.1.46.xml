    <lb n="24" break="no"/><supplied reason="lost">τυρῶ τῷ </supplied><expan><supplied reason="lost">γρ</supplied>μματί<ex>ῳ</ex></expan>, ἀκούσας παρὰ τῶν θεμένων μαρτυρῶ 

    <lb n="25"/><supplied reason="lost">δὲ καὶ τῇ κατ</supplied><unclear>α</unclear>βολῇ τοῦ χρυσίου τ<supplied reason="lost">ῆς</supplied> <unclear>τ</unclear>ιμῆς. <handShift new="m4"/> <g type="stauros"/> <expan>Φλ<ex>άουιος</ex></expan> Φ<unclear>ῖ</unclear>βις 

    <lb n="26"/><supplied reason="lost">τοῦ δεῖνος </supplied><gap reason="lost" extent="unknown" unit="character"/><unclear>ω</unclear>ος <expan>στρ<ex>ατιώτης</ex></expan> μαρτ<supplied reason="lost">υρ</supplied><unclear>ῶ</unclear> τῷ γραμματίῳ, 

    <lb n="27"/><supplied reason="lost">ἀκούσας π</supplied>αρὰ τῶν θεμ<supplied reason="lost">έ</supplied><unclear>ν</unclear>ων μαρτυρῶ τῇ 

    <lb n="28"/><supplied reason="lost">κα</supplied><unclear>τ</unclear>αβολῇ τοῦ χ<supplied reason="lost">ρ</supplied><unclear>υ</unclear>σίου τῆς τιμῆς. <handShift new="m5"/> <g type="stauros"/> 

