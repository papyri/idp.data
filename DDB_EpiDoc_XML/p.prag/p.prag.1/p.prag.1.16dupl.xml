         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r" type="textpart"><!--milestone unit="4"--><ab>

    <lb n="1"/><supplied reason="lost">Αὐρηλίῳ </supplied><gap reason="lost" quantity="3" unit="character"/><supplied reason="lost"> πραιποσίτῳ </supplied><gap reason="lost" quantity="2" unit="character"/><supplied reason="lost"> πάγου</supplied> 

    <lb n="2"/><supplied reason="lost">παρὰ Αὐρηλίων</supplied> Νααρω<supplied reason="lost">οῦτος Ναα</supplied><unclear>ρ</unclear>ῶτος 

    <lb n="3"/><supplied reason="lost">καὶ Ἰσὰκ </supplied><expan><supplied reason="lost">Εὐδ</supplied><unclear>έ</unclear>μων<ex>ος</ex></expan> ἀ<supplied reason="lost">μφοτέ</supplied><unclear>ρ</unclear>ων 

