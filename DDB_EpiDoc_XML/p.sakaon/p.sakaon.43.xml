    <lb n="8"/>ἃς ἔχω π<unclear>ερ</unclear>ὶ τῆ<unclear>ς</unclear> <unclear>αὐτῆς</unclear> <unclear>κώ</unclear>

    <lb n="9" type="inWord"/>μης Φ<gap reason="illegible" quantity="2" unit="character"/><unclear>ν</unclear>διδ<unclear>ος</unclear> ἀρ<supplied reason="lost">ούρ</supplied>ας 

    <lb n="10"/>πέντε <num value="5"/>, καὶ <choice><reg>ἐμοῦ</reg><orig>καμου</orig></choice> τήν <unclear>τε</unclear> <unclear>νομ</unclear>ὴ<unclear>ν</unclear> 

    <lb n="11"/>καὶ δεσποτίαν ἔχοντ<unclear>ος</unclear> 

    <lb n="12"/>τῶν αὐτῶν ἀρουρῶν 

    <lb n="13"/>καὶ <choice><reg>διευθύνοντό<supplied reason="omitted">ς</supplied></reg><orig>διευθυνωντο</orig></choice> μου 

    <lb n="14"/>τῷ <choice><reg>ἱερωτάτῳ</reg><orig>ιερωτατου</orig></choice> <choice><reg>ταμιείῳ</reg><orig>ταμιου</orig></choice> τὰ ὑπὲρ 

    <lb n="15"/><choice><reg>αὐτῶ<supplied reason="omitted">ν</supplied></reg><orig>αυτω</orig></choice> εὐσεβῆ τελέσματα, οὐ

    <lb n="16" type="inWord"/>κ <choice><reg>οἶδα</reg><orig>υδα</orig></choice> <choice><reg>τίνι</reg><orig>τινα</orig></choice> <choice><reg>λόγῳ</reg><orig>λογο</orig></choice> Ἥρων καὶ Ἁτρῆς 

    <lb n="17"/>καὶ Πέννις καὶ Ἀῦγ ἀπὸ τῆς 

    <lb n="18"/>αὐτῆς κώμης <choice><reg>Θεαδελφίας</reg><orig>τεαδελφιας</orig></choice> 

    <lb n="19"/><choice><reg>ἐποφθαλμιάσαντες</reg><orig>εφαποπθαλμιας</orig></choice> τῶ<add place="above">ν</add> ἡμετέρων 

    <lb n="20"/>καμάτων βεβούληνται ἐξ αὐτῶν ἀρουρῶν 

    <lb n="21"/>δύο ἥμισυ <num value="2"/><num value="1/2"/> ἀποσπᾶσαι του<gap reason="illegible" quantity="2" unit="character"/>ου <unclear>τὰς</unclear> μηδὲ<unclear>ν</unclear> 

    <lb n="22"/>αὐτοῖς διαφερούσας. καὶ <choice><reg>δικαιολογεῖ</reg><orig>δικαιολογι</orig></choice> <unclear>Χ</unclear><gap reason="illegible" quantity="3" unit="character"/><unclear>α</unclear><gap reason="illegible" quantity="2" unit="character"/> 

    <lb n="23"/>πρὸς αὐτούς, οἱ δὲ ἐκ τῆς ἐναντιώσ<supplied reason="lost">εως ἀ</supplied>

    <lb n="24" type="inWord"/><choice><reg>πειλοῦσι</reg><orig><supplied reason="lost">α</supplied>πι<unclear>λο</unclear>υσι</orig></choice> τὰ <choice><reg>μηδὲν</reg><orig><unclear>μητεν</unclear></orig></choice> επικτ<gap reason="illegible" quantity="1" unit="character"/><unclear>ν</unclear>αδυο<gap reason="illegible" quantity="3" unit="character"/><unclear>απ</unclear><gap reason="illegible" quantity="1" unit="character"/> 

    <lb n="25"/><unclear>μ</unclear>ε<gap reason="illegible" quantity="2" unit="character"/>ιοι καὶ δεδε<gap reason="illegible" quantity="3" unit="character"/>υντ<gap reason="illegible" quantity="1" unit="character"/>τ<gap reason="illegible" quantity="1" unit="character"/>ου<gap reason="illegible" quantity="2" unit="character"/>ιλην<unclear>αρουρ</unclear> 

    <lb n="26"/>παρασυνγραφ<unclear>α</unclear><gap reason="illegible" quantity="2" unit="character"/> <choice><reg><supplied reason="lost">δ</supplied>έομαί</reg><orig><supplied reason="lost">δ</supplied><unclear>εο</unclear>μι</orig></choice> <choice><reg>σοι</reg><orig>σι</orig></choice> ἀπὸ τούτων <unclear>προσ</unclear>

    <lb n="27" type="inWord"/><unclear>ά</unclear>χθα<unclear>ι</unclear> αὐτοὺς <unclear>ἐπὶ</unclear> <supplied reason="lost">σὲ</supplied> καὶ πρῶτον μὲν κ<gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="2" unit="character"/> 

    <lb n="28"/>τῶν μηδὲ τούτοις διαφερόντων <unclear>δια</unclear><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="3" unit="character"/> 

    <lb n="29"/>ων πραξ<gap reason="illegible" quantity="2" unit="character"/>                   διευτύχει. 

    <lb n="30"/>ὑπατείας <expan><unclear>Κωνσταν</unclear><ex>τίου</ex></expan> <unclear>καὶ</unclear> <unclear>Οὐαλερίου</unclear> Μαξίμ<supplied reason="lost">ου</supplied> 

    <lb n="31"/>Ἁθὺρ <num value="10">ι</num>. Αὐρήλιος Σακαῶν ἐπιδέδωκα. <gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="2" unit="character"/> 

