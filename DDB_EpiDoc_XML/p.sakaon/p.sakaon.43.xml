         <head n="13062" xml:lang="en">
            <date>AD 327</date>
            <placeName>Theadelphia</placeName>
            <ref n="sb;1;5356" type="reprint-from">P.Thead. 61; SB 1 5356</ref>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><ab>
    <lb n="1"/>Αὐρηλί<unclear>ῳ</unclear> Χ<unclear>α</unclear>ιρ<supplied reason="lost">ήμο</supplied>νι <expan>πραι<ex>ποσίτῳ</ex></expan> <unclear>πάγου</unclear> <supplied reason="lost"><num value="8">η</num></supplied> 

    <lb n="2"/>παρὰ Αὐρηλίου Σακαῶνος <unclear>Σατα</unclear>

    <lb n="3" break="no"/>βοῦτος ἀπὸ κώμης Θεαδ<supplied reason="lost">ελφίας</supplied> 

    <lb n="4"/>τοῦ αὐτοῦ π<unclear>ά</unclear>γο<supplied reason="lost">υ</supplied>. <unclear>ἀπο</unclear>γραψα

    <lb n="5" break="no"/>μ<unclear>ένου</unclear> μ<unclear>ου</unclear> <unclear>πρώην</unclear> ἐν τῷ 

    <lb n="6"/>συσταθ<unclear>έ</unclear>ν<unclear>τ</unclear>ι <choice><reg>κή<supplied reason="omitted">ν</supplied>σῳ</reg><orig>κησῳ</orig></choice> παρ<unclear>ὰ</unclear> 

    <lb n="7"/><app type="editorial"><lem resp="BL 9.231">Σαβείνῳ <certainty match=".." locus="value"/></lem><rdg><unclear>εκδικι</unclear><gap reason="illegible" quantity="2" unit="character"/><unclear>ω</unclear></rdg></app> <choice><reg>κηνσίτορσι</reg><orig>κενσήτο<unclear>ρ</unclear>σι</orig></choice> 

    <lb n="8"/>ἃς ἔχω π<unclear>ερ</unclear>ὶ τῆ<unclear>ς</unclear> <unclear>αὐτῆς</unclear> <unclear>κώ</unclear>

    <lb n="9" break="no"/>μης <app type="editorial"><lem resp="BL 8.301">Φ<unclear>ιλαγρ</unclear>ίδ<unclear>ος</unclear> <certainty match=".." locus="value"/></lem><rdg>Φ<gap reason="illegible" quantity="2" unit="character"/><unclear>ν</unclear>διδ<unclear>ος</unclear></rdg></app> ἀρ<supplied reason="lost">ούρ</supplied>ας 

    <lb n="10"/>πέντε <num value="5"/>, καὶ <choice><reg>ἐμοῦ</reg><orig>καμοῦ</orig></choice> τήν <unclear>τε</unclear> <unclear>νομ</unclear>ὴ<unclear>ν</unclear> 

    <lb n="11"/>καὶ δεσποτίαν ἔχοντ<unclear>ος</unclear> 

    <lb n="12"/>τῶν αὐτῶν ἀρουρῶν 

    <lb n="13"/>καὶ <choice><reg>διευθύνοντό<supplied reason="omitted">ς</supplied></reg><orig>διευθυνωντο</orig></choice> μου 

    <lb n="14"/>τῷ <choice><reg>ἱερωτάτῳ</reg><orig>ἱερωτάτου</orig></choice> <choice><reg>ταμιείῳ</reg><orig>ταμίου</orig></choice> τὰ ὑπὲρ 

    <lb n="15"/><choice><reg>αὐτῶ<supplied reason="omitted">ν</supplied></reg><orig>αὐτω</orig></choice> εὐσεβῆ τελέσματα, οὐ

    <lb n="16" break="no"/>κ <choice><reg>οἶδα</reg><orig>ὖδα</orig></choice> <choice><reg>τίνι</reg><orig>τινα</orig></choice> <choice><reg>λόγῳ</reg><orig>λόγο</orig></choice> Ἥρων καὶ Ἁτρῆς 

    <lb n="17"/>καὶ Πέννις καὶ Ἀῦγ ἀπὸ τῆς 

    <lb n="18"/>αὐτῆς κώμης <choice><reg>Θεαδελφίας</reg><orig>Τεαδελφίας</orig></choice> 

    <lb n="19"/><choice><reg>ἐποφθαλμιάσαντες</reg><orig>ἐφαποπθαλμιας</orig></choice> τῶ<add place="above">ν</add> ἡμετέρων 

    <lb n="20"/>καμάτων βεβούληνται ἐξ αὐτῶν ἀρουρῶν 

    <lb n="21"/>δύο ἥμισυ <num value="2"/><num value="1/2"/> ἀποσπᾶσαι του<gap reason="illegible" quantity="2" unit="character"/>ου <unclear>τὰς</unclear> μηδὲ<unclear>ν</unclear> 

    <lb n="22"/>αὐτοῖς διαφερούσας. καὶ <choice><reg>δικαιολογεῖ</reg><orig>δικαιολογῖ</orig></choice> <unclear>Χ</unclear><gap reason="illegible" quantity="3" unit="character"/><unclear>α</unclear><gap reason="illegible" quantity="2" unit="character"/> 

    <lb n="23"/>πρὸς αὐτούς, οἱ δὲ ἐκ τῆς ἐναντιώσ<supplied reason="lost">εως </supplied><choice><reg><supplied reason="lost">ἀ</supplied><lb n="24" break="no"/>πειλοῦσι</reg><orig><supplied reason="lost">α</supplied><lb n="24" break="no"/>πι<unclear>λο</unclear>ῦσι</orig></choice> τὰ <choice><reg>μηδὲν</reg><orig><unclear>μητὲν</unclear></orig></choice> επικτ<gap reason="illegible" quantity="1" unit="character"/><unclear>ν</unclear>αδυο<gap reason="illegible" quantity="3" unit="character"/><unclear>απ</unclear><gap reason="illegible" quantity="1" unit="character"/> 

    <lb n="25"/><unclear>μ</unclear>ε<gap reason="illegible" quantity="2" unit="character"/>ιοι καὶ δεδε<gap reason="illegible" quantity="3" unit="character"/>υντ<gap reason="illegible" quantity="1" unit="character"/>τ<gap reason="illegible" quantity="1" unit="character"/>ου<gap reason="illegible" quantity="2" unit="character"/>ιλην<unclear>αρουρ</unclear> 

    <lb n="26"/>παρασυνγραφ<unclear>α</unclear><gap reason="illegible" quantity="2" unit="character"/> <choice><reg><supplied reason="lost">δ</supplied>έομαί</reg><orig><supplied reason="lost">δ</supplied><unclear>εο</unclear>μί</orig></choice> <choice><reg>σοι</reg><orig>σι</orig></choice> ἀπὸ τούτων <unclear>προσ</unclear>

    <lb n="27" break="no"/><unclear>ά</unclear>χθα<unclear>ι</unclear> αὐτοὺς <unclear>ἐπὶ</unclear> <supplied reason="lost">σὲ</supplied> καὶ πρῶτον μὲν κ<gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="2" unit="character"/> 

    <lb n="28"/>τῶν μηδὲ τούτοις διαφερόντων <unclear>δια</unclear><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="3" unit="character"/> 

    <lb n="29"/>ων πραξ<gap reason="illegible" quantity="2" unit="character"/>                   διευτύχει. 

    <lb n="30"/>ὑπατείας <expan><unclear>Κωνσταν</unclear><ex>τίου</ex></expan> <unclear>καὶ</unclear> <unclear>Οὐαλερίου</unclear> Μαξίμ<supplied reason="lost">ου</supplied> 

    <lb n="31"/>Ἁθὺρ <num value="10">ι</num>. Αὐρήλιος Σακαῶν ἐπιδέδωκα. <gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="2" unit="character"/> 

    <lb n="32"/>ἔγραψα ὑπὲρ αὐτοῦ ἀγραμμάτου. </ab></div>
