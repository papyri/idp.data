    <lb n="4"/><gap reason="lost" extent="unknown" unit="character"/> ὧδε ἀπὸ Ἀν<supplied reason="lost">τ</supplied>ι

    <lb n="5" type="inWord"/><supplied reason="lost" cert="low">οχείας  </supplied><gap reason="lost" quantity="8" unit="character"/><gap reason="illegible" quantity="1" unit="character"/>μοι εἰς η<gap reason="illegible" quantity="1" unit="character"/>χαλαν<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="5" unit="character"/>χετε ἐξ ὑμῶν

