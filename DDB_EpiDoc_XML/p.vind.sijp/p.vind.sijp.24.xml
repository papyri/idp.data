    <lb n="27"/><gap reason="lost" quantity="4" unit="character"/><gap reason="illegible" quantity="1" unit="character"/>οποινεις<gap reason="illegible" quantity="4" unit="character"/> 

    <lb n="28"/><gap reason="lost" quantity="4" unit="character"/><unclear>α</unclear>λογον <gap reason="illegible" quantity="3" unit="character"/><gap reason="lost" quantity="2" unit="character"/> 

