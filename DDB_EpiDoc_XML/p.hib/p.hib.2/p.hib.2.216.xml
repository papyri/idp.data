    <lb n="14"/>Παρθικοῦ Μεγ<supplied reason="lost">ίστου κ</supplied>αὶ Π<supplied reason="lost">ουβλίου </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="15"/><gap reason="illegible" extent="unknown" unit="character"><desc>vestiges</desc></gap> 

