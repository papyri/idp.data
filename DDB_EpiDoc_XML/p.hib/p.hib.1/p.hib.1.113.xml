         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="1" type="textpart"><ab>

    <lb n="1"/><gap reason="lost" extent="unknown" unit="line"/> 

    <lb n="1"/>κ<supplied reason="lost">αὶ</supplied> <gap reason="lost" quantity="2" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="lost" quantity="9" unit="character"/> 

    <lb n="2"/>ο<add place="above">ς</add> αἰτούμενο<supplied reason="lost">ς </supplied><gap reason="lost" quantity="8" unit="character"/><supplied reason="lost"> ὑ</supplied>

    <lb n="3" break="no"/><unclear>δάτων</unclear> <gap reason="lost" quantity="9" unit="character"/> 

    <lb n="4"/>καὶ ἐν Ἀθεμμ<supplied reason="lost">εῖ </supplied><gap reason="lost" quantity="9" unit="character"/> 

    <lb n="5"/>καὶ τὸ <subst><add place="inline"><add place="above">ἐπι</add>γραφὲν</add><del rend="corrected"><del rend="erasure">ανα</del>γραφεν</del></subst> τ<unclear>ο</unclear><supplied reason="lost">ῖς </supplied><gap reason="lost" quantity="4" unit="character"/><gap reason="illegible" quantity="3" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="6"/><subst><add place="inline">τοῖς</add><del rend="corrected">ταις</del></subst> τὸ ζῦτος μὴ εἰλ<unclear>η</unclear><supplied reason="lost">φόσι </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="7"/>ἐν τ<unclear>ῆ</unclear>ι τῶν <unclear>κωμῶ</unclear>ν <gap reason="lost" quantity="6" unit="character"/> <expan><unclear>χα</unclear><ex>λκοῦ</ex></expan> <num value="91">ϙα</num> <expan><ex>ἡμιωβέλιον</ex></expan> <num value="1/2"/>. 

    <lb n="8"/>καὶ τὸ ἐν Πάσιτι <gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="8" unit="character"/><unclear>ς</unclear> 

    <lb n="9"/>λογευτῆι τῶι παρ<supplied reason="lost">α </supplied><gap reason="lost" quantity="5" unit="character"/><unclear>τ</unclear>ι 

    <lb n="10"/>Βότρωι τῶι φυλακίτ<supplied reason="lost" cert="low">ηι καὶ </supplied> 

    <lb n="11"/>ἀναχωρήσαντι ζυτη<supplied reason="lost">ρᾶς</supplied> <expan>χα<ex>λκοῦ</ex></expan> <num value="34">λδ</num> <expan><ex>ἡμιωβέλιον</ex></expan> <num value="1/2"/> <expan><ex>τέταρτον ὀβολοῦ</ex></expan> <num value="1/4"/>, 

    <lb n="12"/>καὶ ἐλαικῆς <expan>χα<ex>λκοῦ</ex></expan> <num value="167">ρξζ</num> <expan><ex>ὀβολὸς</ex></expan> <num value="1">α</num> <num value="1/2">𐅵</num>. 

    <lb n="13"/>καὶ ἐν τοῖς ἀντιλέγουσιν μὴ εἰλη

    <lb n="14" break="no"/>φέναι ἐλαίου <expan>χα<ex>λκοῦ</ex></expan> <num value="6">ϛ</num>. 

    <lb n="15"/>ἐν Τοτοῆι Πάσιτος λογευτῆι 

    <lb n="16"/>ὧι μηθὲν ὑπάρχει <num value="66">ξϛ</num> <expan><ex>ὀβολοὺς</ex></expan> <num value="3">γ</num> <num value="1/4" rend="tick">δ</num>. 

    <lb n="17"/>καὶ τὸ δοθὲν τοῖς ἀσθενοῦσιν 

    <lb n="18"/>τῶν γεωργῶν εἰς τὰ ἔργα τῶν 

    <lb n="19"/>κτημάτων <expan>ἀργ<ex>υρίου</ex></expan> <num value="20">κ</num>. 
