         <head n="16053" xml:lang="en">
            <date>AD 174</date>
            <placeName>Thelbonthon Siphtha</placeName>
            <ref type="reprint-from">PLugdBat5</ref>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><ab>
    <lb n="1"/><handShift new="m20"/> <num value="105">ρε</num> 

    <lb n="1"/><handShift new="m6"/> Ἀπίωνι <expan>βασιλ<ex>ικῷ</ex></expan> <expan>γρ<ex>αμματεῖ</ex></expan> Προσωπείτου 

    <lb n="2"/>παρὰ <expan>Παντβ<ex>εῦτος</ex></expan> καὶ <choice><reg>Τιθοεννήσιος</reg><orig>Τιθοενησις</orig></choice> καὶ Παλακρήους 

    <lb n="3"/>καὶ <choice><reg>Ἁροννήσιος</reg><orig>Ἁροννῆσις</orig></choice> τῶν <num value="4">δ</num> <expan>Πετῶτ<ex>ος</ex></expan> 

    <lb n="4"/>Πνεφερῶτος. 

    <lb n="5"/><expan>ἀπογρ<ex>αφόμεθα</ex></expan> κατὰ τὰ ὑπὸ Καλουεισίου 

    <lb n="6"/>Στατιανοῦ τοῦ <choice><reg>λαμπρο<supplied reason="omitted">τά</supplied>του</reg><orig>λαμπροτου</orig></choice> 

    <lb n="7"/>ἡγεμόνος προστεταγμένα 

    <lb n="8"/>εἰς τὴν ἐπʼ ἀγαθοῖς <choice><reg>γινομένην</reg><orig>γεινομενος</orig></choice> 

    <lb n="9"/><expan>κ<ex>ατʼ</ex></expan> <expan>οἰ<ex>κίαν</ex></expan> <expan>ἀπογρ<ex>αφὴν</ex></expan> τὰ ὑπάρχοντα 

    <lb n="10"/>ἐν τῇ κώμῃ <expan>Θελβ<ex>όνθον</ex></expan> <expan>Σί<ex>φθα</ex></expan> 

    <lb n="11"/><expan>οἰκ<ex>ίας</ex></expan> καὶ <expan>ψιλ<ex>οῦ</ex></expan> <expan>τόπ<ex>ου</ex></expan> <add place="above"><expan>ἄλλο<ex>υ</ex></expan></add> Θαειήους <expan>Ψεναμούν<ex>ιος</ex></expan> 

    <lb n="12"/><expan>πήχ<ex>εων</ex></expan> <num value="100">ρ</num>, ἐν <choice><reg>οἷς</reg><orig>ἧς</orig></choice> <choice><reg>ἐλαιούργιον</reg><orig>ἐλεούργιον</orig></choice>, 

    <lb n="13"/><expan>Παντβ<ex>εῦτος</ex></expan> <expan>μ<ex>έρη</ex></expan> <num value="2">β</num> καὶ <choice><reg>Τιθοεννήσιος</reg><orig>Τιθοενήσιος</orig></choice> <expan>μ<ex>έρος</ex></expan> <num value="1">α</num> 

    <lb n="14"/>καὶ <choice><reg>Παλακρήους</reg><orig>Παλακρης</orig></choice> <expan>μ<ex>έρος</ex></expan> <num value="1">α</num> καὶ Ἁροννήσιος <expan>μ<ex>έρος</ex></expan> <num value="1">α</num> 

    <lb n="15"/>τῶν <num value="4">δ</num> <expan>Πετῶτ<ex>ος</ex></expan> Πνεφερῶτος, 

    <lb n="16"/>ἐν οἷς οὐδείς,  καὶ Πνεφερῶτος 

    <lb n="17"/><expan>ψιλ<ex>ῶν</ex></expan> <expan>τόπ<ex>ων</ex></expan> <expan>πήχ<ex>εων</ex></expan> <num value="668">χξη</num> <num value="1/2">𐅵</num> τῶν <expan>α<ex>ὐτῶν</ex></expan> 

    <lb n="18"/><expan>προγεγρα<ex>μμένων</ex></expan> τὸ <expan>ἐπιβάλ<ex>λον</ex></expan> ἑκάστῳ <expan>μ<ex>έρος</ex></expan> 

    <lb n="19"/>καὶ <expan>Πετῶ<ex>τος</ex></expan> <supplied reason="omitted"><expan>ψιλ<ex>οῦ</ex></expan></supplied> <expan>τόπ<ex>ου</ex></expan> τῶν <expan>α<ex>ὐτῶν</ex></expan> <surplus>καὶ</surplus> <expan>προγεγ<ex>ραμμένων</ex></expan> 

    <lb n="20"/>καὶ <supplied reason="omitted">τὸ</supplied> <expan>ἐπιβάλ<ex>λον</ex></expan> <choice><reg>ἑκάστῳ</reg><orig>ἑκαστη</orig></choice> <expan>μ<ex>έρους</ex></expan> <num value="1/2">𐅵</num> καὶ 

    <lb n="21"/>τὸ <expan>λοιπ<ex>ὸν</ex></expan> <expan>μ<ex>έρος</ex></expan> <num value="1/2">𐅵</num> <expan>Θερμούθ<ex>ιος</ex></expan> <expan>Πνεφερ<unclear>οτιθ</unclear><ex>οήους</ex></expan> 

    <lb n="22"/>κατὰ τὸ <num value="1/4" rend="tick">δ</num> μέρος καὶ <choice><reg>Νουάτιος</reg><orig>Νουατις</orig></choice> <expan>Παλαχ<ex>ρήους</ex></expan> 

    <lb n="23"/>ἀπὸ <expan>κληρ<ex>ονομίας</ex></expan> <expan>Πνεφερῶτ<ex>ος</ex></expan> <expan>Πνεφ<ex>ερῶτος</ex></expan> <surplus>καὶ</surplus> τὸ <expan>λοιπ<ex>ὸν</ex></expan> 

    <lb n="24"/>μέρος <num value="1/4" rend="tick">δ</num>          ἐν οἷς οὐδείς. 

    <lb n="25"/><expan><ex>ἔτους</ex></expan> <num value="14">ιδ</num> Αὐτοκράτορος <supplied reason="omitted">Καίσαρος Μάρκου Αὐρηλίου</supplied> <choice><reg>Ἀντωνίνο<supplied reason="lost">υ</supplied></reg><orig>Ἀντωνείνο<supplied reason="lost">υ</supplied></orig></choice> 

    <lb n="26"/>Σεβαστοῦ Ἀρμενιακοῦ 

    <lb n="27"/>Μηδικοῦ <choice><reg>Παρθικοῦ</reg><orig>Παρ<del rend="erasure">τ</del>θικοῦ</orig></choice> <choice><reg>Γερμανι<supplied reason="omitted">κ</supplied>οῦ</reg><orig>Γερμανιοῦ</orig></choice> 

    <lb n="28"/>Μεγίστου, Ἐπεὶφ <num value="25">κε</num>. 

    <lb n="29"/><handShift new="m3"/> <expan>Παντβ<ex>εῦς</ex></expan> καὶ Τοθοεννῆσις 

    <lb n="30"/>καὶ <choice><reg>οἱ</reg><orig>τω</orig></choice> <expan>ἀδελ<ex>φοὶ</ex></expan> <expan>ἐπιδεδώκ<ex>αμεν</ex></expan> ὡς <expan>π<ex>ρόκειται</ex></expan>. 

    <lb n="31"/>Ἀγαθὸς <expan>Δαί<ex>μων</ex></expan> <expan>Σαίφιο<ex>ς</ex></expan> <expan>ἔγρ<ex>αψα</ex></expan> 

    <lb n="32"/>ὑπὲρ <expan>α<ex>ὐτῶν</ex></expan> μὴ <choice><reg>εἰδότων</reg><orig>εἰδοδες</orig></choice> <expan>γρ<ex>άμματα</ex></expan>. 

    <lb n="33"/><handShift new="m21"/> διʼ <abbr>Ἁρποκ</abbr> <expan>σε<ex>λίδος</ex></expan> <num value="42">μβ</num>. </ab></div>
