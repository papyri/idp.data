    <lb n="12"/>αἰτιασάμενος ὁ τῆς ὑπομνήσ<unclear>εω</unclear><supplied reason="lost">ς</supplied> αἴτιος γεγονὼς καὶ τῆς τῶ<unclear>ν</unclear> <expan><unclear>εἰ</unclear>ρ<unclear>η</unclear><supplied reason="lost">μ</supplied>έ<ex>νων</ex></expan> σπο<unclear>ρ</unclear><supplied reason="lost">τούλων ἀπαιτήσεως </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="13"/>τῷ μ<unclear>ὴ</unclear> εὐλόγως ἀπῃτημ<unclear>έ</unclear><supplied reason="lost">νῳ</supplied> ἀναγκασθήσεται, εἰ δὲ καὶ <unclear>τ</unclear>αξεώ<supplied reason="lost">τ</supplied><unclear>ης</unclear> <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="14"/>τῶν ἀφισταμένων ἐπαχθ<supplied reason="lost">ήσ</supplied><unclear>ετ</unclear>αι, ὥστε <unclear>γ</unclear>ὰρ <subst><add place="inline">ἕκαστον</add><del rend="corrected"><gap reason="illegible" quantity="1" unit="character"/>καστον</del></subst> <unclear>τ</unclear>ῶ<unclear>ν</unclear> <unclear>α</unclear><gap reason="illegible" quantity="5" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="15"/>ἀναπέμψαι ὑπὲρ σωτηρί<unclear>ας</unclear> <supplied reason="lost">καὶ</supplied> <unclear>δ</unclear><supplied reason="lost">ια</supplied>μονῆς τοῦ <expan><supplied reason="lost">θ</supplied>είο<ex>υ</ex></expan> κράτους τ<supplied reason="lost">οῦ γ</supplied><unclear>α</unclear><supplied reason="lost">ληνοτά</supplied>το<supplied reason="lost" cert="low">υ βασιλέως</supplied><gap reason="lost" extent="unknown" unit="character"/>

    <lb n="16"/>καὶ τῇ ἐπιχωρίῳ μεθερμην<unclear>ε</unclear><supplied reason="lost">υ</supplied><unclear>θῆ</unclear>ναι δια<unclear>λέ</unclear>κτῳ καὶ προτεθῆ<supplied reason="lost">ν</supplied>αι <gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

