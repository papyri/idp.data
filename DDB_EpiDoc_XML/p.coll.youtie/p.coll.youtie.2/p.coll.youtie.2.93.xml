    <lb n="1"/><gap reason="lost" extent="unknown" unit="character"/> <unclear>ἑβ</unclear>δόμ<unclear>η</unclear>ς ἰνδικτίονο<unclear>ς</unclear> <unclear>ἐ</unclear>ν Ἡρακλέους <choice><reg>πόλει</reg><orig>πόλι</orig></choice> 

