    <lb n="3"/>Ἀχιλλέω<supplied reason="lost">ς <app type="editorial"><lem resp="BL 9.133"><expan>Ἑρμοπολ<ex>ίτου</ex></expan></lem><rdg/></app> <expan>ἀναγρ<ex>αφομένου</ex></expan> ἐ</supplied>πὶ <expan>Πόλ<ex>εως</ex></expan> <expan>Ἀπηλ<ex>ιώτου</ex></expan> <expan><supplied reason="lost">ἀπογρ</supplied><ex>άφομαι</ex></expan> εἰς τ<supplied reason="lost">ὸ ἐ</supplied>νεστὸς <num value="6">ϛ</num> <expan><ex>ἔτος</ex></expan> 

