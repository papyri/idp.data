    <lb n="1"/><choice><reg>τετελώνηται</reg><orig>τετελώνιται</orig></choice> διὰ 

    <lb n="2"/>πύλης Σοκνο

    <lb n="3" break="no"/>παίου <expan><ex>ἑκατοστῆς</ex></expan> <num value="1/100"/> καὶ <expan><ex>πεντηκοστῆς</ex></expan> <num value="1/50"/> 

    <lb n="4"/>Φανοῦφις εἰσά

    <lb n="5" break="no"/>γων ἀπὸ <app type="editorial"><lem resp="BL 9.142"><choice><reg>Αὐάσεως</reg><orig><app type="alternative"><lem>Αὔσως</lem><rdg>Αὔεως</rdg></app></orig></choice></lem><rdg>Ἀ<unclear>ρ</unclear>ε<unclear>ω</unclear><supplied reason="lost">ς</supplied></rdg></app> 

    <lb n="6"/><expan>κάμηλ<ex>ον</ex></expan> ἑνὰ <num value="1"/> <expan>οἴνο<ex>υ</ex></expan> 

    <lb n="7"/>σπάθια ἕξ <num value="6"/>. 

    <lb n="8"/><expan><ex>ἔτους</ex></expan> <num value="22">κβ</num> Ἐπεὶφ 

    <lb n="9"/>πεντεκαιδε<supplied reason="lost">κάτῃ</supplied> 

    <lb n="10"/><num value="15">ιε</num>. 

