         <head n="33772 33773" xml:lang="en">
            <date>IV spc</date>
            <placeName>Panopolis</placeName>
            <ref type="reprint-from">pg235</ref>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><ab>
    <lb n="1"/>ὑπατεί<supplied reason="lost">α</supplied>ς τῶ<supplied reason="lost">ν δεσποτῶν ἡμῶν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="2"/>Αὐρήλιο<supplied reason="lost">ς</supplied> <app type="editorial"><lem resp="BL 1.292">Κοτ<gap reason="lost" extent="unknown" unit="character"/>ρη υ<unclear>π</unclear><gap reason="lost" extent="unknown" unit="character"/></lem><rdg>Κωτ<gap reason="lost" extent="unknown" unit="character"/></rdg></app> 

    <lb n="3"/>ὀρνιθᾶ<supplied reason="lost">ς</supplied> ἀπὸ Πανοσπό<unclear>λε</unclear>ως <unclear>τ</unclear><supplied reason="lost">ῆς λαμπρο</supplied>τάτης Αὐρηλί<supplied reason="lost">α Ἀπολλωνία </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="4"/>ἀπὸ τῆς α<supplied reason="lost">ὐ</supplied>τῆς πόλεως χαίρειν· <supplied reason="lost">ὁμολογ</supplied>ῶ ἐσχηκέναι παρ<supplied reason="lost">ὰ σοῦ διὰ χειρὸς ἐξ οἴκου χρῆσιν ἔντοκον</supplied> 

    <lb n="5"/>χρυσίνο<supplied reason="lost">υ</supplied>ς πέντε <expan>γί<ex>νονται</ex></expan> <expan>χρ<ex>υσίνοι</ex></expan> <num value="5">ε</num> εἰς διάπρασιν καὶ εἰς καταβολ<supplied reason="lost">ὴν </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> οὓς καί σοι</supplied> 

    <lb n="6"/>ἀποδώσω ὁπόταν εὐπορήσω ἀνυπερθέτως ἐπάναγκ<unclear>ε</unclear><supplied reason="lost">ς </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> καὶ</supplied> 

    <lb n="7"/>ὑποτίθημί σοι τὸ ὑπάρχον μοι καὶ <app type="editorial"><lem resp="BL 1.292"><choice><reg>ἐπιβάλλον</reg><orig>ἐπειβάλ<supplied reason="lost">λ</supplied>ον</orig></choice></lem><rdg>επει ἐὰ<supplied reason="lost">ν</supplied> ἦν</rdg></app> μέρος ἀ<unclear>πὸ</unclear> <supplied reason="lost">μέρους ἡμίσους τῆς ὅλης οἰκίας κοινῆς</supplied> 

    <lb n="8"/>ἐν ῥυμίῳ τυφλῷ ὃ ἔσχον ἀπὸ <expan>κλήρο<ex>υ</ex></expan> τοῦ προκειμένου κατὰ <supplied reason="lost">μετοχὴν τοῦ ἄλλου μέρους ἡμίσους</supplied> 

    <lb n="9"/>τῆς ὅλης οἰκίας κοινῆς πρὸς σὲ τὴν προκειμένην Ἀπολλω<supplied reason="lost">νίαν, γείτονες ἀπηλιώτου τὸ προκεί</supplied>

    <lb n="10" break="no"/>μενόν σοι μέρος, νότου οἰκία Τασάιτος <app type="editorial"><lem resp="BL 7.87">ἀπὸ Ψώνεως</lem><rdg>Απ<gap reason="illegible" quantity="1" unit="character"/>τωνατος</rdg></app>, βο<supplied reason="lost">ρρᾶ </supplied><gap reason="lost" extent="unknown" unit="character"/> 


    <lb n="11"/>Πτολεμαίῳ, λιβὸς τὸ αὐτὸ ῥυμίον τυφλὸν ἢ οἳ ἐὰν ὦσι γ<supplied reason="lost">είτονες πάντοθεν <app type="editorial"><lem resp="BL 1.292">καὶ μὴ ἐξείη μοι</lem><rdg>κυριεύειν σε</rdg></app> ἀπὸ τῆς</supplied> 

    <lb n="12"/>σήμερον τὸ προκείμενον <del rend="erasure">μοι</del> μέρο<supplied reason="lost">ς</supplied> ἀπὸ μέρους ἡμίσο<supplied reason="lost">υς τῆς ὅλης οἰκίας <app type="editorial"><lem resp="BL 1.292"><gap reason="illegible" quantity="16" unit="character"/></lem><rdg><supplied reason="lost">κοινῆς καὶ μὴ ἐξείη</supplied> σ<unclear>οι</unclear></rdg></app> </supplied>

    <lb n="13"/>σαι <supplied reason="lost">ὑ</supplied>ποθέ<unclear>σθα</unclear>ι ἢ <choice><reg>πωλῆσαι</reg><orig>πωλλῆσαι</orig></choice> <unclear>ἢ</unclear> ἄλλην τινα <app type="editorial"><lem resp="BL 1.292"><choice><reg>οἰκονομίαν</reg><orig>οἰκον<supplied reason="lost">ομ</supplied>ε<supplied reason="lost">ί</supplied>αν</orig></choice></lem><rdg>οἰ<unclear>κ</unclear>ο<unclear>ν</unclear><supplied reason="lost">ομίαν</supplied> <unclear>ἀντι</unclear><gap reason="lost" extent="unknown" unit="character"/></rdg></app> 

    <lb n="14"/><supplied reason="lost">π</supplied>ληρ<unclear>ώ</unclear>σω, εἰ δὲ μὴ πληρώσω <app type="editorial"><lem resp="BL 1.292">κυρ<supplied reason="lost">ιεύειν σε τῆς</supplied> πρ<supplied reason="lost">ο</supplied>κει<supplied reason="lost">μένης ὑποθήκης</supplied></lem><rdg>κυρ<supplied reason="lost">ίαν εἶναι τὴν</supplied> πρ<supplied reason="lost">ο</supplied>κει<supplied reason="lost">μένην ὑποθήκην</supplied></rdg></app><supplied reason="lost"> καὶ βεβαιώσω σοι διὰ παν</supplied>

    <lb n="15" break="no"/>τὸς κατὰ πᾶσαν βεβαίωσιν ἀπὸ παντὸς <app type="editorial"><lem resp="BL 1.292"><gap reason="lost" quantity="2" unit="character"/><gap reason="illegible" quantity="2" unit="character"/> του</lem><rdg><supplied reason="lost">κινδύ</supplied><unclear>ν</unclear>ου</rdg></app> ἐπε<unclear>λ</unclear><supplied reason="lost">ευσομένου </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="16"/>ἐ<supplied reason="lost">π</supplied>άναγκες. τὸ δὲ γράμμα τοῦτο τῆς ὑπο<supplied reason="lost">θήκη</supplied>ς ἁπλοῦ<supplied reason="lost">ν σοι ἐξεδόμην πρὸς ἀσφάλειαν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="17"/>τ<gap reason="lost" quantity="2" unit="character"/>ου ὑπὲρ ἐμοῦ <app type="editorial"><lem resp="BL 1.292">ὑπογράφοντος</lem><rdg>ὑπογράψαντοσ</rdg></app> κύριον καὶ βέβ<unclear>α</unclear>ι<unclear>ον</unclear> <unclear>ὡ</unclear>ς <app type="editorial"><lem resp="BL 1.292">ἐν <supplied reason="lost">δημοσίῳ κατακείμενον</supplied></lem><rdg>επ<gap reason="lost" extent="unknown" unit="character"/></rdg></app><supplied reason="lost"> καὶ ἐπερωτη</supplied>

    <lb n="18" break="no"/>θεὶς ὡμολόγησα </ab></div>
