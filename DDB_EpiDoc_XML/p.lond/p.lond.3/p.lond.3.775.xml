         <head n="22682" xml:lang="en">
            <date>AD 567</date>
            <placeName>Oxyrhynchus</placeName>
            <ref n="p.oxy;1;195" type="reprint-from">pg279;POxy1,195</ref>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r" type="textpart"><ab>

    <lb n="1"/><g type="stauros"/> βασιλείας <supplied reason="lost">κ</supplied>αὶ <choice><reg>ὑπατείας</reg><orig><hi rend="diaeresis">ὑ</hi>πατίας</orig></choice> τοῦ θειοτάτου καὶ <expan>εὐ<unclear>σεβεστ</unclear><supplied reason="lost">άτ<ex>ου</ex></supplied></expan> 

    <lb n="2"/>ἡμῶν δ<supplied reason="lost">εσ</supplied>πότο<supplied reason="lost">υ</supplied> <expan>Φλ<ex>αυίου</ex></expan> Ἰ<supplied reason="lost">ο</supplied>υστίν<supplied reason="lost">ο</supplied>υ το<supplied reason="lost">ῦ αἰ</supplied>ωνί<supplied reason="lost">ου Αὐ</supplied>γού<supplied reason="lost">σ</supplied>τ<supplied reason="lost">ου</supplied> 

    <lb n="3"/>καὶ <expan>Αὐτοκρ<ex>άτορος</ex></expan> ἔτους <num value="2">β</num> Θὼθ <num value="14">ιδ</num> <expan>ἰνδ<ex>ικτίονος</ex></expan> <num value="1">α</num> ἐν <app type="editorial"><lem resp="BL 7.86"><expan>Ὀξυρ<ex>ύγχων</ex></expan></lem><rdg><expan>Ὀξυρ<ex>υγχιτῶν</ex></expan></rdg></app> <expan>πόλ<ex>ει</ex></expan>. 

    <lb n="4"/>Φλαουίῳ Ἀπ<supplied reason="lost">ί</supplied>ωνι τῷ πανευφήμῳ καὶ <expan>ὑπερφ<ex>υεστάτῳ</ex></expan> 

    <lb n="5"/>ἀπὸ ὑπάτων ὀρδιναρίων καὶ πατρικίῳ <expan>γεουχ<ex>οῦντι</ex></expan> 

    <lb n="6"/>καὶ <expan>ἐνταῦθ<ex>α</ex></expan> τῇ λαμπρᾷ Ὀξυρυγχιτῶν πόλει διὰ 

    <lb n="7"/>Μηνᾶ οἰκέτου τοῦ ἐπερωτῶντος καὶ <expan>προσπορί<unclear>ζον</unclear><supplied reason="lost">το<ex>ς</ex></supplied></expan> 

    <lb n="8"/>τῷ <hi rend="diaeresis">ἰ</hi>δίῳ δεσπότῳ τῷ αὐτῷ πανευφήμῳ ἀνδρὶ τὴν ἀγωγὴν 

    <lb n="9"/>καὶ ἐνοχὴν <supplied reason="lost">Αὐ</supplied>ρήλιος Ἐπίμαχος ὁ καὶ Ἀπίμα <app type="editorial"><lem resp="BL 1.299">υἱ<supplied reason="lost">ὸ</supplied>ς <unclear>Λ</unclear>αμ<unclear>ί</unclear>ωνος<certainty match=".." locus="value"/></lem><rdg>υἱ<supplied reason="lost">ὸς</supplied> <unclear>Φ</unclear>λαι<supplied reason="lost">ου </supplied><gap reason="lost" quantity="2" unit="character"/>ωνο<supplied reason="lost">ς</supplied></rdg></app> 

    <lb n="10"/>μητρὸς Ἡραίδος ὁρμώμενος ἀπὸ ἐποικίο<unclear>υ</unclear> <choice><reg>Ἑκκαιδεκάτου</reg><orig>Ἑκαιδεκά<unclear>τ</unclear>ου</orig></choice> 

    <lb n="11"/>τοῦ Ὀξυρυγχ<supplied reason="lost">ίτ</supplied>ου <expan><supplied reason="lost">νο</supplied>μο<ex>ῦ</ex></expan> διαφέροντος τῇ <hi rend="diaeresis">ὑ</hi>μῶν <choice><reg>ὑπερφυίᾳ</reg><orig>ὑπερφυείᾳ</orig></choice> 

    <lb n="12"/><unclear>ἐ</unclear><supplied reason="lost">να</supplied>πόγραφο<supplied reason="lost">ς αὐτῆς γ</supplied>ε<supplied reason="lost">ω</supplied><unclear>ρ</unclear><supplied reason="lost">γὸς</supplied> χαίρε<supplied reason="lost">ιν</supplied>. <unclear>χ</unclear>ρε<supplied reason="lost">ίας</supplied> κ<supplied reason="lost">αὶ νῦν <expan>γεναμ<ex>ένης</ex></expan></supplied> 

    <lb n="13"/><supplied reason="lost">εἰς τὴν</supplied> <hi rend="acute">ὑ</hi>πʼ ἐμ<supplied reason="lost">ὲ μηχανὴν καλουμένην </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ἀντ</supplied>

    <lb n="14" break="no"/><supplied reason="lost">λοῦ</supplied>σαν εἰς ἀ<unclear>ρ</unclear><supplied reason="lost">όσιμον γῆν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="15"/><gap reason="lost" extent="unknown" unit="line"/> 
</ab></div><div n="v" type="textpart"><ab>

    <lb n="15"/><g type="stauros"/> <expan>χειρογραφ<ex>ία</ex></expan> Ἐπ<supplied reason="lost">ιμάχου </supplied><gap reason="lost" extent="unknown" unit="character"/> </ab></div></div>
