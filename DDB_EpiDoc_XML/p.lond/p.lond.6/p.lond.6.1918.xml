         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r" type="textpart"><!--milestone unit="4"--><ab>

    <lb n="1"/>τῷ τιμιωτάτῳ καὶ <choice><reg>ἠγαπημένῳ</reg><orig>αγαπημενω</orig></choice> πατρὶ ἄπα <choice><reg><supplied reason="lost">Πα</supplied>

    <lb n="2" type="inWord"/>ιηοῦτι</reg><orig><supplied reason="lost">πα</supplied><hi rend="diaeresis">ι</hi>ηους</orig></choice> <del rend="erasure">ο</del> Χαρί<unclear>σι</unclear>ος πλεῖστα χαίρειν. πρὸ μὲ<supplied reason="lost">ν</supplied> 

    <lb n="3"/>πάντων ἀσπ<supplied reason="lost">ά</supplied>ζομαί <choice><reg>σε</reg><orig>σοι</orig></choice> πολλὰ ἐν <expan>κ<ex>υρί</ex></expan>ῳ, ἀσπά<supplied reason="lost">ζο</supplied><unclear>μ</unclear>αι 

    <lb n="4"/>ἄπα Διόσκορο<supplied reason="lost">ν</supplied> καὶ ἄπα <choice><reg>Τρύφωνα</reg><orig>δρυφων<unclear>α</unclear></orig></choice> καὶ ἄπα <choice><reg>Γερόν

    <lb n="5" type="inWord"/>τιον</reg><orig>κ<unclear>ε</unclear>ρ<unclear>ο</unclear>νδιος</orig></choice> καὶ ἄπα <choice><reg>Ἱέρακα</reg><orig>ιεραξ</orig></choice> καὶ <choice><reg>τοὺς</reg><orig>οι</orig></choice> σὺν ὑμῖν <choice><reg>ἀδελφοὺς</reg><orig>αδελφοι</orig></choice> 

    <lb n="6"/>κατʼ ὄνομα, ἀσπάζομαι <choice><reg>Πετνίριον</reg><orig>πε<unclear>τν</unclear>ιριος</orig></choice> καὶ <choice><reg>Λεωνί

    <lb n="7" type="inWord"/>δην</reg><orig>λεωνιδης</orig></choice> καὶ <choice><reg>Πετθυβέστιον</reg><orig>πετθυβεστιος</orig></choice> καὶ <choice><reg>Παπνούθην</reg><orig>πα<unclear>π</unclear>νουθης</orig></choice> καὶ ἄπα 

    <lb n="8"/>Τουᾶν καὶ ἄπα Πανᾶρε, ἀσπάζ<supplied reason="lost">ο</supplied>μαι <choice><reg>Παπνούθην</reg><orig>παπνουθης</orig></choice> 

    <lb n="9"/><choice><reg>τὸν</reg><orig>ο</orig></choice> <choice><reg>Ἀνταιοπολίτην</reg><orig>ανταιοπολιτης</orig></choice> καὶ <choice><reg>Κολοβὸν</reg><orig>κολοβος</orig></choice> καὶ <choice><reg>τοὺς</reg><orig>οι</orig></choice> <choice><reg>ἀδελ

    <lb n="10" type="inWord"/>φοὺς</reg><orig>αδελφοι</orig></choice> κατʼ <subst><add place="inline">ὄνομα</add><del rend="corrected">ωνομα</del></subst>. <subst><add place="inline"><choice><reg>εἰδὼς</reg><orig>ειδοτες</orig></choice></add><del rend="corrected"><unclear>η</unclear>δοτες</del></subst> οὖν τὴν σπουδήν 

    <lb n="11"/>σου, πάτερ ἄπα Πα<hi rend="diaeresis">ι</hi>ηοῦς, περὶ <subst><add place="inline">πάντων</add><del rend="corrected">παντ<unclear>ο</unclear>ν</del></subst> ἀδελφῶν 

    <lb n="12"/><choice><reg>μάλιστα</reg><orig>μαλλιστα</orig></choice> <choice><reg>ἐμοῦ</reg><orig>εμε</orig></choice>, ἰδο<supplied reason="lost">ὺ ἔπεμψά</supplied> σοι φοινίκια 

    <lb n="13"/>ἀρτάβας δύο <num value="2"/> καὶ ἄλευρα φ<supplied reason="lost">α</supplied>κοῦ ἀρτάβας 

    <lb n="14"/><choice><reg>μίαν</reg><orig>μιας</orig></choice> καὶ πρὸς καὶ <choice><reg>σταφύλια</reg><orig>σταφοιλια</orig></choice> κνίδιον <choice><reg>ἕν</reg><orig>μιαν</orig></choice> <num value="1"/> 

    <lb n="15"/>καὶ <choice><reg>γλυκελαίας</reg><orig>κλοκελεας</orig></choice> κνίδιον <choice><reg>ἕν</reg><orig>μιαν</orig></choice> <num value="1"/>· καὶ εἴ τι 

    <lb n="16"/>χρείαν ἔχεις πέμψον μοι καὶ μὴ <choice><reg><supplied reason="omitted">ἀ</supplied>μέλησα</reg><orig>μ<supplied reason="lost">ελησ</supplied><unclear>α</unclear></orig></choice> 

    <lb n="17"/><surplus>α</surplus> περὶ <choice><reg>ὧν</reg><orig>των</orig></choice> <choice><reg>εἶπόν</reg><orig>ειπεν</orig></choice> σοι· καὶ ἐὰν θέλῃ ὁ <expan>θ<ex>εὸ</ex>ς</expan> <supplied reason="lost" cert="low">τάχ</supplied><unclear>α</unclear>

    <lb n="18"/>ἔρχομαι πρὸς ὑμᾶς ἐν <choice><reg>τάχει</reg><orig>ταχεις</orig></choice>. <choice><reg>ἀσπάζονται</reg><orig><unclear>ασπα</unclear><supplied reason="lost">ζ</supplied><unclear>ετε</unclear></orig></choice> 

    <lb n="19"/><surplus>ται</surplus> Πσαλιοῦς καὶ οἱ σὺν <choice><reg>ἡμῖν</reg><orig>υμιν</orig></choice> πάντας 

