         <head n="16853" xml:lang="en">
            <date>AD 330-340?</date>
            <placeName>Kynopolite</placeName>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r,ctr" type="textpart"><ab>

    <lb n="1"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ἀδελ</supplied><unclear>φ</unclear><supplied reason="lost">ῷ</supplied> 

    <lb n="2"/>Π<supplied reason="lost">αιηοῦτι Ἑριηοῦς ἐν</supplied> <expan>κ<ex>υρί</ex>ῳ</expan> χαίρειν. 

    <lb n="3"/>τ<unclear>οῖ</unclear><supplied reason="lost">ς ἐν </supplied><gap reason="lost" quantity="4" unit="character"/><unclear>ηφθ</unclear>ονει <unclear>σ</unclear>υμφορᾷ παραπεσοῦσιν 

    <lb n="4"/>βοη<supplied reason="lost">θεῖ</supplied><unclear>ν</unclear> <choice><reg>παραγγέλ<supplied reason="omitted">λ</supplied>εται</reg><orig><unclear>π</unclear><supplied reason="lost">α</supplied><unclear>ρ</unclear><supplied reason="lost">α</supplied>γγέλεται</orig></choice> ἡ<unclear>μ</unclear>ῖν ὁ θεῖος λόγος πᾶσι, μάλιστα 

    <lb n="5"/>τοῖς ἀδελφοῖς ἡμῶν. ἐπειδὴ οὖν ὁ ἀδελφὸς ἡμῶν 

    <lb n="6"/>Παμώνθεις περιστάσεσι <supplied reason="omitted">οὐ</supplied> <choice><reg>ταῖς</reg><orig>τε͂ς</orig></choice> <surplus>ς</surplus> τυχούσαις παρα

    <lb n="7" break="no"/>πεσὼν <choice><reg>αἴσχιστα</reg><orig>ἔσχιστα</orig></choice> πέπονθεν ὑπὸ ἀνθρώπων ἀνε

    <lb n="8" break="no"/>λεημόνων καὶ ἀθέων ὥσ<supplied reason="lost">τ</supplied><unclear>ε</unclear> ὡς <choice><reg>ἔπος</reg><orig>ἔπως</orig></choice> εἰπεῖν ἀναγ<g type="apostrophe"/>

    <lb n="9" break="no"/>κασθῆναι τῆς <subst><add place="inline">μακαρίας</add><del rend="corrected">μακαραας</del></subst> ἐλπίδος ἡμῶν ἀποστερη

    <lb n="10" break="no"/>θῆναι, ὅθεν ἐδέησεν ἡμᾶς <choice><reg>ἐπιθεῖναι</reg><orig>ἐπιθεῖνε</orig></choice> διʼ αὐτὰ 

    <lb n="11"/><subst><add place="inline">ταῦ<unclear>τα</unclear></add><del rend="corrected">ταυ<unclear>θα</unclear></del></subst> τὰ γράμματα πρ<unclear>ὸς</unclear> <unclear>τὴ</unclear>ν <choice><reg>ἀδελφότητα</reg><orig>ἀδελφώτητα</orig></choice> 

    <lb n="12"/><subst><add place="inline">ὑμῶν</add><del rend="corrected">ημων</del></subst>, <choice><reg>δηλώσαντας</reg><orig>δηλωσας</orig></choice> τὴν τούτου <choice><reg>πᾶσαν</reg><orig>πασιν</orig></choice> πρᾶξιν, 

    <lb n="13"/><choice><reg>ἵν<supplied reason="omitted">α</supplied></reg><orig><hi rend="diaeresis">ἱ</hi>ν</orig></choice> καὶ <choice><reg>ὑμεῖς</reg><orig>ὑμῖς</orig></choice> <choice><reg>γνοντες</reg><orig>γνωντες</orig></choice> συ<unclear>μ</unclear>βάλλεσθε αὐτῷ μεμνη

    <lb n="14" break="no"/>μ<unclear>έ</unclear>νοι <supplied reason="lost">το</supplied><unclear>ῦ</unclear> <unclear>μακαρ</unclear>ί<unclear>ου</unclear> <unclear>ἀ</unclear>ποστ<unclear>ό</unclear><supplied reason="lost">λ</supplied>ο<unclear>υ</unclear> <choice><reg>λέγοντος</reg><orig>λέγοντες</orig></choice> τοὺς 

    <lb n="15"/>ἀσθεν<unclear>οῦντας</unclear> μὴ παρορᾶ<unclear>ν</unclear> <supplied reason="lost">ο</supplied><unclear>ὐ</unclear> μ<unclear>όνον</unclear> <supplied reason="lost">ἐ</supplied><unclear>ν</unclear> τῇ <choice><reg>πίστει</reg><orig>πίστι</orig></choice> 

    <lb n="16"/>ἀλλα <unclear>κα</unclear><supplied reason="lost">ὶ ἐ</supplied><unclear>ν</unclear> ταῖς <choice><reg>κο<supplied reason="omitted">σ</supplied>μικαῖς</reg><orig>κομικαῖς</orig></choice> <unclear>π</unclear><supplied reason="lost">ρ</supplied>άξεσι. <subst><add place="inline">οὗτ<unclear>ο</unclear>ς</add><del rend="corrected"><gap reason="illegible" quantity="1" unit="character"/>υτο<unclear>ι</unclear>ς</del></subst> <add place="above">γὰρ ὁ</add> ἀδελφὸς 

    <lb n="17"/>ἡμῶν ἔτυχέν ποτε οἰνοπράτης καὶ ἐπὶ <choice><reg>πολὺ</reg><orig>πολλὺ</orig></choice> 

    <lb n="18"/>ἐνοχληθεὶς ὑπὸ τῶν ἐν τῇ αὐτοῦ πατρίδι <choice><reg><expan>ἀρχόντω<ex>ν</ex></expan></reg><orig>ἀρχωντω</orig></choice> 

    <lb n="19"/><del rend="erasure">παρα</del> παρὰ τὴν δύναμιν αὐ<supplied reason="lost">το</supplied>ῦ <choice><reg>εἰσπράτ<supplied reason="omitted">τ</supplied>εσθαι</reg><orig>εἰσπρά<unclear>τε</unclear>σθαι</orig></choice> καὶ ἐκ 

    <lb n="20"/>τούτου ὄγ<g type="apostrophe"/>κον ἀργυρίου δαν<supplied reason="lost">ει</supplied>σάμενος καὶ ταῦτα 

    <lb n="21"/><choice><reg>ἀπαιτηθεὶς</reg><orig>ἀπετηθεὶς</orig></choice> καὶ μὴ δυνάμενος ἀπαντᾶν πρὸς τὰ 

    <lb n="22"/>χρεωστούμενα <subst><add place="inline">ἠναγκάσθη</add><del rend="corrected">ηναν<g type="apostrophe"/>κασθη</del></subst> ὑπὸ τῶν <choice><reg><expan>δανειστῶ<ex>ν</ex></expan></reg><orig>δανιστω</orig></choice> 

    <lb n="23"/>πάντα τὰ ἑαυτοῦ ἄχρι καὶ τῶν <hi rend="diaeresis">ἱ</hi>ματίων τῶν 

    <lb n="24"/>τὴν ἀσχημοσύνην <subst><add place="inline">αὐτοῦ</add><del rend="corrected"><unclear>πο</unclear>ου</del></subst> περισκεπασμένων 

    <lb n="25"/><choice><reg>πωλῆσαι</reg><orig>πολῆσαι</orig></choice>· καὶ τούτων πραθέντων μόγις τὴν <choice><reg>ἡμίσειαν</reg><orig>ἡμίσια<unclear>ν</unclear></orig></choice> 

    <lb n="26"/>τῶν ἀργυρίων <choice><reg>δεδύνηται</reg><orig>τεδύνηται</orig></choice> <choice><reg>περιποιῆσαι</reg><orig>περινοῆσαι</orig></choice> τοῖς <choice><reg>δανεισταῖς</reg><orig>δανισταῖς</orig></choice>, 

    <lb n="27"/>οἵτινες οἱ ἀνελεήμονες ἐκεῖνοι καὶ ἄθεοι ἀπέσπα

    <lb n="28" break="no"/>σαν <subst><add place="inline">τὰ</add><del rend="corrected">αα</del></subst> πά<unclear>ντ</unclear>α τὰ ἑαυτοῦ τέκνα νήπια κομιδῇ. 

    <lb n="29"/>ὅθεν <choice><reg>ἐπιτείνομεν</reg><orig>ἐπιτίνωμεν</orig></choice> πρὸς ὑμᾶς ταυτηνὶ τὴν ἐπιστο

    <lb n="30" break="no"/>λήν, ἀξιοῦ<unclear>ν</unclear>τες ὑ<unclear>μ</unclear><supplied reason="lost">ᾶς σ</supplied>υμβαλέσθαι αὐτῷ εἰς ὃ <unclear>ἐὰ</unclear><supplied reason="lost">ν</supplied>

    <lb n="31"/><unclear>δύ</unclear>να<unclear>σθε</unclear> <choice><reg><supplied reason="lost">δοῦν</supplied>αι</reg><orig><supplied reason="lost">δοῦν</supplied><unclear>ε</unclear></orig></choice> ἵν<supplied reason="lost">α</supplied> <unclear>παρ</unclear>ʼ αὐτ<unclear>ῶ</unclear><supplied reason="lost">ν αὐ</supplied><unclear>τὰ</unclear> <supplied reason="lost">ἀπολάβῃ</supplied> 
</ab></div><div n="r,ms" type="textpart"><ab>

    <lb n="32" rend="perpendicular"/> <gap reason="lost" extent="unknown" unit="character"/>εσ<unclear>θ</unclear>αι υ<hi rend="diaeresis">ἱ</hi>οὶ τοῦ οὐρανίου πατρὸς ἡμ<unclear>ῶ</unclear>ν. 
</ab></div><div n="v" type="textpart"><ab>

    <lb n="33"/><gap reason="lost" extent="unknown" unit="character"/> <unclear>ἡ</unclear> <unclear>ἐ</unclear>μ<unclear>ὴ</unclear> α<unclear>τ</unclear><gap reason="illegible" quantity="1" unit="character"/><unclear>ν</unclear><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><unclear>π</unclear><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><unclear>ρου</unclear>χε<gap reason="illegible" quantity="1" unit="character"/><unclear>ραν</unclear> <unclear>σου</unclear> 

    <lb n="34"/><gap reason="lost" extent="unknown" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><unclear>ω</unclear>ν πάνυ αὐτ<unclear>ῷ</unclear> βοήθησον <choice><reg>ἐπειδὴ</reg><orig>ἐπιδὴ</orig></choice> <unclear>μ</unclear>ά<unclear>λισ</unclear>τα 

    <lb n="35"/><gap reason="lost" extent="unknown" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><unclear>ν</unclear>βη ὁ ἀδελφὸς ὅτι ἥρπασαν τὰ τέκ<unclear>ν</unclear><supplied reason="lost">α</supplied> 

    <lb n="36"/><supplied reason="lost">αὐτοῦ εἰ</supplied><unclear>ς</unclear>  <choice><reg>δουλείαν</reg><orig>δου<supplied reason="lost">λ</supplied>ίαν</orig></choice>. μὴ <unclear>οὖν</unclear> <unclear>ἀμ</unclear><supplied reason="lost">έ</supplied><unclear>λησ</unclear><supplied reason="lost">ο</supplied><unclear>ν</unclear> <unclear>τὸ</unclear> 

    <lb n="37"/><gap reason="lost" extent="unknown" unit="character"/><gap reason="illegible" quantity="1" unit="character"/> <unclear>καὶ</unclear> παντὶ τρόπῳ. 

