         <head n="19710" xml:lang="en">
            <date>AD555</date>
            <placeName>Aphrodito, Aphroditopolis</placeName>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><!--B--><ab>

    <lb n="1"/><g type="chirho"/> βασιλείας <expan>το<ex>ῦ</ex></expan> θειοτάτου ἡ<supplied reason="lost">μ</supplied>ῶν δεσπότου <expan><unclear>Φλ</unclear><ex>αυίου</ex></expan> <unclear>Β</unclear><supplied reason="lost">α</supplied>σιλεί<supplied reason="lost">ου τοῦ αἰωνίου Αὐγούστου Αὐτοκράτορος ἔτους τριακοστοῦ μετὰ τὴν</supplied> 

    <lb n="2"/><hi rend="diaeresis">ὑ</hi>πατείαν <expan>Φλ<ex>αυίου</ex></expan> <expan>Βασιλείο<ex>υ</ex></expan> το<supplied reason="lost">ῦ</supplied> <expan>ἐνδοξοτάτο<ex>υ</ex></expan> ἔτο<supplied reason="lost">υ</supplied>ς πεντεκαι<supplied reason="lost">δεκάτου <unclear>Παχὼν</unclear> ὀγδόῃ ἀρξομένης πέμπτης ἰνδικτίονος.</supplied> 

    <lb n="3"/>Φλαυ<hi rend="diaeresis">ί</hi>ῳ Διοσκόρῳ υ<hi rend="diaeresis">ἱ</hi>ῷ τοῦ τῆς μ<unclear>α</unclear>καρ<supplied reason="lost">ία</supplied>ς μνήμης Ἀ<supplied reason="lost">πολλῶτος Διοσκόρου τῷ θαυμασιωτάτῳ ἀπὸ κώμης</supplied> 

    <lb n="4"/><unclear>Ἀ</unclear>φροδίτης <expan>το<ex>ῦ</ex></expan> Ἀνταιοπολίτου <expan>νομο<ex>ῦ</ex></expan> <expan><unclear>π</unclear><ex>αρὰ</ex></expan> <expan>Αὐρ<unclear>η</unclear><supplied reason="lost">λίο</supplied><ex>υ</ex></expan> Γεωργίου ἐ<supplied reason="lost">κ πατρὸς Ψαίου Φαροοῦτος ἐκ μητρὸς Θεονόης θυγατρὸς</supplied> 

    <lb n="5"/><hi rend="supraline"><unclear>Ἑρ</unclear>μῆ</hi> Βατρῆτος <expan>Δίο<ex>υ</ex></expan> ἀπὸ <expan>κώμ<ex>ης</ex></expan> Ψινάβ<unclear>λ</unclear><supplied reason="lost">α</supplied> τοῦ Πανοπο<supplied reason="lost">λίτου νομοῦ ποιμὴν <expan>χαίρ<ex>ειν</ex></expan>. ὁμολογῶ ἑκουσίως καὶ αὐθαιρέτως</supplied> 

    <lb n="6"/>μεμισθῶσθαι παρὰ τῆς σῆς θαυμασιότ<supplied reason="lost">η</supplied>τος πρὸς ἐν<unclear>ι</unclear><supplied reason="lost">αυσιαῖον χρόνον καρπῶν τῶν νῦν ὄντων τῆς σὺν Θεῷ εἰσιούσης</supplied> 

    <lb n="7"/>πέμπτης ἰνδικτίονος τὰ <hi rend="diaeresis">ὑ</hi>πάρχον<supplied reason="lost">τ</supplied>ά σοι πατρ<unclear>ῷ</unclear><supplied reason="lost">α βοσκήματα </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> τοῦ τε γεωργίου</supplied> 

    <lb n="8"/><expan>ὁλοκλήρο<supplied reason="lost"><ex>υ</ex></supplied></expan> ἀρουρῶν ὅσων ἐστὶν <unclear>μ</unclear><supplied reason="lost">ετὰ</supplied> παντὸς α<supplied reason="lost">ὐτῶν τοῦ δικαίου καλουμένου Πιαϩ Παλλ</supplied><gap reason="lost" quantity="2" unit="character"/><supplied reason="lost"> ἀπὸ <unclear>Ἀκανθῶνος</unclear></supplied> 

    <lb n="9"/>τοῦ <expan>μακαρί<unclear>το</unclear><ex>υ</ex></expan> <expan>Ἡλιο<ex>υ</ex>πολιτευσαμένο<ex>υ</ex></expan> <supplied reason="lost">καὶ τοῦ</supplied> ὁλοκλή<unclear>ρ</unclear><supplied reason="lost">ου γεωργίου λεγομένου </supplied><gap reason="lost" quantity="2" unit="character"/><supplied reason="lost">με </supplied><gap reason="lost" quantity="4" unit="character"/><supplied reason="lost">ν αὐτοῦ τῶν ἀρουρῶν</supplied> 

    <lb n="10"/>κατὰ τὰ ὅρι<supplied reason="lost">α</supplied>, λέγω δὴ τοῦ <expan>γεωργίο<ex>υ</ex></expan> τ<unclear>ῶ</unclear><supplied reason="lost">ν </supplied><expan><supplied reason="lost">κλ</supplied>ηρονόμ<ex>ων</ex></expan> <supplied reason="lost">τοῦ τῆς <expan>ἐνδ<ex>οξοτάτης</ex></expan> <expan>μνήμ<ex>ης</ex></expan> Κο</supplied><gap reason="lost" quantity="3" unit="character"/><supplied reason="lost">του <expan>διακ<ex>ειμένου</ex></expan> ἐν τῇ νοτίνῃ πεδιάδι τῆς <expan><ex>αὐτῆς</ex></expan></supplied> 

    <lb n="11"/>κώμης Ἀφ<supplied reason="lost">ρ</supplied>οδίτης λεγομένῃ <hi rend="diaeresis">Ἱ</hi>ερ<supplied reason="lost">άδος Ἀν</supplied>ανείας <gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="10" unit="character"/><supplied reason="lost">ος ποιμένος ἐκ νότου <app type="editorial"><lem resp="BL 9.147">Ψιλ<unclear>ά</unclear>μπων</lem><rdg>ψιλ<gap reason="illegible" quantity="2" unit="character"/>μπων</rdg></app> καὶ μεμίσθωμαι</supplied> 

    <lb n="12"/>ὁμοίως παρὰ <expan>σο<ex>ῦ</ex></expan> καὶ τὸ ὁλόκληρον β<unclear>ό</unclear><supplied reason="lost">σκημα</supplied> <unclear>τ</unclear>ῆς κατα<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="13"/>κατὰ κέλε<unclear>υ</unclear>σιν <expan>το<ex>ῦ</ex></expan> σοῦ πατρὸς Ἀπολλῶ<supplied reason="lost">το</supplied><unclear>ς</unclear> διακείμεν<supplied reason="lost">ον </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ἀρουρῶν</supplied> 

    <lb n="14"/><app type="editorial"><lem resp="BL 4.46"><choice><reg>δεκανίας</reg><orig>δεκανείας</orig></choice></lem><rdg>δέκα <num value="10"/> Νέ<unclear>ας</unclear></rdg></app> <hi rend="supraline">Φοννέως</hi> λεγομέν<supplied reason="lost" cert="low">ης</supplied><supplied reason="lost"> Ἀ</supplied>θανασίας ἐ<unclear>φ</unclear>ʼ <supplied reason="lost">ᾧ με τὰ <expan>προκ<ex>είμενα</ex></expan> <expan>βοσκ<ex>ήματα</ex></expan> μετὰ παντὸς αὐτῶν τοῦ δικαίου ἔχειν ὑπʼ ἐμὲ</supplied> 

    <lb n="15"/>εἰς βοσκ<supplied reason="lost">ὴ</supplied><unclear>ν</unclear> <unclear>κ</unclear><supplied reason="lost">α</supplied>ὶ εἰς κατανομὴν τ<supplied reason="lost">ῶν</supplied> ἐμ<unclear>ῶν</unclear> θρ<supplied reason="lost">εμμάτων καὶ διδόναι σοι λόγῳ φόρου ἤτοι μισθοῦ ἐτησίας</supplied> 

    <lb n="16"/>νομῆς τ<unclear>ῶ</unclear>ν εἰρημέν<unclear>ω</unclear>ν τριῶν <num value="3"/> <unclear>κτη</unclear>μάτω<unclear>ν</unclear> <unclear>ἀρ</unclear><supplied reason="lost">ουρῶν ὅσων εἰσὶν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="17"/>νομίσμα<supplied reason="lost">τ</supplied><unclear>ο</unclear>ς χρυσοχ<unclear>οι</unclear><supplied reason="lost">κ</supplied><unclear>ῷ</unclear> <expan>σταθμ<ex>ῷ</ex></expan> τῆς αὐτῆς κώ<unclear>μ</unclear><supplied reason="lost">ης Ἀφροδίτης. ἡ <expan>μισθωτικ<ex>ὴ</ex></expan> ὁμολογία κυρία ἔσται καὶ βεβαία</supplied> 

    <lb n="18"/>ἐφʼ <expan><hi rend="diaeresis">ὑ</hi>πογραφ<ex>ῆς</ex></expan> <expan>το<ex>ῦ</ex></expan> <hi rend="acute">ὑ</hi>πὲρ <expan>ἐμο<ex>ῦ</ex></expan> ὑπογράφοντος καὶ τῶ<supplied reason="lost">ν ἑξῆς συνηθῶν ἐπιμαρτυρούντων </supplied><gap reason="lost" quantity="10" unit="character"/><supplied reason="lost"> καὶ ἐφʼ ἅπαντα</supplied> 

    <lb n="19"/>ἐπερωτηθεὶς ὡμολόγησα. <g type="chirho"/> <handShift new="m2"/> <g type="stauros"/> Αὐρήλιος <unclear>Γ</unclear><supplied reason="lost">εώργιος Ψαίου ποιμὴν</supplied> 

    <lb n="20"/>ἀπὸ κώμης Ψινάβλα ὁ προγεγρα<supplied reason="lost">μμένος μεμίσθωμαι ὡς <expan>πρόκ<ex>ειται</ex></expan>. </supplied><gap reason="lost" extent="unknown" unit="character"/>  

    <lb n="21"/>ἀξιωθεὶς ἔγραψα <hi rend="diaeresis">ὑ</hi>πὲρ <expan>αὐτ<supplied reason="lost">ο<ex>ῦ</ex></supplied></expan> γράμματ<supplied reason="lost">α μὴ εἰδότος. <g type="stauros"/> <handShift new="m3"/> </supplied><gap reason="lost" quantity="7" unit="character"/><supplied reason="lost"> μαρτυρῶ</supplied> 

    <lb n="22"/>τῇ μισθώσει ἀκούσας παρὰ Γεωργ<supplied reason="lost">ίου.</supplied> 

    <lb n="23"/><handShift new="m4"/> μαρτυρῶ τῇ μισθώσει ἀκ<supplied reason="lost">ο</supplied>ύσας παρὰ <supplied reason="lost">Γεωργίου.</supplied> 

    <lb n="24"/><handShift new="m5"/> <expan>Ἀβρα<unclear>α</unclear>μίο<ex>υ</ex></expan> μαρτυρῶ τ<unclear>ῇ</unclear> <unclear>μ</unclear>ισθώσει ἀκούσας παρὰ <supplied reason="lost">Γεωργίου.</supplied> 

    <lb n="25"/><handShift new="m6"/> μαρτυρῶ τῇ μισθώσ<unclear>ε</unclear>ι ἀκούσας <unclear>π</unclear>αρὰ Γεωργίου. 

    <lb n="26"/><handShift new="m1"/> <g type="stauros"/> διʼ ἐμο<supplied reason="lost">ῦ Ἑρμαυῶτος νομικοῦ <expan>ἐγράφ<ex>η</ex></expan>.</supplied> </ab></div>
