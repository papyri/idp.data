         <head n="19811" xml:lang="en">
            <date>AD710</date>
            <placeName>Aphrodito, Aphroditopolis</placeName>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r" type="textpart"><ab>

    <lb n="1"/><expan><ex>πε</ex>ρ<ex>ὶ</ex></expan> <expan>ἐκπομπ<ex>ῆς</ex></expan> <expan>να<ex>υτῶν</ex></expan> <expan><ex>καὶ</ex></expan> <expan>τ<ex>ε</ex>χ<ex>νιτῶν</ex></expan> <expan><ex>καὶ</ex></expan> <expan>δαπ<ex>ανῶν</ex></expan> <expan>κού<ex>ρσου</ex></expan> <expan>Αἰγ<ex>ύπτου</ex></expan> <expan>ἰ<ex>νδικτίονος</ex></expan> <num value="9">θ</num>  <foreign xml:lang="ar"> إلى صحب إشقوة في نوتيّة المعبر و الصّنّاع أن يبعث بهم  </foreign>

    <lb n="2"/>ἐν ὀνόματι τοῦ Θεοῦ Κορ<g type="apostrophe"/>ρα <expan>βε<ex>ν</ex></expan> <supplied reason="lost">Σ</supplied>ζε<supplied reason="lost">ριχ σύμ</supplied>βο<supplied reason="lost">υλ</supplied>ος 

    <lb n="3"/>Βασιλείῳ διοικητῇ κώμης Ἀφροδιτώ. εὐχαριστοῦμε<supplied reason="lost">ν</supplied> 

    <lb n="4"/>τῷ Θεῷ καὶ μετὰ ταῦτα<g type="apostrophe"/>. <space extent="unknown" unit="character"/> <choice><reg>κατέλαβον</reg><orig>κατέλαβαν</orig></choice> πρὸς σὲ τὰ ἐντ<supplied reason="lost">ά</supplied>για 

    <lb n="5"/>τῆς διαστολῆς τῶν <choice><reg><supplied reason="omitted">ν</supplied>αυτῶν</reg><orig>αυτῶν</orig></choice> καὶ<g type="apostrophe"/> τεχνιτῶν<g type="apostrophe"/> καὶ τῆς δαπάνης 

    <lb n="6"/>αὐτῶν τε καὶ τῆς τῶν μάχων τοῦ κούρ<g type="apostrophe"/>σο<add place="above">υ</add> Αἰγύπτο<add place="above">υ</add> ἰνδικτίονος 

    <lb n="7"/><supplied reason="lost">ἐνάτης </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="8"/>οὐ<unclear>κ</unclear> <unclear>εξ</unclear><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="2" unit="character"/> <unclear>αὐ</unclear>τ<supplied reason="lost">ο</supplied><unclear>ῖ</unclear><supplied reason="lost">ς </supplied><gap reason="lost" quantity="5" unit="character"/><supplied reason="lost"> δεχόμενος οὖν τὰ παρόντα γράμματα</supplied> 

    <lb n="9"/>ἀπόστειλον <choice><reg>πρωτοτύπως</reg><orig>προτοτύπως<g type="apostrophe"/></orig></choice> διὰ πάσης συντομ<hi rend="diaeresis">ί</hi>ας 

    <lb n="10"/>τὰς δ<hi rend="diaeresis">ι</hi>ασταλείσας διὰ τῆς<g type="apostrophe"/> διοικήσεώς σο<add place="above">υ</add> δαπάνας <choice><reg>πρὸ</reg><orig>πρὼ</orig></choice> <expan>το<ex>ῦ</ex></expan> 

    <lb n="11"/>ἀποβῇ τὸ ὕδωρ τῆς διώρυγος Ἀλεξ<g type="apostrophe"/>ανδρείας καὶ ἀναγκασθῇς 

    <lb n="12"/>δοῦναι τὰ φόρετρα τῶν αὐτῶν δαπαν<g type="apostrophe"/>ῶν ἕως αὐτῆς Ἀλεξανδρεία<supplied reason="lost">ς</supplied> 

    <lb n="13"/>οὐ μὴν ἀλλὰ καὶ τοὺς ναύτας γενναίους καὶ ἐμπείρους. ἐπετρέψαμ<unclear>ε</unclear><supplied reason="lost">ν</supplied> <add place="above"><unclear>γ</unclear><supplied reason="lost">ὰ</supplied><unclear>ρ</unclear></add> 

    <lb n="14"/>τῷ παρόντι ἀποστόλῳ μὴ δοῦναί σοι ἄνεσιν ἕως δʼ ἂν εἰς πλῆρες 

    <lb n="15"/>ἐκπέμψῃς εἴ τί ἐστι διὰ τῆς διοικήσεώς σο<add place="above">υ</add>. λοιπὸν 

    <lb n="16"/>μὴ γένηται ἐμπόδιος δ<hi rend="diaeresis">ι</hi>ὰ <expan>σο<ex>ῦ</ex></expan> καὶ ἐκ μὲν τοῦ σίτο<add place="above">υ</add> καὶ το<add place="above">ῦ</add> <expan>ἄρτο<ex>υ</ex></expan> 

    <lb n="17"/><choice><reg>ὑστερήσῃς</reg><orig>ὑστερέσῃς</orig></choice> τι τὸ σύνολον μήτε μὴν ἀνύσῃς ἐκ τῶν 

    <lb n="18"/>χωρ<hi rend="diaeresis">ί</hi>ων ἀπαρ<g type="apostrophe"/>γυρισ<g type="apostrophe"/>μὸν ὑπὲρ<g type="apostrophe"/> αὐτῶν τὸν ὁτιοῦν εἰ μὴ αὐτὰ τὰ εἴδη. 

    <lb n="19"/>ἐκ δὲ τῶν ὑπολοίπων δαπαν<g type="apostrophe"/>ῶν οἷον εἶδός ἐστι ἐξ αὐτῶν 

    <lb n="20"/>ἐν ἑτοίμῳ <unclear>π</unclear><supplied reason="lost">α</supplied><unclear>ρ</unclear>ὰ τοῖς τῶ<supplied reason="lost">ν</supplied> χωρίω<supplied reason="lost">ν</supplied> εὐθέως πέμψον, ὃ δὲ 

    <lb n="21"/>ἀδύνατό<unclear>ς</unclear> <unclear>ἐ</unclear><supplied reason="lost">σ</supplied>τ<supplied reason="lost">ιν</supplied> δοῦναι ἐν εἴδεσιν<g type="apostrophe"/> ἀνύων τὸν ἀπαρ<g type="apostrophe"/>γυρισμὸν 

    <lb n="22"/>αὐτο<add place="above">ῦ</add> πρὸς τὴν ἐντεθεῖσαν <unclear>δι</unclear><supplied reason="lost">α</supplied>τίμησιν ἐν τοῖ<supplied reason="lost">ς</supplied> ἡ<supplied reason="lost">μ</supplied>ετέρο<supplied reason="lost">ι</supplied>ς 

    <lb n="23"/>ἐνταγίοις καὶ ἀπόστειλον π<g type="apostrophe"/>ρὸς ἡμᾶς διʼ ἀνθρ<g type="apostrophe"/>ώπου σο<add place="above">υ</add> πισ<g type="apostrophe"/>τοῦ 

    <lb n="24"/>το<add place="above">ῦ</add> ὀφείλοντος καταβαλέσθαι ἡμῖν μὴ παρέχων τοῖς <expan>ὑποδεχομ<unclear>έ</unclear><ex>νοις</ex></expan> 


    <lb n="25"/>πιστικοῖς τὰς τοιαύτας δαπάν<g type="apostrophe"/>ας παν<g type="apostrophe"/>τοῖ<unclear>ο</unclear>ν ἀπαργυρισμὸν 

    <lb n="26"/>καὶ μὴ ἀμελήσῃς εἰς τὸ διὰ τάχους πέμψαι τούς τε ναύτας καὶ τεχνίτας 

    <lb n="27"/>καὶ τὰς δαπάνας καὶ δώσῃς κατὰ <expan>σεαυτο<ex>ῦ</ex></expan> πρόφασιν. <expan>ἐγρ<ex>άφη</ex></expan> <expan>μη<ex>νὸς</ex></expan> <expan>Μ<ex>ε</ex>χ<ex>εὶρ</ex></expan> <num value="5">ε</num> <expan>ἰνδ<ex>ικτίονος</ex></expan> <num value="8">η</num> 
</ab></div><div n="v" type="textpart"><ab>

    <lb n="28"/>Κορ<g type="middot"/>ρα <expan><unclear>βε</unclear><ex>ν</ex></expan> Σζεριχ <expan>σύμβο<supplied reason="lost">υ</supplied>λ<ex>ος</ex></expan>  <supplied reason="lost">Βασι</supplied><unclear>λ</unclear><supplied reason="lost">ε</supplied><unclear>ί</unclear><supplied reason="lost">ῳ </supplied><expan><supplied reason="lost">διοικ</supplied><ex>ητῇ</ex></expan> <supplied reason="lost">κώ</supplied><unclear>μ</unclear><supplied reason="lost">ης Ἀ</supplied>φρ<supplied reason="lost">οδ</supplied>ιτώ. 

    <lb n="29" rend="inverse"/> <supplied reason="lost"><expan>μ<ex>ηνὸς</ex></expan></supplied> <expan>Μ<ex>ε</ex><unclear>χ</unclear><ex>εὶρ</ex></expan> <num value="15">ιε</num> <expan>ἰ<ex>ν</ex>δ<ex>ικτίονος</ex></expan> <num value="8">η</num> <choice><reg><expan>ἠνέχ<ex>θη</ex></expan></reg><orig>ἠ<supplied reason="lost">ν</supplied>ηχ</orig></choice> <expan>δ<ex>ιὰ</ex></expan> Σαειδ <expan>βερ<ex>ε</ex>δ<ex>αρίου</ex></expan> <expan><ex>πε</ex>ρ<ex>ὶ</ex></expan> <expan>δαπ<ex>άνης</ex></expan> <expan>μάχ<ex>ων</ex></expan> <expan><ex>καὶ</ex></expan> <expan>ναυτ<ex>ῶν</ex></expan> <expan>κού<ex>ρσου</ex></expan> <expan>Αἰγ<ex>ύ</ex>π<ex>του</ex></expan> <expan>ἰ<ex>ν</ex>δ<ex>ικτίονος</ex></expan> <num value="9">θ</num> </ab></div></div>
      </body>
