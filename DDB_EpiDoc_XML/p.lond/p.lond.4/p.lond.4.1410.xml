    <lb n="5"/><expan>τέκ<supplied reason="lost">τ<ex>ονα</ex></supplied></expan> <supplied reason="lost"><num value="1">α</num></supplied> <unclear>ἕ</unclear><supplied reason="lost">να τ</supplied><add place="above">οῦ</add> <expan><unclear>μ</unclear><ex>ηνὸς</ex></expan> <expan><unclear>νό</unclear><ex>μισμα</ex></expan> <num value="1">α</num> <num value="1/3" rend="tick">γ</num> <expan><unclear>κ</unclear><supplied reason="lost">α</supplied><unclear>λ</unclear><supplied reason="lost">α</supplied><unclear>φ</unclear>ά<ex>την</ex></expan> <num value="1">α</num> ἕν<unclear>α</unclear> τ<add place="above">οῦ</add> <expan>μη<ex>νὸς</ex></expan> <expan>νό<ex>μισμα</ex></expan> <num value="1">α</num> <num value="1/2">𐅵</num> <expan><ex>καὶ</ex></expan> ἐὰν <expan>δώση<ex>τε</ex></expan> <expan>ἀπαρ<supplied reason="lost">γυ<ex>ρισμὸν</ex></supplied></expan><supplied reason="lost"> <expan>παράσχ<ex>ετε</ex></expan></supplied> 

    <lb n="6"/><expan><ex>ὑπὲρ</ex></expan> <expan>μισθ<ex>οῦ</ex></expan> <expan><ex>καὶ</ex></expan> <expan>δαπάνη<ex>ς</ex></expan> <expan>αὐτ<ex>ῶν</ex></expan> ὡς ἀνωτέρω ἐμφέρεται <expan>μό<ex>νον</ex></expan>. <g type="slanting-stroke"/> <g type="slanting-stroke"/> <expan>ἐγρά<ex>φη</ex></expan> <expan>μ<ex>ηνὸς</ex></expan> <expan>Ἁθὺ<ex>ρ</ex></expan> <num value="6">ϛ</num> <expan>ἰν<supplied reason="lost">δ</supplied><ex>ικτίονος</ex></expan> <supplied reason="lost"><num value="8">η</num></supplied> 

