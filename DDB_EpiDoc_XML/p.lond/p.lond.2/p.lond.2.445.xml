    <lb n="10" break="no"/>μένες τῶν ἀπὸ Βακχιάδος 

    <lb n="11"/><gap reason="lost" extent="unknown" unit="character"/>α <choice><reg>τὰ ἄλλα</reg><orig>τὰ<unclear>̓́</unclear>λλα</orig></choice> 

    <lb n="12"/>Ψαμμένσ<supplied reason="lost">ε</supplied>ω<supplied reason="lost">ς ὡς <expan><ex>ἐτῶν</ex></expan></supplied> <num value="36">λϛ</num> ὑπὲρ 

    <lb n="13"/>ὧν καὶ ἐντύχων <unclear>κ</unclear>ατʼ αὐτῶν 

    <lb n="14"/>ἔσχον τὰ <unclear>τ</unclear><supplied reason="lost">ο</supplied><unclear>ύτ</unclear><supplied reason="lost">ων</supplied> <unclear>ἐ</unclear>νέχυρα 

    <lb n="15"/>πρὸς τὸ κομίσε<supplied reason="lost">σ</supplied>θαί με τὸ 

    <lb n="16"/><gap reason="lost" extent="unknown" unit="character"/><unclear>το</unclear>β<gap reason="lost" quantity="1" unit="character"/> π<unclear>άν</unclear>

    <lb n="17" break="no"/>τε<unclear>ς</unclear> <abbr>επαφ<unclear>ρ</unclear></abbr> κα<unclear>ὶ</unclear> τὰ <choice><reg>ἑαυτῶν</reg><orig>ἑατῶν</orig></choice> πρό

    <lb n="18" break="no"/>βατα ἐ<supplied reason="lost">πὶ</supplied> τοὺς λοιποὺς σπόρο<supplied reason="lost">υς</supplied> 

    <lb n="19"/>μετὰ τὸ ἐφοδεῦσαι τὸ βλάβος 

    <lb n="20"/>κατενέμησαν τῶι <choice><reg>ὅλωι</reg><orig>ὥλωι</orig></choice> ἐ<unclear>ν</unclear> 

    <lb n="21"/><gap reason="lost" quantity="2" unit="character"/>ων<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="22"/>διὸ <choice><reg>ἀξιῶ</reg><orig>ἀξι<unclear>ῶ</unclear>ι</orig></choice> <gap reason="lost" quantity="4" unit="character"/>τα<app type="editorial"><lem resp="BL "><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ἀχθῆναι</supplied></lem><rdg><gap reason="lost" extent="unknown" unit="character"/><note xml:lang="en">om</note></rdg></app> 

