    <lb n="21"/><app type="editorial"><lem resp="BL 1.256"><gap reason="lost" quantity="5" unit="character"/>οντες ἐν τῇ εἱρ<unclear>κτ</unclear>ῇ</lem><rdg><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="1" unit="character"/>ο νῦ<unclear>ν</unclear> ἐν τῇ ε<gap reason="illegible" quantity="4" unit="character"/></rdg></app> ε<gap reason="illegible" quantity="4" unit="character"/> διὰ δὲ τοῦτο τῶν γεωργίων ἀφανιζομένων <app type="editorial"><lem resp="BL 1.256"><gap reason="lost" quantity="6" unit="character"/><gap reason="illegible" quantity="2" unit="character"/>α<unclear>ς</unclear><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="1" unit="character"/></lem><rdg><gap reason="lost" quantity="11" unit="character"/></rdg></app> 

