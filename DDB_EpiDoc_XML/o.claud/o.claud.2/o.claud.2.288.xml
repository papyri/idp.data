    <lb n="1"/>κ<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/>

    <lb n="2"/>χαίρειν <gap reason="lost" extent="unknown" unit="character"/>

    <lb n="3"/>κατὰ τὸ ἦθός <gap reason="illegible" quantity="1" unit="character"/><unclear>α</unclear><gap reason="illegible" quantity="1" unit="character"/> <gap reason="lost" extent="unknown" unit="character"/> <choice><reg><supplied reason="lost">πέμ</supplied>

