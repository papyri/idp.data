    <lb n="2"/><gap reason="lost" quantity="4" unit="character"/><supplied reason="lost"> ἦλ</supplied>θον ουχ <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="3"/><gap reason="lost" quantity="5" unit="character"/> σπουδαστὴς <gap reason="illegible" quantity="4" unit="character"/><unclear>δος</unclear> φορια<gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="4"/><gap reason="lost" quantity="3" unit="character"/><gap reason="illegible" quantity="1" unit="character"/> ἵνα τὰ σκεύη <gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="4" unit="character"/>το<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="5"/>ἀποφέρω οὐδὲ <choice><reg>εἰς</reg><orig>ις</orig></choice> τὸ ἐμὸν ἔ<unclear>ρ</unclear><supplied reason="lost">γ</supplied><unclear>ον</unclear> αιτω<unclear>ς</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="6"/>καὶ τὰ ὄντα ὧδε.  ἄλλαι δύο <num value="2"/> ξοίδες <surplus>δύο</surplus> <supplied reason="lost">παρὰ τῷ</supplied> 

    <lb n="7"/>οἰκοδόμῳ εἰσίν.  ἐγὼ οὐδὲν ἔχω ο<unclear>ὐ</unclear><supplied reason="lost" cert="low">δὲ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="8"/>παρὰ Νεφερῶτος ἐγὼ οὐδὲν <choice><reg>ἔλαβον</reg><orig>ελαβα</orig></choice>, ἀλλὰ <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="9"/>αὐτὸς αὐτὰ ἦρκε ἄξειν.  αἱ τρεῖς <num value="3"/> 

