         <head n="18428" xml:lang="en">
            <date>AD 531</date>
            <placeName>Aphrodito, Aphroditopolis</placeName>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r" type="textpart"><ab>

    <lb n="1"/><unclear><g type="stauros"/></unclear> χμγ 

    <lb n="2"/><expan>νο<ex>μισμάτιον</ex></expan> <num value="1"><unclear>α</unclear></num> <expan><unclear>π</unclear><ex>αρὰ</ex></expan> <expan>κ<ex>εράτιον</ex></expan> <num value="1">α</num>, <expan>ἀ<unclear>ρ</unclear><ex>τάβαι</ex></expan> <num value="4">δ</num> 

    <lb n="1"/><g type="chirho"/> <hi rend="diaeresis">ὑ</hi>πατείας Φλαου<hi rend="diaeresis">ί</hi>ων Ὀρέστου καὶ 

    <lb n="2"/>Λαμπαδίου τῶν ἐνδοξοτάτων 

    <lb n="3"/>Μεσορὴ ἑβδόμῃ καὶ <choice><reg>εἰκάδι</reg><orig>εἰκὰς</orig></choice> τῆς <choice><reg>ἐνάτης</reg><orig>ἐννάτης</orig></choice> <expan>ἰνδ<ex>ικτίωνος</ex></expan>. 

    <lb n="4"/><g type="chirho"/> Ἀπολλῶτι τῷ <subst><add place="inline">εὐδοκιμωτάτῳ</add><del rend="corrected">ευδοκιμοτατω</del></subst> <expan>πρωτοκ<ex>ωμήτῃ</ex></expan> 

    <lb n="5"/>κώμης Ἀφροδίτης τοῦ Ἀνταιοπολίτου νομοῦ, 

    <lb n="6"/>υ<hi rend="diaeresis">ἱ</hi>ῷ τοῦ τῆς μακαρίας μνήμης <expan>Διοσκόρο<ex>υ</ex></expan>, 

    <lb n="7"/><g type="chirho"/> Αὐρήλιος Ἀπολλὼς υ<hi rend="diaeresis">ἱ</hi>ὸς Ἀβρααμίου, μητρὸς 

    <lb n="8"/>Θηρακλείας, οἰκῶν ἐν τῇ νοτινῇ πεδιάδι τῆς 

    <lb n="9"/>κώμης Τανυαίθεως τοῦ <choice><reg>Ἀπολλωνοπολίτου</reg><orig>Ἀπολωνοπολίτου</orig></choice> 

    <lb n="10"/>μικροῦ νομοῦ <expan>χ<ex>αί</ex>ρ<ex>ειν</ex></expan>. ὁμολογῶ ἑκὼν καὶ πεπεισμένος 

    <lb n="11"/>μεμισθῶσθαι παρὰ τῆς σῆς εὐδοκιμήσεως 

    <lb n="12"/>πρὸς <hi rend="supraline">ἑξα</hi>ετῆ χρόνον, λογιζόμενον ἀπὸ καρπῶν 

    <lb n="13"/>τῆς εὐτυχῶς ἐσομένης σὺν Θεῷ δεκάτης <expan>ἰνδ<ex>ικτίωνος</ex></expan> 

    <lb n="14"/>καὶ αὐτῆς καὶ τοῦ ἑξῆς <expan>χρόνο<ex>υ</ex></expan>, τὰς <hi rend="diaeresis">ὑ</hi>παρχούσας 

    <lb n="15"/>αὐτῇ ἀρούρας, <choice><reg>ὅσαι</reg><orig>ὅσα</orig></choice> εἰσὶ, διακειμένας ἐπὶ 

    <lb n="16"/><expan>κλήρο<ex>υ</ex></expan> <expan>λεγομένο<ex>υ</ex></expan> <hi rend="supraline">Φήνεως</hi> ἐν τῇ πεδιάδι 

    <lb n="17"/>τῆ<supplied reason="lost">ς <expan><ex>αὐτῆς</ex></expan></supplied> <unclear>κ</unclear>ώμη<unclear>ς</unclear> <expan>Ἀφροδ<ex>ίτης</ex></expan>, κ<unclear>α</unclear>λουμένας <hi rend="supraline">Ψαλεκ</hi>, 

    <lb n="18"/>ἀνύδρου <unclear>γ</unclear>ῆς, εἰς γεωργίαν καὶ εἰς κατάθεσιν 

    <lb n="19"/>ὧν ἐὰν βούλομαι γενημάτων, ἐπʼ ἀποτάκτῳ 

    <lb n="20"/>φ<supplied reason="lost">όρ</supplied>ῳ, ἐφʼ ᾧ με ταύτας γεωργῆσαι καὶ 

    <lb n="21"/><choice><reg>σ<supplied reason="lost">π</supplied>ερμοβολῆσαι</reg><orig>σ<supplied reason="lost">π</supplied>ερμαβολῆσαι</orig></choice> καὶ τὴν πᾶσαν γεωργικὴν 

    <lb n="22"/>ἐργασίαν ποιήσασθαι <hi rend="diaeresis">ἰ</hi>δίοις μου ζῴοις 

    <lb n="23"/>καὶ ἀνα<unclear>λ</unclear><supplied reason="lost">ώ</supplied><unclear>μ</unclear>ασι, και παρασχεῖν τῇ <hi rend="diaeresis">ὑ</hi>μέτερᾳ 

    <lb n="24"/><expan>εὐδοκιμ<ex>ήσει</ex></expan>, <hi rend="diaeresis">ὑ</hi>πὲρ ἀποτάκτου <expan>φόρο<ex>υ</ex></expan> τούτων 

    <lb n="25"/>ἐν τελείῳ καὶ ἀβροχικῷ, ὃ μὴ εἴη, καθʼ ἕκαστον 

    <lb n="26"/>ἐνιαυτὸν ἀπὸ καρπῶν τῆς <expan><ex>αὐτῆς</ex></expan> δεκάτης <expan>ἰνδ<ex>ικτίωνος</ex></expan> καὶ 

    <lb n="27"/>αὐτῆς, ἐπὶ τῶν αὐτῶ<add place="above">ν</add> ἓξ <num value="6"/> <choice><reg>ἐτῶν</reg><orig>ἐτιῶν</orig></choice>, χρυσοῦ νομισμάτιον 

    <lb n="28"/>ἓν παρὰ κεράτιον ἕν <num value="1"/>, καὶ ἀράκου <expan>καθαρο<ex>ῦ</ex></expan> καὶ <expan>φορικο<ex>ῦ</ex></expan> 

    <lb n="29"/>ἀρτάβας τέσσερας, καὶ <hi rend="diaeresis">ὑ</hi>πὲρ τῆς συμπληρώσεως 

    <lb n="30"/>τῶν ἐκφορίων τῶν παρελθόντων χρόνων 

    <lb n="31"/>ἕως τῆς <choice><reg>ἐνάτης</reg><orig>ἐννάτης</orig></choice> <expan>ἰνδ<ex>ικτίωνος</ex></expan>, ἀράκου ἀρτάβας <hi rend="supraline">δέκα</hi> καὶ 

    <lb n="32"/>χρυσοῦ κεράτια <hi rend="supraline">δύο</hi> <expan>χρυσοχ<ex>οϊκῷ</ex></expan>, <expan>γί<ex>νεται</ex></expan> ὁμοῦ <expan>χρ<ex>υσοῦ</ex></expan> <expan>νο<ex>μισμάτιον</ex></expan> <num value="1">α</num> <expan>π<ex>αρὰ</ex></expan> <expan>κερ<ex>άτιον</ex></expan> <num value="1">α</num>, καὶ <expan>κερ<ex>άτια</ex></expan> <num value="2">β</num> 

    <lb n="33"/>καὶ <expan>ἀράκ<ex>ου</ex></expan> <expan><ex>ἀρτάβαι</ex></expan> <num value="14">ιδ</num> <expan>ἀναμφ<ex>ιλόγως</ex></expan>. ἡ <expan>μίσθ<ex>ωσις</ex></expan> κυρία καὶ <expan>βεβ<ex>αία</ex></expan>, καὶ <expan>ἐπερ<ex>ωτηθεὶς</ex></expan> <expan>ὡ<hi rend="superscript">μολ</hi><ex>όγησα</ex></expan> 

    <lb n="34"/><g type="chirho"/> Αὐρήλιο<unclear>ς</unclear> <choice><reg>Ἀπολλὼς</reg><orig>Ἀπολλὸς</orig></choice> <expan>Ἀβρααμί<unclear>ο</unclear><ex>υ</ex></expan> <supplied reason="omitted">ὁ</supplied> 

    <lb n="35"/><expan><unclear>ρ</unclear>ο<unclear>κ</unclear><supplied reason="lost"><ex>είμενος</ex></supplied></expan><choice><reg><supplied reason="lost">συμ</supplied>φωνεῖ</reg><orig><supplied reason="lost">συμ</supplied>φ<unclear>ο</unclear>νοῖ</orig></choice> <choice><reg>μοι</reg><orig>μοιν</orig></choice> <choice><reg>ὣς</reg><orig>ὃς</orig></choice> <expan>πρόκ<ex>ειται</ex></expan> 

    <lb n="36"/><supplied reason="lost"><g type="chirho"/> Αὐρήλι</supplied>ος Ἐνὼχ <choice><reg>Ἰερημία</reg><orig>Ἰερημιας</orig></choice>, <choice><reg>ἀξιω

    <lb n="37" break="no"/><supplied reason="lost">θεὶς</supplied></reg><orig>ἀ<unclear>ξ</unclear>ι<unclear>ο</unclear><lb n="37" break="no"/><supplied reason="lost">θεὶς</supplied></orig></choice>, <supplied reason="lost">ἔγρ</supplied><unclear>α</unclear>ψα ὑπὲρ <choice><reg><expan>αὐτο<ex>ῦ</ex></expan></reg><orig><expan>αὐτοι<ex>ῦ</ex></expan></orig></choice> <supplied reason="omitted">γράμματα</supplied> <choice><reg>μὴ</reg><orig>μὲ</orig></choice> <unclear>εἰ</unclear>δό<add place="above"><unclear>τος</unclear></add> 

    <lb n="38"/><space extent="unknown" unit="character"/> <g type="stauros"/> ἐγράφη <expan>δ<ex>ιὰ</ex></expan> Οὐίκτορος <expan>νομικ<ex>οῦ</ex></expan> 
</ab></div><div n="v" type="textpart"><ab>

