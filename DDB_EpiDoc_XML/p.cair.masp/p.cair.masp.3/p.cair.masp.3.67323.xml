         <head n="36532" xml:lang="en">
            <date>VI spc</date>
            <placeName>Aphrodito &lt; Antinoopolis?</placeName>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r" type="textpart"><ab>

    <lb n="1"/><expan>π<ex>αρὰ</ex></expan> 

    <lb n="1"/><supplied reason="lost"><g type="rho-cross"/> ὁ</supplied> <unclear>δ</unclear>εσπότης μου ὁ ἐνδοξότατος <choice><reg>κόμης</reg><orig>κόμες</orig></choice> <hi rend="supraline">Ἀμμώνιο<supplied reason="lost">ς</supplied></hi><supplied reason="lost"> οὐκ ἠμέλησε</supplied> 

    <lb n="2"/><unclear>δ</unclear>ιὰ Θεὸν τῆς συστ<supplied reason="lost">ά</supplied>σεως τῆς <hi rend="diaeresis"><unclear>ὑ</unclear></hi><unclear>μ</unclear>ῶν κώμης. ἐλάλησε<gap reason="lost" quantity="3" unit="character"/> 

    <lb n="3"/>ζητῆσαι τὴν κατάστασιν τῆς αὐτῆς κώμης, καὶ π<unclear>ρ</unclear><supplied reason="lost">οσέταξε τοῦτο</supplied> 

    <lb n="4"/>γραφῆναι πᾶσι τοῖς ἀπὸ τῆς κώμης καὶ τοῖς ἐξπελλ<unclear>ε</unclear><supplied reason="lost">υταῖς διά τε</supplied> 

    <lb n="5"/>τὴν σὴν θαυμασιότητα καὶ Σενοῦθιν Ἀπολλῶτος κ<unclear>αὶ</unclear> <supplied reason="lost">τοὺς ἄλλους</supplied> 

    <lb n="6"/>πρωτοκωμήτας τῆς αὐτῆς κώμης, καὶ βουλομέν<unclear>ο</unclear><supplied reason="lost">υς, ὑμᾶς μέντοι</supplied> 

    <lb n="7"/>καὶ τοὺς ἄλλους, λέγω δὴ Βότ<g type="apostrophe"/>τον καὶ τὸν υ<hi rend="diaeresis">ἱ</hi>ὸν αὐτοῦ καὶ το<supplied reason="lost" cert="low">ὺς περὶ αὐτὸν</supplied> 

    <lb n="8"/>ἀποστῆναι καὶ σχολάσαι ἑαυτοῖς, φ<unclear>ρ</unclear>ο<supplied reason="lost">ν</supplied>τίζοντας τῷ <supplied reason="lost">ἰδίῳ πράγματι</supplied>. 

    <lb n="9"/>καὶ τὰ τοιαῦτα γράμματα ἀπέ<unclear>στ</unclear>ειλα διὰ τοῦ γραμματη<unclear>φ</unclear><supplied reason="lost">όρου </supplied><gap reason="lost" quantity="3" unit="character"/> 

    <lb n="10"/>τῇ σῇ ἀδελφότητι. ταῦτα οὖν λαβοῦσα, παρασκευάσῃ τ<unclear>ο</unclear><gap reason="lost" quantity="3" unit="character"/> 

    <lb n="11"/>συνάξαι τοὺς ἀπὸ τῆς κώμης καὶ ἐπι<unclear>δ</unclear>οῦναι αὐτοῖς κ<unclear>α</unclear><supplied reason="lost">ὶ </supplied><gap reason="lost" quantity="3" unit="character"/> 

    <lb n="12"/>ἐξπελλευτῶν δοῦναι αὐτῷ τὰ γράμματα τὰ πρὸς αὐτοὺς κ<gap reason="lost" quantity="3" unit="character"/> 

    <lb n="13"/>εἰς τὴν πόλιν καὶ φέρει αὐτοὺς ἄνω καθ<hi rend="diaeresis">ι</hi>στῶντας· ὑ<unclear>μ</unclear><supplied reason="lost" cert="low">εῖς δὲ μετὰ</supplied> 

    <lb n="14"/>τῶν ἐξπελλευτῶν καθʼ ἑαυτοὺς ποιήσατε τὴν χρ<unclear>εί</unclear><supplied reason="lost" cert="low">αν τὴν περὶ</supplied> 

    <lb n="15"/>τὴν κατάστασιν, ἀλλὰ μὴ ἀποστρέψῃς <unclear>τ</unclear><supplied reason="lost">ὰ π</supplied>ράγμ<supplied reason="lost">ατα </supplied><gap reason="lost" quantity="3" unit="character"/> 

    <lb n="16"/>παρὰ τοῦ εἰρημένου μου δεσπότου τοῦ κόμιτος καὶ θ<gap reason="lost" quantity="3" unit="character"/> 

    <lb n="17"/>ἀλλὰ πάντως ἀκολούθησον τῇ προστάξει. τὸν δ<supplied reason="lost">ὲ <app type="editorial"><lem resp="BL 1.450">πάππον σου<certainty match=".." locus="value"/></lem><rdg>Διόσκορον</rdg></app></supplied> 

    <lb n="18"/><hi rend="supraline">Ψιμανώβετ</hi> παραυτὰ ἐκπέμψατε ἐντ<unclear>α</unclear><add place="above">ῦ</add><supplied reason="lost">θα </supplied><gap reason="lost" quantity="11" unit="character"/> 

    <lb n="19"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="3" unit="character"/><unclear>υ</unclear> <unclear>τοῦ</unclear> <unclear>ἐνδ</unclear><supplied reason="lost">ο</supplied><unclear>ξο</unclear>τά<unclear>τ</unclear>ου κ<unclear>όμ</unclear><supplied reason="lost">ιτο</supplied><unclear>ς</unclear> <supplied reason="lost"><g type="rho-cross"/></supplied> 
</ab></div><div n="v" type="textpart"><ab>

