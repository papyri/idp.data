         <head n="19025 19026 59706" xml:lang="en">
            <date>AD 573/4</date>
            <placeName>Aphrodito, Aphroditopolis</placeName>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r" type="textpart"><ab>

    <lb n="1"/><gap reason="lost" extent="unknown" unit="line"/> 

    <lb n="1"/><gap reason="lost" quantity="1" unit="character"/><unclear>ι</unclear><gap reason="lost" quantity="1" unit="character"/><unclear>λ</unclear><gap reason="lost" quantity="5" unit="character"/>νων γ<unclear>ῃδίων</unclear> ἐκ <unclear>λ</unclear><supplied reason="lost">ι</supplied><unclear>βὸ</unclear>ς <unclear>τῆς</unclear> χ<unclear>ε</unclear>ρ<unclear>σαμπέλ</unclear><supplied reason="lost">ου, σὺν χρηστηρίοις <unclear>ἅπασι</unclear></supplied> 

    <lb n="2"/>καὶ <subst><add place="inline">παντοί<unclear>ο</unclear>ις</add><del rend="corrected">παντοιας</del></subst> <del rend="erasure">καὶ</del> <subst><add place="inline">φοίνιξι</add><del rend="corrected">φοινηξι</del></subst> καὶ ληνοπίθῳ καὶ ὁλοκλήρῳ λάκκῳ καὶ μονῇ 

    <lb n="3"/><unclear>καὶ</unclear> πύργ<unclear>ῳ</unclear> <supplied reason="lost">τε καὶ τ</supplied><unclear>ῇ</unclear> <unclear>ἐκ</unclear>εῖ<unclear>σ</unclear>ε ἁ<unclear>γ</unclear>ίᾳ <unclear>ἐ</unclear>κ<unclear>κλησί</unclear><supplied reason="lost">ᾳ</supplied> <subst><add place="inline"><unclear>κ</unclear><supplied reason="lost">αλο</supplied>υμένῃ</add><del rend="corrected"><unclear>κ</unclear><supplied reason="lost">αλο</supplied>υμενου</del></subst> Ἄ<unclear>πα</unclear> <unclear>Π</unclear>όννιτος 

    <lb n="4"/> <gap reason="lost" quantity="17" unit="character"/><supplied reason="lost"> καὶ πᾶσι τοῖς <unclear>συνημμένοις</unclear> τῷ</supplied> <unclear>αὐ</unclear>τῷ κτ<unclear>ήμ</unclear>ατι 

    <lb n="5"/>δικαίοις <unclear>τε</unclear> <unclear>κ</unclear><supplied reason="lost">αὶ </supplied><gap reason="lost" quantity="40" unit="character"/> 

    <lb n="6"/>γείτ<unclear>ο</unclear>ν<supplied reason="lost">ε</supplied><unclear>ς</unclear> δὲ τοῦ <expan>προ<unclear>κ</unclear><ex>ειμένου</ex></expan> <supplied reason="lost">κτήματος <expan>το<ex>ῦ</ex></expan> πραθέντος σοι πα</supplied><unclear>ρ</unclear>ʼ ἐμο<unclear>ῦ</unclear> νῦ<unclear>ν</unclear> 

    <lb n="7"/>μετὰ παντὸς <expan>αὐτο<ex>ῦ</ex></expan> <expan>το<ex>ῦ</ex></expan> δικαίου καὶ τῆς καθόλου περιοχῆς, εἰσ<hi rend="diaeresis">ὶ</hi>ν 

    <lb n="8"/><subst><add place="inline"><supplied reason="lost">ο</supplied><add place="above">ὗ</add><supplied reason="lost">τ</supplied><add place="above">οι</add></add><del rend="corrected"><supplied reason="lost">ο</supplied><add place="above">υ</add><supplied reason="lost">τ</supplied><unclear>η</unclear></del></subst>· <unclear>ἐ</unclear>πὶ μὲν νότου ὅρια τῶν ἀρουρῶν Ὡρου<supplied reason="lost">ω</supplied>γχίου Σενελαλε, 

    <lb n="9"/><supplied reason="lost">ἐπὶ δὲ ἀπη</supplied>λι<supplied reason="lost">ώ</supplied><unclear>τ</unclear>ου γῄδια <expan><unclear>το</unclear><ex>ῦ</ex></expan> γεωργίο<unclear>υ</unclear> τῆς ἁγίας <supplied reason="lost" cert="low">καθολικῆς</supplied><supplied reason="lost"> ἐ</supplied><unclear>κκ</unclear><supplied reason="lost">λ</supplied><unclear>η</unclear><supplied reason="lost">σίας</supplied> 

    <lb n="10"/>Ἄπα Μουσαίου, ἐπὶ δὲ βορρᾶ γῄδια <expan>Ταντβεο<ex>υ</ex></expan> <add place="above">το<unclear>ῦ</unclear></add> <subst><add place="inline"><unclear>Β</unclear><supplied reason="lost">ό</supplied>ττου</add><del rend="corrected"><unclear>β</unclear><supplied reason="lost">ο</supplied>ττος</del></subst> Προμαῶτος, 

    <lb n="11"/>ἐπὶ δὲ λιβὸς δημοσία ὁδὸς τοῦ ὄρους, ἢ ο<unclear>ἷοι</unclear> ἂν <subst><add place="inline">ὦσι</add><del rend="corrected">ωσει</del></subst> γείτονες πάντῃ 

    <lb n="12"/>πάντοθεν· τιμῆς δὲ τῆς πρὸς ἀλλήλους συμπεφωνημένης 

    <lb n="13"/>καὶ σ<unclear>υν</unclear><supplied reason="lost">α</supplied>ρ<supplied reason="lost">εσ</supplied><unclear>ά</unclear>σης τελείας κ<unclear>α</unclear><supplied reason="lost">θαρ</supplied><unclear>οῦ</unclear> <expan><unclear>χρ</unclear>υσο<ex>ῦ</ex></expan> νομίσματα <hi rend="supraline">δώδ</hi>εκα <num value="12"/>, 

    <lb n="14"/>ἕκαστον παρὰ κεράτια <hi rend="supraline">δύο</hi> <num value="2"/>, τ<supplied reason="lost">ῷ</supplied> χρυσοχο<hi rend="diaeresis">ϊ</hi>κῷ σταθμῷ τῆς α<add place="above">ὐ</add>τῆς 

    <lb n="15"/>κώμης. <expan>γί<ex>νεται</ex></expan> <expan>χρ<ex>υσοῦ</ex></expan> <expan>νο<ex>μίσματα</ex></expan> <num value="12">ιβ</num> <expan>π<ex>αρὰ</ex></expan> <expan>κ<ex>εράτια</ex></expan> <num value="24">κδ</num>, <unclear>ἅ</unclear>περ ταῦτα τῆς τιμῆς χρύσινα 

    <lb n="16"/><app type="editorial"><lem resp="BL 1.446, under P.Cair.Masp. I 67079"><supplied reason="lost">ἀπείλη</supplied><unclear>φ</unclear>α</lem><rdg><supplied reason="lost">ἀνείλη</supplied><unclear>φ</unclear>α</rdg></app> ἐγὼ ὁ ἀποδόμενος Ἑρμαύως π<unclear>α</unclear><supplied reason="lost">ρὰ </supplied><expan><supplied reason="lost">σ</supplied><unclear>ο</unclear><ex>ῦ</ex></expan> <supplied reason="lost">τοῦ</supplied> πριαμένου 

    <lb n="17"/><hi rend="diaeresis">Ἰ</hi>σακίου Γεωργίου διὰ χει<unclear>ρός</unclear> <expan><unclear>σ</unclear><supplied reason="lost">ο<ex>υ</ex></supplied></expan> εἰς χεῖράς <expan><unclear>μ</unclear><supplied reason="lost">ο<ex>υ</ex></supplied></expan>, <unclear>π</unclear><supplied reason="lost">αρόντων τῶν ἑξῆς</supplied> 

    <lb n="18"/>ὑπογραφόντων μαρτύρων, ἀριθμῷ <add place="above">καὶ</add> στα<unclear>θ</unclear>μῷ, ἅ<supplied reason="lost">μ</supplied>α <add place="above">τ</add>ῇ παρο<unclear>ύσ</unclear>ῃ πράσει 

    <lb n="19"/><subst><add place="inline">γενομένῃ</add><del rend="corrected">γενομενη<del rend="erasure"><unclear>ς</unclear></del></del></subst> σοι παρʼ <expan>ἐμο<ex>ῦ</ex></expan>, ἐφʼ ᾗτινι εὐδοκῶ καὶ <choice><reg>πείθομαι</reg><orig>πίθωμαι</orig></choice> πρὸς  <choice><reg>τὸ</reg><orig>τὼ</orig></choice> σὲ 

    <lb n="20"/>ἀπεντεῦθεν κρατεῖν καὶ κυριεύειν καὶ δεσπόζειν <expan>το<ex>ῦ</ex></expan> προδηλ<unclear>ω</unclear>θέντος 

    <lb n="21"/>ὁλοκλήρου πατρῴου κτήμ<supplied reason="lost">ατος μετὰ πα</supplied><unclear>ν</unclear>τὸς <expan>αὐτο<ex>ῦ</ex></expan> <expan>το<ex>ῦ</ex></expan> δικαίου καὶ 

    <lb n="22"/>χρηστηρίων ἁπάντων, ἀπὸ τοῦ νῦν ὡς προεῖπον ἐπὶ τὸν ἀεὶ καὶ παντελῆ 

    <lb n="23"/>χρόνον, καὶ ἐξεῖν<unclear>α</unclear><supplied reason="lost">ί</supplied> σοι ἐπʼ αὐτῷ πάντα πράττειν κατὰ πᾶσαν 

    <lb n="24"/><supplied reason="lost">αὐτοτελ</supplied><unclear>ῆ</unclear> ἐ<unclear>ξ</unclear>ουσίαν <choice><reg>τε</reg><orig>δε</orig></choice> καὶ α<add place="above">ὐ</add>θεντίαν, καὶ νέμεσθ<supplied reason="lost">αι τοῦτο</supplied> <unclear>καὶ</unclear> κτήσασθαι 

    <lb n="25"/>καὶ παραπέμπειν ἐπὶ κληρονόμους σοὺς παντοίους <supplied reason="lost">καὶ διαδόχους</supplied> 

    <lb n="26"/>καὶ διακατόχους, ἢ καὶ μεταποιεῖν καὶ ἐκποιεῖν <expan>το<ex>ῦ</ex>το</expan> καὶ χρήσασθαι, 

    <lb n="27"/>καὶ <subst><add place="inline">ἀντικαταλ<add place="above">λ</add>άττ<add place="above">ε</add>σθαι</add><del rend="corrected">αντικαταλαττ<del rend="erasure">η</del>σθαι</del></subst> οἷς βούλει <subst><add place="inline">προσώποις</add><del rend="corrected">προ<del rend="erasure">σ</del>σωποις</del></subst> ἁπαξαπλῶς, παντὶ τρόπῳ 

    <lb n="28"/>κυριευτικῷ χρήσασθαι διὰ παντὸς εἰς τὸ παντελές, ἀκωλύτως καὶ 

    <lb n="29"/>ἀνεμποδίστως καὶ <subst><add place="inline">ἀνεγκλήτ<add place="above">ω</add><supplied reason="lost">ς</supplied></add><del rend="corrected">ανεγκλητο<supplied reason="lost">ς</supplied></del></subst> <supplied reason="lost">ἐπάνα</supplied><unclear>γ</unclear>κες ὑπʼ ο<add place="above">ὐ</add>δενὸς τὸ σύνολον 

    <lb n="30"/>τῶν ἐμοὶ διαφερόντων ἐκ πάσης συνγενείας <expan>μο<ex>υ</ex></expan>. καὶ ἑτοίμως ἔχω ἐγὼ 

    <lb n="31"/>καὶ κληρονόμοι μου παντοῖοι ἀεί ποτε καὶ <subst><add place="inline">διάδοχ<add place="above">οι</add></add><del rend="corrected">διαδοχ<del rend="erasure">η</del></del></subst> καὶ <subst><add place="inline">διακάτοχ<add place="above">οι</add></add><del rend="corrected">διακατοχ<del rend="erasure">η</del></del></subst> βεβαιώσασθαί 

    <lb n="32"/><unclear>σοι</unclear> <unclear>καὶ</unclear> καθ<unclear>α</unclear>ροπ<unclear>οιήσ</unclear>ασθαι, καὶ κληρονόμοις σοῖς <supplied reason="lost">καὶ δια</supplied><unclear>δ</unclear>όχοις καὶ 

    <lb n="33"/>διακατόχοις, τὴν <expan>το<ex>ῦ</ex></expan> <expan>προκ<ex>ειμένου</ex></expan> <expan>ὁλοκλήρο<ex>υ</ex></expan> κ<unclear>τή</unclear><supplied reason="lost">ματο</supplied>ς <unclear>κα</unclear>ὶ πραθέντος σο<unclear>ι</unclear> 

    <lb n="34"/>παρʼ ἐμοῦ καθαρὰν νομήν τε καὶ δεσποτείαν, μὴ <unclear>π</unclear>ρουποκειμένην οἵῳ 

    <lb n="35"/>δήποτε ὀφλήματι δημοσίῳ τε καὶ <hi rend="diaeresis">ἰ</hi>διωτικῷ, ἀλλὰ πᾶσαν ἄδειάν σοι δίδωμι, 

    <lb n="36"/>ἀπὸ τοῦ νῦν διὰ παντὸς εἰς τὸ π<unclear>α</unclear>ντ<supplied reason="lost">ελ</supplied><unclear>έ</unclear>ς, <expan>το<ex>ῦ</ex>το</expan> οἰκονομεῖν καὶ βελ<supplied reason="lost">τ</supplied>ιοῦν, 

    <lb n="37"/>ἢ καὶ φορο<add place="above">λό</add>γειν καὶ ἐκμισθώσασθαι καὶ καρποῦσθαι, καὶ ἀνάξαι εἰς φυτε<unclear>ί</unclear>αν 

    <lb n="38"/>ἀμπέλου τε καὶ παντοίων φυτῶν, καὶ συγκομίσασθαι τοῦτο <choice><reg>κατʼ</reg><orig>καθ’</orig></choice> ἔτος, καὶ εἰσφέρειν 

    <lb n="39"/>τῷ δημοσίῳ λόγῳ τὴν πᾶσαν <expan>αὐτο<ex>ῦ</ex></expan> συντέλειαν ἔν <subst><add place="inline">τ<add place="above">ε</add></add><del rend="corrected">τ<del rend="erasure">η</del></del></subst> σίτῳ ἐμβολῆς καὶ ναύλοις 

    <lb n="40"/>καὶ χρυσικοῖς παντοίοις δημοσίοις τίτλοις, πρὸς <unclear>τ</unclear><supplied reason="lost">ὴν </supplied><app type="editorial"><lem resp="BL 1.446"><supplied reason="lost">ἀπο</supplied><unclear>γρα</unclear>φὴν</lem><rdg><supplied reason="lost">ἀνα</supplied><unclear>γρα</unclear>φὴν</rdg></app> τοῦ 

    <lb n="41"/><supplied reason="lost">δη</supplied>μοσίου κώδικ<unclear>ο</unclear>ς <supplied reason="lost">τ</supplied>οῦ τῆς λογίας μ<supplied reason="lost">ν</supplied>ήμης <hi rend="diaeresis">Ἰ</hi>ωάννου ἀπογενομένου 

    <lb n="42"/><expan>σχολαστικο<ex>ῦ</ex></expan> καὶ κηνσίτορος, καὶ κατὰ μίμησιν πά<unclear>ν</unclear>τ<unclear>ω</unclear><supplied reason="lost">ν</supplied> τῶ<supplied reason="lost">ν</supplied> 

    <lb n="43"/><subst><add place="inline">κτητόρων</add><del rend="corrected">κτητωρων</del></subst> τῆς αὐτῆς κώμης Ἀφροδίτης, ἀποκρίνασθαί σε τὰ 

    <lb n="44"/>παντοῖα <expan>τούτο<ex>υ</ex></expan> βάρη <subst><add place="inline">δι<add place="above">η</add>νεκῶς</add><del rend="corrected">δι<del rend="erasure">ε</del>νε<del rend="erasure">γ</del>κως</del></subst> 

πρὸς τὴν <subst><add place="inline"><choice><reg>ἀνενεχθεῖσαν</reg><orig>ἀν<add place="above">η</add>νεχθεῖσαν</orig></choice></add><del rend="corrected">αν<del rend="erasure">ε</del>νεχθεισαν</del></subst> κατὰ τοῦ αὐτοῦ 

    <lb n="45"/>κτήματος τοῦ αὐτοῦ σοφωτάτου ἀνδρός <app type="editorial"><lem resp="BL 1.446"><supplied reason="omitted">μέτρησιν</supplied></lem><rdg/></app>, ἀρξάμενόν <add place="above">σε</add> τὴν τούτο<add place="above">υ</add> δημοσίου 

    <lb n="46"/>εἰσφορὰν ποιήσασθαι ἀπὸ καρπῶν καὶ κανόνος τῆς σὺν Θεῷ μελλούσης ἕκτης <expan><hi rend="diaeresis">ἰ</hi>νδ<ex>ικτίονος</ex></expan> 

    <lb n="47"/>καὶ αὐτῆς καὶ τοῦ ἑξῆς χρόνου διὰ παντὸς εἰς τὸ παντ<supplied reason="lost">ελές σου καὶ τ</supplied><unclear>ῶν</unclear> σῶν 

    <lb n="48"/><supplied reason="lost">ἀ</supplied><unclear>ε</unclear><supplied reason="lost">ί π</supplied><unclear>ο</unclear>τε κ<supplied reason="lost">ληρονόμων καὶ</supplied> διαδόχων καὶ δι<supplied reason="lost">ακα</supplied>τό<unclear>χ</unclear><supplied reason="lost">ων</supplied>· μηδενὸς ὑμῖν 

    <lb n="49"/><expan>ἐνα<supplied reason="lost">ντ</supplied>ι<unclear>ουμ</unclear>ένο<ex>υ</ex></expan> ἢ <choice><reg><expan>ἀντινοησομένο<ex>υ</ex></expan></reg><orig><expan>ἀναντινοησομένο<ex>υ</ex></expan></orig></choice> πώ<unclear>ποτε</unclear>, μήτε <expan>δυνησομένο<ex>υ</ex></expan> 

    <lb n="50"/><unclear>κ</unclear><supplied reason="lost">ρ</supplied><unclear>ί</unclear>νειν καθʼ <hi rend="diaeresis">ὑ</hi>μῶν περὶ τούτο<add place="above">υ</add> ἐν οἵ<supplied reason="lost">ῳ</supplied> δήποτε καιρῷ ἢ χρόνῳ, ἐν δικαστηρίῳ 

    <lb n="51"/>οἵῳ δήποτε ἢ ἐκτὸς δικαστηρίου, μικροῦ ἢ μεγάλου, περὶ <subst><add place="inline">πλείονος</add><del rend="corrected">πλιωνος</del></subst> 

    <lb n="52"/>ἢ περὶ ἐλάσσονος τιμῆς, <del rend="erasure">περὶ</del> περαιτέρω ταύτης τῆς <choice><reg>προσημανθείσης</reg><orig>προσημανθεῖσαν</orig></choice> 

    <lb n="53"/>καὶ ἀρεσθείσης νῦν μεταξὺ ἡμῶν ἀμφοτέρων, ἀλλὰ καὶ ἑτοίμως ἔχω 

    <lb n="54"/>ἐγώ <choice><reg>τε</reg><orig>δε</orig></choice> καὶ παντοῖοί μου κληρονόμοι ἀποστ<unclear>ρ</unclear>έψ<unclear>αι</unclear> <supplied reason="lost">καὶ ἀποστ</supplied><unclear>ῆ</unclear><supplied reason="lost">σ</supplied>αι 

    <lb n="55"/>πάντα <subst><add place="inline">ἄνθρωπον</add><del rend="corrected">ανθρωπων</del></subst> ἐπελευσόμενον κατὰ <expan>σο<ex>ῦ</ex></expan> τε καὶ κληρονόμων σῶν 

    <lb n="56"/><supplied reason="lost">καὶ διαδόχων, ἢ ἀ</supplied>ντι<supplied reason="lost">ποιη</supplied><unclear>θ</unclear>ησό<unclear>μ</unclear><supplied reason="lost">ενον</supplied> <expan>το<ex>ῦ</ex></expan> προει<unclear>ρ</unclear>η<unclear>μέν</unclear>ου κ<supplied reason="lost">τή</supplied>ματος ἐκ <expan>μέ<unclear>ρο</unclear><ex>υ</ex></expan><unclear>ς</unclear> 

    <lb n="57"/>αὐτο<add place="above">ῦ</add> <unclear>ἢ</unclear> <unclear>ἐκ</unclear> <unclear>κ</unclear>εφαλαίου, <hi rend="diaeresis">ἰ</hi>δίοι<supplied reason="lost">ς ἡ</supplied>μῶν ἀναλώμασι καὶ <subst><add place="inline">δαπανήμασι</add><del rend="corrected">ταπανημασι</del></subst>, κινδύνῳ 

    <lb n="58"/>καὶ πόρῳ τῆς παντο<hi rend="diaeresis">ί</hi>ας μου ὑποστάσ<supplied reason="lost">εως, γ</supplied><unclear>εν</unclear>ικῶς <unclear>καὶ</unclear> <hi rend="diaeresis"><unclear>ἰ</unclear></hi><unclear>δ</unclear>ι<supplied reason="lost">κῶς</supplied> 

    <lb n="59"/>ἐνεχύρου λόγῳ καὶ <hi rend="diaeresis">ὑ</hi>ποθήκης δικαίῳ <hi rend="diaeresis">ὑ</hi>ποκειμένης ὑμῖν εἰς τὴν 

    <lb n="60"/>βεβαίωσιν ἀεί ποτε καὶ καθαροποίησιν ταύτης τῆς παρʼ ἐμοῦ διαπράσεως 

