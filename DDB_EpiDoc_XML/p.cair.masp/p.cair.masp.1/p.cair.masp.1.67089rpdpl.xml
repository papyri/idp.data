    <lb n="1"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="5" unit="character"/><gap reason="illegible" quantity="3" unit="character"/>αρε<gap reason="lost" quantity="13" unit="character"/>μω<gap reason="illegible" quantity="4" unit="character"/><gap reason="lost" quantity="4" unit="character"/> 

    <lb n="2"/>διαξας μὲν τοῦ ἐν ἁγίοις μου π<unclear>α</unclear><supplied reason="lost">τ</supplied><unclear>ρ</unclear>ὸς καιρὸν οὐκ ὀλ<supplied reason="lost">ίγ</supplied><unclear>ο</unclear>ν <unclear>πλείστην</unclear> <gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="1" unit="character"/> 

