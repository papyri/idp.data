    <lb n="1"/><unclear><g type="rho-cross"/></unclear> <expan><unclear>ο</unclear><ex>ὗ</ex></expan> τῶν γραμμ<supplied reason="lost">άτων </supplied><gap reason="lost" quantity="27" unit="character"/> 

