         <head n="5291" xml:lang="en">
            <date>33-`30 BC</date>
            <placeName>Arsinoite</placeName>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><ab>
    <lb n="1"/>Ἁρνῆσις Σενθέ<unclear>ω</unclear>ς καὶ 

    <lb n="2"/><choice><reg>ο<supplied reason="omitted">ἱ</supplied></reg><orig>ο</orig></choice> τούτου υἱοὶ Σενθεὺς καὶ 

    <lb n="3"/>Ὀννῶφρις οἱ <num value="3">γ</num> τῶν <supplied reason="lost">ἀπὸ</supplied> 

    <lb n="4"/>Σετρεμπάειτος καὶ Εὔβ<unclear>ι</unclear><supplied reason="lost">ος</supplied> 

    <lb n="5"/>Πνεφερῶτος Ἁρμιύσει 

    <lb n="6"/>Ψεμ<unclear>π</unclear>νούτιος <choice><reg>προβατο

    <lb n="7" break="no"/>κτηνοτρόφῳ</reg><orig>προβατ<supplied reason="lost">ο</supplied><lb n="7" break="no"/>κτηνοτροφων</orig></choice> <expan>Εὐη<supplied reason="lost">μερε<ex>ίας</ex></supplied></expan> 

    <lb n="8"/>χαίρειν. ἀπέχομεν παρ<supplied reason="lost">ὰ σοῦ</supplied> 

    <lb n="9"/>τὴν τιμὴν τῶν <unclear>χ</unclear><supplied reason="lost">ό</supplied>ρ<unclear>τω</unclear>ν 

    <lb n="10"/>ὧν <app type="editorial"><lem resp="BL 1.388">κατανενέμηκεν</lem><rdg><subst><add place="inline">κατενενέμηκεν</add><del rend="corrected">κατενενεμησι</del></subst></rdg></app> τὰ 

    <lb n="11"/>πρόβατα ὧν γεωργοῦμεν 

    <lb n="12"/>περὶ Σετρεμπάει ἐδαφῶν 

    <lb n="13"/>προσοδικῶν <choice><reg>ἀρουρῶν</reg><orig>ἀρουρας</orig></choice> 

    <lb n="14"/>δέκα ἑπτὰ <num value="17"/> <app type="editorial"><lem resp="BL cf. 7.172">τοῦ <num value="21">κα</num> <supplied reason="lost"><expan><ex>ἔτους</ex></expan></supplied></lem><rdg><choice><reg>τῶν</reg><orig>τ<unclear>ας</unclear></orig></choice> κα<supplied reason="lost">τὰ</supplied></rdg></app>

    <lb n="15"/>σπόρου, καὶ οὐδέν σο<supplied reason="lost">ι ἐγκα</supplied>

    <lb n="16" break="no"/>λοῦμεν. <choice><reg>ἔγρ<supplied reason="omitted">α</supplied>ψεν</reg><orig>ἐγρψεν</orig></choice> ὑ<supplied reason="lost">πὲρ</supplied> 

    <lb n="17"/>αὐτῶν Δίδυμος <unclear>κ</unclear>οιν<unclear>ὸς</unclear> 

    <lb n="18"/>γραμματεὺς <choice><reg>ἀξι<supplied reason="omitted">ω</supplied>θεὶς</reg><orig>ἀξιθεὶς</orig></choice> διὰ τὸ 

    <lb n="19"/><app type="editorial"><lem resp="BL 6.122">φ<supplied reason="lost">άσ</supplied>κειν</lem><rdg><choice><reg>ἡμᾶς</reg><orig>ἡμειν</orig></choice></rdg></app> μὴ ἐπίστασθαι 

    <lb n="20"/>γράμματα. <expan><ex>ἔτους</ex></expan> <num value="2">β</num><gap reason="lost" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 
<milestone rend="paragraphos" unit="undefined"/>
