         <head n="31169" xml:lang="en">
            <date>III spc</date>
            <placeName>Arsinoite</placeName>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><ab>
    <lb n="1"/>Ὡρίων Εὐπόρῳ χαίρειν. 

    <lb n="2"/>ἔλεγέ μοι Κάστωρ ὅ<unclear>τι</unclear> <gap reason="lost" quantity="1" unit="character"/><gap reason="illegible" quantity="2" unit="character"/><unclear>α</unclear>ρελ<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="6" unit="character"/><unclear>θενου</unclear> 

    <lb n="3"/>τὸ ἔμβλημα τοῦ <unclear>κή</unclear>π<supplied reason="lost">ου, ἐ</supplied>ὰν δὲ <unclear>ε</unclear><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="2" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="1" unit="character"/><gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" quantity="1" unit="character"/> 

    <lb n="4"/>ρῆναι εἰς Σεντρεπαεὶ <supplied reason="lost">ε</supplied><unclear>ὕ</unclear>ρῃ τω<gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" quantity="4" unit="character"/>λα <gap reason="illegible" quantity="1" unit="character"/> 

    <lb n="5"/>πάντα ὑπερθέμενο<supplied reason="lost">ς</supplied> παρά<unclear>λαβ</unclear>έ τ<supplied reason="lost" cert="low">ε</supplied><supplied reason="lost"> Κάσ</supplied>το<unclear>ρ</unclear>α 

    <lb n="6"/>καὶ ποίησον ἐκ πανταχ<unclear>όθεν</unclear> <unclear>ἐν</unclear> <unclear>ταύ</unclear>

    <lb n="7" break="no"/>τῃ γενέσθαι εἰς <unclear>τὸ</unclear> <supplied reason="lost">μὴ</supplied> <unclear>ἀ</unclear>με<unclear>λη</unclear>θῆ<unclear>ναι</unclear> <supplied reason="lost">τὸν</supplied> 

    <lb n="8"/><choice><reg>ποτισμὸν</reg><orig>ποτισμος</orig></choice> τοῦ <unclear>κήπ</unclear><supplied reason="lost">ου</supplied> <unclear>τ</unclear>ῆς Σεντρεπα<supplied reason="lost">εί,</supplied> 

    <lb n="9"/>καὶ <choice><reg>ἐπίμεινον</reg><orig>ἐπιμινου</orig></choice> τοῖς ἐκεῖ. δέξαι παρ<unclear>ὰ</unclear> <supplied reason="lost">τῆς</supplied> 

    <lb n="10"/>μητρός μου ἐπὶ λ<unclear>ό</unclear><supplied reason="lost">γου</supplied> <expan><ex>δραχμὰς</ex></expan> <num value="20">κ</num>, <choice><reg>καὶ ἂν</reg><orig>κἂν</orig></choice> π<unclear>άλιν</unclear> <choice><reg>χρῄζῃς</reg><orig>χρῃζεις</orig></choice>, <choice><reg>δηλώσεις</reg><orig>δηλώσις</orig></choice> <supplied reason="lost">μ</supplied>οι, παρʼ ἕκαστα γὰρ 

    <lb n="11"/>τὰ κτήνη ἀνέρχε<supplied reason="lost">ται.</supplied> ἐὰν δέ τις ἀμ<supplied reason="lost">έ</supplied>

    <lb n="12" break="no"/>λεια γένηται, οὐκ ἐ<unclear>μ</unclear><supplied reason="lost">ὲ ο</supplied><unclear>ὖ</unclear>ν <choice><reg>μέμψει</reg><orig>μέμψι</orig></choice> <unclear>ἀλλ</unclear><supplied reason="lost">ὰ</supplied> 

    <lb n="13"/>σεαυτόν. πέμψο<supplied reason="lost">ν</supplied> δὲ πάντως αὐ<supplied reason="lost">τῶν</supplied> 

    <lb n="14"/>τὸ σύμβολον <unclear>τ</unclear><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="2" unit="character"/><gap reason="illegible" quantity="1" unit="character"/>αδασ<unclear>εω</unclear><gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" quantity="2" unit="character"/> 

    <lb n="15"/>πολλάκις Σερῆνο<unclear>ς</unclear> <gap reason="lost" quantity="2" unit="character"/><gap reason="illegible" quantity="2" unit="character"/><unclear>η</unclear>των <supplied reason="lost">ἐ</supplied><unclear>νέ</unclear>

    <lb n="16" break="no"/>δρευσεν ἡμᾶς. ὑπ<unclear>α</unclear>γ<unclear>έ</unclear>τω Κ<unclear>άστ</unclear>ωρ εἰς 

    <lb n="17"/><choice><reg>Φιλαγρίδα</reg><orig>Φιλαγριδαν</orig></choice> ἐπιτη<unclear>δὲς</unclear> <del rend="erasure">ἐπʼ αὐτὸν</del> πρὸς 

    <lb n="18"/>αὐτὸν καὶ ἀπαιτησ<supplied reason="lost">ά</supplied>τω <expan><ex>πυροῦ</ex></expan> <expan><ex>ἀρτάβας</ex></expan> <num value="8">η</num> <num value="1/2">𐅵</num>. ἔρρω<supplied reason="lost">σο.</supplied> 

    <lb n="19"/>Τῦβι <num value="5">ε</num>. 

    <lb n="20"/>πανταχόθεν <hi rend="diaeresis">ἴ</hi>δε αὐ<unclear>τ</unclear>ῷ μικρ<unclear>ὸν</unclear> <unclear>ὀν</unclear>αρίδιον. </ab></div>
