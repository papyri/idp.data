         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r" type="textpart"><ab>

    <lb n="1"/><supplied reason="lost">βασιλεύοντος Πτολεμαίου</supplied> καὶ Κλεοπά<supplied reason="lost">τρ</supplied>ας τῆς καὶ <supplied reason="lost">Τρυφαί</supplied><unclear>ν</unclear><supplied reason="lost">η</supplied>ς 

    <lb n="2"/><supplied reason="lost">τῆς ἀδελ</supplied>φῆ<unclear>ς</unclear> <supplied reason="lost">θεῶν Φιλοπα</supplied>τόρων καὶ <supplied reason="lost">Φιλα</supplied>δέλφων ἔτ<supplied reason="lost">ους τετάρτ</supplied><unclear>ου</unclear> 

    <lb n="3"/><unclear>ἐ</unclear><supplied reason="lost">φʼ ἱερ</supplied>έως Ἀλεξ<unclear>άν</unclear><supplied reason="lost">δρου καὶ</supplied> τῶν ἄλλω<supplied reason="lost">ν τ</supplied>ῶν γραφ<supplied reason="lost">ομένων ἐν Ἀ</supplied>

    <lb n="4" break="no"/>λεξ<supplied reason="lost">α</supplied>νδρείᾳ μην<supplied reason="lost">ὸς Δίου</supplied> ὀγδόηι Θωὺθ ὀγδόηι ἐν Π<unclear>τ</unclear><supplied reason="lost">ο</supplied><unclear>λ</unclear>εμ<supplied reason="lost">αΐδι</supplied> 

    <lb n="5"/>Εὐεργέτιδ<unclear>ι</unclear> <unclear>τ</unclear>οῦ Ἀ<supplied reason="lost">ρσινοείτ</supplied>ου νομοῦ. ὁμολ<supplied reason="lost">ογ</supplied>εῖ Σο<supplied reason="lost">χ</supplied><unclear>ώ</unclear>τη<unclear>ς</unclear> 

    <lb n="6"/>Σεσνώσιος Ἀρ<supplied reason="lost">σινοεί</supplied>της Σωσιβίωι τῶι <supplied reason="omitted">καὶ</supplied> Ψέλ<supplied reason="lost">λ</supplied>ωι καὶ 

    <lb n="7"/>Πτολεμαίωι τ<supplied reason="lost">ῶι κα</supplied><unclear>ὶ</unclear> Πετεσούχωι τοῖς δυ<supplied reason="lost">σ</supplied>ὶ <num value="2"/> Πρωτάρχου 

    <lb n="8"/>τ<supplied reason="lost">ο</supplied>ῦ καὶ Ἁρμάι<supplied reason="lost">ος Μα</supplied><unclear>κ</unclear>εδόσι, καθὰ δὲ συνήλ<supplied reason="lost">λα</supplied>ξαν 

    <lb n="9"/>Π<supplied reason="lost">έ</supplied>ρσαι τῆς ἐ<supplied reason="lost">πιγον</supplied>ῆς ἀπέχειν παρʼ αὐτ<supplied reason="lost">ῶ</supplied>ν ὑπέρ 

    <lb n="10"/>τε αὐτῶν κ<supplied reason="lost">αὶ ὑπὲρ τ</supplied>οῦ ἀδελφοῦ αὐτῶν <supplied reason="lost">Πρ</supplied><unclear>ω</unclear>τάρχου 

    <lb n="11"/>το<supplied reason="lost">ῦ</supplied> καὶ Ἁρμα<supplied reason="lost">ίος τοῦ</supplied> <unclear>Π</unclear>ρωτάρχου χαλκοῦ νομίσματος 

    <lb n="12"/>τάλαντα ὀκτ<supplied reason="lost">ὼ <num value="8"/> δρ</supplied>αχμὰς δισχιλίας πεντ<supplied reason="lost">ακο</supplied>σίας <num value="2500"/> 

    <lb n="13"/>καὶ τοὺς τού<supplied reason="lost">των</supplied> <unclear>τ</unclear>όκους, τὸ δάνειον ὃ ἐδά<supplied reason="lost">νεισε</supplied>ν 

    <lb n="14"/>ὁ Σοχώτης <unclear>τ</unclear><supplied reason="lost">οῖς πρ</supplied><unclear>ο</unclear><supplied reason="lost">γ</supplied><unclear>ε</unclear>γραμμένοις κατ<unclear>ὰ</unclear> <supplied reason="lost">συγγρα</supplied><unclear>φ</unclear>ὴν 

    <lb n="15"/>τὴν <choice><reg>ἀν<supplied reason="omitted">αγ</supplied>εγ<unclear>ρ</unclear><supplied reason="lost">αμμένην</supplied></reg><orig>ἀνεγρ<supplied reason="lost">αμμένην</supplied></orig></choice><supplied reason="lost"> ἐ</supplied>πὶ τὸ α<unclear>ὐ</unclear>τὸ ἀρχε<supplied reason="lost">ῖον τοῦ</supplied> 

    <lb n="16"/>τ<unclear>ρί</unclear>του καὶ τρι<supplied reason="lost">ακοστοῦ</supplied> <unclear>ἔ</unclear>τους Μ<supplied reason="lost">εχ</supplied>εὶρ πέμ<supplied reason="lost">πτηι διʼ ἧ</supplied>ς

    <lb n="17"/>ἐνεγράφησ<unclear>α</unclear><supplied reason="lost">ν ἔγγυ</supplied><unclear>ο</unclear>ι ἀλλήλω<supplied reason="lost">ν</supplied> τοῦ δαν<supplied reason="lost">είου τού</supplied><unclear>του</unclear>, 

    <lb n="18"/>καὶ μὴ ἐπε<unclear>λ</unclear><supplied reason="lost">εύσεται</supplied> μήτε α<unclear>ὐ</unclear><supplied reason="lost">τ</supplied>ὸς Σοχώ<unclear>τη</unclear><supplied reason="lost">ς μήτε ἄ</supplied>λλος 

    <lb n="19"/>ὑπὲρ αὐ<unclear>τ</unclear><supplied reason="lost">οῦ ἐπὶ το</supplied><unclear>ὺ</unclear>ς προγεγραμμένους <unclear>μ</unclear><supplied reason="lost">ήτε α</supplied><unclear>ὐ</unclear>τοὺς 

    <lb n="20"/><unclear>μ</unclear>ήτε <unclear>πε</unclear><supplied reason="lost">ρὶ τοῦ σ</supplied>ημαινομένου δανείου κυ<supplied reason="lost">ρω</supplied>

    <lb n="21" break="no"/><supplied reason="lost">θέντος ὃ ἀπ</supplied>έχει <unclear>μη</unclear>δὲ περὶ ἄλλου μηδ<unclear>ε</unclear><supplied reason="lost">νὸ</supplied><unclear>ς</unclear> 

    <lb n="22"/><supplied reason="lost">τῶν διὰ τῆσδ</supplied><unclear>ε</unclear> τ<unclear>ῆ</unclear><supplied reason="lost">ς σ</supplied>υγγραφῆς σημανθέ<unclear>ν</unclear><supplied reason="lost">των</supplied> 

    <lb n="23"/><gap reason="lost" quantity="5" unit="character"/><supplied reason="lost"> μηδὲ π</supplied><unclear>ερ</unclear>ὶ ἑ<unclear>τέ</unclear>ρου ἁ<unclear>πλ</unclear>ῶς ὀφει<unclear>λ</unclear><supplied reason="lost">ή</supplied><unclear>μ</unclear><supplied reason="lost">ατο</supplied>ς 

    <lb n="24"/><supplied reason="lost">ἢ πράγματος</supplied> ἐ<supplied reason="lost">γ</supplied>γράπτου καὶ ἀγράφου <supplied reason="lost">ἀπὸ τ</supplied>ῶν 

    <lb n="25"/><supplied reason="lost">ἐπάνω χρόν</supplied>ων ἕως τῆς προκει<unclear>μ</unclear><supplied reason="lost">ένης ἡμέρας,</supplied> 

    <lb n="26"/>ἐὰ<unclear>ν</unclear> <unclear>δὲ</unclear> <unclear>ἐ</unclear><supplied reason="lost">πέλθηι</supplied>, ἡ μὲν ἔφ<supplied reason="lost">ο</supplied>δος καὶ ἡ ἔνκλ<supplied reason="lost">ησις αὐ</supplied>τῶι 

    <lb n="27"/>τε Σοχώτηι καὶ <unclear>τ</unclear>ῶι περὶ αὐτοῦ ἐπελθό<unclear>ν</unclear><supplied reason="lost">τι πε</supplied><unclear>ρί</unclear> <unclear>τινα</unclear> 

    <lb n="28"/>το<unclear>ύ</unclear>των κατὰ <unclear>πᾶ</unclear><supplied reason="lost">ν μ</supplied>έρος ἄκυρος ἔστω καὶ π<supplied reason="lost">ροσαπ</supplied><unclear>ο</unclear>

    <lb n="29" break="no"/>τε<supplied reason="lost">ι</supplied>σάτω Σοχώ<supplied reason="lost">τη</supplied>ς τοῖς προγεγραμμέ<supplied reason="lost">νοις</supplied> 

    <lb n="30"/>ἢ <unclear>ὧ</unclear>ι ἂν αὐτῶν ἐ<unclear>πέ</unclear>λθ<unclear>ηι</unclear> παραχρ<supplied reason="lost">ῆ</supplied>μα ἐπίτ<supplied reason="lost">ιμον ἀργυ</supplied><unclear>ρ</unclear>ίου 

    <lb n="31"/>ἐπισήμου δραχμὰς πεντακοσίας <num value="500"/> κ<unclear>αὶ</unclear> τὸ βλά<unclear>β</unclear><supplied reason="lost">ος</supplied>. 

    <lb n="32"/>ἡ συγγραφὴ κυρία πάντηι. 

    <lb n="33"/>διὰ <expan>Πτολ<ex>εμαίου</ex></expan> τοῦ Μαρ<unclear>ρή</unclear>ους 

    <lb n="34"/><handShift new="m2"/> Ἡρακλείδης <expan>κ<supplied reason="lost">ε</supplied>χρη<ex>μάτικα</ex></expan> ἔτους <num value="4">δ</num> Θ<supplied reason="lost">ωὺ</supplied><unclear>θ</unclear> <num value="8">η</num> 

    <lb n="35"/><handShift new="m3"/> Σοχώτης <unclear>Σ</unclear>ε<supplied reason="lost">σ</supplied>νώσιος Ἀρσινοεί<unclear>τ</unclear><supplied reason="lost">η</supplied>ς <choice><reg>ὁμο

    <lb n="36" break="no"/>λογῶ</reg><orig>ὁμο<lb n="36" break="no"/>λογῶι</orig></choice> ἀπέχειν τὸ δάνειον τὰ <unclear>ὀκ</unclear>τὼ <num value="8"/> <unclear>τ</unclear>άλαντα 

    <lb n="37"/>καὶ τὰς δισχιλίας πεντακοσίας <num value="2500"/> δ<unclear>ρα</unclear>χ<unclear>μ</unclear>ὰς 

    <lb n="38"/>τοῦ χαλκοῦ καὶ τοὺς τόκους καὶ ο<supplied reason="lost">ὐ</supplied>κ ἐ<unclear>π</unclear>ε

    <lb n="39" break="no"/>λεύσομαι καθότι προγέγραπται <unclear>κ</unclear><supplied reason="lost">α</supplied><unclear>ὶ</unclear> ἀνενή

    <lb n="40" break="no"/>νοχα τὴν συγγραφὴν ἐπὶ τὸ <unclear>ἀ</unclear><supplied reason="lost">ρχεῖον τῶν</supplied>
<choice><reg>δε<lb n="41" break="no"/>δανεισμένων</reg><orig>δε<lb n="41" break="no"/>δανημένω<unclear>ν</unclear></orig></choice>. ἔγραψεν ὑπ<supplied reason="lost">ὲρ αὐ</supplied><unclear>τ</unclear>οῦ 

    <lb n="42"/>Πτολεμαῖος Π<supplied reason="lost">τ</supplied>ολεμαίου ἀξ<unclear>ι</unclear><supplied reason="lost">ωθεὶ</supplied>ς διὰ 

    <lb n="43"/>τὸ φάσκειν αὐτ<supplied reason="lost">ὸ</supplied><unclear>ν</unclear> μὴ ἐπίστασ<unclear>θ</unclear><supplied reason="lost">αι γρά</supplied>μματα. 
