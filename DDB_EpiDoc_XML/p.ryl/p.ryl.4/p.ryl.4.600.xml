         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r" type="textpart"><!--milestone unit="4"--><ab>

    <lb n="1"/>Ποσιδωνίῳ τῷ <choice><corr>προνοοῦντι</corr><sic>πρωνοουντι</sic></choice> τῶν Θεα

    <lb n="2" type="inWord"/>νοῦτος τῆς Ἀλεξάνδρου παρὰ Ἀμμω

    <lb n="3" type="inWord"/>νίου τοῦ Ἀρ<unclear>χε</unclear><supplied reason="lost">δή</supplied>μου καὶ <unclear>Δ</unclear><supplied reason="lost">ω</supplied><unclear>ρίωνος</unclear> <unclear>τ</unclear><supplied reason="lost">οῦ</supplied> 

    <lb n="4"/>Δωρίωνος τῶ<supplied reason="lost">ν</supplied> <choice><corr>δ<supplied reason="lost">ύ</supplied>ο</corr><sic><unclear>δ</unclear><supplied reason="lost">υ</supplied>ω</sic></choice> <num value="2"/>. <choice><corr>βουλόμ<supplied reason="lost">εθα</supplied></corr><sic><unclear>β</unclear>ολο<unclear>μ</unclear><supplied reason="lost">εθα</supplied></sic></choice><choice><corr><supplied reason="lost">μισθώ</supplied>

    <lb n="5" type="inWord"/>σασθαι</corr><sic><supplied reason="lost">μισθω</supplied>σεσθ<unclear>α</unclear>ι</sic></choice> τὸν ὑ<supplied reason="lost">πάρ</supplied><unclear>χοντα</unclear> <unclear>τ</unclear><supplied reason="lost">ῇ Θεανοῦτι</supplied> 

    <lb n="6"/><choice><corr>ἐλαιῶνα</corr><sic>ελεωνος</sic></choice> <choice><corr>νεόφυτον</corr><sic>νεωφυτου</sic></choice> περ<supplied reason="lost">ὶ</supplied> Καρανί<supplied reason="lost">δα</supplied> 

    <lb n="7"/>τοὺς καρποὺς <supplied reason="lost">τ</supplied>ο<unclear>ῦ</unclear> <choice><corr>δευτέρου</corr><sic>δετερου</sic></choice> καὶ ε<unclear>ἰκ</unclear><supplied reason="lost">ο</supplied>σ

    <lb n="8" type="inWord"/>τοῦ <expan><ex>ἔτους</ex></expan> Καίσα<supplied reason="lost">ρ</supplied>ος <choice><corr>ἐκπίπτοντας</corr><sic>εκπεπτωκωτας</sic></choice> 

    <lb n="9"/>εἰς τὸ <choice><corr>τρίτον</corr><sic>τριτου</sic></choice> καὶ <choice><corr>εἰκοστὸν</corr><sic>εικοστου</sic></choice> <expan><ex>ἔτος</ex></expan> <expan>Καίσαρ<ex>ος</ex></expan> 

    <lb n="10"/>φόρου τοῦ <supplied reason="lost">πα</supplied><unclear>ντὸς</unclear> ἀργυρίου <choice><corr>δραχμῶν</corr><sic>δραχμ<unclear>ας</unclear></sic></choice> <choice><corr><supplied reason="lost">τε</supplied>

    <lb n="11" type="inWord"/>σσάρακοντα</corr><sic>τεσσαρκοντα</sic></choice> <num value="40"/> ἃς καὶ <choice><corr>ἀποδώσομεν</corr><sic>αποδωσωμε<unclear>ν</unclear></sic></choice> <unclear>ἐ</unclear><supplied reason="lost">ν</supplied> 

    <lb n="12"/>μηνὶ Ἁθὺρ <unclear>ἕως</unclear> τριακάδος <unclear>ὄντ</unclear><supplied reason="lost">ων ἀλλή</supplied>

    <lb n="13" type="inWord"/>λων <choice><corr>ἐγγύων</corr><sic>ενγυν</sic></choice> εἰς ἔκτισιν ἄνευ πάση<supplied reason="lost">ς ὑπερ</supplied>

    <lb n="14" type="inWord"/>θέσ<unclear>εω</unclear>ς καὶ <choice><corr>εὑρησιλογίας</corr><sic>ερησιλογιας</sic></choice> καὶ <choice><corr>ἐξεῖναί</corr><sic>εξιναι</sic></choice> <supplied reason="lost">σοι</supplied> 

    <lb n="15"/>τῷ <del rend="erasure"><gap reason="illegible" quantity="1" unit="character"/></del> <choice><corr>πλέον</corr><sic>πληων</sic></choice> <choice><corr>διδόντι</corr><sic>διδωντι</sic></choice> <choice><corr>μεταμισθοῦν</corr><sic>μεταμισθυν</sic></choice>, τ<unclear>ὸ</unclear><supplied reason="lost">ν</supplied> 

    <lb n="16"/>δὲ κατασπασμὸν <choice><corr>ποιησόμεθα</corr><sic>ποησωμεθα</sic></choice> ἐ<supplied reason="lost">ν</supplied> 

    <lb n="17"/>Ἁθὺρ μηνὶ τοῦ τρίτου καὶ εἰκοσ<unclear>τ</unclear><supplied reason="lost">οῦ <expan><ex>ἔτους</ex></expan></supplied>

    <lb n="18"/>Καίσαρος.   <expan><ex>ἔτους</ex></expan> <num value="23">κγ</num> <expan>Καίσαρ<ex>ος</ex></expan> <choice><corr>Φαῶφι</corr><sic>φωφι</sic></choice> <num value="20">κ</num>. 

    <lb n="19"/><handShift new="m2"/> Δωρίων <choice><corr>Δωρίωνος</corr><sic>δωριονος</sic></choice> ὁμολογῶ συμμεμισθ<unclear>ῶ</unclear><supplied reason="lost">σθαι</supplied> 

    <lb n="20"/><choice><corr>ἀκολούθως</corr><sic>ακλουθως</sic></choice> τοῖς προκειμένοις πᾶσι καὶ συνεκτ<supplied reason="lost">είσω</supplied> 

    <lb n="21"/>τὸν φόρον <supplied reason="omitted">ὃν</supplied> καὶ <choice><corr>συμπέπεισμαι</corr><sic>συνπεπισμε</sic></choice>. <expan><ex>ἔτους</ex></expan> <num value="23">κγ</num> Καίσαρος 

    <lb n="22"/><choice><corr>Φαῶφι</corr><sic>φαιωφι</sic></choice> εἰκάδι. 

    <lb n="23"/><handShift new="m3"/> Ποσειδώνιος ὁ προνοῶν τῶν Θεανιο<supplied reason="lost">ῦτος</supplied> 

    <lb n="24"/>τῆς Ἀλεξάνδρου <choice><corr>ἐπικεχώρηκα</corr><sic>επεικεχωρηκα</sic></choice> μ<unclear>ι</unclear><supplied reason="lost">σθοῦν</supplied> 

    <lb n="25"/><choice><corr>ἐπὶ</corr><sic>επει</sic></choice> τοῖς προκειμένοις <choice><corr>πᾶσι</corr><sic>πασει</sic></choice> καὶ ἐ<supplied reason="lost">ξέσ</supplied>

    <lb n="26" type="inWord"/>ται μοι τῶι τὸ <choice><corr>πλὲον</corr><sic><unclear>πλ</unclear>ην</sic></choice> <supplied reason="omitted">διδόντι</supplied> <choice><corr>μεταμισθοῦν</corr><sic>μεταμισθοιν</sic></choice>. 


