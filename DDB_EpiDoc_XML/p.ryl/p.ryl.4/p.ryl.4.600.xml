         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r" type="textpart"><!--milestone unit="4"--><ab>

    <lb n="1"/>Ποσιδωνίῳ τῷ <choice><reg>προνοοῦντι</reg><orig>πρωνοουντι</orig></choice> τῶν Θεα

    <lb n="2" type="inWord"/>νοῦτος τῆς Ἀλεξάνδρου παρὰ Ἀμμω

    <lb n="3" type="inWord"/>νίου τοῦ Ἀρ<unclear>χε</unclear><supplied reason="lost">δή</supplied>μου καὶ <unclear>Δ</unclear><supplied reason="lost">ω</supplied><unclear>ρίωνος</unclear> <unclear>τ</unclear><supplied reason="lost">οῦ</supplied> 

    <lb n="4"/>Δωρίωνος τῶ<supplied reason="lost">ν</supplied> <choice><reg>δ<supplied reason="lost">ύ</supplied>ο</reg><orig><unclear>δ</unclear><supplied reason="lost">υ</supplied>ω</orig></choice> <num value="2"/>. <choice><reg>βουλόμ<supplied reason="lost">εθα</supplied></reg><orig><unclear>β</unclear>ολο<unclear>μ</unclear><supplied reason="lost">εθα</supplied></orig></choice><choice><reg><supplied reason="lost">μισθώ</supplied>

    <lb n="5" type="inWord"/>σασθαι</reg><orig><supplied reason="lost">μισθω</supplied>σεσθ<unclear>α</unclear>ι</orig></choice> τὸν ὑ<supplied reason="lost">πάρ</supplied><unclear>χοντα</unclear> <unclear>τ</unclear><supplied reason="lost">ῇ Θεανοῦτι</supplied> 

    <lb n="6"/><choice><reg>ἐλαιῶνα</reg><orig>ελεωνος</orig></choice> <choice><reg>νεόφυτον</reg><orig>νεωφυτου</orig></choice> περ<supplied reason="lost">ὶ</supplied> Καρανί<supplied reason="lost">δα</supplied> 

    <lb n="7"/>τοὺς καρποὺς <supplied reason="lost">τ</supplied>ο<unclear>ῦ</unclear> <choice><reg>δευτέρου</reg><orig>δετερου</orig></choice> καὶ ε<unclear>ἰκ</unclear><supplied reason="lost">ο</supplied>σ

    <lb n="8" type="inWord"/>τοῦ <expan><ex>ἔτους</ex></expan> Καίσα<supplied reason="lost">ρ</supplied>ος <choice><reg>ἐκπίπτοντας</reg><orig>εκπεπτωκωτας</orig></choice> 

    <lb n="9"/>εἰς τὸ <choice><reg>τρίτον</reg><orig>τριτου</orig></choice> καὶ <choice><reg>εἰκοστὸν</reg><orig>εικοστου</orig></choice> <expan><ex>ἔτος</ex></expan> <expan>Καίσαρ<ex>ος</ex></expan> 

    <lb n="10"/>φόρου τοῦ <supplied reason="lost">πα</supplied><unclear>ντὸς</unclear> ἀργυρίου <choice><reg>δραχμῶν</reg><orig>δραχμ<unclear>ας</unclear></orig></choice> <choice><reg><supplied reason="lost">τε</supplied>

    <lb n="11" type="inWord"/>σσάρακοντα</reg><orig>τεσσαρκοντα</orig></choice> <num value="40"/> ἃς καὶ <choice><reg>ἀποδώσομεν</reg><orig>αποδωσωμε<unclear>ν</unclear></orig></choice> <unclear>ἐ</unclear><supplied reason="lost">ν</supplied> 

    <lb n="12"/>μηνὶ Ἁθὺρ <unclear>ἕως</unclear> τριακάδος <unclear>ὄντ</unclear><supplied reason="lost">ων ἀλλή</supplied>

    <lb n="13" type="inWord"/>λων <choice><reg>ἐγγύων</reg><orig>ενγυν</orig></choice> εἰς ἔκτισιν ἄνευ πάση<supplied reason="lost">ς ὑπερ</supplied>

    <lb n="14" type="inWord"/>θέσ<unclear>εω</unclear>ς καὶ <choice><reg>εὑρησιλογίας</reg><orig>ερησιλογιας</orig></choice> καὶ <choice><reg>ἐξεῖναί</reg><orig>εξιναι</orig></choice> <supplied reason="lost">σοι</supplied> 

    <lb n="15"/>τῷ <del rend="erasure"><gap reason="illegible" quantity="1" unit="character"/></del> <choice><reg>πλέον</reg><orig>πληων</orig></choice> <choice><reg>διδόντι</reg><orig>διδωντι</orig></choice> <choice><reg>μεταμισθοῦν</reg><orig>μεταμισθυν</orig></choice>, τ<unclear>ὸ</unclear><supplied reason="lost">ν</supplied> 

    <lb n="16"/>δὲ κατασπασμὸν <choice><reg>ποιησόμεθα</reg><orig>ποησωμεθα</orig></choice> ἐ<supplied reason="lost">ν</supplied> 

    <lb n="17"/>Ἁθὺρ μηνὶ τοῦ τρίτου καὶ εἰκοσ<unclear>τ</unclear><supplied reason="lost">οῦ <expan><ex>ἔτους</ex></expan></supplied>

    <lb n="18"/>Καίσαρος.   <expan><ex>ἔτους</ex></expan> <num value="23">κγ</num> <expan>Καίσαρ<ex>ος</ex></expan> <choice><reg>Φαῶφι</reg><orig>φωφι</orig></choice> <num value="20">κ</num>. 

    <lb n="19"/><handShift new="m2"/> Δωρίων <choice><reg>Δωρίωνος</reg><orig>δωριονος</orig></choice> ὁμολογῶ συμμεμισθ<unclear>ῶ</unclear><supplied reason="lost">σθαι</supplied> 

    <lb n="20"/><choice><reg>ἀκολούθως</reg><orig>ακλουθως</orig></choice> τοῖς προκειμένοις πᾶσι καὶ συνεκτ<supplied reason="lost">είσω</supplied> 

    <lb n="21"/>τὸν φόρον <supplied reason="omitted">ὃν</supplied> καὶ <choice><reg>συμπέπεισμαι</reg><orig>συνπεπισμε</orig></choice>. <expan><ex>ἔτους</ex></expan> <num value="23">κγ</num> Καίσαρος 

    <lb n="22"/><choice><reg>Φαῶφι</reg><orig>φαιωφι</orig></choice> εἰκάδι. 

    <lb n="23"/><handShift new="m3"/> Ποσειδώνιος ὁ προνοῶν τῶν Θεανιο<supplied reason="lost">ῦτος</supplied> 

    <lb n="24"/>τῆς Ἀλεξάνδρου <choice><reg>ἐπικεχώρηκα</reg><orig>επεικεχωρηκα</orig></choice> μ<unclear>ι</unclear><supplied reason="lost">σθοῦν</supplied> 

    <lb n="25"/><choice><reg>ἐπὶ</reg><orig>επει</orig></choice> τοῖς προκειμένοις <choice><reg>πᾶσι</reg><orig>πασει</orig></choice> καὶ ἐ<supplied reason="lost">ξέσ</supplied>

    <lb n="26" type="inWord"/>ται μοι τῶι τὸ <choice><reg>πλὲον</reg><orig><unclear>πλ</unclear>ην</orig></choice> <supplied reason="omitted">διδόντι</supplied> <choice><reg>μεταμισθοῦν</reg><orig>μεταμισθοιν</orig></choice>. 


    <lb n="27"/><expan><ex>ἔτους</ex></expan> τρίτου καὶ <choice><reg>εἰκοστοῦ</reg><orig>ικοστου</orig></choice> Καίσαρος Ἁθὺρ <num value="2">β</num>. 
