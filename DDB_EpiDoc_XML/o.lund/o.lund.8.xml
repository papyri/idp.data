    <lb n="1"/><choice><reg><expan>συναίρε<ex>μα</ex></expan></reg><orig><expan>συναίρη<ex>μα</ex></expan></orig></choice> <expan>θησ<ex>αυροῦ</ex></expan> <expan>Μεμνο<ex>νείων</ex></expan> <expan>γενή<ex>ματος</ex></expan> <num value="20">κ</num> <expan><ex>ἔτους</ex></expan> <expan>ὑπ<ex>ὲρ</ex></expan> 

