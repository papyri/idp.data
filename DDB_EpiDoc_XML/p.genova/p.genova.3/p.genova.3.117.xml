    <lb n="3"/><gap reason="illegible" quantity="3" unit="character"/> μοὶ Νικόλαος παρακ<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="4"/><gap reason="lost" extent="unknown" unit="character"/> ἐν ἧι ἦν σκεύη τα<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="5"/><gap reason="lost" extent="unknown" unit="character"/><gap reason="illegible" quantity="1" unit="character"/>είλετό μοι· ἐὰν α<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="6"/>σὺν τοῖς <choice><reg>σκεύεσιν</reg><orig>σκεύησιν</orig></choice> καὶ <unclear>ταῖς</unclear> <unclear>ἐ</unclear>ρια<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="7"/>υἱῶι ὧι ἐγκαλεῖ Εὐβίοτος <unclear>ὥσ</unclear>τε ς<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="8"/>κα τὴν παρακαταθήκην λε<gap reason="lost" extent="unknown" unit="character"/> 

