    <lb n="1,msup"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="low">Ἰβι</supplied><unclear>ῶνος</unclear> <expan><unclear>Ταυ</unclear><supplied reason="lost">κ</supplied>ελ<unclear>μ</unclear>ε<ex>ος</ex></expan> 

    <lb n="1"/><handShift new="m2"/> Ἑλλοῦς Τριαδέλφου <choice><reg>μετὰ</reg><orig>μαιτα</orig></choice> κυρίου 

