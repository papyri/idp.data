         <head n="15206" xml:lang="en">
            <date>ca. AD 267</date>
            <placeName>Oxyrhynchus?</placeName>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><ab>
    <lb n="1"/>Καλπουρνίῳ Ὡ<unclear>ρ</unclear><supplied reason="lost">ίωνι </supplied><gap reason="lost" quantity="14" unit="character"/> 

    <lb n="2"/>Ἀμμωνίωνι α<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="15" unit="character"/> 

    <lb n="3"/>παρὰ Αὐρηλίου Σαρα<unclear>π</unclear><gap reason="lost" quantity="13" unit="character"/> 

    <lb n="4"/>ωνος τέκτονος ἀ<unclear>π</unclear>ʼ <supplied reason="lost" cert="low">Ὀξυρύγχων πόλεως </supplied>. 

    <lb n="5"/>ἐπεὶ ὁ λαμπρότατος <unclear>ἡμ</unclear><supplied reason="lost">ῶν ἡγεμὼν</supplied> 

    <lb n="6"/><hi rend="diaeresis">Ἰ</hi>ουουένιος Γενιάλιος <unclear>ἐκ</unclear><supplied reason="lost">έλευσεν τὰ </supplied><gap reason="lost" quantity="5" unit="character"/> 

    <lb n="7"/>πα ἐν τῷδε τῷ νομῷ κα<gap reason="lost" quantity="14" unit="character"/> 

    <lb n="8"/>ἥ τε κρατίστη βουλὴ ἐδοκί<unclear>μ</unclear><supplied reason="lost">ασεν τὴν τιμὴν ἐξο</supplied>

    <lb n="9" break="no"/>διασθῆναι τῶν εἰς ταῦτ<unclear>α</unclear> <gap reason="lost" quantity="14" unit="character"/> 

    <lb n="10"/>λων ἐπὶ τῷ τὴν συντί<unclear>μ</unclear><supplied reason="lost">ησιν </supplied><gap reason="lost" quantity="10" unit="character"/> 

    <lb n="11"/>μων γενέσθαι ὁμολογ<unclear>ῶ</unclear> <supplied reason="lost">ὀμνὺς τὴν τοῦ</supplied> 

    <lb n="12"/>κυρίου ἡμῶν Αὐτοκρ<unclear>ά</unclear><supplied reason="lost">τορος Γαλλιηνοῦ</supplied> 

    <lb n="13"/>Σεβαστοῦ τύχην τα<unclear>υ</unclear><gap reason="lost" quantity="14" unit="character"/> 

    <lb n="14"/><gap reason="lost" quantity="4" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="2" unit="character"/><gap reason="illegible" quantity="3" unit="character"/>να <unclear>α</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="15"/><gap reason="lost" extent="unknown" unit="line"/> </ab></div>
