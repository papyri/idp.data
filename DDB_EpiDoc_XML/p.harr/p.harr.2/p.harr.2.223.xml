    <lb n="10"/>ἐπιστολὴ<unclear>ν</unclear> <unclear>παρ</unclear><gap reason="illegible" quantity="3" unit="character"/><unclear>ε</unclear><gap reason="lost" quantity="2" unit="character"/> 

    <lb n="11"/>πρὸς <unclear>Ὡ</unclear>ρεῖν ειν<unclear>ε</unclear><gap reason="lost" quantity="6" unit="character"/> 

    <lb n="12"/>ου ἔχει <gap reason="illegible" quantity="3" unit="character"/>τρι<gap reason="illegible" quantity="3" unit="character"/><unclear>δ</unclear><gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" quantity="3" unit="character"/> 

    <lb n="13"/>πῶς ἐ<unclear>πρ</unclear><supplied reason="lost">ά</supplied><unclear>θη</unclear> τὸ ἔλαιο<supplied reason="lost">ν</supplied> 

    <lb n="14"/>καὶ πόσου ἠγόρασ<unclear>ε</unclear> <supplied reason="lost">τὸν</supplied> 

    <lb n="15"/>σίτον <unclear>δήλ</unclear>ωσόν <supplied reason="lost">μοι.</supplied> 

