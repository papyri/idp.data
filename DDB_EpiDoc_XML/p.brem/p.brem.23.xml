    <lb n="19"/>καὶ Παχομ<unclear cert="high" reason="undefined">π</unclear>αῶτος Πετεήσιος καὶ 

    <lb n="20"/><gap extent="5" reason="lost" unit="character"/>φριος <expan>νεω<ex cert="high">τέρου</ex></expan> Ψεαθύριος <expan>εὐσχημό<ex cert="high">νων</ex></expan> 

    <lb n="21"/>κ<supplied cert="high" reason="lost">α</supplied>ὶ <supplied cert="high" reason="lost">ἀ</supplied>πὸ οἰκί<supplied cert="high" reason="lost">ας </supplied><gap extent="2" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/><gap extent="3" reason="lost" unit="character"/>ου <expan>Παυ<gap extent="1" reason="illegible" unit="character"/>ώφιο<ex cert="high">ς</ex></expan> 

    <lb n="22"/>καὶ οἰκίας Ψ<supplied cert="high" reason="lost">ε</supplied>α<supplied cert="high" reason="lost">θ</supplied>ύριος Πανκολλαύθου 

    <lb n="23"/>μέχρι οἰκίας <gap extent="3" reason="illegible" unit="character"/>ρ<gap extent="2" reason="illegible" unit="character"/>τος Ἀρχιβίου καὶ 

