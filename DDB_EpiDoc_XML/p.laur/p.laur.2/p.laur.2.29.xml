    <lb n="1"/><gap reason="lost" extent="unknown" unit="line"/> 

    <lb n="1"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost">  πρὸς χει</supplied><unclear>ρ</unclear><supplied reason="lost">ικὴ</supplied><unclear>ν</unclear> <unclear>ἀμ</unclear>πελ<supplied reason="lost">ουργ</supplied><unclear>ικ</unclear><supplied reason="lost">ὴν ἐργασίαν καὶ πᾶσαν ἐπιμέλειαν</supplied> 

    <lb n="2"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost">       καὶ</supplied> καλαμοστασίαν καὶ <supplied reason="lost">κα</supplied><unclear>λ</unclear>λιερ<supplied reason="lost">γίαν καὶ περίσκαψιν</supplied> 

    <lb n="3"/><supplied reason="lost">πέμπτον κατʼ ἔτος ἀμέμπ</supplied><unclear>τ</unclear><supplied reason="lost">ως</supplied> <unclear>καὶ</unclear> <supplied reason="lost">ἀ</supplied><unclear>κ</unclear><supplied reason="lost">α</supplied><unclear>τ</unclear><supplied reason="lost">α</supplied><unclear>φρ</unclear><supplied reason="lost">ο</supplied><unclear>ν</unclear>ήτως ἐ<unclear>φ</unclear>ʼ <supplied reason="lost">ᾧ</supplied> σὲ μ<supplied reason="lost">ὲν τὸν γεοῦχον</supplied> 

    <lb n="4"/><supplied reason="lost">λαβεῖν τὸ ἥμισυ</supplied> μέρος παντὸς τοῦ γε<unclear>νησ</unclear>ομένου οἴνου καὶ τοῦ καρ<supplied reason="lost">ποῦ τῶν παντοίων</supplied> 

    <lb n="5"/><supplied reason="lost">φυτῶν καὶ φοινί</supplied>κων καὶ ἐμὲ μετὰ το<unclear>ῦ</unclear> γεωργοῦ δέξ<supplied reason="lost">α</supplied>σθαι τὸ <supplied reason="lost">ἄ</supplied><unclear>λ</unclear><supplied reason="lost">λο ἥμισυ</supplied> 

    <lb n="6"/><supplied reason="lost">μέρος ἀνθʼ ὧν πο</supplied>ιούμεθα καμάτων· παρέξω δὲ κατʼ ἔτος λόγῳ συ<unclear>ν</unclear><supplied reason="lost">ηθείας</supplied> 

    <lb n="7"/><supplied reason="lost">χοιράδα μίαν <num value="1"/> τιμ</supplied>ῆς κερατίων τριῶν <num value="3"/> καὶ τ<hi rend="diaeresis">υ</hi>ρία ἑκατὸν πεντ<supplied reason="lost">ήκοντα <num value="150"/> </supplied><gap reason="lost" quantity="3" unit="character"/> 

    <lb n="8"/><gap reason="lost" extent="unknown" unit="character"/><gap reason="illegible" quantity="1" unit="character"/>αγα ἐν καιρῷ τῶν τρυγῶν καὶ οἴνου παλαιοῦ μεγά<unclear>λ</unclear><supplied reason="lost">α ἀγγεῖα</supplied> 

    <lb n="9"/><gap reason="lost" extent="unknown" unit="character"/><gap reason="illegible" quantity="1" unit="character"/> δύο <num value="2"/>. ἡ παροῦσα μίσθωσις κυρ<hi rend="diaeresis">ί</hi>α καὶ βεβαία καὶ ἐπ<supplied reason="lost">ε</supplied><unclear>ρ</unclear><supplied reason="lost">ωτηθεὶς</supplied> 

    <lb n="10"/><supplied reason="lost">ὡμολόγησα. <handShift new="m2"/> Α</supplied>ὐρήλιος Μηνᾶ υ<hi rend="diaeresis">ἱ</hi>ὸς <foreign xml:lang="cop">ϩ</foreign><hi rend="supraline">οο</hi>π ὁ <choice><reg>πρ<supplied reason="omitted">ο</supplied>κείμενος</reg><orig>πρκείμενος</orig></choice> μεμίσθ<unclear>ωμ</unclear><supplied reason="lost">αι ὡς <expan>πρόκ<ex>ειται</ex></expan></supplied> 

    <lb n="11"/><supplied reason="lost">Αὐρήλιος </supplied><gap reason="lost" quantity="3" unit="character"/><supplied reason="lost"> υἱὸ</supplied>ς Ἰω<unclear>ά</unclear>ννου ἀπὸ <expan>Ἑρμ<ex>οῦ</ex></expan> <expan>π<ex>όλεως</ex></expan> ἀξιωθεὶς ἔγραψα ὑπὲρ αὐτοῦ γράμμ<supplied reason="lost">α</supplied><unclear>τ</unclear><supplied reason="lost">α μὴ εἰδότος</supplied> 

    <lb n="12"/><supplied reason="lost"><handShift new="m3"/> Αὐρήλιος </supplied><gap reason="lost" quantity="3" unit="character"/><supplied reason="lost"> υἱὸς Ἰ</supplied>ωάννο<unclear>υ</unclear> ἀπὸ <expan>Ἑρμ<ex>οῦ</ex></expan> <expan>π<ex>όλεως</ex></expan> μαρτυρῶ τῇ μισθώσει <supplied reason="lost">ἀκούσας</supplied> 

    <lb n="13"/><supplied reason="lost">παρὰ τῶ</supplied><unclear>ν</unclear> θεμένων <g type="stauros"/> 

    <lb n="14"/><supplied reason="lost"><handShift new="m4"/> Αὐρήλιος </supplied><expan><supplied reason="lost">Κ</supplied><unclear>ο</unclear>λλοῦθ<ex>ος</ex></expan> <expan>νοτάρι<ex>ος</ex></expan> υἱὸς Πέτρ<unclear>ου</unclear> ἀπὸ Ἑρμοῦ πόλεως <unclear>μ</unclear><supplied reason="lost">αρτυρῶ</supplied> 

    <lb n="15"/><supplied reason="lost">τῇδε τῇ π</supplied>αρούσῃ μισθωτικῇ ὁμολογίᾳ <supplied reason="omitted">καὶ</supplied> ὑπέγραψ<supplied reason="lost">α δὲ ἀκούσας</supplied> 

