         <head n="21277" xml:lang="en">
            <date>AD 709</date>
            <placeName>Aphrodito, Aphroditopolis</placeName>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r" type="textpart"><ab>

    <lb n="1"/><supplied reason="lost">ἐν</supplied> ὀνόματι τοῦ θε<supplied reason="lost">οῦ Κορρα υἱὸς Σζεριχ σύ</supplied><unclear>μ</unclear><supplied reason="lost">βουλος</supplied> 

    <lb n="2"/><unclear>Βα</unclear>σιλείῳ διο<unclear>ι</unclear><supplied reason="lost">κη</supplied><unclear>τ</unclear><supplied reason="lost">ῇ κώμης Ἀ</supplied><unclear>φ</unclear><supplied reason="lost">ροδιτώ. ἀκανθ</supplied><unclear>έ</unclear><supplied reason="lost">ων ποσόν</supplied> 

    <lb n="3"/>τι <choice><reg>ἡμῖν</reg><orig>ἡμεν</orig></choice> δια<unclear>σ</unclear>τ<unclear>εί</unclear><supplied reason="lost">λαντ</supplied><unclear>ε</unclear><supplied reason="lost">ς διὰ</supplied> <unclear>τ</unclear>ῆς διοικήσεώς σου 

    <lb n="4"/><gap reason="illegible" quantity="3" unit="character"/>λ<gap reason="lost" quantity="3" unit="character"/>κτ<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="3" unit="character"/><supplied reason="lost"> λογῳ</supplied> φ<supplied reason="lost">ιλο</supplied><unclear>κ</unclear>αλεία<supplied reason="lost">ς</supplied> <unclear>τε</unclear> <unclear>κ</unclear><supplied reason="lost">α</supplied><unclear>ὶ</unclear> <supplied reason="lost">ἐ</supplied>ξαρτίας 

    <lb n="5"/>καράβων καὶ <unclear>ἀκ</unclear><supplied reason="lost">α</supplied><unclear>τηνα</unclear>ρίων καὶ δρομοναρίων κούρσου 

    <lb n="6"/>ἰνδικτίονος ἐνάτης, ἀποστείλαντες πρὸς σὲ καὶ τὰ τούτων 

    <lb n="7"/>ἐντάγια πρὸ εἴκ<supplied reason="lost">ο</supplied>σι <app type="editorial"><lem resp="BL 13.120">ἡμερίων</lem><rdg>ἡμεριῶν</rdg></app> καὶ μέχρι τῆς δεῦρο οὐκ ἔπεμψά<supplied reason="lost">ς</supplied> 

    <lb n="8"/><supplied reason="lost">τ</supplied><unclear>ί</unclear> <supplied reason="lost">ποτ</supplied><unclear>ε</unclear> <unclear>ἐκ</unclear> <supplied reason="lost">τούτ</supplied>ων· ἡμῶν ἤδη ἐπιτρεψάντων τῷ ἐπικειμένῳ 

    <lb n="9"/>τῆς ἐργασίας τῶν αὐτῶν καράβων ἄρξασθαι εἰς κτίσιν 

    <lb n="10"/>καὶ φιλοκαλείαν αὐτῶν διὰ τῶν αὐτῶν τεχνιτῶν 

    <lb n="11"/><gap reason="lost" quantity="6" unit="character"/><unclear>ε</unclear><gap reason="lost" quantity="3" unit="character"/><unclear>σ</unclear>δαν, <choice><reg>διεστείλαμεν</reg><orig>διαστείλομεν</orig></choice> κελεύσει θεοῦ 

    <lb n="12"/><unclear>τ</unclear><supplied reason="lost">ὴν ὑπο</supplied><unclear>τ</unclear><supplied reason="lost">ε</supplied><unclear>τ</unclear><supplied reason="lost">αγμένην δι</supplied>αστολὴν τῶν αὐτῶν τεχνιτῶν. 

    <lb n="13"/>δεχόμενος <unclear>ο</unclear><supplied reason="lost">ὖν</supplied> <unclear>τ</unclear><supplied reason="lost">ὰ π</supplied><unclear>α</unclear>ρόντα γράμματα ἀπόστειλον εὐθέως 

    <lb n="14"/>καί <supplied reason="lost">π</supplied>άραυτα εἴ τι ἐστὶν ἐκ τῶν τοιούτων εἰδῶν διά τῆς 

    <lb n="15"/>διοικήσεώς σου, τὸ πρῶτον ὡς πρῶτον ἐκπέμπων 

    <lb n="16"/>πρὸ παντὸς τὰς ἀκανθέας, ἐὰν μέντοι οὔπω 

    <lb n="17"/>ταύτας ἐξέπεμψας, μὴ ἀποστέλλων τι ἐξ αὐτῶν 

    <lb n="18"/>ἐκτός τοῦ σαφηνισθέντος σοι μέτρου ἐν τοῖς προτέροις 

    <lb n="19"/>ἡμῶν γράμμασιν. ἔσῃ δὲ ἐπιστάμενος ὡς εἰ 

    <lb n="20"/>ἀναγκασθῶμεν γράψαι πρός σὲ ἕτερα γράμματα 

    <lb n="21"/>μετὰ τὰ παρόντα γράμματα, διὰ τὸ ὑστερῆσαί σε καὶ μὴ 

    <lb n="22"/>πέμψαι τὰς τοιαύτας ἀκανθέας, μ<unclear>έ</unclear>λλει σύν τοῖς 

    <lb n="23"/>αὐτοῖς γράμμασιν καταλαβεῖν σε <unclear>ἀντ</unclear><supplied reason="lost">α</supplied>π<unclear>ό</unclear>δοσις 

    <lb n="24"/><supplied reason="lost">ὀ</supplied>φείλουσα θλίψαι τὴν ψυχήν σου. λοιπὸν 

    <lb n="25"/>μὴ δώσῃς κατὰ σοῦ τὴν οἵαν οὖν ἀφορμὴν 

    <lb n="26"/>καὶ ἀμελήσῃς τοῦ ἀποστεῖλαι τὰς ῥηθείσας 

    <lb n="27"/>ἀκανθέας οὐ μὴν ἀλλὰ καὶ τὰ λοιπὰ εἴδη. <expan>ἐγρά<ex>φη</ex></expan> <expan>μη<ex>νὶ</ex></expan> <expan>Θὼ<ex>θ</ex></expan> <num value="22">κβ</num> <expan>ἰ<ex>ν</ex>δ<ex>ικτίονος</ex></expan> <num value="8">η</num>. 

    <lb n="28"/><g type="stauros"/> <expan>γνῶ<ex>σις</ex></expan> <expan>κώμ<ex>ης</ex></expan> <expan>Ἀφρ<ex>ο</ex>δ<ex>ιτώ</ex></expan>. 

    <lb n="29"/><expan>οὕ<ex>τως</ex></expan> 

