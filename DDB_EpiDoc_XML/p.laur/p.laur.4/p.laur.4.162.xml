         <div xml:lang="grc" type="edition" xml:space="preserve"><ab>
    <lb n="1"/><hi rend="diaeresis">Ὑ</hi>πατείας τῶν δεσποτῶν ἡμῶν Κωνστ<supplied reason="lost">αντίου Αὐγούστου τὸ <num value="7">ζ</num> καὶ Κωνσταντίου τοῦ ἐπιφανεστάτου</supplied> 

    <lb n="2"/>Καίσαρο<supplied reason="lost">ς τὸ <num value="3">γ</num>          </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="3"/><unclear>Α</unclear>ὐρηλίοι<unclear>ς</unclear> Εὐλογίῳ Εὐλογίου καὶ Δι<unclear>ο</unclear><gap reason="lost" quantity="1" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="1" unit="character"/><gap reason="illegible" quantity="2" unit="character"/><unclear>Δι</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="4"/>καὶ Δωροθέῳ Θωνίου Πασίωνος καὶ Ἀ<unclear>π</unclear>ολλ<gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost">     βουλευταῖς</supplied> 

    <lb n="5"/>Ὀξυρυγ<unclear>χ</unclear>ειτῶν πόλεως <unclear>κ</unclear><supplied reason="lost">α</supplied>ὶ ἐ<unclear>π</unclear>ι<supplied reason="lost">μεληταῖς σίτου Ἀλεξανδρείας </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="6"/>παρὰ Αὐρηλίου <expan>Ἀπφοῦ<ex>τος</ex></expan> <expan>Τιθοή<ex>ους</ex></expan> ἀπὸ τοῦ Ὀξυρ<supplied reason="lost">υγχίτου νομοῦ κυβερνέτου πλοίου   </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="7"/>μετʼεγγύου τῆς τοῦ γόμου παραδόσεως καὶ τ<gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost">       Αὐρηλίου</supplied> 

    <lb n="8"/>Β<unclear>ε</unclear>ρνικιανοῦ <expan>βουλ<ex>ευτοῦ</ex></expan> <expan>ἐνάρχ<ex>ου</ex></expan> <expan>πρυτ<ex>άνεως</ex></expan> τῆς αὐ<supplied reason="lost">τὴς πόλεως  </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="9"/><expan><unclear>ὁμ</unclear>ολ<unclear>ο</unclear>γ<unclear>ο</unclear><ex>ῦμεν</ex></expan> <expan>ὀμνύ<unclear>ν</unclear><ex>τες</ex></expan> τὸν σεβ<unclear>άσ</unclear><supplied reason="lost">μιον θε</supplied>ῖον ὅρκ<supplied reason="lost">ον </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="10"/>Κωνστα<supplied reason="lost">ντίου το</supplied>ῦ ἐπιφανεστάτου Καίσαρ<supplied reason="lost">ος ἐγὼ μὲν ὁ κυβερνήτης παρειληφέναι καὶ ἐμβεβλῆσθαι εἰς  </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="11"/>ἀπὸ γεν<supplied reason="lost">ήματος</supplied> <unclear>τ</unclear>ῆς εὐτυχοῦς <num value="13">ιγ</num> <expan><ex>ἔτους</ex></expan> νέας <supplied reason="lost">ἰνδικτίονος σίτου καθαροῦ ἀκρίθου ἐκτὸς</supplied> 

    <lb n="12"/>π<unclear>ά</unclear>σ<unclear>η</unclear>ς <choice><corr>αἰτίας</corr><sic>ετιας</sic></choice> κ<unclear>αθ</unclear>εστηκότος μέτρ<unclear>ω</unclear><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ἀρτάβας  </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost">  <expan><ex>γίνονται</ex></expan> <expan><ex>ἀρτάβαι</ex></expan>  </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="13"/>τὸ δὲ <expan>σί<ex>του</ex></expan> <unclear>πᾶ</unclear>ν καὶ <choice><corr>σ<supplied reason="omitted">ῶ</supplied>ον</corr><sic><unclear>σο</unclear>ν</sic></choice> βασταχὲν ε<gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost">  η</supplied>

    <lb n="14" type="inWord"/>νέχθη πρὸς λιτρι<unclear>σμὸ</unclear>ν εξ<gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="15"/>καὶ ἦξεν ὁ ξέστης <expan>λί<ex>τραν</ex></expan> <num value="1">α</num> <expan><ex>οὐγκίαν</ex></expan><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost">   ἅπερ ἀπενέγκω ἐπὶ τὴν λαμπροτάτην</supplied> 

    <lb n="16"/>Ἀλεξανδρείαν καὶ παρα<supplied reason="lost">δώσω οἷς ἐὰν κελευσθῶ σῶα καὶ ἀκακούργητα ὡς ἐὰν ὑποδέξωμαι χωρὶς θεοῦ βίας</supplied> 

    <lb n="17"/>καὶ τῆς παραδόσεως <unclear>ἄ</unclear><supplied reason="lost">ποχα γράμματα ἐποίσω  </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost">  ἐγὼ δὲ  </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost">  αὐθαιρέτῳ καὶ ἐκουσίᾳ</supplied> 

    <lb n="18"/><supplied reason="lost">γ</supplied>νώμῃ <unclear>ἐγ</unclear><supplied reason="lost">γ</supplied><unclear>υ</unclear>ᾶσθαι τ<supplied reason="lost">ὸν αὐτὸν κυβερνήτην παραδιδόντα    </supplied><gap reason="lost" extent="unknown" unit="character"/> 

