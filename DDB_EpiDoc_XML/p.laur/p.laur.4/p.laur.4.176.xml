    <lb n="23"/>τ<unclear>ις</unclear> τῶν προ<supplied reason="lost">κειμένων σιππίων δεσμιδίων εἴκοσι <num value="20"/>  </supplied><gap reason="lost" quantity="5" unit="character"/><supplied reason="lost">  καὶ ἀ</supplied>

    <lb n="24" type="inWord"/><choice><reg>ποδωσομεν</reg><orig>αποδωσωμεν</orig></choice> <unclear>ἐ</unclear><supplied reason="lost">ξ ἀλληλεγγύης ὡς προκείται  </supplied><gap reason="lost" quantity="10" unit="character"/><supplied reason="lost"> καὶ ἐπε</supplied>

