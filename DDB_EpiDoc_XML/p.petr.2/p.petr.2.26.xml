         <head n="47317" xml:lang="en">
            <date>226/225 BC</date>
            <placeName>Arsinoite</placeName>
            <ref n="p.petr;3;19,F" type="reprint-from">PPetr3,19,FrF</ref>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><ab>
    <lb n="1"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> οὐλὴ</supplied> 

    <lb n="1"/>ἀνὰ μέσον ὀφρύων καὶ ἄλλα<supplied reason="lost">ι </supplied><gap reason="lost" quantity="30" unit="character"/><supplied reason="lost"> Ἀλεξανδρεὺς</supplied> 

    <lb n="2"/><unclear>τ</unclear>ῆς ἐπιγονῆς τῶν οὔπ<unclear>ω</unclear> <supplied reason="lost">ἐπηγμένων εἰς δῆμον </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ὡς ἐτῶν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="3"/>πέντε <num value="5"/> μέσος μεγ<unclear>έ</unclear>θει με<unclear>λ</unclear><supplied reason="lost">ίχρως </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="4"/><supplied reason="lost">Ε</supplied><unclear>ὔ</unclear>βιος Καμπανὸς τῶν Α<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ὡς ἐτῶν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="5"/><supplied reason="lost">β</supplied><unclear>ρ</unclear>αχὺς μελίχρως μ<unclear>ακ</unclear>ροπρ<supplied reason="lost">όσωπος </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="6"/><supplied reason="lost">Ν</supplied>ικάνωρ Ζωπυρίωνος Ἠ<unclear>π</unclear><supplied reason="lost" cert="low">ειρώτης</supplied><supplied reason="lost"> τῆς ἐπιγονῆς ὡς ἐτῶν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="7"/>μέσος μεγέθει μελίχρω<unclear>ς</unclear> <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="8"/><num value="16">ιϛ</num> Ἡρακλ<unclear>εί</unclear><supplied reason="lost">δης </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="9"/><supplied reason="lost">βασιλε</supplied>ύοντος Πτολεμαίου <supplied reason="lost">τοῦ Πτολεμαίου καὶ Ἀρσινόης θεῶν Ἀδελφῶν, ἔτους</supplied> 

    <lb n="10"/><supplied reason="lost">δευτέρου</supplied> καὶ εἰκοστοῦ, ἐφʼ <supplied reason="lost">ἱερέως Ἀλεξικράτους τοῦ Θεογένους Ἀλεξάνδρου καὶ θεῶν</supplied> 

    <lb n="11"/><supplied reason="lost">Ἀδελφῶ</supplied>ν καὶ θεῶν Εὐεργε<supplied reason="lost">τῶν, κανηφόρου Ἀρσινόης Φιλαδέλφου Βερενίκης τῆς</supplied> 

    <lb n="12"/><supplied reason="lost">Καλλιάν</supplied><unclear>ακ</unclear>τος, μηνὸς Ἀρ<supplied reason="lost">τεμισίου ἕκτηι καὶ δεκάτηι, ἐν </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost">  τοῦ Ἀρσινοίτου νομοῦ</supplied>, 

    <lb n="13"/><supplied reason="lost">τάδε διέθετο νοῶν καὶ φρονῶν Ἡρακλείδης </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="14"/><gap reason="lost" extent="unknown" unit="line"/> </ab></div>
