    <lb n="12"/>μετ<unclear>ρητοῦ</unclear> <unclear>τοῦ</unclear> <unclear>ἐλαίου</unclear> <gap reason="lost" quantity="4" unit="character"/><gap reason="illegible" quantity="3" unit="character"/>

    <lb n="13"/>Ἀπφοῦ δικ<unclear>αίου</unclear> <gap reason="illegible" quantity="3" unit="character"/><gap reason="lost" quantity="6" unit="character"/><supplied reason="lost"> καλ</supplied>

