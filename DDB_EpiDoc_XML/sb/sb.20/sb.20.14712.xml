         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r" type="textpart"><!--milestone unit="4"--><ab>

    <lb n="1"/><g type="stauros"/> ὑπα<unclear>τ</unclear>είας <expan>Φλ<ex>αουίων</ex></expan> <unclear>Ἰ</unclear>ω<supplied reason="lost">άννου καὶ Παυ</supplied>λίνου τῶ<supplied reason="lost">ν λαμπροτάτων </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> <num value="7">ζ</num> ίνδικτίωνος.</supplied>

    <lb n="2"/>κυρίᾳ τῇ καὶ Φωτίνῃ οὐ<unclear>ρ</unclear><supplied reason="lost">α</supplied><unclear>ν</unclear>ίῳ τῇ καὶ ζω<unclear>οπ</unclear><supplied reason="lost">οιῷ </supplied><gap reason="lost" extent="unknown" unit="character"/>

    <lb n="3"/>τῆς Ἑρμουπολιτῶν ὑπὸ Σαλαμᾶ ὁσιώτ<unclear>α</unclear><supplied reason="lost">τον διὰ </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> εὐλαβεστάτου</supplied>

    <lb n="4"/>πρεσβυτέρου καὶ οἰκονόμου Αὐρήλιο<unclear>ς</unclear> <supplied reason="lost">Κολλοῦθος Φέυτος κεραμο</supplied>

    <lb n="5" break="no"/>πλάστης ἀπὸ κώμης Φβῦ τοῦ Ἑρμουπο<supplied reason="lost">λίτου νομοῦ. ὁμολογῶ ἐσχηκέναι</supplied>

    <lb n="6"/>καὶ πεπληρῶσθαι παρὰ τῆς ὑμῶν εὐλ<unclear>α</unclear><supplied reason="lost">βείας ἐντεῦθεν τῆς συμπεφωνη</supplied>

    <lb n="7" break="no"/>μένης τελείας τιμῆς κούφων νέων <choice><reg>πεπισσο<supplied reason="lost">μένων</supplied></reg><orig>πεπει<unclear>σσ</unclear>ο<supplied reason="lost">μένων</supplied></orig></choice><supplied reason="lost"> μεγάλων δισχιλίων</supplied>

    <lb n="8"/>τετρ<supplied reason="lost">α</supplied>κοσίων, <choice><reg>γίνεται</reg><orig>γη</orig></choice> <expan>κοῦφ<ex>α</ex></expan> νέα <expan>πεπ<ex>ισσωμένα</ex></expan> <expan>μ<unclear>εγ</unclear><ex>άλα</ex></expan> <num value="2400">Βυ</num>, <supplied reason="lost">ἅπερ ἀποδώσω ὑμῖν ἐν τῷ </supplied><gap reason="lost" extent="unknown" unit="character"/>

    <lb n="9"/>μηνὶ <choice><reg>ἀρχῇ</reg><orig>ἀρχης</orig></choice> τῆς σὺν <expan>θ<ex>ε</ex>ῷ</expan> <unclear>ε</unclear><supplied reason="lost">ἰσ</supplied><unclear>ιούσης</unclear> <supplied reason="lost">ὀγδόης ἰνδικτίωνος ἐν κούφοις</supplied>

