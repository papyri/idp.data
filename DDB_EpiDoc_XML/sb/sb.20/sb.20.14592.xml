    <lb n="1"/><handShift new="m2"/>παραγγεῖλαι.

    <lb n="2"/><expan><ex>ἔτους</ex></expan> <num value="5">ε</num> Τῦβι <num value="9">θ</num>

    <lb n="3"/><handShift new="m1"/>Ἡρώιδει τῶν ὁμοτίμων τοῖς

    <lb n="4"/>συγγενέσι καὶ ἐπὶ τοῦ ν<unclear>ομοῦ</unclear>

    <lb n="5"/>παρ<unclear>ὰ</unclear> Ταοῦτος τῆς Ε<gap reason="illegible" quantity="1" unit="character"/>ε<gap reason="illegible" quantity="1" unit="character"/>μα<unclear>ου</unclear>

    <lb n="6"/>τῶν ἀπὸ Πανῶν πόλεω<unclear>ς</unclear>.

    <lb n="7"/>ἀδικοῦμαι ὑπὸ Πελίλιο<unclear>ς</unclear>

    <lb n="8"/>τοῦ Ψενθώτου τοῦ γ<unclear>ε</unclear><supplied reason="lost">νο</supplied>

    <lb n="9" break="no"/>μένου μου ἀνδρός. συναρ<supplied reason="lost">μοσ</supplied>

    <lb n="10" break="no"/><unclear>θ</unclear>εῖσα τῶι σημαινομένω<unclear>ι</unclear>

    <lb n="11"/>κατὰ συγγραφὴν αἰγυπ<unclear>τία</unclear><supplied reason="lost">ν</supplied>

    <lb n="12"/>φερνῆς, ἧς προσ<unclear>ή</unclear><supplied reason="lost">νεγκα, <app type="alternative"><lem><expan><ex>δραχμῶν</ex></expan> <gap reason="illegible" quantity="1" unit="character"/></lem><rdg><supplied reason="lost"><expan><ex>ταλάντων</ex></expan> </supplied><gap reason="lost" quantity="1" unit="character"/></rdg></app>,</supplied>

    <lb n="13"/>ἐν δὲ τῶι ἀνὰ <supplied reason="lost">μέσον διά τι</supplied>

    <lb n="14" break="no"/><supplied reason="lost">ν</supplied><unclear>α</unclear>ς διαφορὰς <unclear>ἀ</unclear><supplied reason="lost" cert="low">παλλαγέντες </supplied>

