    <lb n="1"/><handShift new="m2"/><unclear>ε</unclear><gap reason="illegible" quantity="5" unit="character"/>

    <lb n="2"/><handShift new="m1"/>Ἀπολλώνιος καὶ Ἑρμεῖνος <expan>ἀσχολ<ex>ούμενοι</ex></expan>

    <lb n="3"/>τὸ <choice><reg>ἐγκύκλιον</reg><orig>ἐνκυκλ</orig></choice> <num value="21">κα</num> <expan><ex>ἔτους</ex></expan> Ἡρώδῃ καὶ <expan>μ<ex>ετόχοις</ex></expan> <expan>δημ<ex>οσίοις</ex></expan> <expan>τραπ<ex>εζίταις</ex></expan>

    <lb n="4"/><expan>χαίρε<ex>ιν</ex></expan>. <expan>δέξα<ex>σθε</ex></expan> παρὰ Διεῦτο<unclear>ς</unclear> <unclear>τῆ</unclear>ς καὶ

    <lb n="5"/><unclear>Πα</unclear>υλείνης <expan>Ζωίλ<ex>ου</ex></expan> τοῦ <expan>Ἡλιοδώ<ex>ρου</ex></expan> <expan>μητ<ex>ρὸς</ex></expan>

    <lb n="6"/>Ἡρακλείας ἀπʼ <expan>Ὀξ<ex>υρύγχων</ex></expan> <expan>πόλ<ex>εως</ex></expan> <expan>τέλ<ex>ος</ex></expan> <expan>δούλ<ex>ων</ex></expan> Εὐπορί

    <lb n="7" break="no"/>ας καὶ τοῦ <expan>ὑπο<unclear>τιτ</unclear>θ<ex>ίου</ex></expan> <expan>α<ex>ὐτῆς</ex></expan> υἱοῦ Σερήνου

    <lb n="8"/>ὧν <expan>ἠγόρα<ex>σεν</ex></expan> παρὰ Ἀρτεμιδώρου

    <lb n="9"/>τοῦ <expan>κ<ex>αὶ</ex></expan> Διογένους Πολυκράτους

    <lb n="10"/>τοῦ <expan>Ἀρτεμιδώ<ex>ρου</ex></expan> <expan>μητ<ex>ρὸς</ex></expan> <expan>Δημητρ<unclear>ο</unclear><ex>ῦτος</ex></expan>

    <lb n="11"/>θέσει Παποντῶτος Διογένους

    <lb n="12"/>ἀπὸ <expan>τ<ex>ῆς</ex></expan> <expan>α<ex>ὐτῆς</ex></expan> <expan>πόλ<ex>εως</ex></expan> διὰ <expan>τ<ex>οῦ</ex></expan> ἐν τῇ <expan>α<ex>ὐτῇ</ex></expan> <expan>πόλ<ex>ει</ex></expan> <expan>ἀ<supplied reason="lost">γο<ex>ρανομείου</ex></supplied></expan>

