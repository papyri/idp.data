         <head n="18868" xml:lang="en">
            <date>VII spc</date>
            <placeName>Aphrodito, Aphroditopolis</placeName>
            <ref n="p.lond;4;1393" type="reprint-from">P.Lond. 4 1393</ref>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><ab>
    <lb n="1"/><gap reason="lost" extent="unknown" unit="line"/> 

    <lb n="1"/>αὐτούς, εἴ τι ἐθεματίσ<supplied reason="lost">α</supplied>μεν ἐν τοῖς <unclear>ἡ</unclear>μ<supplied reason="lost">ε</supplied><unclear>τ</unclear><supplied reason="lost">έ</supplied><unclear>ρ</unclear><supplied reason="lost">οι</supplied><unclear>ς</unclear> <unclear>ἐ</unclear>νταγίο<supplied reason="lost">ις</supplied>, 

    <lb n="2"/>αὐτῶν μέντοι παρεχόντων,<g type="apostrophe"/> εἴ τί <add place="above">ἐστι</add> διʼ αὐτῶν δημόσιά τε 

    <lb n="3"/>καὶ ἐκστραόρ<g type="apostrophe"/>δινα, <g type="high-punctus"/> λαμβάνων ἀντιφωνητὰς αὐτῶ<hi rend="acute">ν</hi> 

    <lb n="4"/>εὐπόρ<g type="apostrophe"/>ους ὀφείλοντας ἀποδοῦναι <g type="apostrophe"/> τὴν ἀντιφών<unclear>η</unclear><supplied reason="lost">σ</supplied><unclear>ι</unclear><supplied reason="lost">ν</supplied> <expan>αὐτ<ex>ῶν</ex></expan>, 

    <lb n="5"/>εἰ δόξῃ τιν<g type="apostrophe"/>ὰ <g type="apostrophe"/> ἀποδρᾶσαι ἐξ αὐτῶν, πέμπων πρὸ<supplied reason="lost">ς ἡμᾶς</supplied> 

    <lb n="6"/>τὸ κατάγραφον τῆς ὀνομασίας καὶ <g type="apostrophe"/> <choice><reg>πατρωνυμίας</reg><orig>πατρωνοιμίας</orig></choice> 

    <lb n="7"/>καταχωρίου<unclear>ς</unclear> τῶν αὐτ<unclear>ῶ</unclear>ν ν<g type="high-punctus"/>αυτῶν <g type="apostrophe"/> καὶ τεχνιτῶν, φ<g type="apostrophe"/>ανερο

    <lb n="8" break="no"/>ποιῶν ἡμῖν <g type="apostrophe"/> ἐν τῷ αὐτῷ καταγράφῳ τοὺς <choice><reg>ἐξελθό<supplied reason="omitted">ν</supplied>τας</reg><orig>ἐξελθοτας</orig></choice> 

    <lb n="9"/>ὑπὲρ τῶν ἰδίων χ<supplied reason="lost">ω</supplied><unclear>ρ</unclear><supplied reason="lost">ί</supplied><unclear>ων</unclear> καὶ τοὺς μισθωθέντας ὑπὲρ 

    <lb n="10"/>ἑτέρων, καὶ εἴ τι <choice><reg>ἐδόθ<supplied reason="lost">η</supplied></reg><orig>ἐδώθ<supplied reason="lost">η</supplied></orig></choice> <supplied reason="lost">ὑπὲρ</supplied> <choice><reg>τῶ<supplied reason="omitted">ν</supplied></reg><orig>τω</orig></choice> <g type="high-punctus"/> αὐτῶν ὑπὲρ μισθοῦ, μὴ 

    <lb n="11"/><choice><reg>ἐκδίδους</reg><orig>ἐδκιδων</orig></choice> τινὶ τὸ κ<unclear>ε</unclear>φ<unclear>ά</unclear>λ<unclear>αιο</unclear>ν αὐτῶν, <supplied reason="lost">ἀ</supplied><unclear>λλὰ</unclear> <gap reason="lost" quantity="1" unit="character"/><gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" quantity="2" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="12"/>ἀποστέλλων·  <g type="apostrophe"/> τὰς δὲ δαπάνας τὰς <choice><reg>ταγείσας</reg><orig>ταγ<unclear>ίσας</unclear></orig></choice> διὰ τῆς διοικήσεώς σου 

    <lb n="13"/>ἀπόστειλον εἰς πλῆρε<supplied reason="lost">ς π</supplied><unclear>ε</unclear><supplied reason="lost">π</supplied>οιημένας, <g type="high-punctus"/> <choice><reg>τυπώνων</reg><orig>τυπώννων</orig></choice> 

    <lb n="14"/>ἐπάνω <g type="high-punctus"/> αὐτῶν <g type="apostrophe"/> ἀ<unclear>ν</unclear>θρώπ<supplied reason="lost">ους χρησ</supplied><unclear>ίμο</unclear><supplied reason="lost">υς</supplied> <unclear>πι</unclear><supplied reason="lost">σ</supplied><unclear>τ</unclear>ού<unclear>ς</unclear> τε κα<supplied reason="lost">ὶ</supplied> 

    <lb n="15"/>εὐπόρους <g type="apostrophe"/> μαρτ<supplied reason="lost">υ</supplied>ρ<supplied reason="lost">ῶν </supplied><gap reason="lost" quantity="3" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><unclear>σ</unclear><supplied reason="lost">α</supplied><unclear>ι</unclear> <supplied reason="lost">ὑ</supplied>πὲρ ἑτέρων πιστῶν <supplied reason="lost">καὶ</supplied> 

    <lb n="16"/>μὴ εἶναι αὐτοὺς φυρ<supplied reason="lost">α</supplied><unclear>τ</unclear><supplied reason="lost">ὰς ἢ</supplied> ῥᾳδιουρ<g type="apostrophe"/>γούς, <g type="high-punctus"/> καὶ ἐπίτρεψο<supplied reason="lost">ν </supplied><expan><supplied reason="lost">α</supplied><unclear>ὐτ</unclear><ex>οῖς</ex></expan> 

    <lb n="17"/>πληρωθῆναι <g type="high-punctus"/> τὴν τ<supplied reason="lost">οιαύτ</supplied><unclear>η</unclear><supplied reason="lost">ν δαπά</supplied>νην ἐκ τῶν <expan>χωρ<ex>ίων</ex></expan> 

    <lb n="18"/>μετὰ τοῦ φόβου τοῦ θεοῦ <gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ὅρῳ</supplied> 

    <lb n="19"/>δικαίῳ καὶ σταθμῷ ἴσ<supplied reason="lost">ῳ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="20"/>πρακτηθῆναί τινὰ ἐν τ<supplied reason="lost">ῇ τοιαύτῃ διαστολῇ <unclear>διὰ</unclear></supplied> 

    <lb n="21"/>τὴν διοίκησίν σου καὶ <unclear>ἐπι</unclear>σ<unclear>υνα</unclear>πτομ<unclear>έ</unclear>νω<supplied reason="lost">ν</supplied> <gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="2" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="22"/>πάντων διὰ σεαυτοῦ μὴ καταθαρρῶν <g type="high-punctus"/> τινί. <choice><reg>καὶ αὐτῶν</reg><orig>καὐτῶν</orig></choice> 

    <lb n="23"/>εἰς πλῆρες ὑποδεχομένων τὴν τοιαύτην δαπάνην 

    <lb n="24"/>ὡς εἴρηται ἐκ τῶν χωρ<supplied reason="lost">ίων κα</supplied><unclear>τ</unclear><supplied reason="lost">α</supplied>σκεύασον ποιῆσ<supplied reason="lost">αι</supplied> <expan><unclear>αὐ</unclear>τ<ex>οῖς</ex></expan> 

    <lb n="25"/>ἀποδείξεις ὑπὲρ αὐτῆς, <unclear>γ</unclear><supplied reason="lost">ρ</supplied>άφων ἡμῶν τὸ <hi rend="diaeresis">ἴ</hi>σον τῆς παραδοθείσης 

    <lb n="26"/>δαπάνης ἑκάστῳ πιστικῷ καὶ πέμπων. μὴ εὕρωμέν σε δὲ 

    <lb n="27"/>πέμψαντα ἀπαργυρισμὸν <unclear>τ</unclear><supplied reason="lost">ὸ</supplied> σύνολον ὑπ<unclear>ὲρ</unclear> <gap reason="illegible" quantity="2" unit="character"/><unclear>τ</unclear>ο<unclear>ι</unclear><supplied reason="lost">ο</supplied>υ <gap reason="lost" quantity="2" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="1" unit="character"/><gap reason="illegible" quantity="4" unit="character"/> 

    <lb n="28"/>εἰ μὴ <g type="apostrophe"/> αὐτὸ τὸ σωμάτιον· <supplied reason="lost">ὑ</supplied><unclear>π</unclear>ὲρ τούτων <choice><reg>ἀποστρέφοντες</reg><orig>ἀποστρέφωντες</orig></choice> <expan>ἀποδίδουμέ<ex>ν</ex></expan> σοι 

    <lb n="29"/>ἀνταπόδωσιν βλάπτουσάν σε·<g type="high-punctus"/> οὐκ ἔχεις γὰρ ἀφορμὴν τὴν οἵαν οὖν 

    <lb n="30"/>περὶ τοῦ σωματίου. ἐνδρανῶς <add place="above">δὲ</add> ἐνέχθητι εἰς τὸ πρᾶγμα τῶν 

    <lb n="31"/>δαπανῶν, κρατῶν τοὺς τῶν χωρίων ἀποδοῦναι, εἴ τι ἐτάγη διʼ <expan>αὐτ<ex>ῶν</ex></expan> 

    <lb n="32"/>ἐν εἴδεσιν, <g type="apostrophe"/> ἄνευ μέντοι τοῦ ἑψήματος· τοῦτο γὰρ <choice><reg>ὡρίσαμεν</reg><orig>ὁρίσαμεν</orig></choice> σταλ<unclear>ῆν</unclear>αι 

    <lb n="33"/>ἐν ἀπαργυρισμῷ πρὸς <add place="above">τὴν</add> <subst><add place="inline">ἐντεθεῖσαν</add><del rend="corrected">εντηθεισαν</del></subst> <g type="apostrophe"/> <choice><reg>διατίμησιν</reg><orig>διατήμισιν</orig></choice> ἐν τοῖς <choice><reg>ἡμετ<supplied reason="omitted">έ</supplied>ροις</reg><orig>ἡμετροις</orig></choice> <expan>ἐντ<ex>α</ex>γ<ex>ίοις</ex></expan>· 

    <lb n="34"/>λοιπὸν μὴ <g type="apostrophe"/> πέμψῃς ἕψημα τὸ σύνολον, εἰ μὴ <g type="apostrophe"/> τὸν ἀπαρ<g type="high-punctus"/>γυρισμόν, 

    <lb n="35"/>οὐ μὴν ἀλλὰ καὶ <g type="apostrophe"/> εἴ τι δʼ ἂν ἀδυνατήσωσιν δοῦναι ἐν εἴδεσιν <g type="high-punctus"/> 

    <lb n="36"/>ἔκ τε τῆς ἁλυκῆς <g type="high-punctus"/> καὶ τοῦ ὄξ<supplied reason="lost">ο</supplied>υς, μὴ συγχωρῶν ἀνυσθῆναί τι ἐκ περιττοῦ 

    <lb n="37"/>τῆς <choice><reg>θεματισθείσης</reg><orig>θεματισθήσης</orig></choice> διατιμήσεως, <g type="high-punctus"/> πέμπων <choice><reg>τὸ<supplied reason="omitted">ν</supplied></reg><orig>το</orig></choice> ἀνυόμενον 

    <lb n="38"/>ἀπαργυρισμὸν <hi rend="diaeresis">ὑ</hi>πὲρ δαπάνης μετὰ <choice><reg>ἀνθρώπου</reg><orig>ἀνθρώπω</orig></choice> σου πιστοῦ ὀφείλοντος 

    <lb n="39"/>καταβαλέσθαι <g type="high-punctus"/> εἰς πλῆρες <g type="high-punctus"/> καὶ μηδὲν <g type="high-punctus"/> παρέχων τοῖς ὑποδ<unclear>εχ</unclear>ομένοις 

    <lb n="40"/>πιστικοῖς <g type="high-punctus"/> τὴν λεχθεῖσαν δαπάνην ὑπὲρ ἀπαρ<g type="high-punctus"/>γυρισμοῦ, ἀλλὰ τὰ μάλιστα 

    <lb n="41"/>τούτους ἐξασφαλιζόμενος δεόντως <g type="high-punctus"/> διʼ ἐγγράφου <unclear>ὁμ</unclear><supplied reason="lost">ο</supplied>λ<supplied reason="lost">ογ</supplied><unclear>ί</unclear><supplied reason="lost">α</supplied><unclear>ς</unclear> 

    <lb n="42"/>αὐτῶν καὶ μαρ<g type="apostrophe"/>τυρῶν, ὅπως <hi rend="diaeresis">ἰ</hi>διοκινδύνως παραδώσωσιν 

    <lb n="43"/>τὸ <choice><reg>παραδοθὲν</reg><orig>παραδωθὲν<g type="apostrophe"/></orig></choice> αὐτοῖς, <unclear>κ</unclear><supplied reason="lost">α</supplied><unclear>τ</unclear><supplied reason="lost">α</supplied>χωρίων παριστούντ<unclear>ω</unclear><supplied reason="lost">ν σο</supplied><unclear>ι</unclear><g type="apostrophe"/> 

    <lb n="44"/>ἀντιφωνητὰς χρησίμους <g type="high-punctus"/> καὶ εὐπόρους <g type="high-punctus"/> τούτου ἕνεκα. 

    <lb n="45"/>μὴ ἀμελῇς <choice><reg>δὲ</reg><orig>τὲ</orig></choice> εἰς τὸ ἀποστεῖλαι διὰ πάσης συντομία<supplied reason="lost">ς</supplied>, εἴ τι <g type="apostrophe"/> 

    <lb n="46"/>διεστείλαμεν διὰ τῆς διοικήσεώς σου λόγῳ τοῦ αὐτοῦ κούρσου 

    <lb n="47"/>ἀπό τε ναυτῶν καὶ τεχνιτῶν καὶ δαπανῶν·<g type="apostrophe"/> ἐπετρέψαμεν γὰρ<g type="apostrophe"/> τῷ 

    <lb n="48"/>παρόντι ἀποστόλῳ ἡμῶν μὴ ἀποστῆναι ἐκ σοῦ καί ἐστι διὰ σοῦ 

    <lb n="49"/>τί ποτε τὸ καθόλου ἐν <choice><reg>λοιπάδι</reg><orig>λοιπάδει</orig></choice>. λοιπὸν μὴ γένηται <choice><reg>ἀμέλεια</reg><orig>ἀμελι<unclear>α</unclear>ς</orig></choice> 

    <lb n="50"/>καὶ <choice><reg>ὑστερηεθῇ</reg><orig>ὑστερεθῇ</orig></choice> τι· ἔσῃ γὰρ ἐ<unclear>π</unclear>ιστάμενος, <g type="high-punctus"/> ὡς, εἰ ἀπομείνῃ τι <g type="apostrophe"/> ἐν <g type="apostrophe"/> 

    <lb n="51"/><choice><reg>λοιπάδι</reg><orig>λοιπάδει</orig></choice>, μέλλομεν κελεύσει θεοῦ ἀπαιτῆσαί σε αὐτὸ ἐν διπλῇ 

    <lb n="52"/>ποσότητι ὑπὲρ παρακοῆς καὶ καταφρονήσεως. <choice><reg><add place="above">γιγνώσκων</add></reg><orig>γιν<g type="apostrophe"/>ώσκων<g type="high-punctus"/></orig></choice> τοίνυν, <g type="apostrophe"/> 

    <lb n="53"/>ὡς ἐπισπούδαστόν ἐστι τὸ πρᾶγμα τοῦ τοιούτου κούρσου, <supplied reason="lost">ἀ</supplied>μελλητὶ 

    <lb n="54"/>ἐκτέλεσον, <g type="apostrophe"/> μὴ <choice><reg>δίδους</reg><orig>διδων</orig></choice> κατὰ σοῦ τὴν οἵαν οὖν ἀφορμήν. <g type="apostrophe"/> 

    <lb n="55"/><expan>ἐγρά<ex>φη</ex></expan> <expan>μ<ex>ηνὸς</ex></expan> <expan>Τῦ<ex>βι</ex></expan> <num value="12">ιβ</num>, <expan>ἰ<ex>ν</ex>δ<ex>ικτίωνος</ex></expan> <num value="8">η</num>. 

    <lb n="56"/><expan>γνῶ<ex>σις</ex></expan> <expan>ο<ex>ὕτως</ex></expan>· <expan>να<ex>ῦται</ex></expan> <expan>ὀν<ex>όματα</ex></expan> <num value="69">ξθ</num> ἐκ <expan>νο<ex>μισμάτων</ex></expan> <num value="5">ε</num> <num value="1/2">𐅵</num> <num value="1/4" rend="tick">δ</num> <expan>μισθ<ex>οῦ</ex></expan> <expan>σὺ<ex>ν</ex></expan> <expan>νο<ex>μίσματος</ex></expan> <num value="1/2">𐅵</num> <expan>ἀποτρο<ex>φῆς</ex></expan>· 
<expan>τ<ex>ε</ex>χ<ex>νίτης</ex></expan> <num value="1">α</num> <expan>ἤτ<ex>οι</ex></expan> <expan>καλα<ex>φάτης</ex></expan> <expan>νο<ex>μίσματα</ex></expan> <num value="11">ια</num> <expan>σὺ<ex>ν</ex></expan> <expan>νο<ex>μίσματος</ex></expan> <num value="1/2">𐅵</num> <expan>ἀποτρο<ex>φῆς</ex></expan>. 

    <lb n="57"/><expan>δαπάνη<ex>ς</ex></expan> τῶν <expan>αὐτ<ex>ῶν</ex></expan> <expan>να<ex>υτῶν</ex></expan> <expan><ex>καὶ</ex></expan> <expan>τ<ex>ε</ex>χ<ex>νίτου</ex></expan> <expan>ὀν<ex>ομάτων</ex></expan> <num value="70">ο</num> <expan>μ<ex>ηνῶν</ex></expan> <num value="7">ζ</num> <expan>οὕτω<ex>ς</ex></expan>· ἐν <expan>σίτ<ex>ῳ</ex></expan> <expan>μ<ex>ηνῶν</ex></expan> <num value="2">β</num> <expan>ἀρτ<ex>άβαι</ex></expan> <num value="140">ρμ</num> τοῦ <num value="10">ι</num>, 

    <lb n="58"/>ἐν <expan>ψω<ex>μίοις</ex></expan> <expan>μ<ex>ηνῶν</ex></expan> <num value="5">ε</num> <expan>ἀρτ<ex>άβαι</ex></expan> <num value="350">τν</num> τοῦ <num value="8">η</num>· <expan>ὀσπρέ<ex>ου</ex></expan> <expan>ἀρτ<ex>άβαι</ex></expan> <num value="81">πα</num> <num value="2/3">𐅷</num> τοῦ <num value="10">ι</num>· ἐλαίου <expan>μέ<ex>τρα</ex></expan> <num value="49">μθ</num> τοῦ <expan>μέ<ex>τρου</ex></expan> <expan>νο<ex>μίσματος</ex></expan> <num value="1/3" rend="tick">γ</num> <num value="1/12" rend="tick">ιβ</num>· 

    <lb n="59"/><expan>ὄξου<ex cert="low">ς</ex></expan> <expan>μ<ex>έτρα</ex></expan> <num value="49">μθ</num> τοῦ <num value="10">ι</num>· <expan>ἁλυ<ex>κῆς</ex></expan> <expan>κ<ex>όλλα</ex>θ<ex>α</ex></expan> <num value="122">ρκβ</num> <num value="1/2">𐅵</num> τοῦ <num value="20">κ</num>. 

    <lb n="60"/><expan>δαπάνη<ex>ς</ex></expan> μάχων <expan>σί<ex>του</ex></expan> <expan>ἀρτ<ex>άβαι</ex></expan> <num value="300">τ</num> τοῦ <num value="10"><unclear>ι</unclear></num>· <expan><unclear>ὀσπ</unclear>ρ<unclear>έ</unclear><ex>ου</ex></expan> <expan>ἀρ<unclear>τ</unclear><ex>άβαι</ex></expan> <num value="100">ρ</num> τοῦ <num value="10"><unclear>ι</unclear></num>· <expan><ex>καὶ</ex></expan> <expan><ex>ὑπὲρ</ex></expan> <expan>ἑψή<ex>ματος</ex></expan> <expan>μέ<ex>τρα</ex></expan> 
<num value="246">σμϛ</num> ἐκ <expan>νο<ex>μίσματος</ex></expan> <num value="1/4" rend="tick">δ</num> <expan>νο<ex>μίσματα</ex></expan> <num value="61">ξα</num> <num value="1/2">𐅵</num>. 

