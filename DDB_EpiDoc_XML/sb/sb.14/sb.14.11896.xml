         <head n="15470" xml:lang="en">
            <date>AD 425-55</date>
            <placeName>Hermoupolis Magna</placeName>
            <ref n="psi;3;185" type="reprint-from">PSI3,185</ref>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r" type="textpart"><ab>

    <lb n="1"/>καὶ σήμερον ἐπʼ ἐμοὶ <unclear>κ</unclear>αὶ <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="1"/><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="1" unit="character"/><gap reason="illegible" quantity="4" unit="character"/> κατὰ τουτ<gap reason="lost" quantity="1" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="2"/>ἐπὶ παρουσία<unclear>ι</unclear> Μαξίμου <num value="3">γ</num><gap reason="lost" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="3"/><gap reason="lost" quantity="1" unit="character"/><unclear>α</unclear>σ<gap reason="lost" quantity="1" unit="character"/>ς λ<unclear>ο</unclear>γ<unclear>ι</unclear>ωτάτῳ σχο<unclear>λ</unclear><supplied reason="lost">αστι</supplied>κῳ <unclear>καὶ</unclear> <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="4"/>πόλεως τούτου ἕνεκεν ὁμολο<unclear>γ</unclear><supplied reason="lost">ῶ ἐπομνύμενος τὴν θείαν</supplied> 

    <lb n="5"/>καὶ οὐράνιον τύχην τῶν τὰ πάντα νι<supplied reason="lost">κώντων δεσποτῶν</supplied> 

    <lb n="6"/>ἡμῶν Θεοδοσίου καὶ Οὐαλ<supplied reason="lost">ε</supplied>ντινιανοῦ <supplied reason="lost">τῶν αἰωνίων</supplied> 

    <lb n="7"/>Αὐγούστων μηδένα λόγον ἔχειν <unclear>πρ</unclear><supplied reason="lost">ὸς σὲ περὶ ταύτης</supplied> 

    <lb n="8"/>τῆς προφάσεως κ<supplied reason="lost">αὶ μὴ</supplied> ἐγκαλεῖν σοι ἐντεῦ<supplied reason="lost">θεν μήτε</supplied> 

    <lb n="9"/><choice><reg>ἐγκαλέσειν</reg><orig>ἐγκαλέσιν</orig></choice> <choice><reg>μητʼ</reg><orig>μεθʼ</orig></choice> ἐμ<supplied reason="lost">ὲ μήθʼ ἕ</supplied>να τῶν παρʼ ἐμοῦ σοι <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="10"/>διὰ τὸ εἰληφέναι μ<unclear>ε</unclear> νῦν <gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="1" unit="character"/><gap reason="illegible" quantity="1" unit="character"/> πάσης ὑγι<gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="11"/>ἐξέδωκά σοι ταύτην τὴν διάλυσιν πρὸς ἀσφά<supplied reason="lost">λειάν σου· καὶ</supplied> 

    <lb n="12"/><expan>ἐπερ<ex>ωτηθεὶς</ex></expan> <expan>ὡμολ<ex>όγησα</ex></expan>. <expan>Αὐρ<ex>ήλιος</ex></expan> Σίμων <hi rend="diaeresis">Ἰ</hi>σακίου <supplied reason="lost">πεποίημαι τὴν</supplied> 

    <lb n="13"/>διάλυσιν καὶ οὐκ ἐγκ<supplied reason="lost">αλ</supplied>ῶ οὔτε ἐγκαλέσω <supplied reason="lost">σοι καὶ ὤμοσα</supplied> 

    <lb n="14"/>τὸν θεῖον ὅρκον ὡς <expan>πρόκ<ex>ειται</ex></expan>. <expan>Αὐρ<ex>ήλιος</ex></expan> Ταυρῖνος Ε<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="15"/>ἀξιωθεὶς ἔγραψα ὑπὲρ αὐτοῦ παρόντος <expan>γράμμ<ex>ατα</ex></expan> <unclear>μ</unclear><supplied reason="lost">ὴ εἰδότος.</supplied> 

    <lb n="16"/><gap reason="illegible" quantity="4" unit="character"/>ων <g type="stauros"/> διʼ ἐμοῦ Φοιβάμμωνος ἐγράφη. 
</ab></div><div n="v" type="textpart"><ab>

    <lb n="1"/><gap reason="lost" extent="unknown" unit="character"/> ἀπὸ Ἑρμουπόλεως. </ab></div></div>
