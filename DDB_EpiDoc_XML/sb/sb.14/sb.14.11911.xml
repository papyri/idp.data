    <lb n="39"/><handShift new="m1"/> <expan>μίσθ<ex>ωσις</ex></expan> <choice><reg>παραδείσου</reg><orig>παραδίσου</orig></choice> 

    <lb n="40"/><expan>γενή<ex>ματα</ex></expan> <num value="4">δ</num> <expan><ex>ἔτους</ex></expan> </ab></div></div>
