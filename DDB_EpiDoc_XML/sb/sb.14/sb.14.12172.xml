    <lb n="25"/>δισου<unclear>ζυ</unclear>ν<gap reason="illegible" quantity="2" unit="character"/>κι<gap reason="illegible" quantity="1" unit="character"/> 

    <lb n="26"/><choice><reg>φήσαντα</reg><orig>φίσαντα</orig></choice> τ<supplied reason="lost">ὸ</supplied><unclear>ν</unclear> τόκον 

    <lb n="27"/>καὶ τὸ <choice><reg>κεφάλαιον</reg><orig>κεφαιλαιν</orig></choice> 

