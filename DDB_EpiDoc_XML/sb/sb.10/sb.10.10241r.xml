    <lb n="11"/>τ<supplied reason="lost">οὺς καλά</supplied>μους καὶ τὸ <choice><reg>σμιλίο<supplied reason="lost">ν</supplied></reg><orig>σμηλίο<supplied reason="lost">ν</supplied></orig></choice>, <supplied reason="lost">ὅ</supplied>

