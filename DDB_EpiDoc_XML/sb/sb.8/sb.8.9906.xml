    <lb n="1"/><handShift new="m3" cert="low"/> <abbr>απο<unclear>χ</unclear><gap reason="illegible" quantity="1" unit="character"/></abbr> 

    <lb n="2"/><handShift new="m4" cert="low"/> Λουκίου Σεπτιμίου Σεο<supplied reason="lost">υήρου Περτίνακος</supplied> 

    <lb n="3"/><supplied reason="lost">ἔτους <num value="2">β</num></supplied> <del rend="erasure">Γαίου Πεσκεννίου Ν<supplied reason="lost">ίγερος Ἰούστου</supplied> </del> Σεβαστοῦ <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="4"/><supplied reason="lost">ἐν Πτολεμαί</supplied>δι Εὐεργέτιδι τοῦ Ἀρσιν<supplied reason="lost">οίτου νομοῦ. ὁμολογεῖ Ἡράκλεια</supplied> 

    <lb n="5"/><supplied reason="lost" cert="low">θυγάτηρ</supplied><supplied reason="lost"> Λυσ</supplied>ανίου <add place="above">ὡς <expan><ex>ἐτῶν</ex></expan> <num value="22">κβ</num> ἄσημ<supplied reason="lost">ος</supplied></add> ἀστὴ μετὰ κυρ<supplied reason="lost" cert="low">ίου τοῦ ἀνδρὸς </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> τοῦ</supplied> 

    <lb n="6"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> Ἀ</supplied>ντινοέως Ἀθηναιέω<supplied reason="lost">ς τοῦ καὶ </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> Κοπρέᾳ <expan>ὡ<ex>ς</ex></expan> <expan><ex>ἐτῶν</ex></expan> </supplied><gap reason="lost" quantity="2" unit="character"/> 

    <lb n="7"/><supplied reason="lost" cert="low">ἀσήμῳ</supplied><supplied reason="lost"> μετὰ</supplied> κυρίου τοῦ <expan>πατρὸ<ex>ς</ex></expan> Λεωνίδ<supplied reason="lost">ου τοῦ </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> παρακεχωρηκέναι</supplied> 

    <lb n="8"/><supplied reason="lost">τὴν Ἡράκλει</supplied><unclear>α</unclear>ν κατὰ τήνδε τὴν ὁμ<supplied reason="lost">ολογίαν ἀπὸ τοῦ ὑπάρχοντος αὐτῇ τῇ</supplied> 

    <lb n="9"/><supplied reason="lost">Ἡρακλείᾳ π</supplied>ερὶ κώμην Θεαδέλφει<supplied reason="lost">αν τῆς Θεμίστου μερίδος τοῦ αὐτοῦ</supplied> 

    <lb n="10"/><supplied reason="lost">νομοῦ ἐλαιῶ</supplied><unclear>ν</unclear>ος ἐν κατοικικῇ <choice><reg>τάξει</reg><orig>τάξι</orig></choice> <supplied reason="lost">ἀρούρας </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ἢ ὅσας ἐὰν ὦσι</supplied> 

    <lb n="11"/><supplied reason="lost">καὶ </supplied><gap reason="lost" quantity="6" unit="character"/><gap reason="illegible" quantity="1" unit="character"/>ρα δύο <num value="2"/> καὶ <choice><reg>ὑποδοχεῖα</reg><orig>ὑποδοχῖα</orig></choice> καὶ <gap reason="lost" quantity="12" unit="character"/><supplied reason="lost"> ἐπὶ τοῖς οὖσι αὐτῶν</supplied> 

    <lb n="12"/><supplied reason="lost">ὁρίοις καὶ </supplied><choice><reg><supplied reason="lost">πο</supplied>τίστρ<supplied reason="omitted">αις</supplied></reg><orig><supplied reason="lost">πο</supplied><unclear>τ</unclear>ιστ<unclear>ρ</unclear></orig></choice> δύο <num value="2"/> καὶ ἐκχύσεσι <supplied reason="lost">καὶ εἰσόδοις καὶ ἐξόδοις, ὧν γείτονες</supplied> 

    <lb n="13"/><supplied reason="lost">καθὼς ἐκ σ</supplied>υμφώνου ὑπηγόρευσαν <gap reason="lost" quantity="30" unit="character"/> 


    <lb n="14"/><gap reason="lost" extent="unknown" unit="character"/>αλε<unclear>ψ</unclear>τις λεγομένη ἀπηλιώ<unclear>τ</unclear><supplied reason="lost">ου </supplied><gap reason="lost" quantity="15" unit="character"/><supplied reason="lost"> καὶ ἀπεσχηκέ</supplied>

    <lb n="15" break="no"/><supplied reason="lost">ναι τὴν ὁμολ</supplied>ογοῦσαν Ἡράκλειαν παρ<supplied reason="lost">ὰ τῆς Κοπρέας τὴν συμπεφωνημένην</supplied> 

    <lb n="16"/><supplied reason="lost">τιμὴν πᾶσα</supplied><unclear>ν</unclear> δραχμὰς ἐνακοσίας πε<supplied reason="lost">ντήκοντα <num value="950"/> καὶ βεβαιώσειν τὴν Ἡρά</supplied>

    <lb n="17" break="no"/><supplied reason="lost">κλειαν τῇ Κο</supplied>πρέᾳ καὶ τοῖς <unclear>π</unclear><supplied reason="lost">αρʼ αὐτῆς τὴν παραχώρησιν πάσῃ βε</supplied>

    <lb n="18" break="no"/><supplied reason="lost">βαιώσι ἣν κ</supplied><unclear>α</unclear>ὶ παρέξεσθαι ἀνέπαφον καὶ ἀν<supplied reason="lost">ενεχύραστον καὶ ἀνεπιδάνιστον καὶ κα</supplied>

    <lb n="19" break="no"/><supplied reason="lost">θαρὰν ἀπὸ μὲν</supplied> <unclear>δ</unclear>ημοσίων ἀρταβιῶν τε καὶ <supplied reason="lost">ναυβίων καὶ ἀριθμητικῶν καὶ ἐπι</supplied>

    <lb n="20" break="no"/><supplied reason="lost">βολῆς τῆς κώ</supplied>μης καὶ κατακριμάτων πά<supplied reason="lost">ντων τῶν μέχρι τοῦ διεληλυθότος</supplied> 

    <lb n="21"/><supplied reason="lost">πρώτου ἔτο</supplied>υς καὶ αὐτοῦ τοῦ ἔτους, ἀπὸ δὲ ἰ<supplied reason="lost">διωτικῶν καὶ πάσης ἐμποιήσεως καὶ</supplied> 

    <lb n="22"/><supplied reason="lost">μηδένα κωλύ</supplied><unclear>ο</unclear>ντα τὴν Κοπρέαν μ<unclear>η</unclear><supplied reason="lost">δὲ τοῦς παρʼ αὐτῆς κυριεύειν τῶν ἀ</supplied>

    <lb n="23" break="no"/><supplied reason="lost">ρουρῶν καὶ τὰ</supplied> παρʼ αὐτῶν περιγεινόμε<unclear>ν</unclear><supplied reason="lost">α ἀποφέρεσθαι εἰς τὸ ἴδιον. ἐὰν δέ</supplied> 

    <lb n="24"/><supplied reason="lost">τι τούτων</supplied> <unclear>τῶν</unclear> <unclear>γεγ</unclear>ρα<supplied reason="lost">μ</supplied><unclear>μ</unclear>ένων πα<supplied reason="lost">ραβῇ ἡ Ἡράκλεια, ἀποτίσει τῇ Κο</supplied>

    <lb n="25" break="no"/><supplied reason="lost">πρέᾳ τὸ παρ</supplied><unclear>α</unclear>χωρητικὸν <expan>κ<supplied reason="lost">ε</supplied>φάλαιο<ex>ν</ex></expan> σὺ<unclear>ν</unclear> <supplied reason="lost">ἡμιολίᾳ καὶ ἐπίτιμον ἄλλας</supplied> 

    <lb n="26"/><supplied reason="lost">δραχμὰς ἐν</supplied>ακοσίας πεντήκοντα <num value="950"/> καὶ <supplied reason="lost">εἰς τὸ δημόσιον τὰς ἴσας. κύρια τὰ <expan>ὡμολ<ex>ογμένα</ex></expan></supplied> 

    <lb n="27"/><supplied reason="lost">καταβεβλη</supplied>μένου τοῦ ὑπὲρ <expan>ἐπιστάλ<ex>ματος</ex></expan> ὡρι<unclear>σ</unclear><supplied reason="lost">μένου. </supplied><gap reason="lost" quantity="20" unit="character"/> 

    <lb n="28"/><handShift new="m2"/> <gap reason="lost" extent="unknown" unit="character"/><unclear>μ</unclear>ερους <unclear>τ</unclear>ῶν <gap reason="lost" quantity="1" unit="character"/>ρειθηα<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="29"/><gap reason="lost" extent="unknown" unit="character"/><gap reason="illegible" quantity="1" unit="character"/>ειας δ<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> </ab></div>
