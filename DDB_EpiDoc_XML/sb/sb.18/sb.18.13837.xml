         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r" type="textpart"><!--milestone unit="4"--><ab>

    <lb n="1"/>βασιλεῖ Πτολεμαίωι χαίρειν Πευκέστης δ<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="26" unit="character"/><supplied reason="lost"> ἀδικοῦμαι ὑπὸ</supplied> 

    <lb n="2"/>Ἁρσοντωῦτος καὶ <unclear>Ἐ</unclear>φιμήνιος. ὑπάρχοντος γάρ μοι <gap reason="lost" quantity="38" unit="character"/> 

    <lb n="3"/>ἥμισυ <num value="1/2"/> διοικοδομήσας ἔκτισα γυμνάσιον· ἐν δὲ τ<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="38" unit="character"/> 

    <lb n="4"/><gap reason="illegible" quantity="2" unit="character"/> καὶ καταγεινομένου ἔτι πλεῖον καταγνόντε<unclear>ς</unclear> <gap reason="lost" quantity="38" unit="character"/> 

    <lb n="5"/>νιε<unclear>γτω</unclear>ν ἐν τῶι γυμ<supplied reason="lost">ν</supplied><unclear>ασ</unclear><supplied reason="lost">ί</supplied>ωι <gap reason="illegible" quantity="1" unit="character"/>υ<gap reason="illegible" quantity="1" unit="character"/>σο<gap reason="lost" quantity="3" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="1" unit="character"/><unclear>ι</unclear>β<gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" quantity="40" unit="character"/> 

    <lb n="6"/>σταθμὸν εἰσπορεύεσ<unclear>θ</unclear>αι φάμενος <gap reason="illegible" quantity="4" unit="character"/>κέναι αυ<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="2" unit="character"/><supplied reason="lost">. δέομαι οὖν σου, βασιλεῦ, προστάξαι Διοφάνηι τῶι</supplied> 

    <lb n="7"/>στρατηγῶι γράψαι Δημητρίωι τῶ<unclear>ι</unclear> <unclear>ἐ</unclear>πιστά<unclear>τ</unclear><supplied reason="lost">ηι </supplied><gap reason="lost" quantity="36" unit="character"/> 

    <lb n="8"/>τοὺς προγεγραμμένους ἀποστεῖλαι ἐπὶ Διο<supplied reason="lost">φάνην </supplied><gap reason="lost" quantity="26" unit="character"/><supplied reason="lost"> τούτων</supplied> 

    <lb n="9"/>γὰρ γενομένων διὰ σέ, βασιλεῦ, τοῦ δικαίου τ<supplied reason="lost">εύξομαι. <space extent="unknown" unit="character"/> </supplied> 

    <lb n="10"/><handShift new="m2"/> Δημητρίωι· μάλιστα <supplied reason="lost">μὲν διάλυσον αὐτούς· εἰ δὲ μή, ἀπόστειλον ὅπως ἐπὶ τοῦ καθήκοντος</supplied> 

    <lb n="11"/><supplied reason="lost">κριτηρίο</supplied>υ διακριθῶσιν. <gap reason="illegible" extent="unknown" unit="character"><desc>vestiges</desc></gap> <supplied reason="lost"> <space extent="unknown" unit="character"/> </supplied> 
