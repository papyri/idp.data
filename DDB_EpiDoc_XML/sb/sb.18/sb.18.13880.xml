    <lb n="4"/><expan>ἀριθμ<ex>ητικὸν</ex></expan> <choice><reg>κατοίκων</reg><orig>κατύκον</orig></choice> <expan>κέρ<ex>ματος</ex></expan> <expan><ex>δραχμὰς</ex></expan> δεκαὲξ <num value="16"/> κ<supplied reason="lost">αὶ ἄλλων</supplied> 

    <lb n="5"/>εἰδῶν καὶ γεωμετρίας δραχμὰς <unclear>θ</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="6"/>σειου Παρ<unclear>άμο</unclear>ν<unclear>ο</unclear>ς καὶ Κεφ<unclear>άλω</unclear>ν πο<gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="7"/>ς<gap reason="illegible" quantity="2" unit="character"/> <expan><ex>πυροῦ</ex></expan> <num value="4"><unclear>δ</unclear></num> Πανεφρύμις μου ναυβί<unclear>ου</unclear> <expan>κατ<supplied reason="lost">ο<ex>ίκων</ex></supplied></expan> <gap reason="lost" quantity="2" unit="character"/> 

    <lb n="8"/><expan>προσ<ex>διαγραφόμενα</ex></expan> <num value="30">λ</num>, <expan>κολ<ex>λύβου</ex></expan> <num value="10">ι</num>, <expan>συμ<ex>βολικά</ex></expan>. <expan>Τρύφω<ex>ν</ex></expan> <expan>συνέσχ<ex>ον</ex></expan> ὡς <expan>πρόκ<ex>ειται</ex></expan>. 

