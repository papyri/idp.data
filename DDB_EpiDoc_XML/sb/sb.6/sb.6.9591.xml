         <head n="38830" xml:lang="en">
            <date>VIIspc?</date>
            <placeName>Hermoupolis Magna</placeName>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><ab>
    <lb n="1"/><g type="stauros"/> ἐν ὀνόματι τῆς ἁγίας καὶ ζωοποιοῦ καὶ ὁμοουσίου 

    <lb n="2"/>Τριάδος πατρὸς καὶ υἱοῦ καὶ ἁγίου πνεύματος ἐγράφη 

    <lb n="3"/>Φαῶφι <unclear>τρεισ</unclear>καιδεκάτῃ δεκάτης <expan>ἰ<supplied reason="lost">ν</supplied>δ<ex>ικτί</ex>ο<ex>νος</ex></expan>. 

    <lb n="4"/><g type="chirho"/> Φλαυίοις Θεοδώρᾳ καὶ Ὥρῳ <choice><reg>ἐπιτιμ<supplied reason="omitted">ι</supplied>ωτάτῳ</reg><orig>ἐ<supplied reason="lost">π</supplied><unclear>ι</unclear>τιμωτάτῳ</orig></choice> 

    <lb n="5"/>υἱῷ Δαμασκίου ὑπὲρ τοῦ τρίτ<supplied reason="lost">ου</supplied> <choice><reg>ἐνάτου</reg><orig><unclear>ἐν</unclear>νάτου</orig></choice> 

    <lb n="6"/>τῶν ἑξῆς <choice><reg>ἐντεταγμένων</reg><orig>ἐντεταγμενου</orig></choice> τόπων <choice><reg>οἰκίας</reg><orig>οἰκείας</orig></choice> 

    <lb n="7"/>καὶ <choice><reg>Φοιβάμμωνι</reg><orig>Φοιβάμμ<supplied reason="lost">ων</supplied>ος</orig></choice> <choice><reg>υἱῷ</reg><orig>υἱοῦ</orig></choice> Ἀφοῦτος <surplus>τος</surplus> <unclear>τ</unclear>ῷ <unclear>ἐπισ</unclear>η<unclear>μο</unclear>

    <lb n="8" break="no"/>τάτῳ ὑπὲρ ἄλλου <choice><reg>ἐνάτου</reg><orig>ἐννάτου</orig></choice> μέρους τῶν αὐτῶν 

    <lb n="9"/>τόπων <choice><reg>οἰκίας</reg><orig>οἰκείας</orig></choice> ἀπὸ τῆς <expan>Ἑρμ<ex>ου</ex>πόλ<ex>εως</ex></expan> Αὐρηλίου 

    <lb n="10"/>Σινουθίου υἱοῦ Ἠλία ἑξῆς <supplied reason="lost">ὑπο</supplied>γράφοντος 

    <lb n="11"/>ὑπὸ τῆσ<supplied reason="lost">δε ὁ</supplied>μολ<unclear>ο</unclear>γίας ἐγ<supplied reason="lost">γ</supplied>υωμένου αὐτοῦ 

    <lb n="12"/>καὶ ἀναδεχ<supplied reason="lost">ομένου τ</supplied>ὴν ἀπό<unclear>δ</unclear>οσιν τοῦ ἑξῆς 

    <lb n="13"/>ἐντετα<supplied reason="lost">γμένου</supplied> <choice><reg>ἐνοικίου</reg><orig>ἐνοι<supplied reason="lost">κ</supplied>είου</orig></choice> <supplied reason="omitted">ὑπὲρ</supplied> ἐμοῦ Φλαυίου 

    <lb n="14"/>Σερήν<supplied reason="lost">ου </supplied><gap reason="lost" quantity="6" unit="character"/><unclear>ρ</unclear>ίου ἀπὸ <supplied reason="lost">τῆς</supplied> <unclear>αὐ</unclear>τῆς <expan>Ἑρμ<ex>ου</ex>πό<ex>λεως</ex></expan>. 

    <lb n="15"/><supplied reason="lost">ὁμολογῶ ἑκ</supplied>ουσίως καὶ αὐ<supplied reason="lost">θαιρέτ</supplied>ως <supplied reason="lost">μ</supplied>εμισ

    <lb n="16" break="no"/><supplied reason="lost">θῶσθαι ἐμαυτ</supplied>ὸν παρὰ σοῦ <supplied reason="lost">πρὸς</supplied> <choice><reg>ἑξαέτη</reg><orig>ἑξ<supplied reason="lost">α</supplied>ετους</orig></choice> 

    <lb n="17"/>χρόνον λογιζόμενον ἀπὸ <supplied reason="lost">τῆς σή</supplied>μερον 

    <lb n="18"/>ἡμέρας, ἥ ἐστιν <supplied reason="lost">Φ</supplied>αῶφι τρεισκαιδεκάτη 

    <lb n="19"/><supplied reason="lost">τῆς</supplied> παρού<supplied reason="lost">ση</supplied>ς <num value="10">ι</num> <expan><supplied reason="lost">ἰ</supplied>νδ<ex>ικτί</ex>ο<ex>νος</ex></expan> <supplied reason="lost">τὸ</supplied> διαφέρον 

    <lb n="20"/><supplied reason="lost">ὑμ</supplied>ῖν προ<unclear>ς</unclear><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/>ετέρα<supplied reason="lost">ν ἐ</supplied>πιστολὴν 

    <lb n="21"/><gap reason="lost" quantity="4" unit="character"/><unclear>ου</unclear> <unclear>τὸ</unclear> <supplied reason="lost">τρίτον ἔνατον μέρος ἀπὸ</supplied> ὁλοκλήρου 

    <lb n="22"/><supplied reason="lost">οἰκίας διακειμένης ἐπʼ ἀμφόδου</supplied> <gap reason="illegible" quantity="1" unit="character"/>πια<gap reason="lost" quantity="3" unit="character"/><gap reason="illegible" quantity="2" unit="character"/> 

    <lb n="23"/><gap reason="lost" extent="unknown" unit="line"/> </ab></div>
