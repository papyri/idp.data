    <lb n="7"/><gap reason="lost" quantity="35" unit="character"/><supplied reason="lost"> εὐ</supplied><unclear>χ</unclear>αριστείας. <handShift new="m2"/> ἐγράφη μηνὶ Τῦβι <num value="15">ιε</num> <gap reason="illegible" quantity="4" unit="character"/> <handShift new="m3"/> Χριστόφ<supplied reason="lost">ο</supplied>ρος <expan>ταβουλάριο<ex>ς</ex></expan> 

