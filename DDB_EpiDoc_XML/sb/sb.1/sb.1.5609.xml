         <head n="23228" xml:lang="en">
            <date>AD734</date>
            <placeName>Thebes</placeName>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r,A" type="textpart"><!--milestone unit="4"--><ab>

    <lb n="1"/><g type="stauros"/> ἐν ὀνόματι τῆς ἁγίας καὶ <choice><reg>ζωοποιοῦ</reg><orig>ζωοπιοῦ</orig></choice> 

    <lb n="2"/>ὁμοουσίου <expan>τριάδο<ex>ς</ex></expan> πατρὸς καὶ τοῦ υἱοῦ 

    <lb n="3"/>καὶ τοῦ <choice><reg>ἁγίου</reg><orig>ἁγίω</orig></choice> πνεύματος <g type="stauros"/>. 

    <lb n="4"/>ἐγράφη <choice><reg>μηνὶ</reg><orig>μενὶ</orig></choice> Παῦνι <num value="6">ϛ</num>, <expan>ἰνδ<ex>ικτίωνος</ex></expan> <expan>τρίτη<ex>ς</ex></expan>. 

    <lb n="5"/>ἐπὶ Μαμὲτ ἀμίρᾳ <expan>εὐκλ<ex>εεστάτῳ</ex></expan> ἀμίρᾳ τῆς παγαρχίας 

    <lb n="6"/>Ἑρμόνθεος καὶ Χαὴλ υἱὸς Ψμο λαμπρο

    <lb n="7" break="no"/>τάτου <expan>διοικ<ex>η</ex>τ<ex>οῦ</ex></expan> ἀπὸ <choice><reg>κάστρων</reg><orig>κάστρον</orig></choice> <expan>Μεμνωνίω<ex>ν</ex></expan>, 

    <lb n="8"/>ἔτους <expan>Διοκλη<ex>τιανοῦ</ex></expan> <choice><reg>βασιλέως</reg><orig>βασιλευς</orig></choice> <num value="451">υνα</num> καὶ ἔτους <choice><reg>Σαρακηνῶν</reg><orig>Σαρακοινο͂ν</orig></choice> <num value="114">ριδ</num> <g type="stauros"/> <g type="stauros"/> <g type="stauros"/>. 
</ab></div><div n="r,B" type="textpart"><!--milestone unit="4"--><ab>

    <lb n="1"/><expan>ἐγρ<ex>άφη</ex></expan> <expan>μ<ex>ηνὸς</ex></expan> <expan>Παῦ<ex>νι</ex></expan> <num value="6">ϛ</num>, <expan>ἰ<ex>ν</ex>δ<ex>ικτίωνος</ex></expan> <num value="3">γ</num> <expan>ἀρχ<ex>ῇ</ex></expan> <g type="stauros"/> <g type="stauros"/> <g type="stauros"/>. 
</ab></div><div n="v" type="textpart"><!--milestone unit="4"--><ab>

    <lb n="9"/>ἔγγραφον <choice><reg>δωρεαστικὸν</reg><orig>δωριαστικὸν</orig></choice> <expan>γενάμε<ex>νον</ex></expan> παρὰ Ἄννα<unclear>ς</unclear> 

    <lb n="10"/><gap reason="lost" extent="unknown" unit="character"/> ἀπὸ κάστρου <expan>Μεμνωνίω<ex>ν</ex></expan> 

    <lb n="11"/><expan>ὑ<ex>πὲρ</ex></expan> <choice><reg>οἴκου</reg><orig>οἶκον</orig></choice> <expan>Ἰωάνν<ex>ου</ex></expan> <expan>πατρὸ<ex>ς</ex></expan> <expan>αὐτ<ex>ῆς</ex></expan> <expan>κ<ex>αὶ</ex></expan> <expan>ὑ<ex>πὲρ</ex></expan> <expan>μέρο<ex>υς</ex></expan> <choice><reg><expan>ἀρτοπωλ<ex>ίου</ex></expan></reg><orig><unclear>ἀ</unclear>ρτακοπολ</orig></choice> 

    <lb n="12"/><expan>κ<ex>αὶ</ex></expan> <expan>ὑ<ex>πὲρ</ex></expan> <expan>το<ex>ῦ</ex></expan> <expan><ex>ἡμίσους</ex></expan> <expan>μέρο<ex>υς</ex></expan> οἴκου Ταὰμ <choice><reg><expan>μητρὸ<ex>ς</ex></expan></reg><orig>μετρο</orig></choice> <expan>αὐτ<ex>ῆς</ex></expan>. <expan>μ<ex>ηνὸς</ex></expan> <expan>Παῦ<ex>νι</ex></expan> <num value="6">ϛ</num>, <expan>ἰ<ex>ν</ex>δ<ex>ικτίωνος</ex></expan> <num value="3">γ</num> <g type="stauros"/>. </ab></div></div>
