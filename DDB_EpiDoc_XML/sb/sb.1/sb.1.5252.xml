    <lb n="30"/><supplied reason="lost">δρ</supplied>αχμὰς τεσσαράκο<supplied reason="lost">ντα ὀκ</supplied>τώ <num value="48"/>, <supplied reason="lost"><expan>κ<ex>αὶ</ex></expan></supplied> βε<supplied reason="lost">βαιούτω </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="31"/><gap reason="lost" quantity="2" unit="character"/>τος δὲ καὶ <gap reason="lost" extent="unknown" unit="character"/>λλ<gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> ὡ<supplied reason="lost">ς ἐτ</supplied>ῶν 

