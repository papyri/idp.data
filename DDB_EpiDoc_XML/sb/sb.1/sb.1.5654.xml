    <lb n="12"/><unclear>ἕκτ</unclear>ον, <expan>κ<ex>αὶ</ex></expan> <expan>ὑ<ex>πὲρ</ex></expan> <expan>ἐμ<unclear>β</unclear>ολῆ<ex>ς</ex></expan> <expan>σί<ex>του</ex></expan> <expan>ἀρτ<ex>άβας</ex></expan> <num value="235">σλε</num>, <choice><reg>διακοσίας</reg><orig>διακοσιω</orig></choice> τριάκον<supplied reason="lost">τα</supplied> 

