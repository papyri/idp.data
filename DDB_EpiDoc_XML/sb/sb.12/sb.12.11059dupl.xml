         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r" type="textpart"><ab>

    <lb n="1"/><supplied evidence="parallel" reason="undefined">βασιλεύοντος Πτολεμ</supplied>αίου τοῦ Πτολεμαίου καὶ Ἀρσινόη<supplied reason="lost">ς θεῶν Ἀδελφῶν ἔτους τετάρ<supplied evidence="parallel" reason="undefined">του</supplied> </supplied><gap reason="lost" quantity="8" unit="character"/><supplied reason="lost" cert="low"> <space extent="unknown" unit="character"/> </supplied> 

    <lb n="2"/><supplied evidence="parallel" reason="undefined">ἐφʼ ἱερέως Ἀρχελάου τοῦ Δαμᾶ τὸ δ</supplied>εύτερον ἔτος Ἀλεξάνδ<unclear>ρ</unclear><supplied reason="lost">ου καὶ θεῶν Ἀδελ</supplied><supplied evidence="parallel" reason="undefined"><supplied reason="lost">φῶν, κανηφόρου</supplied> <unclear>Ἀρ</unclear><supplied reason="lost">σινόης</supplied> 

    <lb n="3"/>Φιλαδ</supplied>έλφου Ἀρσινόης τῆς <supplied evidence="parallel" reason="undefined">Πολ</supplied>ε<supplied evidence="parallel" reason="undefined">μοκράτους μη</supplied>νὸς Ἀρτε<supplied reason="lost">μισίου </supplied><gap reason="lost" quantity="13" unit="character"/><supplied reason="lost"> ἐν Ὀξυρ</supplied><unclear>ύ</unclear>γ<supplied reason="lost">χων</supplied> 

    <lb n="4"/>πόλει <supplied evidence="parallel" reason="undefined">τῆς Θηβαίδος. ἐδάνε<unclear>ι</unclear><supplied reason="lost">σ</supplied></supplied><supplied reason="lost">ε</supplied> Παρμενίων Φίλωνος Μ<unclear>υσὸς</unclear> <supplied reason="lost">τῆς ἐπιγονῆς Φιλίππωι </supplied><supplied evidence="parallel" reason="undefined"><supplied reason="lost">Κυρη</supplied>να<supplied reason="lost">ίωι τῶν</supplied> 

    <lb n="5"/>Ζωίλο</supplied>υ ἰ<unclear>δ</unclear>ιώτ<supplied evidence="parallel" reason="undefined">ηι ἀργυρίου</supplied> <add place="above">ὀφθαλμοφ<unclear>ανο</unclear>ῦς</add> δραχμὰς ἑκατὸ<supplied evidence="parallel" reason="undefined">ν πεντήκον<supplied reason="lost">τα <num value="150"/> εἰς ἔτ</supplied></supplied><supplied reason="lost">η τρία <num value="3"/> <supplied evidence="parallel" reason="undefined">ἄτοκον. τού</supplied></supplied><unclear>τ</unclear>ου <supplied reason="lost">δὲ τοῦ</supplied> 

    <lb n="6"/>ἀργυρίου τὴν ἀπόδοσ<unclear>ιν</unclear> <supplied evidence="parallel" reason="undefined">ποιείσθω Φίλιππος Παρ</supplied>με<supplied reason="lost">νί<supplied evidence="parallel" reason="undefined">ωνι ἀρχόμενος ἀ</supplied>πὸ τοῦ <num value="4">δ</num> <expan><ex>ἔτους</ex></expan> </supplied><add place="above"><supplied reason="lost" cert="low"><choice><reg>ἐν</reg><orig>ἐμ</orig></choice> μηνὶ Ἀπ<supplied evidence="parallel" reason="undefined">ελλαίωι</supplied>  καθʼ ἕκαστον</supplied> 

    <lb n="7"/><unclear>ἔτ</unclear>ος <supplied reason="lost">ἀρ</supplied>γυρίου δραχ</add>μὰς <unclear>π</unclear>ε<unclear>ν</unclear>τήκοντα <num value="50"/> ἐπὶ τὰ τρία  ἔ<unclear>τ</unclear>η· <supplied reason="lost">ἐ</supplied>ὰ<unclear>ν</unclear> δ<unclear>ὲ</unclear> <supplied reason="lost"><supplied evidence="parallel" reason="undefined">τὴν ἀπόδοσιν ταύτην μὴ εὐτακτήση</supplied>ι,</supplied> 

    <lb n="8"/><supplied reason="lost">ἀποτεισάτω τὸ ἡμιό</supplied><unclear>λιον</unclear> <supplied reason="lost">κ</supplied><unclear>αὶ</unclear> <unclear>τόκ</unclear><supplied reason="lost" cert="low">ο</supplied><unclear>ν</unclear> <supplied reason="lost">το</supplied><unclear>ῦ</unclear> ὑπ<supplied evidence="parallel" reason="undefined">ερπεσόντος <unclear>χ</unclear><supplied reason="lost">ρόνου ὡς ἐκ δύο <num value="2"/> δραχμ</supplied></supplied><supplied reason="lost">ῶν τῆι μνᾶι τὸν μῆ</supplied>

    <lb n="9" break="no"/><supplied reason="lost">να ἕκαστον. ἔγγυος Φ</supplied><unclear>ι</unclear>λίππου <del rend="erasure"><unclear>τῶν</unclear></del> <add place="above"><supplied reason="lost">το</supplied>ῦ δανείου</add> κατὰ τὴν συγ<unclear>γ</unclear><supplied reason="lost">ρα</supplied>φ<unclear>ὴ</unclear><supplied reason="lost">ν ε</supplied><unclear>ἰ</unclear>ς <supplied reason="lost">ἔκτισιν </supplied><gap reason="lost" quantity="15" unit="character"/> 

    <lb n="10"/><gap reason="lost" quantity="17" unit="character"/>ς, καὶ ἡ πρᾶξις ἔστω Πα<supplied reason="lost">ρ</supplied><unclear>μ</unclear>ενί<unclear>ω</unclear><supplied reason="lost">νι ἢ τοῖς παρὰ Παρμενίωνος κατὰ τὸ διά</supplied>

    <lb n="11" break="no"/><supplied reason="lost">γραμμα· κυρία δʼ ἔστω <unclear>ἥδε</unclear></supplied> ἡ συγγραφὴ τοῦ δανείου οὗ ἂν ἐπιφέρηι <unclear>Παρ</unclear><supplied reason="lost">μενίων ἢ ἄλλος <unclear>τις</unclear> ὑπὲρ Παρμε</supplied>

