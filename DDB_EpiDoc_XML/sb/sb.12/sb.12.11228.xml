         <head n="16400" xml:lang="en">
            <date>AD 204</date>
            <placeName>Oxyrhynchus</placeName>
            <ref n="p.yale;1;68" type="reprint-from">PYale1</ref>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><ab>
    <lb n="1"/>Ἀπίων Ἀπολλωνίου τοῦ Σαραπίωνος μητρὸς <gap reason="lost" quantity="6" unit="character"/><supplied reason="lost"> καὶ ὁ ὁμοπάτριος αὐτοῦ ἀδελφὸς</supplied> 

    <lb n="2"/>Διογένης ὁ καὶ Ἄρειος μητρὸς Δημητροῦτος ἀπʼ <supplied reason="lost">Ὀξυρύγχων πόλεως Ζωίλῳ τῷ καὶ </supplied><gap reason="lost" quantity="7" unit="character"/> 

    <lb n="3"/>τι Γερμανοῦ μητρὸς Θαήσιος ἀπὸ τῆς αὐτῆς πόλ<supplied reason="lost">εως χαίρειν. ὁμολογοῦμεν ἐσχηκέναι καὶ πα</supplied>


    <lb n="4" break="no"/>ραμεμετρῆσθαι παρὰ σοῦ διὰ χειρὸς ἐξ οἴκου σου <supplied reason="lost">ἀργυρίου Σεβαστῶν νομίσματος δραχμὰς χι</supplied>

    <lb n="5" break="no"/>λίας ἑπτακοσίας <num value="1700"/> καὶ πυροῦ γενήματος τοῦ διελ<unclear>θ</unclear><supplied reason="lost">όντος δωδεκάτου ἔτους μέτρῳ </supplied><gap reason="lost" quantity="9" unit="character"/> 

    <lb n="6"/>κω<gap reason="illegible" quantity="1" unit="character"/> Ἀντᾶτος Μεγίστου ἀπὸ Ἐντείεως ἀρτάβ<unclear>α</unclear><supplied reason="lost">ς εἴκοσι τέσσαρας <num value="24"/> κεφαλαίου, αἷς οὐδὲν</supplied> 

    <lb n="7"/>προσῆκται, καὶ <choice><reg>ταξόμεθά</reg><orig>δαξόμεθά</orig></choice> σοι μόνου τοῦ ἀργ<supplied reason="lost">υρίου κατὰ μῆνα ἕκαστον ἀπὸ τοῦ ὄντος</supplied> 

    <lb n="8"/>μηνὸς Ἁθὺρ τοῦ ἐνεστῶτος <choice><reg>τρεισκαιδεκ<supplied reason="lost">άτου</supplied></reg><orig>τρισκαιδεκ<supplied reason="lost">άτου</supplied></orig></choice> <supplied reason="lost">ἔτους τόκου δραχμὰς δεκατρεῖς <num value="13"/>, εἰς δὲ</supplied> 

    <lb n="9"/>τὴν τούτου καὶ τόκων αὐτοῦ καὶ τοῦ πυροῦ <supplied reason="lost">διάλυσιν ἐξεῖναί σοι σπείρειν καὶ καρ</supplied>

    <lb n="10" break="no"/>πίζεσθαι ἔτι δὲ καὶ <choice><reg>ἐκμισθοῦν</reg><orig>ἐκμισθοιν</orig></choice> οἷς ἐὰν αἱρῇ ἐ<unclear>π</unclear><supplied reason="lost">ὶ χρόνον ἔτη τέσσαρα <num value="4"/> ἀπὸ τοῦ ἐνεστῶτος</supplied> 

    <lb n="11"/><choice><reg>τρεισκαιδεκάτου</reg><orig>τρισκαιδεκάτου</orig></choice> <supplied reason="omitted">ἔτους</supplied> τὰς ὑπαρχούσας τῷ ἑνὶ ἡμῶν <supplied reason="lost">Διογένει τῷ καὶ Ἀρείῳ περὶ τὴν αὐτὴν Ἐν</supplied>

    <lb n="12" break="no"/>τεῖ<hi rend="diaeresis">ι</hi>ν ἐκ τοῦ Σποκέως κλήρου <unclear>ἀ</unclear>ρούρας δ<supplied reason="lost">ε</supplied>κ<supplied reason="lost">απέντε <num value="15"/> καὶ τῷ Ἀπίωνι περὶ τὴν αὐτὴν κώμην</supplied> 

    <lb n="13"/>ἐκ τοῦ Φιλητίωνος κλήρου ἀρούρας πέντε <supplied reason="lost">ἥμισυ <num value="5"/><num value="1/2"/> σπείροντι τὰς ἐπὶ τὸ αὐτὸ ἀρούρας</supplied> 

    <lb n="14"/>εἴκοσι ἥ<unclear>μι</unclear><supplied reason="lost">σ</supplied><unclear>υ</unclear> <num value="20"/><num value="1/2"/> <supplied reason="lost">ο</supplied><unclear>ἷ</unclear><supplied reason="lost">ς ἐὰ</supplied>ν αἱρῇ χωρὶς ἰσάτεως καὶ <supplied reason="lost">ὀχομενίου, ἐκφορίου τῶν ὅλων ἀρουρῶν</supplied> 

    <lb n="15"/>ἐπὶ τὴν τετραετίαν κατʼ ἔτος πυ<unclear>ρο</unclear>ῦ μέτρῳ τ<unclear>ετ</unclear><supplied reason="lost">ραχοινίκῳ ἀρταβῶν </supplied><gap reason="lost" quantity="16" unit="character"/><supplied reason="lost"> ἑ</supplied>

    <lb n="16" break="no"/>πτά <num value="7"/>, ἀφʼ ὧν <choice><reg>ἐκκρούσεις</reg><orig>ἐκκρούσις</orig></choice> σεαυτ<unclear>ῷ</unclear> τῷ μὲν <num value="13">ι<unclear>γ</unclear></num> <supplied reason="lost"><expan><ex>ἔτει</ex></expan> </supplied><gap reason="lost" quantity="38" unit="character"/> 

    <lb n="17"/>μέν<supplied reason="lost">α</supplied>ς ἀρτά<unclear>β</unclear><supplied reason="lost">ας εἴ</supplied><unclear>κ</unclear>οσι τέσσαρ<supplied reason="lost">ας <num value="24"/> </supplied><gap reason="lost" quantity="3" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="33" unit="character"/><supplied reason="lost"> ἀρτά</supplied>

    <lb n="18" break="no"/>βα<supplied reason="lost">ς τ</supplied>ριάκοντα ἓξ <num value="36"/> <supplied reason="lost">τῆς</supplied> <unclear>ἐ</unclear>σομένης <gap reason="lost" quantity="44" unit="character"/> 

    <lb n="19"/>τιμῆς, ἀφʼ ἧς εἰς μὲν <unclear>τ</unclear><supplied reason="lost">ό</supplied>κου <gap reason="illegible" quantity="3" unit="character"/><gap reason="lost" quantity="45" unit="character"/> 

    <lb n="20"/>Ἁθὺρ <gap reason="illegible" quantity="3" unit="character"/> Παῦνι <gap reason="lost" quantity="3" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" quantity="43" unit="character"/><supplied reason="lost"> κεφα</supplied>

    <lb n="21" break="no"/>λαίου, ἀπὸ δὲ τοῦ ἑ<supplied reason="lost">ξῆς </supplied><num value="14"><supplied reason="lost">ι</supplied>δ</num> <expan><ex>ἔτους</ex></expan> ἐπὶ τὰ λ<supplied reason="lost">ο</supplied>ιπ<unclear>ὰ</unclear> <supplied reason="lost">ἔτη τρία <num value="3"/> </supplied><gap reason="lost" quantity="18" unit="character"/><supplied reason="lost"> ἐκκρούσεις σεαυ</supplied>

    <lb n="22" break="no"/>τῷ <supplied reason="lost">π</supplied>υροῦ ἀρτάβ<unclear>α</unclear><supplied reason="lost">ς</supplied> <unclear>π</unclear><supplied reason="lost">ε</supplied>ντήκοντ<supplied reason="lost">α</supplied> <num value="50"/>, τῆ<unclear>ς</unclear> ἐσ<supplied reason="lost">ομένης τιμῆς </supplied><gap reason="lost" quantity="26" unit="character"/> 

    <lb n="23"/>κατʼ ἔτος εἰς λόγον τόκου καὶ κεφαλαίου. τὸ δ<supplied reason="lost">ὲ περιλειπόμενον ἐκφόριον ἀποδώσεις</supplied> 

    <lb n="24"/>κατʼ ἔτος μηνὶ Παῦνι ἐφʼ ἅλω τῆς Ἐντείεως μέτρ<supplied reason="lost">ῳ τετραχοινίκῳ τῆς μετρήσεως γινομέ</supplied>

    <lb n="25" break="no"/>νης <hi rend="diaeresis">ὑ</hi>πὸ τῶν παρʼ ἡμῶν. ἐὰν δὲ ἐν τῷ τετραετί<supplied reason="lost">ας χρόνῳ </supplied><gap reason="lost" quantity="26" unit="character"/> 

    <lb n="26"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="2" unit="character"/><unclear>ι</unclear> τόκοι, ἕξεις τ<unclear>ὴν</unclear> τῶν αὐτῶν ἀρουρ<supplied reason="lost">ῶν </supplied><gap reason="lost" quantity="28" unit="character"/><supplied reason="lost"> συγ</supplied>

    <lb n="27" break="no"/>κομιδὴν ἐπὶ τοῖς προκειμένοις πᾶσι ἕως οὗ πληρω<supplied reason="lost">θῇς. καὶ τελέσομεν ἡμεῖς τὰ ὑπὲρ τοῦ τῶν</supplied> 

    <lb n="28"/>αὐτῶν ἀρουρῶν ἡμίσους <num value="1/2"/> κατʼ ἔτος δημόσια πάντ<supplied reason="lost">α, περὶ ὧν κατὰ πάντα τρόπον ἀπαρενόχλη</supplied>

    <lb n="29" break="no"/>τόν σε καὶ ἀνείσπρακτον παρεξόμεθα, ἐπὶ δὲ π<unclear>α</unclear><gap reason="lost" quantity="32" unit="character"/> 

    <lb n="30"/>λογίας καὶ βεβαιώσομέν σοι τὴν δηλουμένη<supplied reason="lost">ν γῆν πάσῃ βεβαιώσει, τῆς πράξεώς σοι γι</supplied>

    <lb n="31" break="no"/>νομένης παρά τε ἡμῶν ἀλληλεγγύων ὄντω<supplied reason="lost">ν εἰς ἔκτισιν καὶ ἐξ οὗ ἐὰν ἡμῶν αἱρῇ καὶ</supplied> 

    <lb n="32"/>ἐκ τῶν προκειμένων ἀρουρῶν καὶ ἐκ τῶν ἄλ<supplied reason="lost">λων τῶν ὑπαρχόντων ἡμῖν πάντων καθάπερ</supplied> 

    <lb n="33"/>ἐκ δίκης. κύριον τὸ χειρόγραφον δισσὸν γραφὲν π<supplied reason="lost">ανταχῇ ἐπιφερόμενον καὶ παντὶ τῷ ἐπι</supplied>

    <lb n="34" break="no"/>φέροντι. <expan><ex>ἔτους</ex></expan> <num value="13">ιγ</num> Αὐτ<supplied reason="lost">ο</supplied>κρατόρων <supplied reason="lost">Καισά</supplied>ρων Λουκίου Σε<supplied reason="lost">πτιμίου Σεουήρου Εὐσεβοῦς Περτίνακος Ἀραβικοῦ</supplied> 

    <lb n="35"/>Ἀδιαβηνικοῦ Παρθικοῦ με<unclear>γίστου</unclear> καὶ Μάρκου Αὐρηλί<supplied reason="lost">ου Ἀντωνίνου Εὐσεβοῦς Σεβαστῶν καὶ Πουβλίου</supplied> 

    <lb n="36"/>Σεπτιμίου Γέτα <choice><reg>Καίσαρο<supplied reason="omitted">ς</supplied></reg><orig>Καίσαρο</orig></choice> Σεβαστοῦ, Ἁθὺρ <num value="20">κ</num>. <handShift new="m2"/> Ἀπίων <supplied reason="lost">Ἀπολλωνίου καὶ ὁ ἀδελφὸς Διογένης</supplied> 

    <lb n="37"/>ὁ καὶ Ἄριος <choice><reg>ὀφείλομεν</reg><orig>ὀφίλομεν</orig></choice> τῷ Ζωίλῳ τὰς <supplied reason="lost">δραχμὰς χιλίας ἑπτακοσίας <num value="1700"/> καὶ πυροῦ</supplied> 

    <lb n="38"/>ἀρτάβας εἴκοσι τέσσαρας <num value="24"/> καὶ ταξόμε<supplied reason="lost">θα μόνου τοῦ ἀργυρίου τόκου κατὰ μῆ</supplied>

    <lb n="39" break="no"/>να ἕκαστον δραχμὰς <choice><reg>δεκατρεῖς</reg><orig>δεκατρῖς</orig></choice> <num value="13"/>, εἰς δ<unclear>ι</unclear><supplied reason="lost">άλυσιν δὲ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="40"/>την τῶν <gap reason="lost" quantity="12" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="2" unit="character"/><unclear>σι</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="41"/>προδ<gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="42"/><gap reason="lost" extent="unknown" unit="line"/> </ab></div>
