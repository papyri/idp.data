    <lb n="1"/><gap reason="lost" extent="unknown" unit="line"/> 

    <lb n="1"/><gap reason="lost" extent="unknown" unit="character"/><gap reason="illegible" quantity="1" unit="character"/>ρου καὶ ἐπὶ τούτοις <choice><reg>καὶ ἐγὼ</reg><orig>κἀγὼ</orig></choice> ὁ Παῦλος ἐπερωτηθεὶς εἰς τὸ κατʼ ἐμὲ τρίτον μέρος ὡμολόγ<unclear>ησα</unclear><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="2"/><gap reason="lost" extent="unknown" unit="character"/><unclear>ι</unclear>κῷ πύργῳ συμπόσιον τὸ ἐπάνω τῆς ἐξέδρας καὶ τὰς δύο <num value="2"/> καμάρας τὰς <hi rend="diaeresis">ὑ</hi>ποκάτω τῆς αὐτῆ<unclear>ς</unclear> <supplied reason="lost">ἐξέδρας </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="3"/><gap reason="lost" extent="unknown" unit="character"/> <unclear>ὑ</unclear>μῖν μεμενηκότω<unclear>ν</unclear> τοῦ τε αἰθρίου καὶ φρέατος καὶ τοῦ ξυστοῦ τοῦ καταγαίου καὶ εἰσόδων <supplied reason="lost">καὶ ἐξόδων </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="4"/><gap reason="lost" extent="unknown" unit="character"/><gap reason="illegible" quantity="1" unit="character"/> οἰκίας αὐλῆς τῆς ῥ<supplied reason="lost">ύμ</supplied>ης διειργούσης· καὶ εἰ <choice><reg><hi rend="diaeresis">ὑ</hi>πορραφή</reg><orig>ὑποραφή</orig></choice> <choice><reg>τι<supplied reason="omitted">ς</supplied></reg><orig>τι</orig></choice> γένηται ἐν τῇ οἰκίᾳ καὶ τῇ αὐλῇ ἐπὶ τῷ ἡ<supplied reason="lost">μᾶς </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="5"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> κε</supplied><unclear>κλ</unclear>ήρωται τό<unclear>π</unclear>ων σὺν <supplied reason="lost">τέ</supplied>κνοις καὶ ἐκγόνοις καὶ τοῖς μεταλημψομένοις καὶ ἐξουσίαν ἔχειν χρᾶσθα<unclear>ι</unclear> <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="6"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> δεσπο</supplied>τῶν ἡμῶν Θεοδοσί<supplied reason="lost">ο</supplied>υ Οὐαλεντινιανοῦ τῶν αἰωνίων Αὐγούστων ἐμμένειν <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="7"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ταύ</supplied>τῃ τῇ <choice><reg>διαιρέσει</reg><orig>διαιρέσι</orig></choice> δοξά<supplied reason="lost">ν</supplied>των ἐπὶ τῷ <choice><reg>τὸν</reg><orig>τὸμ</orig></choice> παραβαίνοντα παρασχεῖν <hi rend="diaeresis">ὑ</hi>πὲρ λόγου π<supplied reason="lost">ροστίμου </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="8"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ἐγ</supplied>ράφη τρισσὴ πρὸς τὸ ἑ<unclear>κ</unclear>άτερον μέρος ἔχε<unclear>ι</unclear>ν ἀνὰ μοναχὸν πρὸς <choice><reg>ἀσφάλειαν</reg><orig>ἀσφάλιαν</orig></choice> καὶ ἐπερω<supplied reason="lost">τηθέντες ὡμολόγησαν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="9"/><handShift new="m2"/> <gap reason="lost" extent="unknown" unit="character"/><choice><reg><supplied reason="lost">διαί</supplied>ρεσις</reg><orig><supplied reason="lost">δι</supplied>ερισις</orig></choice> καὶ <choice><reg>ἐκλη<supplied reason="omitted">ρώ</supplied>θην</reg><orig>ἐκληθην</orig></choice> τοὺς ἐπʼ ἐμοὶ προδηλωθέντας <supplied reason="lost">τόπους </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="10"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ἐπερ</supplied>ωτηθεὶς <choice><reg>ὡμολόγησ<supplied reason="omitted">α</supplied></reg><orig>ὡμολογης</orig></choice>. <choice><reg>Θε<supplied reason="omitted">ό</supplied>δωρος</reg><orig>Θέδ<unclear>ωρ</unclear>ος</orig></choice> διάκονος καθολικῆς ἐκκλησίας <hi rend="diaeresis">υ</hi>ἱὸς Βη<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="11"/><gap reason="lost" extent="unknown" unit="character"/> καὶ σ<unclear>υ</unclear>φ<gap reason="lost" quantity="4" unit="character"/><gap reason="illegible" quantity="1" unit="character"/> <choice><reg>πά<supplied reason="omitted">ν</supplied>τα</reg><orig>πατα</orig></choice> τὰ προ<supplied reason="lost">γεγρ</supplied>αμμένα ὡς π<unclear>ρ</unclear><supplied reason="lost">όκ</supplied>ιται καὶ ἐπερωτηθεὶς <supplied reason="lost">ὡμολόγησα </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="12"/><gap reason="lost" extent="unknown" unit="character"/> <space extent="unknown" unit="character"/> 

    <lb n="13"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ἐκληρώ</supplied>θην <supplied reason="lost">τοὺς ἐπʼ</supplied> ἐμοὶ προδ<supplied reason="lost">ηλωθέν</supplied>τας <supplied reason="lost">τό</supplied><unclear>π</unclear>ους καὶ ὤμοσ<supplied reason="lost">α τὸν θεῖον ὅρκον </supplied><gap reason="lost" extent="unknown" unit="character"/> </ab></div>
