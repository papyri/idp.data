         <head n="43212" xml:lang="en">
            <date>AD 65</date>
            <placeName>Ptolemais Euergetis</placeName>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><ab>
    <lb n="1"/>ἔτους δωδεκάτου Νέρωνος <unclear>Κ</unclear>λαυδίου

    <lb n="2"/>Καίσαρος Σεβαστοῦ Γερμανικοῦ Αὐτοκρά<unclear>τορ</unclear><supplied reason="lost">ος</supplied>

    <lb n="3"/>μηνὸς Νερωνείου Σεβαστοῦ <num value="7">ζ</num> ἐν Πτ<unclear>ο</unclear><supplied reason="lost">λε</supplied>

    <lb n="4" break="no"/>μαείδι Εὐεργέτιδι τοῦ <choice><reg>Ἀρσινοίτου</reg><orig>Ἀρσενοίτου</orig></choice> νομο<supplied reason="lost">ῦ.</supplied>

    <lb n="5"/>ἐπρίατο Ἡρακλείδης Μάρωνος ὡς <unclear>ἐ</unclear><supplied reason="lost">τῶν</supplied>

    <lb n="6"/>πεντήκοντα ἓξ <num value="56"/> μέσος μελίχρως <app type="alternative"><lem><supplied reason="lost">μακρο</supplied>

    <lb n="7" break="no"/>πρόσωπος</lem><rdg><supplied reason="lost">μικρο</supplied>πρόσωπος</rdg></app> εὐθύριν οὐλὴ μήλωι δε<supplied reason="lost">ξιῶι παρὰ </supplied><gap reason="lost" extent="unknown" unit="character"/>

    <lb n="8"/>Μάρωνος ὡς ἐτῶν ἑξήκοντα<supplied reason="lost">α</supplied> δύ<supplied reason="lost">ο <num value="62"/> </supplied><gap reason="lost" extent="unknown" unit="character"/>

    <lb n="9"/>μελίχρω μακροπροσώ<supplied reason="lost">που</supplied> εὐθ<unclear>ύ</unclear><supplied reason="lost">ρινος οὐλὴ</supplied>

    <lb n="10"/>ἀντικνημίωι δεξιῶι <supplied reason="lost">παι</supplied><unclear>δίσκ</unclear><supplied reason="lost" cert="low">ην δούλην </supplied>

    <lb n="11"/><choice><reg>οἰκογενῆ</reg><orig>οἰκωγενῆ</orig></choice> ᾗ <choice><reg>ὄνομα</reg><orig>ὤνομα</orig></choice> <gap reason="lost" extent="unknown" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ὡς ἐτῶν</supplied>

    <lb n="12"/>ἓξ <num value="6"/> καὶ <unclear>μ</unclear>ηνῶν <num value="8"><unclear>η</unclear></num> ἄ<supplied reason="lost">σημον ταύτην τοι</supplied>

    <lb n="13" break="no"/>αύτην <unclear>ἀνα</unclear><supplied reason="lost">πόριφον πλὴν ἐπαφῆς καὶ ἱε</supplied>

    <lb n="14" break="no"/><unclear>ρ</unclear>ᾶς νό<supplied reason="lost">σου </supplied><gap reason="lost" extent="unknown" unit="character"/>

