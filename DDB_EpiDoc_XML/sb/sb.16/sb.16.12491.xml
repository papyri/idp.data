         <head n="36003" xml:lang="en">
            <date>VI/VII spc</date>
            <placeName>Hermopolite</placeName>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r" type="textpart"><ab>

    <lb n="1"/><gap reason="lost" extent="unknown" unit="line"/> 

    <lb n="2"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> υἱὸς Χάριτος </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ὁμολογῶ ἐσχηκέναι καὶ πεπληρῶσθαι</supplied> 

    <lb n="1"/><supplied reason="lost">παρʼ ὑμῶν τῆς τελείας καὶ ἀξίας τιμῆς οἴνου νέου</supplied> 

    <lb n="2"/><choice><reg>μούστου</reg><orig>μούσθου</orig></choice> μ<supplied reason="lost">έτρω</supplied>ν τρι<unclear>α</unclear><supplied reason="lost">κοσίων <expan>γ<ex>ίνεται</ex></expan> <expan>οἴ<ex>νου</ex></expan> <expan>μ<ex>έτρα</ex></expan> <num value="300">τ</num> ὅνπερ</supplied> 

    <lb n="3"/>οἶνον ὁμολο<supplied reason="lost">γῶ π</supplied>αρα<supplied reason="lost">σχεῖν ὑμῖν παρὰ ληνὸν ἐν τῷ</supplied> 

    <lb n="4"/>μηνὶ Μεσορὴ καιρῷ <supplied reason="lost">τρύγης τῆς σὺν θεῷ εἰσιούσης</supplied> 

    <lb n="5"/>ὀγδόης <expan>ἰνδι<ex>κτί</ex>ο<ex>νος</ex></expan> ἐν οἴνῳ <supplied reason="lost">νέῳ καλλίστῳ καὶ εὐαρέστῳ</supplied> 

    <lb n="6"/>μέτρῳ τοῦ ὑμετέρο<supplied reason="lost">υ πίθου καὶ εἰ εὑρεθείη ὄξος</supplied> 

    <lb n="7"/>ἢ ἀποίητος ἢ ὀζόμεν<supplied reason="lost">ος ἐξ αὐτοῦ μέχρι ὅλου τοῦ</supplied> 

    <lb n="8"/>Τῦβι μηνὸς τῆς αὐτῆς <supplied reason="lost"><expan>ἰνδι<ex>κτί</ex>ο<ex>νος</ex></expan> ὁμολογῶ ἀλλάξαι ὑμῖν</supplied> 

    <lb n="9"/>ἐν καλῷ οἴ<unclear>ν</unclear>ῳ ἀνυπ<unclear>ε</unclear><supplied reason="lost">ρθέτως χωρίς τινος ὑπερθέσεως καὶ</supplied> 

    <lb n="10"/>προφάσεως. εἰ δὲ <supplied reason="lost">μὴ ἀποδώσω ὑμῖν τὸν αὐτὸν</supplied> 

    <lb n="11"/>οἶνον ἐν τῇ α<unclear>ὐτῇ</unclear> <unclear>π</unclear><supplied reason="lost">ροθεσμίᾳ ὁμολογῶ παρασχεῖν</supplied> 

    <lb n="12"/>ὑμῖν ὑπὲρ τῆς αὐτοῦ <supplied reason="lost">τιμῆς χρυσοῦ νομισμάτια</supplied> 

    <lb n="13"/>τέσσαρα <num value="4"/> εὐθὺς μ<supplied reason="lost">ετὰ τὴν προθεσμίαν κινδύνῳ</supplied> 

    <lb n="14"/>ἐμῷ καὶ πόρῳ τῆς <supplied reason="lost">ἐμῆς παντοίας ὑποστάσεως.</supplied> 

    <lb n="15"/>τὸ παρὸν γραμματ<unclear>ε</unclear><supplied reason="lost">ῖον κύριον καὶ βέβαιον καὶ</supplied> 

    <lb n="16"/>ἐπερωτηθεὶς ὡμο<unclear>λ</unclear><supplied reason="lost">όγησα. </supplied><gap reason="lost" quantity="15" unit="character"/> 

    <lb n="17"/>υἱὸς Χάριτος ὁ πρ<supplied reason="lost">οκείμενος ἐθέμην τοῦτο</supplied> 

    <lb n="18"/>τὸ γραμματεῖον ὡ<supplied reason="lost">ς πρόκειται. </supplied><gap reason="lost" quantity="10" unit="character"/> 

    <lb n="19"/>υἱὸς Ὡρουωγχίου <supplied reason="lost">ἀπὸ Ἑρμοῦ πόλεως ἀξιωθεὶς</supplied> 

    <lb n="20"/>ἔγραψα ὑπὲρ αὐτοῦ <supplied reason="lost">γράμματα μὴ εἰδότος. <handShift new="m2"/> </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="21"/>Τραιαν<unclear>ο</unclear><add place="above"><unclear>ῦ</unclear></add> ἀπὸ <expan>Ἑρμ<ex>ο</ex>π<ex>όλεως</ex></expan> μαρτυρῶ <supplied reason="lost">τῷ γραμματείῳ</supplied> 

    <lb n="22"/><supplied reason="lost">ἀκούσας παρὰ τοῦ θεμένου. </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="23"/><gap reason="lost" extent="unknown" unit="line"/> 
