         <head n="16271" xml:lang="en">
            <date>AD 602</date>
            <placeName>Hermoupolis Magna</placeName>
            <ref n="sb;6;9403" type="reprint-from">SB6,9403</ref>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><ab>
    <lb n="1"/><g type="stauros"/> ἐν ὀνόματι τῆς ἁγίας καὶ ὁμοου<supplied reason="lost">σίου</supplied> <choice><reg>τριάδος</reg><orig>τρι<add place="above">α</add>δα</orig></choice> <choice><reg>πατρὸς</reg><orig>πατρι</orig></choice> καὶ <choice><reg>υἱοῦ</reg><orig>υἱῶ</orig></choice> καὶ ἁγίου <expan>πνεύ<unclear>μ</unclear><supplied reason="lost">α</supplied>τ<ex>ος</ex></expan> 

    <lb n="2"/>βασιλείας τοῦ <choice><reg>θειοτάτου</reg><orig>θεοτάτου</orig></choice> καὶ εὐσεβεστάτου ἡ<unclear>μ</unclear>ῶν δεσπότου Φλαυίου Φωκᾶ τοῦ αἰωνίου 

    <lb n="3"/>Αὐγούστου Αὐτοκράτορος καὶ μεγίστου <supplied reason="lost">εὐερ</supplied><unclear>γ</unclear>έτου ἔτους π<supplied reason="lost">ρ</supplied>ώτο<add place="above">υ</add> Χοιὰκ <num value="25">κ<unclear>ε</unclear></num> <num value="6"><unclear>ϛ</unclear></num> <expan><unclear>ἰνδ</unclear><ex>ικτίονος</ex></expan> <unclear>ἐν</unclear> <expan><unclear>Ἑρ</unclear><ex>μοῦ πόλει</ex></expan>. 

    <lb n="4"/>Αὐρηλία Μαρία Φοιβάμμωνος <supplied reason="lost">μ</supplied>ητρὸς Νεμε<supplied reason="lost">σι</supplied><unclear>α</unclear>νῆς ἀπὸ κώμης <unclear>Ἰ</unclear>β<unclear>ιῶ</unclear><supplied reason="lost">νος</supplied> 

    <lb n="5"/>Σεσεμβύθεως τοῦ Ἑρμουπολίτου νο<unclear>μ</unclear>οῦ Αὐρηλίῳ Φοιβάμμωνι τῷ ἐμῷ πατρὶ 

    <lb n="6"/>χαίρειν. ὁμολογῶ ἐγὼ ἡ προγεγραμ<supplied reason="lost">μέ</supplied><unclear>ν</unclear>η Μαρία διὰ ταύτης μου τῆς ἐγγράφου 

    <lb n="7"/>ἀσφαλείας ἤτοι ὁμολογίας <choice><reg>ἐπομνυ<supplied reason="lost">μ</supplied>ένη</reg><orig>ἐπομν<unclear>υ</unclear><supplied reason="lost">μ</supplied>ενος</orig></choice> τόν <choice><reg>τε</reg><orig>δε</orig></choice> π<supplied reason="lost">α</supplied>ντοκράτορα θεὸν καὶ 

    <lb n="8"/>τὴν εὐσέβειαν καὶ νίκην τοῦ δεσπό<supplied reason="lost">το</supplied>υ ἡμῶν Φλα<supplied reason="lost">υ</supplied><unclear>ί</unclear>ου Φωκᾶ <unclear>τ</unclear><supplied reason="lost">οῦ αἰωνίου</supplied> 

    <lb n="9"/>Αὐγούστου Αὐτοκράτορος ὥσδε εἴληφ<supplied reason="lost">α καὶ ἐπληρ</supplied>ώθην πά<supplied reason="lost">ντα τὰ ἐλθόντα ἀπὸ</supplied> 

    <lb n="10"/>τῆς ἐμῆς κληρονομίας τῆς ἐμ<unclear>ῆ</unclear><supplied reason="lost">ς μητρὸς Νε</supplied><unclear>μ</unclear>εσιανῆς <gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> κατὰ τὴν</supplied> 

    <lb n="11"/><choice><reg>δύναμιν</reg><orig>δύναμειν</orig></choice> τῆς ἐμ<unclear>ῆ</unclear>ς διαθήκης <gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/><gap reason="illegible" quantity="1" unit="character"/>ς καὶ τὰς <gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="12"/>εἰδέας ἐν <choice><reg>τὲ</reg><orig>δὲ</orig></choice> <choice><reg>ἐσθήμασι</reg><orig>εὐθεμασει</orig></choice> καὶ στρώμ<unclear>α</unclear><supplied reason="lost">σι καὶ </supplied><choice><reg><supplied reason="lost">ἐνδ</supplied>ύμασι</reg><orig><supplied reason="lost">ἐνδ</supplied><unclear>ύ</unclear>μασει</orig></choice> καὶ ξ<supplied reason="lost">υ</supplied><unclear>λ</unclear><supplied reason="lost">ίνοις καὶ λιθίνοις</supplied> 

    <lb n="13"/>σκεύοις <choice><reg>κινητῶν</reg><orig>ακινητῶν</orig></choice> <choice><reg>τε</reg><orig>δε</orig></choice> καὶ ἀκινήτ<unclear>ω</unclear><supplied reason="lost">ν καὶ αὐτοκιν</supplied>ήτων <choice><reg>ἐν</reg><orig>ἐμ</orig></choice> πα<unclear>ν</unclear><supplied reason="lost">τὶ γένει καὶ εἴδει</supplied> 

    <lb n="14"/>καὶ ἃ κινεῖ ἄχρι ἀσαρίου ἑνὸς <num value="1"/> π<gap reason="lost" extent="unknown" unit="character"/> <unclear>ἥ</unclear>μισυ μέρος τ<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="15"/><gap reason="illegible" extent="unknown" unit="character"><desc>vestiges</desc></gap> 

    <lb n="16"/><gap reason="lost" extent="unknown" unit="line"/> </ab></div>
