         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r" type="textpart"><!--milestone unit="4"--><ab>

    <lb n="1"/><g type="stauros"/> ἐν ὀνόματι τοῦ κυρ<supplied reason="lost">ίου καὶ δεσπότου</supplied> 

    <lb n="2"/>Ἰησοῦ Χριστοῦ τοῦ θε<supplied reason="lost">οῦ καὶ σωτῆρος</supplied> 

    <lb n="3"/>ἡμῶν βασιλείας τοῦ ε<supplied reason="lost">ὐσεβεστάτου ἡμῶν</supplied> 

    <lb n="4"/><expan>δεσπ<ex>ότου</ex></expan> <expan>Φλ<ex>αουίου</ex></expan> Ἡρακλεί<supplied reason="lost">ου τοῦ αἰωνίου</supplied> 

    <lb n="5"/>Αὐγούστου καὶ Αὐτοκ<supplied reason="lost">ρά</supplied><unclear>τ</unclear><supplied reason="lost">ορος ἔτους <num value="28">κη</num></supplied> 

    <lb n="6"/>Θὼθ <num value="10">ι</num> δωδεκάτ<supplied reason="lost">ης <expan>ἰν<ex>δικτίονος</ex></expan> ἐν Ἀρσινόῃ</supplied> 

    <lb n="7"/><expan>Φλ<ex>αουίῳ</ex></expan> Καλομηνᾷ τῷ π<supplied reason="lost">εριβλέπτῳ</supplied> 

    <lb n="8"/>υἱῷ τοῦ τῆς μακ<supplied reason="lost">αριωτάτης</supplied> 

    <lb n="9"/>μνήμης Πέτρου <supplied reason="lost">ἀπὸ ταύτης τῆς</supplied> 

    <lb n="10"/>Ἀρσινοιτῶν <subst><add place="inline">πόλεω<supplied reason="lost">ς</supplied></add><del rend="corrected">πελεω<supplied reason="lost">ς</supplied></del></subst> <supplied reason="lost"> <space extent="unknown" unit="character"/> </supplied> 

    <lb n="11"/>Αὐρήλιος Ἐπίμαχος <supplied reason="lost">τοῦ τῆς <expan>μακ<ex>αρίας</ex></expan> μνήμης</supplied> 

    <lb n="12"/>Ἐπιφανίου ἀπὸ τῆ<supplied reason="lost">ς αὐτῆς Ἀρσινοιτῶν</supplied> 

    <lb n="13"/>πόλεως οἰκῶν <supplied reason="lost">καὶ καταγινόμενος ἐν</supplied> 

    <lb n="14"/><choice><reg>ἐποικίῳ</reg><orig>ἐποικίου</orig></choice> τῶν ἁγίων <supplied reason="lost">Κοσμᾶ καὶ</supplied> 

    <lb n="15"/><unclear>Δ</unclear><supplied reason="lost">αμι</supplied><unclear>α</unclear>νοῦ <expan>χ<ex>αίρειν</ex></expan>. ὁμολ<supplied reason="lost">ογῶ ἑκουσίᾳ</supplied> 

    <lb n="16"/>γνώμῃ μεμισ<unclear>θ</unclear><supplied reason="lost">ωκέναι </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="17"/>ἐμαυτὸν τῇ ὑμ<supplied reason="lost">ετέρᾳ λαμπρότητι</supplied> 

    <lb n="18"/>ἐπὶ ἐνιαυτὸν ἕν<supplied reason="lost">α <num value="1"/> ἀριθμούμενον ἀπὸ</supplied> 

    <lb n="19"/><gap reason="illegible" quantity="1" unit="line"/> 

