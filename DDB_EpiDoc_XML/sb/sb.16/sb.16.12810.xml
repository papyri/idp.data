         <head n="4171" xml:lang="en">
            <date>252 BC</date>
            <placeName>Philadelphia</placeName>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r" type="textpart"><ab>

    <lb n="1"/><supplied reason="lost">Θρασυμ</supplied>ήδης Ζήνωνι χαίρειν. τὸ ἀν<supplied reason="lost">ήλωμα εἰς τὴν παρακομιδὴν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="2"/><supplied reason="lost">ἔλαβον</supplied> καὶ ἀπέδωκα Φιλίσκωι <unclear>τ</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="3"/><supplied reason="lost">ἤδη ᾐτή</supplied>σατο δὲ ἡμέρας ἕως ἦλθεν <supplied reason="lost">ἵνα </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="4"/><supplied reason="lost">προστάσσ</supplied><unclear>η</unclear>ται ναῦλον κίκιος μετρη<supplied reason="lost">τῶν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="5"/><supplied reason="lost">πάντα δὲ</supplied> λῆμμα καὶ ἀνήλωμα ἕως <supplied reason="lost">Μεχεὶ</supplied>ρ <gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="6"/><supplied reason="lost">ναῦλον ἐπὶ</supplied> τῶν <num value="100">ρ</num> <expan>ἀρ<ex>ταβῶν</ex></expan> <expan><ex>δραχμῶν</ex></expan> <num value="25">κε</num> ὥσ<supplied reason="lost">τ</supplied>ε οὐχὶ μικρόν σοι <gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="7"/><supplied reason="lost">οἱ ἐλθόν</supplied>τες ἐκ πόλεως φάσκουσιν τοὺς ἐπὶ τῶν συνβολῶν εἶ<supplied reason="lost">ναι </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="8"/><supplied reason="lost">καθότι</supplied> ἡ παρʼ Ἀπολλωνίου ἐντολή ἐστιν ὅτι οἱ παρʼ ἡμῶν τ<supplied reason="lost">ιμῶσι </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="9"/><supplied reason="lost">ὅσαπερ</supplied> ἔσται ὁ διδούς. οὐ γὰρ ἐπʼ ὀνό<supplied reason="lost">μ</supplied>ατος <choice><reg>διασαφεῖ</reg><orig>διασαφῖ</orig></choice> τότε ναῦ<supplied reason="lost">λον</supplied> 

    <lb n="10"/><supplied reason="lost">τότε ἕτερο</supplied>ν. εἰσὶν δὲ <expan><ex>δραχμαὶ</ex></expan> <num value="300">τ</num> τὸ τασσόμ<supplied reason="lost">ε</supplied><unclear>ν</unclear><supplied reason="lost">ο</supplied>ν <unclear>ἐ</unclear>ρ<unclear>ῶ</unclear> οὖν οὐχὶ λυσιτελ<supplied reason="lost">ὲς</supplied> 

    <lb n="11"/><supplied reason="lost">καὶ ἵνα το</supplied>ῦτο ἴδοις. ἔρρωσο. <expan><ex>ἔτους</ex></expan> <num value="33">λγ</num>, Παχών. 
</ab></div><div n="v" type="textpart"><ab>

    <lb n="12"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> Ζή</supplied>νωνι </ab></div></div>
