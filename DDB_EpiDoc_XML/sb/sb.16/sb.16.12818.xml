    <lb n="4"/><num value="6">ϛ</num> περὶ τῶν βυβλίων τ<unclear>ῶ</unclear><supplied reason="lost">ν </supplied><gap reason="lost" quantity="5" unit="character"/><gap reason="illegible" quantity="1" unit="character"/>ε<gap reason="lost" quantity="5" unit="character"/> γράψαι 

    <lb n="5"/>ὑπὸ ταῦτα <space extent="unknown" unit="character"/> <gap reason="lost" extent="unknown" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> <space extent="unknown" unit="character"/> 
