         <div xml:lang="grc" type="edition" xml:space="preserve"><ab>
    <lb n="1"/><handShift new="m1"/> <gap reason="lost" extent="unknown" unit="character"/>υ <expan>μη<ex>νὸς</ex></expan> Νέου Σεβαστοῦ <num value="12">ιβ</num> <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="2"/><handShift new="m2"/> <gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> μ</supplied>ηνὸς Νέου Σεβαστοῦ <num value="12">ιβ</num> ἐν Πτολ<supplied reason="lost">εμαίδι Εὐεργέτιδι τοῦ Ἀρσινοίτου νομοῦ. </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="3"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> τρ</supplied>ιάκοντα τε<unclear>σσ</unclear>άρων <num value="34"/> οὐλὴ μετώπ<supplied reason="lost">ῳ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="4"/><gap reason="lost" extent="unknown" unit="character"/> ἐτῶν τριάκοντα τεσσάρων οὐλὴ ὀφρύσιν ἀμ<supplied reason="lost">φοτέραις </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="5"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> εἰς τὸν ἅπαν</supplied><unclear>τ</unclear>α <unclear>χ</unclear>ρόνον τὸν ὑπάρχοντα τῶι Διδ<unclear>ύ</unclear>μ<supplied reason="lost">ωι </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="6"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ἀμφο</supplied>τέρων Ἀκου<supplied reason="lost">σιλ</supplied>άου τοῦ καὶ Μύσθου <unclear>π</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="7"/><gap reason="lost" extent="unknown" unit="character"/><gap reason="illegible" quantity="1" unit="character"/>ας τετάρτο<supplied reason="lost">υ ἑκ</supplied>καιδεκάτου τετρακ<supplied reason="lost">αιεξηκοστοῦ <num value="1/4"/><num value="1/16"/><num value="1/64"/> </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="8"/><gap reason="lost" extent="unknown" unit="character"/>ησε δὲ παν<gap reason="lost" quantity="4" unit="character"/> καθὼς ἡ εἰ<unclear>ς</unclear> τὸν <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="9"/><gap reason="lost" extent="unknown" unit="character"/>υ<unclear>ν</unclear>η λεγομένη, λιβὸς πρότερ<supplied reason="lost">ον </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="10"/><gap reason="lost" extent="unknown" unit="character"/><gap reason="illegible" quantity="1" unit="character"/>τον τοῦ προ<supplied reason="lost">γ</supplied>ε<supplied reason="lost">γρ</supplied>αμμένου Θε<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="11"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> διὰ τῆς Σαραπ</supplied>ίωνος τραπέζης Βειθυνῶ<supplied reason="lost">ν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="12"/><gap reason="lost" extent="unknown" unit="character"/> ἃ καὶ παρ<unclear>έ</unclear>ξεσθαι καὶ ἀνέπαφα <supplied reason="lost">καὶ ἀνενεχύραστα καὶ ἀνεπιδάνειστα καὶ καθαρὰ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="13"/><gap reason="lost" extent="unknown" unit="character"/>ν καὶ κατακριμάτων πάντων καὶ <supplied reason="lost">παντὸς εἴδους ἀπὸ τῶν ἔμπροσθεν χρόνων μέχρι </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="14"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ἀπὸ ἰδ</supplied>ιωτικῶν καὶ πάσης ἐμπο<supplied reason="lost">ι</supplied>ήσεω<supplied reason="lost">ς </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="15"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> μηδένα κωλύον</supplied>τα τὴν Σάμβιον μηδὲ το<supplied reason="lost">ὺ</supplied>ς παρʼ αὐ<supplied reason="lost">τῆς </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="16"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> κα</supplied>ὶ ἐξαλλοτρι<unclear>οῦ</unclear>ντας καὶ οἰκονομο<supplied reason="lost">ῦντας </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="17"/><gap reason="lost" extent="unknown" unit="character"/> ἐὰν δὲ μὴ βεβαιοῖ ὡς πρόκ<supplied reason="lost">ειται </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="18"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ἀποτισάτω </supplied><choice><reg><supplied reason="lost">τέλ</supplied>η</reg><orig><supplied reason="lost">τελ</supplied>ηι</orig></choice> διπλᾶ καὶ <choice><reg>ἐπίτιμον</reg><orig>ἐπίτειμον</orig></choice> ὡς ἴδ<supplied reason="lost">ιον χρέος </supplied><gap reason="lost" extent="unknown" unit="character"/> 

