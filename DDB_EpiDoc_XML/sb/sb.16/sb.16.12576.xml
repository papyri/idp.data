    <lb n="12"/>Ἁθὺρ <num value="27">κζ</num> <expan>ἄλ<ex>λα</ex></expan> <expan>δι<ex>ὰ</ex></expan> τοῦ αὐτοῦ <add place="above">οἴνου</add> <expan>κερ<ex>άμια</ex></expan> <num value="3">γ</num> 

