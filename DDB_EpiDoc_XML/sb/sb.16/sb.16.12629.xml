    <lb n="1"/><gap reason="lost" quantity="9" unit="character"/><gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" quantity="37" unit="character"/> 

    <lb n="2"/>πρὸς τῷ Ἁδριανίῳ. συνπαρ<unclear>όντων</unclear> πρ<gap reason="illegible" quantity="3" unit="character"/><gap reason="lost" quantity="20" unit="character"/> 

    <lb n="3"/><subst><add place="inline">Τιθο<add place="above">οῦς</add></add><del rend="corrected">τιθο<del rend="erasure">η</del></del></subst> καὶ Σαραπᾶ ὑπηρετῶν καὶ Γερ<unclear>ο</unclear><supplied reason="lost" cert="low">ντίου </supplied><gap reason="lost" quantity="15" unit="character"/> 

    <lb n="4"/>ὑπὲρ Ἡρακλείδου ἀπὸ τῶν αὐτόθι ο<unclear>π</unclear><gap reason="lost" quantity="20" unit="character"/> 

    <lb n="5"/><unclear>ω</unclear>ν διὰ τῶν μηνιάρχων <unclear>Νε</unclear><supplied reason="lost">μ</supplied><unclear>ε</unclear><supplied reason="lost" cert="low">σι </supplied><gap reason="lost" quantity="20" unit="character"/> 

    <lb n="6"/><choice><reg>μειζόνων</reg><orig>μιζόνων</orig></choice> καὶ <unclear>Γ</unclear>ερ<unclear>μ</unclear><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" quantity="23" unit="character"/> 

    <lb n="7"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="1" unit="character"/> συνειδοῦσα <unclear>τ</unclear><gap reason="lost" quantity="24" unit="character"/> 

    <lb n="8"/>μενου οἱ ἀντίδικοι. ἡ ἀρετὴ τοῦ κυρίου μ<supplied reason="lost" cert="low">ου καθολικοῦ ἐκώλυσεν</supplied> 

    <lb n="9"/>τὴν κατʼ αὐτοῦ καινοτομίαν. ἀνα<unclear>γν</unclear><supplied reason="lost">ώσομαι τὴν ἀπόφασιν. </supplied><gap reason="lost" quantity="4" unit="character"/> 

    <lb n="10"/><expan>ῥ<ex>ήτωρ</ex></expan> <expan>εἶ<ex>πεν</ex></expan>· παραγράφομαι. <g type="slanting-stroke"/> ὁ λογιστὴς <expan>εἶ<ex>πεν</ex></expan>· ἀ<supplied reason="lost" cert="low">νάγνωθι ὃ ἀπεφήνατο ὁ</supplied> 

    <lb n="11"/>διασημότατος καθολικός. καὶ ἀνεγνώσθη ἐπὶ ἀποφάσ<unclear>ε</unclear><supplied reason="lost">ως οὕτως</supplied> 

    <lb n="12"/>ἐχούσης· Ἀντωνῖνος ὁ διασημότατος καθολικὸς <expan>εἶ<ex>πεν</ex></expan>· ὁ λ<unclear>ο</unclear><supplied reason="lost">γιστὴς</supplied> 

    <lb n="13"/>προνοήσεται μηδὲν καινοτομηθῆναι κατὰ τοῦ ὑπ<unclear>ο</unclear><supplied reason="lost">τεταγ</supplied>

    <lb n="14" break="no"/><supplied reason="lost">μ</supplied>ένου <foreign xml:lang="la">?.</foreign> μετὰ τὴν ἀνάγνωσιν Μακρόβιος <expan>ῥ<ex>ήτωρ</ex></expan> <expan>εἶ<ex>πεν</ex></expan>· συν<gap reason="lost" quantity="1" unit="character"/><gap reason="illegible" quantity="3" unit="character"/><gap reason="lost" quantity="3" unit="character"/><supplied reason="lost"> τῇ</supplied> 

    <lb n="15"/>παραγραφῇ· τὸν <add place="above"><gap reason="illegible" quantity="4" unit="character"/></add> <unclear>τρ</unclear>όπον τῆς παραγραφῆς προτίθημι. θ<supplied reason="lost" cert="low">έλω</supplied> 

    <lb n="16"/>ὡς οὐδὲ λόγου μεταδοίης τῷ ἀντιδίκῳ. ἐγὼ δὲ προς<gap reason="lost" quantity="4" unit="character"/> 

    <lb n="17"/><supplied reason="lost" cert="low">των</supplied> τῆς φιλοπραγμοσύνης αὐτοῦ τοῖς ἤδη κεκριμέ<supplied reason="lost">νοις</supplied> 

