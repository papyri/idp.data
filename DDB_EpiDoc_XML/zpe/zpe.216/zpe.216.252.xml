    <lb n="1"/><g type="stauros"/> <num value="1">μία</num> παιδίσκη ἤνεγκεν <num value="100">ρ</num> <num value="100">ἑκατὸν</num> <expan>νο<ex>μίσματα</ex></expan> <expan>ζ<supplied reason="lost">υ</supplied><ex>γῷ</ex></expan> <hi rend="diaeresis">ὑ</hi>πὲρ γάλακτ<unclear>ο</unclear><supplied reason="lost">ς</supplied> <gap reason="lost" extent="unknown" unit="character"/>

