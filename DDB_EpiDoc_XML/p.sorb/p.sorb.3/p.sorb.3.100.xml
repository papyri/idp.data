    <lb n="13"/><supplied reason="lost"><gap reason="illegible" quantity="2" unit="character"/>λι</supplied>μπ<unclear>αν</unclear><gap reason="illegible" quantity="3" unit="character"/>ωι τέκνον

    <lb n="14"/><gap reason="illegible" quantity="4" unit="character"/>ποιησηι· ἔστιν δὲ τὸ 

    <lb n="15"/><choice><reg>σ<supplied reason="lost">ι</supplied>τάριον</reg><orig><unclear>σ</unclear><supplied reason="lost">ειτ</supplied>άριον</orig></choice> <app type="editorial"><lem>ἐν φάτνῃ</lem><rdg resp="P. Jouguet (P.Lille 1, p. 272)">ενι φάτνηι</rdg></app> δεξ<gap reason="illegible" quantity="1" unit="character"/>

    <lb n="16"/><gap reason="illegible" quantity="5" unit="character"/> παρʼ ὑμῶν ἔχειν 

