    <lb n="12"/><gap reason="lost" extent="unknown" unit="character"/> Νουμισιανῷ Ἡρώδου τοῦ Ἡρώδου Πέρσῃ τῆς <unclear>ἐ</unclear><supplied reason="lost">πιγονῆς </supplied><gap reason="lost" extent="unknown" unit="character"/>

    <lb n="13"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> μη</supplied>νὸς Φαῶφι τόκου φραχ<unclear>μ</unclear>ῆς μιᾶς τῇ μνᾷ τὸν μῆνα <supplied reason="lost">ἕκαστον </supplied><gap reason="lost" extent="unknown" unit="character"/>

    <lb n="14"/><gap reason="lost" extent="unknown" unit="character"/>άκοντα τέσσαρ<supplied reason="lost">ας</supplied> <num value="4"/> ἀποδότω ὁ Νουμισιανὸς τῇ Διδ<unclear>ύμ</unclear><supplied reason="lost">ῃ </supplied><gap reason="lost" extent="unknown" unit="character"/>

    <lb n="15"/><supplied reason="lost">πράξεως οὔσης ἐκ τε τοῦ ἀμ</supplied><unclear>π</unclear>ελῶνος καὶ ἐκ τῶν ἄλλων ὑπαρχόντων αὐτῷ πάντω<supplied reason="lost">ν </supplied><gap reason="lost" extent="unknown" unit="character"/>

    <lb n="16"/><supplied reason="lost">ὡς ἐτ</supplied>ῶν πεντήκοντα πέντε <num value="55"/> ὀφρύι δεξιᾶι συνκεχ<supplied reason="lost">ωρηκέναι </supplied><gap reason="lost" extent="unknown" unit="character"/>

    <lb n="17"/><gap reason="lost" extent="unknown" unit="character"/><unclear>π</unclear>ίων ἄσημος μετὰ κυρίου τοῦ συνγενοῦς Ἡρα<unclear>κ</unclear><gap reason="lost" extent="unknown" unit="character"/>

    <lb n="18"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> τ</supplied>οῦ ἐνθάδε γραφείου τῷ τετάρτῳ ἔτει τι μέρος δη<unclear>μ</unclear>οσι<gap reason="lost" extent="unknown" unit="character"/>

    <lb n="19"/><gap reason="lost" extent="unknown" unit="character"/>δος Ἡρακλείδου μερίδος ὧν ὅλων γείτονες κ<gap reason="lost" extent="unknown" unit="character"/>

