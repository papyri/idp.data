    <lb n="1"/>Ἀτπὴς Ἑρμοδώ<supplied reason="lost">ρῳ</supplied> 

    <lb n="2"/>τῷ πατρὶ χαίρειν. 

    <lb n="3"/>πρὸ μὲν πᾶν ἀσ<unclear>π</unclear><supplied reason="lost">άζο</supplied>

    <lb n="4" break="no"/>μαί <choice><reg>σε</reg><orig>σοι</orig></choice>, ἀσπάζομ<supplied reason="lost">αι ὅσ</supplied>

    <lb n="5" break="no"/>τις μετὰ τῶν <gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="5" unit="character"/> 

    <lb n="6"/>μου τὰ νῦν ἐξε<gap reason="lost" quantity="4" unit="character"/> 

    <lb n="7"/>ὅπου ημη<unclear>μ</unclear>εα<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="4" unit="character"/> 

    <lb n="8"/>περὶ τοῦ χωρίου <gap reason="lost" quantity="4" unit="character"/> 

    <lb n="9"/>ἐξελθεῖν εἰς λα<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="4" unit="character"/> 

    <lb n="10"/><gap reason="lost" quantity="4" unit="character"/> ἔρχεται σοι <gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="3" unit="character"/> 

