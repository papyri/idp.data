         <head n="39126" xml:lang="en">
            <date>AD 703-715</date>
            <placeName>Apollonopolis Magna</placeName>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r" type="textpart"><ab>

    <lb n="1"/><g type="stauros"/> α<gap reason="lost" extent="unknown" unit="character"/>ουτο<unclear>σαι</unclear>φαν<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="2"/><gap reason="lost" extent="unknown" unit="character"/> τῆς μακαρίας <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="3"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> πρ</supplied>ὸ γὰρ πάντων ὁδ<supplied reason="lost">ο</supplied>ιπορευόμεθα <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="4"/><gap reason="lost" extent="unknown" unit="character"/> καὶ τὸν φιλάνθρωπον ἡμῶν Θεὸν <gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="5"/>καὶ ποιησαντ<gap reason="lost" extent="unknown" unit="character"/>του μεθʼ ἡμῶν καὶ ἐφελ<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="6"/>δεσποτικὴν ἀδελφότητα ἐνταῦθα τὴν <unclear>α</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="7"/>ὄντως ἐν μεγάλῃ περιστάσει ἐστὶ <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="8"/>ἐκ τῶν πολλῶν <choice><reg>συγχύσεων</reg><orig>συγχήσεων</orig></choice> τῶν <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="9"/>ἐν ἑκάστῳ αὐτῶν <choice><reg>ἐπιτηδείῳ</reg><orig>ἐπιτηδίῳ</orig></choice> <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="10"/>θεοφύλακτον ἀδελφότη<supplied reason="lost">τα </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="11"/>ἡμᾶς ὁ Θεῖος Ἀπόστολος <add place="above">ὅτι</add> ὀφείλο<supplied reason="lost">μεν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="12"/>παραλείψητε καὶ καταλάβητε τὰ πρὸς αὐτὸ<supplied reason="lost">ν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="13"/>τὰ παράνω αὐτοῦ ἵνα καὶ αὐτὸς <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="14"/>ἐν περιστάσει ἐστὶ ἄτρι ἐὰν κατ<supplied reason="lost" cert="low">αλάβητε </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="15"/>απαιτ<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="16"/><gap reason="lost" extent="unknown" unit="line"/> 
</ab></div><div n="v" type="textpart"><ab>

    <lb n="16"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> Παπ</supplied>ᾷ τῷ <expan>μεγ<ex>α</ex>λ<ex>οπρεπεστάτῳ</ex></expan> π<supplied reason="lost">αγάρχῳ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="17"/><gap reason="lost" extent="unknown" unit="line"/> </ab></div></div>
