         <head n="20253" xml:lang="en">
            <date>AD 703-715</date>
            <placeName>Apollonopolis Magna</placeName>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r" type="textpart"><ab>

    <lb n="1"/><gap reason="lost" quantity="6" unit="character"/>μενος τῆς<gap reason="lost" quantity="12" unit="character"/>προστ<gap reason="lost" quantity="10" unit="character"/><supplied reason="lost"> ἀ</supplied>νερχομενο<gap reason="lost" quantity="30" unit="character"/> 

    <lb n="2"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ἀσ</supplied>πάσασθαι τὴν ὑμετέραν θεοφύλακτον πατρ<supplied reason="lost">ι</supplied>κὴν δεσπ<supplied reason="lost">ό</supplied>τ<supplied reason="lost">ειαν  </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="3"/><gap reason="lost" extent="unknown" unit="character"/>γράφων τὰ μέρη τ<supplied reason="lost">ῆς</supplied> <expan>φεοφυλάκτ<ex>ου</ex></expan> ὑμῶν ὑγιείας κ<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="4"/><gap reason="lost" quantity="10" unit="character"/>υπροσε<gap reason="lost" quantity="15" unit="character"/> ἀλλʼ οὖν <choice><reg>βαστάξατε</reg><orig><supplied reason="lost">β</supplied>αστάξετε</orig></choice>. <gap reason="lost" quantity="15" unit="character"/> ἐπειδὴ δέομαι <gap reason="lost" quantity="6" unit="character"/> 

    <lb n="5"/><gap reason="lost" quantity="10" unit="character"/> <unclear>τ</unclear>ῶν <unclear>εἰδῶ</unclear><supplied reason="lost">ν </supplied><gap reason="lost" quantity="10" unit="character"/> <expan><supplied reason="lost">γ</supplied>ονάχ<ex>ια</ex></expan> <add place="above">ὀρθόπλουμα</add> ἀπὸ ῥουζικοῦ καὶ <gap reason="lost" quantity="6" unit="character"/> ἐ<supplied reason="lost">ν τ</supplied>ῇ <expan>Ἀπόλλω<ex>νος</ex></expan> Ἄνω· παρακα<supplied reason="lost">λῶ</supplied> 

    <lb n="6"/><supplied reason="lost">ὑμᾶς κε</supplied>λεῦσαι ταῦτα ἐπιταχθῆναι ὑψηλὰ μητ<gap reason="lost" quantity="20" unit="character"/><gap reason="illegible" quantity="1" unit="character"/>ερχόμενοι μετὰ τῆς 

    <lb n="7"/><supplied reason="lost">ἐπιστο</supplied>λῆς καὶ τὴν τούτων τιμὴν παρέξω ὑμῖν μετὰ εὐχ<supplied reason="lost">αριστίας εἴ <unclear>τι</unclear> κ</supplied>ελεύετε. ἀλλὰ τὴν σωτηρίαν 

    <lb n="8"/><supplied reason="lost">τῶν θε</supplied>οφυλάκτων ὑμῶν τέκνων, μὴ ἀποτύχω τῆς παρα<supplied reason="lost">κλήσεως</supplied>. εἰ δὲ κελεύετε μοι πέμψαι 

    <lb n="9"/><supplied reason="lost">τὴν τι</supplied>μὴν αὐτῶν, γράψατέ μοι τὸ τί <supplied reason="lost">θέ</supplied>λετε πέμψαι ὅπως τοῦτ<supplied reason="lost">ο πρ</supplied>ὸς στρατιώ<supplied reason="lost">την </supplied><gap reason="lost" quantity="7" unit="character"/> 

    <lb n="10"/><supplied reason="lost">κ</supplied>αὶ ἀ<supplied reason="lost">σπάζο</supplied>μαι ὑμᾶς δ<supplied reason="lost">ιὰ τοῦ γράμματο</supplied>ς <g type="stauros"/> 
</ab></div><div n="v" type="textpart"><ab>

    <lb n="11"/><supplied reason="lost"><g type="stauros"/> τῷ <expan>θεοφυλ<ex>άκτῳ</ex></expan> μου <expan>δεσπό<ex>τῃ</ex></expan> <expan><ex>καὶ</ex></expan> <expan>πατρ<ex>ὶ</ex></expan></supplied> Παπᾷ τῷ <expan>μεγ<ex>ίστῳ</ex></expan> 

    <lb n="12"/><expan>κόμε<ex>τι</ex></expan> <expan><ex>καὶ</ex></expan> <expan>παγ<ex>άρ</ex>χ<ex>ῳ</ex></expan> <expan>Ἀπόλλ<ex>ωνος</ex></expan> <expan>Ἄν<ex>ω</ex></expan> <g type="stauros"/> Κολλοῦ<supplied reason="lost">θος <expan>ν<ex>ο</ex>τ<ex>άριος</ex></expan> <g type="stauros"/></supplied> </ab></div></div>
