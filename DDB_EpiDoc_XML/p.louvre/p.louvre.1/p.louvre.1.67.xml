    <lb n="4"/><gap reason="lost" extent="unknown" unit="character"/><gap reason="illegible" quantity="1" unit="character"/> ἐμοὶ ανκ<gap reason="illegible" quantity="4" unit="character"/>α <unclear>δ</unclear>ύω παρατουρα<gap reason="illegible" quantity="1" unit="character"/>ου κυ<gap reason="lost" extent="unknown" unit="character"/>

    <lb n="5"/><gap reason="lost" extent="unknown" unit="character"/><gap reason="illegible" quantity="7" unit="character"/> <unclear>δελ</unclear>ματικομαφόρτιν λα<unclear>τ</unclear>εῖ<unclear>ν</unclear><supplied reason="lost">ον </supplied><gap reason="lost" extent="unknown" unit="character"/>

    <lb n="6"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> μω</supplied>θωνήσιν καὶ δελματικὴν λευκὴν <gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/>

    <lb n="7"/><gap reason="lost" extent="unknown" unit="character"/> ἡ <choice><reg>ἀδελφικὴ</reg><orig>δελφικὴ</orig></choice> γείνεται· τὰ δὲ φυτὰ αὐτ<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/>

    <lb n="8"/><gap reason="lost" extent="unknown" unit="character"/>εβλήθη καὶ ἀπηλλάγη ἐξ οὗ ἐπέσχ<unclear>ε</unclear><gap reason="lost" extent="unknown" unit="character"/>

    <lb n="9"/><supplied reason="lost">ὁ κύ</supplied>ριός μου Εὔτροπος· εἰδῶν καὶ <choice><reg>ἐγὼ</reg><orig>γὼ</orig></choice> τ<unclear>η</unclear><gap reason="lost" extent="unknown" unit="character"/>

    <lb n="10"/><gap reason="lost" extent="unknown" unit="character"/><unclear>ν</unclear> τοῦ ἔργου, προθυμότερον μεμένηκ<unclear>α</unclear> <gap reason="lost" extent="unknown" unit="character"/>

    <lb n="11"/><supplied reason="lost">πά</supplied><unclear>ντ</unclear>α μοι ἐνετείλατο τοῦ σοι δηλῶ <gap reason="lost" extent="unknown" unit="character"/>

    <lb n="12"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ο</supplied>ὗ χρίαν ἦν πρὸς τὸν <choice><reg>κύριον</reg><orig>κυριν</orig></choice> τῶν κ<gap reason="lost" extent="unknown" unit="character"/>

    <lb n="13"/><gap reason="lost" extent="unknown" unit="character"/>ων· οὐ γὰρ ἠθέληκεν αὐτὸς οὐδὲ λ<gap reason="lost" extent="unknown" unit="character"/>

    <lb n="14"/><gap reason="lost" extent="unknown" unit="character"/>ν ἐπιστῖλε· οἶδες δὲ τίνα λέγω <subst><add place="inline">γὰρ</add><del rend="corrected">καρ</del></subst> α<gap reason="lost" extent="unknown" unit="character"/>

    <lb n="15"/><gap reason="lost" extent="unknown" unit="character"/><unclear>χαι</unclear> ὁ κύριός μου Εὐτρόπις <subst><add place="inline">καθʼ <gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/></add><del rend="corrected"><app type="alternative"><lem>και<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/></lem><rdg>καη<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/></rdg></app></del></subst>

    <lb n="16"/><gap reason="lost" extent="unknown" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><unclear>μι</unclear> ἐπιστῖλε ὃ ἐὰν θέλῃς δηλως<gap reason="lost" extent="unknown" unit="character"/>

    <lb n="17"/><supplied reason="lost">κυρ</supplied>ίᾳ μου ἀδελφῇ σου <subst><add place="inline">Θεωνίλλ<supplied reason="lost">ᾳ </supplied><gap reason="lost" extent="unknown" unit="character"/></add><del rend="corrected">θεωνια<gap reason="lost" extent="unknown" unit="character"/></del></subst>

    <lb n="18"/><gap reason="lost" extent="unknown" unit="character"/><unclear>ν</unclear>ιον τοῦτό ἐστι <unclear>τ</unclear>ὰ τέσσερα τάλαν<supplied reason="lost">τα </supplied><gap reason="lost" extent="unknown" unit="character"/>

    <lb n="19"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> κα</supplied>ταβληθῆναι· ἐπέσχεν γὰρ αὐτ<gap reason="lost" extent="unknown" unit="character"/>

    <lb n="20"/><gap reason="lost" extent="unknown" unit="character"/><unclear>υ</unclear> αὐτῆ<unclear>ς</unclear> τὸ περὶ τῶν <choice><reg>ὑακινθ<gap reason="lost" extent="unknown" unit="character"/></reg><orig>ἱακινθ<gap reason="lost" extent="unknown" unit="character"/></orig></choice>

    <lb n="21"/><gap reason="lost" extent="unknown" unit="character"/> μοι ὁ διαβάλλων. <choice><reg>ἐρρῶσθαί</reg><orig>ἐρρῶσθέ</orig></choice> <supplied reason="lost">σε εὔχομαι </supplied><gap reason="lost" extent="unknown" unit="character"/>

    <lb n="22"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> κυ</supplied><unclear>ρ</unclear>ία, διὰ παντὸς <choice><reg>εὐτυχεῖν</reg><orig>εὐτυχῖν</orig></choice>.
</ab></div><div n="v" type="textpart"><!--milestone unit="4"--><ab>

