    <lb n="4"/><expan>καμήλ<ex>ων</ex></expan> τοῦ <expan>διεληλυθ<ex>ότος</ex></expan> <num value="19">ιθ</num> <expan><ex>ἔτους</ex></expan> <expan>δρ<ex>αχμὰς</ex></expan> εἴκοσι <subst><add place="inline"><expan>προ<ex>σδιαγραφομένων</ex></expan></add><del rend="corrected">προς<certainty match=".." locus="value"/></del></subst> <choice><reg>μίαν</reg><orig>μια</orig></choice> <expan><ex>ὀβολὸν</ex></expan> <expan><ex>ὀβολοῦ 1/2</ex></expan><num value="1/2"/>

