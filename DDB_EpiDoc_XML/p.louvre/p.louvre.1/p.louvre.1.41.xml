    <lb n="1"/><del rend="erasure">Ἀρχ<unclear>ωνᾶς</unclear> <expan><unclear>φρ</unclear><ex>οντιστὴς</ex></expan> <expan><unclear>οὐ</unclear>σ<ex>ίας</ex></expan></del> <expan>Κλ<ex>αυδίου</ex></expan> <unclear>Σ</unclear>υρίωνος αἰω

