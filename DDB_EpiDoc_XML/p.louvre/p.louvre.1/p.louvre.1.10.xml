    <lb n="4" break="no"/><supplied reason="lost">ψάμενος κύριος.  <handShift new="m2"/> </supplied><gap reason="lost" extent="unknown" unit="character"/>εγάθη<unclear>ς</unclear> καὶ <unclear>Σ</unclear><supplied reason="lost">εταβοῦς</supplied> <unclear>ἀμ</unclear>φότ<supplied reason="lost">εροι </supplied><gap reason="lost" extent="unknown" unit="character"/>

    <lb n="5"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ὁμολογ</supplied>οῦμ<unclear>ε</unclear>ν π<unclear>ε</unclear>πρ<unclear>α</unclear><supplied reason="lost">κέναι</supplied> <unclear>Τ</unclear>αβο<unclear>ῦτ</unclear><supplied reason="lost">ι Στοτουήτιος μετὰ κυρίου τοῦ ὁμοπατρίου καὶ</supplied>

