    <lb n="18" rend="inverse"/> <gap reason="lost" extent="unknown" unit="character"/>ου <expan>γί<ex>νεται</ex></expan> <expan>χρ<ex>υσοῦ</ex></expan> <expan>ν<hi rend="superscript">ο</hi><ex>μισμάτια</ex></expan> <num value="1">α</num> <expan>π<ex>αρὰ</ex></expan> <expan>κερ<ex>άτια</ex></expan> <num value="6">ϛ</num> </ab></div></div>
