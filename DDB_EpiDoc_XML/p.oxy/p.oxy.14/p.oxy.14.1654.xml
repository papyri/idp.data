    <lb n="10"/><num value="6">ϛ</num>, <expan>νομογρά<ex>φου</ex></expan> ἑνὸς τῷ <gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="3" unit="character"/>ιῳ ὑπὲρ <expan>ὑπομν<supplied reason="lost">η</supplied><unclear>μ</unclear><ex>ατισμῶν</ex></expan> <gap reason="lost" quantity="1" unit="character"/> 

