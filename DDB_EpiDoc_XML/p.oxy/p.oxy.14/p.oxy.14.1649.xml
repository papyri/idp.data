    <lb n="1"/><choice><reg>ἔκλημψις</reg><orig>ἔγλημψις</orig></choice> ἐκ τῆ<supplied reason="lost">ς </supplied><app type="editorial"><lem resp="BL 1.336"><supplied reason="lost">τῶν </supplied><supplied reason="lost" cert="low">ἐγ</supplied><unclear>κ</unclear><supplied reason="lost" cert="low">τήσ</supplied>ε<supplied reason="lost" cert="low">ων</supplied><certainty match=".." locus="value"/></lem><rdg><supplied reason="lost">δημοσ</supplied>ί<supplied reason="lost">ων λό</supplied>γ<supplied reason="lost">ων</supplied></rdg></app> <unclear>βιβλιο</unclear><supplied reason="lost">θήκης ἐκ </supplied><gap reason="lost" quantity="18" unit="character"/> 

