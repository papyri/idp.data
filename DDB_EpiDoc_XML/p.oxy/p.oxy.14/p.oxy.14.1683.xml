         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r" type="textpart"><!--milestone unit="4"--><ab>

    <lb n="1"/>τῇ κυρίᾳ μου ἀδ<supplied reason="lost">ελ</supplied>φῇ 

    <lb n="2"/>Μανατίνῃ Πρώβ<supplied reason="lost">ο</supplied>ς <choice><reg>ἀδελ

    <lb n="3" type="inWord"/>φὸς</reg><orig>αδελφω</orig></choice>  <choice><reg>χαίρειν</reg><orig>χαιριν</orig></choice>. 

    <lb n="4"/><choice><reg>πρὸ</reg><orig>πρω</orig></choice> <supplied reason="lost">μ</supplied>ὲν πάντων <choice><reg>εὔχομαι</reg><orig>ευχωμαι</orig></choice> 

    <lb n="5"/>τῷ κυρίῳ θεῷ περὶ τ<unclear>ῆ</unclear><supplied reason="lost">ς</supplied> σῆς 

    <lb n="6"/><choice><reg>ὁλοκληρίας</reg><orig>ωλοκληριας</orig></choice> ὅπως <choice><reg>ὑγιαίνουσα</reg><orig><unclear>υ</unclear><supplied reason="lost">ιε</supplied><unclear>ν</unclear>οντα</orig></choice> <choice><reg>σὺ</reg><orig>σοι</orig></choice> 

    <lb n="7"/>καὶ <choice><reg>εὐθυμοῦσα</reg><orig>ευθυμουντι</orig></choice> <choice><reg>ἀπολάβῃς</reg><orig>απωλαβης</orig></choice> 

    <lb n="8"/>τὰ παρʼ ἐμοῦ γράμματα. <choice><reg><supplied reason="lost">γι</supplied>γνώσκειν</reg><orig><supplied reason="lost">γι</supplied><unclear>γ</unclear><g type="apostrophe"/>νωσκιν</orig></choice> 

    <lb n="9"/>σε θέλω, κυρία μου ἀδ<supplied reason="lost">ελ</supplied>φή, ἄπελθε 

    <lb n="10"/><choice><reg>πρὸς</reg><orig>πρως</orig></choice> Πετρώνιν <choice><reg>τὸν</reg><orig>τ<unclear>ω</unclear>ν</orig></choice> <choice><reg>ἐγγυησά

    <lb n="11" type="inWord"/>μενόν</reg><orig>ενγυησαμενων</orig></choice> <choice><reg>με</reg><orig>μου</orig></choice>· <choice><reg>δέξαι</reg><orig>δεξε</orig></choice> ἀ<supplied reason="lost">πʼ α</supplied>ὐτοῦ ἐκ τοῦ 

    <lb n="12"/>μισθοῦ μου <choice><reg>ἕνα</reg><orig>εναν</orig></choice> <choice><reg>ἥμισυ</reg><orig><hi rend="diaeresis">υ</hi>μισυ</orig></choice>, <expan><ex cert="low">τάλαντον</ex></expan> <num value="1"><unclear>α</unclear></num> <num value="1/2" rend="fraction">𐅵</num>, 

    <lb n="13"/><choice><reg>οἶδας</reg><orig>οιδες</orig></choice> γ<supplied reason="lost">ὰ</supplied>ρ καὶ σὺ ὅτι <choice><reg cert="low">οὐδένα</reg><orig><supplied reason="lost">ο</supplied>υδεν</orig></choice> <choice><reg>ἔχομεν</reg><orig>εχωμεν</orig></choice> 

    <lb n="14"/><choice><reg cert="low">μάρτυρα</reg><orig>μαρτυρων</orig></choice> <unclear>ε</unclear><supplied reason="lost">ἰ</supplied> μὴ <choice><reg>τὸν</reg><orig>ο</orig></choice> <choice><reg>θεὸν</reg><orig>θε<supplied reason="lost">ο</supplied>ς</orig></choice> καὶ <choice><reg>σὲ</reg><orig>συ</orig></choice> καὶ <choice><reg>τὴν</reg><orig>η</orig></choice> 

    <lb n="15"/><choice><reg>γυναῖκα</reg><orig>γυνη</orig></choice> μου. <choice><reg>ἀπόδος</reg><orig>απωδως</orig></choice> οὖν αὐτὰ 

    <lb n="16"/>τῇ <choice><reg>γυναικί</reg><orig>γυναικιν</orig></choice> μου. μὴ <choice><reg>λυπήσῃς</reg><orig>λυ<unclear>πη</unclear>σις</orig></choice> οὖν 

    <lb n="17"/><choice><reg>ἐμέ</reg><orig>εμεναν</orig></choice>. <subst><add place="inline">δὸς</add><del rend="corrected"><gap reason="illegible" quantity="1" unit="character"/>ος</del></subst> οὖν αὐτά, <choice><reg>ἐπειδὴ</reg><orig>επιδε</orig></choice> <choice><reg>χρείαν</reg><orig>χριαν</orig></choice> 

    <lb n="18"/><choice><reg>αὐτῶν</reg><orig>αυτα</orig></choice> <supplied reason="omitted">ἔχει</supplied> ὁ <hi rend="diaeresis">υ</hi>ἰός μου. <choice><reg>σημείου</reg><orig>σημιου</orig></choice> δὲ χάριν, 

    <lb n="19"/><choice><reg>ὅπου</reg><orig>ωπου</orig></choice> ἠπάντηκά σου <choice><reg>εἰς</reg><orig><hi rend="diaeresis">ι</hi><supplied reason="lost">ς</supplied></orig></choice> τὸ <choice><reg>Καισαρεῖ

    <lb n="20" type="inWord"/>ον</reg><orig>κησαριον</orig></choice> καὶ εἴρηκά <choice><reg>σοι</reg><orig>σου</orig></choice> <choice><reg>ὅτι</reg><orig>ωτι</orig></choice> δὸς ἐμοὶ κέρ

    <lb n="21" type="inWord"/>μα <choice><reg>ἀπὸ</reg><orig>απω</orig></choice> <choice><reg>ὧν</reg><orig>των</orig></choice> <choice><reg>ἔχεις</reg><orig>εχις</orig></choice> <choice><reg>μου</reg><orig>με</orig></choice> <hi rend="diaeresis">ἵ</hi><unclear>ν</unclear>α <choice><reg>ἀγοράσω</reg><orig>αγωρασω</orig></choice> 

    <lb n="22"/><choice><reg>ἐμαυτῷ</reg><orig>εματω</orig></choice> <choice><reg>ἕνα</reg><orig>εναν</orig></choice> <num value="1"/> <choice><reg cert="low">λέβητα</reg><orig>λεβι<unclear>τ</unclear>ων</orig></choice> <gap reason="illegible" quantity="3" unit="character"/>, καὶ εἶπές 

    <lb n="23"/><choice><reg>μοι</reg><orig>με</orig></choice> <choice><reg>ὅτι</reg><orig>ωτι</orig></choice> <app type="BL"><lem resp="6.103">ἀ<unclear>λ</unclear><supplied reason="lost">λὰ</supplied></lem><rdg>α<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="2" unit="character"/></rdg></app> <choice><reg>ἆρον</reg><orig><unclear>αρω</unclear>ν</orig></choice> τὰ ἀπʼ ἐσοῦ καὶ 

    <lb n="24"/>ἄρτι δέ <choice><reg>σοι</reg><orig>σε</orig></choice> δ<supplied reason="lost">ίδ</supplied>ω, καὶ <unclear>α</unclear>η <choice><reg>ἐμαυτῆς</reg><orig>ε<unclear>μα</unclear>τ<unclear>η</unclear>ς</orig></choice> ἀπε

    <lb n="25" type="inWord"/>δήμησα ἐ<unclear>π</unclear>ὶ δὲ <unclear>καὶ</unclear> <choice><reg>δημοσίων</reg><orig><unclear>δημ</unclear>ω<unclear>σι</unclear>ων</orig></choice> <supplied reason="lost">ἐ</supplied>

    <lb n="26" type="inWord"/>ὰν δὲ <choice><reg>ὑγιαίνω</reg><orig>υιε<unclear>νω</unclear></orig></choice> <gap reason="illegible" quantity="3" unit="character"/><unclear>ε</unclear><gap reason="illegible" quantity="4" unit="character"/>ε<gap reason="illegible" quantity="3" unit="character"/>ιδια 

    <lb n="27"/>ἡμῶν. <app type="BL"><lem resp="6.103">κυρία <unclear>μ</unclear><supplied reason="lost">ο</supplied><unclear>υ</unclear> ἀδελφή</lem><rdg>κύριέ <unclear>μ</unclear><supplied reason="lost">ο</supplied><unclear>υ</unclear> ἄδελφε</rdg></app>, <choice><reg>β<unclear>ο</unclear>
        
