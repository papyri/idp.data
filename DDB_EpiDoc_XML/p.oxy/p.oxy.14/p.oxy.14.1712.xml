         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r" type="textpart"><ab>

    <lb n="1"/>μετὰ τὴν ὑπατείαν τοῦ δεσπότου ἡμῶν Θεοδ<supplied reason="lost">οσίου τοῦ αἰωνίου <expan>Αὐγ<ex>ούστου</ex></expan> τὸ <num value="3">γ</num></supplied> 

    <lb n="2"/>καὶ Φλαουίου <choice><reg>Ἀβου<supplied reason="omitted">ν</supplied>δαντίου</reg><orig>Ἀβουδαντίου</orig></choice> τοῦ λαμπροτ<supplied reason="lost">άτου </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="3"/>Φλαουίῳ Δομνίνῳ ὀπτίονι φαμιλίας <unclear>ἡ</unclear><supplied reason="lost" cert="low">γεμονικῆς </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="4"/>Αὐρήλιος Φοιβάμμων Θέωνος ἀπὸ τ<supplied reason="lost">ῆς λαμπρᾶς καὶ λαμπροτά</supplied>

    <lb n="5" break="no"/><supplied reason="lost">τ</supplied><unclear>ης</unclear> Ὀξυρυγχειτῶν πόλεως χαίρειν. ὁ<supplied reason="lost">μολογῶ ἐσχηκέναι παρὰ</supplied> 

    <lb n="6"/><supplied reason="lost" cert="low">σοῦ διὰ χειρὸς</supplied> ἐνταῦθα τῇ Ὀξυρυ<supplied reason="lost">γχειτῶν πόλει </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="7"/><gap reason="lost" quantity="3" unit="character"/><supplied reason="lost"> εἰς τὴν ἰδία</supplied>ν μου καὶ ἀναγκαί<supplied reason="lost">αν χρείαν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="8"/><gap reason="lost" quantity="12" unit="character"/><supplied reason="lost"> τ</supplied><unclear>ὸ</unclear> δημόσιο<unclear>ν</unclear> λόγου μ<gap reason="lost" extent="unknown" unit="character"/> 

