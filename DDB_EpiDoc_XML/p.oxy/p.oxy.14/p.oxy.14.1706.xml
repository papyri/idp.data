         <head n="21984" xml:lang="en">
            <date>AD 207</date>
            <placeName>Oxyrhynchus</placeName>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><ab>
    <lb n="1"/>ἔ<supplied reason="lost">το</supplied><unclear>υ</unclear><supplied reason="lost">ς ἑκκαι</supplied><unclear>δε</unclear><supplied reason="lost">κά</supplied>του<num value="16"/> Αὐτοκράτορος Καίσαρ<supplied reason="lost">ος Λουκίου Σεπτιμίου Σεουήρου Εὐσεβοῦς Περτίνακος</supplied> 

    <lb n="2"/>Ἀραβ<supplied reason="lost">ικοῦ Ἀδι</supplied>αβην<supplied reason="lost">ικο</supplied>ῦ Παρθικοῦ Μεγίστου καὶ Μάρκ<supplied reason="lost">ου Αὐρηλίου Ἀντωνίνου Εὐσεβοῦς Σεβαστῶν</supplied> 

    <lb n="3"/>καὶ Πουβλίου Σεπτιμίου Γέτα Καίσαρος Σεβαστοῦ Δίου <gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> Θὼθ </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ἐν Ὀξυρύγχων πόλει διὰ</supplied> 

    <lb n="4"/>ἐπιτηρητῶν ἀγορανομίου <space extent="unknown" unit="character"/> καὶ <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="5"/>ἐπρίατο Σαραπιὰς ἐπικεκλημένη Θαῆσις ἀπελε<supplied reason="lost">υθέρα </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> Παυ</supplied>

    <lb n="6" break="no"/>λινίου τοῦ καὶ <hi rend="diaeresis">Ἰ</hi>σιδίου <choice><reg>ὡ<supplied reason="omitted">ς</supplied></reg><orig>ὡ</orig></choice> <expan><ex>ἐτῶν</ex></expan> <space extent="unknown" unit="character"/> μετὰ κυρίου <supplied reason="lost"><handShift new="m2"/> τοῦ ἑαυτῆς </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> Ἡρα</supplied>

    <lb n="7" break="no"/>κλίδου Ἀθηναιέως τοῦ καὶ Ἀρτεμεισείου ὡς <expan><ex>ἐτῶν</ex></expan> <num value="23">κγ</num> <expan>ἀσή<ex>μου</ex></expan> <handShift new="m1"/> <unclear>τ</unclear><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> Σαραποῦτος </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="8"/>μητρὸς Ταυσοράπιος ἀπʼ Ὀξυρύγχων πόλεως ὡς δ<unclear>ι</unclear><supplied reason="lost">ὰ </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> καὶ </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> τῆς</supplied> 

    <lb n="9"/>αὐτῆς μητρὸς Ταυσοράπιος ἀπελευθέρας Θαήσιος ἀ<unclear>π</unclear><supplied reason="lost">ὸ τῆς αὐτῆς πόλεως ὡς <expan><ex>ἐτῶν</ex></expan> </supplied><gap reason="lost" quantity="2" unit="character"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost" cert="low"> οὐλὴ μετώ</supplied>

    <lb n="10" break="no"/>πῳ, ἑκατέρας δὲ μετὰ κυρίου Σαραποῦτος μὲν τ<unclear>ο</unclear><supplied reason="lost" cert="low">ῦ ἀδελφοῦ </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> μητρὸς</supplied> 

    <lb n="11"/>Ταυσοράπιος ἀπὸ τῆς αὐτῆς πόλεως ὡς <expan><ex>ἐτῶν</ex></expan> <gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost">, δὲ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="12"/>τος ἀπελευθέρου Ἡρακλᾶτος ἀπὸ τῆς αὐτῆς πόλ<supplied reason="lost">εως </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ἐν ἀγυιᾷ τὰς ὑπαρχού</supplied>

    <lb n="13" break="no"/>σας αὐταῖς ἐξ <hi rend="diaeresis">ἴ</hi>σ<supplied reason="lost">ο</supplied>υ δούλας δύο<num value="2"/> Ἀφροδίτην μ<supplied reason="lost" cert="low">ητρὸς </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="14"/>των <choice><reg>ὁμοίως</reg><orig>ὁμύως</orig></choice> ἐξ <hi rend="diaeresis">ἴ</hi>σου κατὰ χρηματισμὸν <unclear>γεγ</unclear><supplied reason="lost" cert="low">ονότα διὰ τοῦ ἐν Ὀξυρύγχων πόλει ἀγορανομίου</supplied> 

    <lb n="15"/>τῷ <subst><add place="inline">προδιεληλυθότι</add><del rend="corrected">προδιεληλ<gap reason="illegible" quantity="1" unit="character"/>θοτι</del></subst> <num value="14">ιδ</num> <expan><ex>ἔτει</ex></expan> μηνὶ <subst><add place="inline">Καισαρίῳ</add><del rend="corrected">κ<unclear>ε</unclear>σαριω</del></subst> παρὰ <unclear>Α</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="16"/>Ἀμμωνίου ἀπὸ τῆς αὐτῆς πόλεως α<unclear>ρθ</unclear><gap reason="illegible" quantity="2" unit="character"/>νδ<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="17"/>ἐνιαυτοῦ ἑνὸς<num value="1"/> καὶ μηνῶν πέντε<num value="5"/> <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="18"/><subst><add place="inline"><handShift new="m2"/> καὶ</add><del rend="corrected"><unclear>ας</unclear></del></subst> <handShift new="m1"/><choice><reg>παρείληφεν</reg><orig>παρίληφεν</orig></choice> ἡ ὠνουμένη παρ<supplied reason="lost">ὰ τῶν ἀποδομένων </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ταύτας τοιαύτας</supplied> 

    <lb n="19"/>ἀναπορρίφους πλὴν ἐπαφῆς λη<unclear>ν</unclear><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> διὰ τὸ τὴν Ἀφροδί</supplied>

    <lb n="20" break="no"/>την ἀνακεκρίσθαι ὡς διὰ τοῦ προ<supplied reason="lost" cert="low">τεταγμένου χρηματισμοῦ δηλοῦται </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="21"/><gap reason="illegible" quantity="1" unit="line"/> 

    <lb n="22"/><gap reason="lost" extent="unknown" unit="line"/> </ab></div>
