    <lb n="1"/><gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" quantity="4" unit="character"/><num value="4"><unclear>δ</unclear></num> 

    <lb n="2"/>Αὐρηλίωι Πλουτί<supplied reason="lost">ω</supplied><unclear>ν</unclear>ι γραμματεῖ σιτηρεσίου 

    <lb n="3"/>παρὰ Αὐρηλίου Ἀπ<supplied reason="lost">ο</supplied>λλωνίου τοῦ καὶ Νεμεσ<unclear>ι</unclear><gap reason="lost" quantity="2" unit="character"/><gap reason="illegible" quantity="2" unit="character"/> 

    <lb n="4"/>Σαραπίωνος καὶ ὡ<supplied reason="lost">ς</supplied> χρηματίζω. Ἀλεξανδρε<unclear>ὺς</unclear> <unclear>τυγ</unclear><g type="apostrophe"/><unclear>χ</unclear><supplied reason="lost">άνω</supplied><unclear>ν</unclear> 

    <lb n="5"/>καὶ ἐφέστιον <unclear>α</unclear><gap reason="illegible" quantity="3" unit="character"/><gap reason="lost" quantity="2" unit="character"/><gap reason="illegible" quantity="3" unit="character"/><unclear>ο</unclear>ς οἰκ<unclear>ί</unclear><supplied reason="lost">αν</supplied> ἐνταῦθα ἐν τῇ λαμπρᾷ 

    <lb n="6"/>καὶ λαμπροτάτῃ Ὀξ<supplied reason="lost">υρ</supplied>ύγ<g type="apostrophe"/>χων <subst><add place="inline">πόλ<unclear>ει</unclear></add><del rend="corrected">πολεως</del></subst> καὶ δεδογμένου ἐν τῇ αὐτῇ 

    <lb n="7"/>ἐνταῦθα κρατίστῃ <supplied reason="lost">βο</supplied><unclear>υ</unclear>λῇ ὥστε τοὺς Ἀλεξανδρεῖς μεταλαβεῖν τῆς 

    <lb n="8"/>τοῦ σίτου δωρεᾶς <supplied reason="lost">καὶ</supplied> αὐτὸς τὴν ἑστίαν ἔχων ἐπʼ ἀμφόδου Βορρᾶ 

    <lb n="9"/><supplied reason="lost">Δρό</supplied><unclear>μ</unclear>ου τὴν ἀξίω<supplied reason="lost">σιν τ</supplied>αύτην προσάγω ὑποτάξας ἅμα καὶ τοῦ ἐφεστί

    <lb n="10" break="no"/><supplied reason="lost">ου</supplied> <unclear>τὸ</unclear> ἀντίγραφ<unclear>ο</unclear><supplied reason="lost">ν ἵνα</supplied> κατʼ ἀκολουθ<unclear>ίαν</unclear> τῶν ὁμοίων <unclear>μοι</unclear> καὶ αὐτὸς 

