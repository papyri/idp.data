         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r" type="textpart"><ab>

    <lb n="1"/><supplied reason="lost">τῷ ὡς</supplied> ἀληθῶς ἀγαπητῷ ἀδελφῷ 

    <lb n="2"/><supplied reason="lost">Ἀ</supplied>φ<supplied reason="lost">υ</supplied>γ<g type="apostrophe"/>γίῳ Πτολεμαῖος ἀπὸ Τακ<supplied reason="lost">όνα.</supplied> 

    <lb n="3"/><supplied reason="lost">ἐ</supplied><unclear>πε</unclear><supplied reason="lost">ί</supplied> σου ἡ ἀγάπη ἤρεσεν τῷ θεῷ, 

    <lb n="4"/><supplied reason="lost">δέ</supplied><unclear>ον</unclear> ἡμῶν ἐστὶν τοῦ σε εὐδοκι<unclear>μ</unclear><supplied reason="lost">οῦν</supplied>

    <lb n="5" break="no"/><supplied reason="lost">τα ἐπ</supplied>αινεῖν, ἄδελφε. καταξίωσο<supplied reason="lost">ν,</supplied> 

    <lb n="6"/><supplied reason="lost">ἐὰν ᾖ</supplied> <unclear>σο</unclear>ι κεχαρισμένον, συνελθε<supplied reason="lost">ῖν</supplied> 

    <lb n="7"/><gap reason="lost" quantity="4" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="1" unit="character"/> τῷ καμηλαρίῳ ἄχρι<supplied reason="lost">ς οὗ</supplied> 

    <lb n="8"/><supplied reason="lost">τὰ ἀποσ</supplied>ταλέντα <hi rend="diaeresis">ὑ</hi>πʼ ἐμοῦ τῷ <hi rend="diaeresis">Ἰ</hi>σὰκ <unclear>λ</unclear><supplied reason="lost" cert="low">άβῃς</supplied> 

    <lb n="9"/><supplied reason="lost">κατὰ τ</supplied>άχος, τουτ<g type="apostrophe"/>έστιν ἄρτω<supplied reason="lost">ν</supplied> 

    <lb n="10"/><supplied reason="lost">ζεύγη</supplied> δύο <num value="2"/> καὶ ἐλαίου ῥαφανίνου <unclear>στ</unclear><supplied reason="lost">α</supplied>

    <lb n="11" break="no"/><supplied reason="lost">μνίο</supplied><unclear>ν</unclear> ἓν <num value="1"/> χωροῦν ὡσεὶ ξέστ<unclear>α</unclear><supplied reason="lost" cert="low">ς δύο <num value="2"/></supplied> 

    <lb n="12"/><supplied reason="lost">καὶ ὄξ</supplied>ους ἀναγκαίου διπλ<unclear>οῦ</unclear>ν ς<gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" quantity="3" unit="character"/> 

    <lb n="13"/><supplied reason="lost">καὶ οἴ</supplied><unclear>ν</unclear>ου <subst><add place="inline">ὀμφακηρὰν</add><del rend="corrected">αμφακηραν</del></subst> μίαν <num value="1"/> <supplied reason="lost">καὶ</supplied> 

    <lb n="14"/><gap reason="lost" quantity="5" unit="character"/><unclear>ω</unclear>ν μίαν <num value="1"/> καὶ ονιμ<unclear>αν</unclear><gap reason="lost" quantity="4" unit="character"/> 

    <lb n="15"/><gap reason="lost" quantity="5" unit="character"/> μίαν <num value="1"/> καὶ τυρὰ τρία <num value="3"/> καὶ <gap reason="lost" quantity="4" unit="character"/> 

    <lb n="16"/><gap reason="lost" quantity="4" unit="character"/><choice><reg><supplied reason="lost">ἥ</supplied>μισ<supplied reason="omitted">υ</supplied></reg><orig><supplied reason="lost">ἡ</supplied><unclear>μ</unclear>ις</orig></choice> <num value="1/2"/> καὶ ῥιπίδιον ἓν <num value="1"/> καὶ <gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" quantity="3" unit="character"/> 

    <lb n="17"/><gap reason="lost" quantity="3" unit="character"/><supplied reason="lost"> ἀ</supplied><unclear>δε</unclear>λφ<unclear>ῶι</unclear>, καὶ εἴ τι δʼ ἂν <unclear>δέ</unclear><supplied reason="lost">χη</supplied>

    <lb n="18" break="no"/><supplied reason="lost">ται</supplied> ὁ καμηλάριος παρὰ τοῦ αὐτ<supplied reason="lost">οῦ,</supplied> 

    <lb n="19"/><supplied reason="lost">τὸν τέλ</supplied><unclear>ει</unclear>ον λόγον δήλωσόν μο<supplied reason="lost">ι</supplied> 

    <lb n="20"/><supplied reason="lost">ποῖον</supplied> ἐστὶν τοῦτο, <hi rend="diaeresis">ἵ</hi>ν<add place="above">α</add> ὁ καμηλάρ<supplied reason="lost">ιος</supplied> 

    <lb n="21"/><supplied reason="lost">αὐτὸ λά</supplied>βῃ. προσαγορεύω τὴν τι<supplied reason="lost">μί</supplied>

    <lb n="22" break="no"/><supplied reason="lost">αν μητ</supplied>έρα Κύραν καὶ <subst><add place="inline">ἐμοὺ<supplied reason="lost">ς</supplied></add><del rend="corrected">ημου<supplied reason="lost">ς</supplied></del></subst><supplied reason="lost"> φίλους</supplied> 

    <lb n="23"/><supplied reason="lost">πάντα</supplied>ς. οὐκ ἐδυνήθην ἐντ<gap reason="lost" quantity="6" unit="character"/> 

    <lb n="24"/><gap reason="lost" quantity="4" unit="character"/><unclear>ε</unclear>σθαι αὐτὸν <add place="above">ἀποστεῖλαι</add>, σὺν θεῷ δὲ <supplied reason="lost" cert="low">τάχα</supplied><supplied reason="lost"> πέμ</supplied>

    <lb n="25" break="no"/><supplied reason="lost">ψω σο</supplied><unclear>ι</unclear> εὐλογίαν.      ἔρ<supplied reason="lost" cert="low">ρωσθαί</supplied> 

    <lb n="26"/><supplied reason="lost" cert="low">σε εὔχομαι</supplied> 
