         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r,1" type="textpart"><ab>

    <lb n="1"/><g type="stauros"/> <expan>γν<ex>ῶσις</ex></expan> σκευῶν <expan>διαφόρ<ex>ων</ex></expan> <choice><reg><expan>ἐνε<supplied reason="omitted">χ</supplied>θέ<ex>ντων</ex></expan></reg><orig>ἐνεθε</orig></choice> ἐν τῷ <choice><reg>προαστείῳ</reg><orig>προαστίῳ</orig></choice> 

    <lb n="2"/>καὶ <expan>παραδοθ<ex>έντων</ex></expan> Ὀννωφρίῳ <expan>συμμ<ex>ά</ex>χ<ex>ῳ</ex></expan> ἐπὶ <expan>μη<ex>νὸς</ex></expan> Θὼθ <num value="13">ιγ</num> <expan>ἰνδ<ex>ικτίονος</ex></expan> <num value="12">ιβ</num> 

    <lb n="3"/><expan>οὕ<ex>τως</ex></expan>· 

    <lb n="4"/>ἐν τῷ τρικλίνῳ             <expan>κραβά<unclear>κ</unclear><ex>τιον</ex></expan> <num value="1">α</num> <expan>μέγ<ex>α</ex></expan>, 

    <lb n="5"/><expan>σκουταρ<ex>ίου</ex></expan> κάτω <expan>μέρ<ex>οσ0</ex></expan> μετὰ τοῦ <choice><reg><expan>βουκ<ex cert="low">όλου</ex></expan></reg><orig>βουκκ</orig></choice> <num value="1">α</num>, 

    <lb n="6"/><choice><reg>εἰκονίδια</reg><orig>ἰκονίδια</orig></choice> δύ<supplied reason="lost">ο</supplied> <num value="2"/>  <choice><reg>ἓν</reg><orig>μιαν</orig></choice> <num value="1"/> μὲν <choice><reg>ἔχον</reg><orig>ἐχους</orig></choice> τὸν <expan>ἅγι<ex>ον</ex></expan> Κόλλουθον 

    <lb n="7"/><expan>περικεχρ<unclear>υ</unclear><supplied reason="lost">σω</supplied>μέ<ex>νον</ex></expan> τῇ <expan>κεφαλ<ex>ῇ</ex></expan>, <choice><reg>τὸ</reg><orig>την</orig></choice> δὲ <choice><reg>ἄλλον</reg><orig>ἀλλην</orig></choice> <choice><reg>ἔχον</reg><orig>ἐχους</orig></choice> 

    <lb n="8"/>τὴν θεοτόκ<supplied reason="lost">ον </supplied><choice><reg><expan><supplied reason="lost">π</supplied>ε<supplied reason="lost">ρ</supplied>ικεχρυσωμέ<ex>νην</ex></expan></reg><orig><supplied reason="lost">π</supplied>ε<supplied reason="lost">ρ</supplied>ικεχρυσομε</orig></choice> ὅλην, 

