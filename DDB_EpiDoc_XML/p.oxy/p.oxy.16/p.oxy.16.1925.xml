    <lb n="12"/><expan>καγκέλλ<ex>ια</ex></expan> <gap reason="lost" quantity="4" unit="character"/><abbr><gap reason="illegible" quantity="3" unit="character"/></abbr> <gap reason="illegible" quantity="1" unit="character"/><abbr><unclear>δ</unclear></abbr> <choice><reg><abbr>ξ<supplied reason="lost">υ</supplied>λιν</abbr></reg><orig>ξ<supplied reason="lost">υ</supplied>λη<unclear>ν</unclear></orig></choice> <num value="3">γ</num>, 

    <lb n="13"/>πτύχιν κ<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="5" unit="character"/> <choice><reg><expan>συκόμ<supplied reason="lost">ο</supplied>ρ<ex>ον</ex></expan></reg><orig>συκαμ<supplied reason="lost">ο</supplied>ρ</orig></choice>              <num value="1">α</num>, 

    <lb n="14"/><expan>ἄλ<ex>λο</ex></expan> πτύχιν <choice><reg><expan><supplied reason="lost">συκ</supplied>όμορ<ex>ον</ex></expan></reg><orig><supplied reason="lost">συκ</supplied>αμωρ</orig></choice> τοῦ <expan>μ<supplied reason="lost">εγ</supplied><unclear>ά</unclear>λ<ex>ου</ex></expan> ἐργάτου  <num value="1">α</num>, 

    <lb n="15"/><expan>κραβάκ<ex>τιον</ex></expan> ε<gap reason="lost" quantity="4" unit="character"/> τοῦ <expan>στρατηλά<ex>του</ex></expan>                <num value="1">α</num>, 

    <lb n="16"/><expan>καγκέλλ<ex cert="low">ια</ex></expan> <gap reason="lost" quantity="3" unit="character"/><unclear>ε</unclear><gap reason="illegible" quantity="1" unit="character"/> τοῦ <expan>λ<unclear>ο</unclear><supplied reason="lost">υτ</supplied>ρ<ex>οῦ</ex></expan>                    <num value="2">β</num>, 

    <lb n="17"/><expan>κεφαλίδ<ex>ια</ex></expan> <gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" quantity="5" unit="character"/> <expan><supplied reason="lost">μ</supplied>ε<unclear>γ</unclear><supplied reason="lost">άλ<ex>α</ex></supplied></expan> καὶ <expan>μικρ<ex>ὰ</ex></expan> <expan>διάφορ<ex>α</ex></expan>    <num value="29">κθ</num>, 

    <lb n="18"/><abbr>βομοσφ<unclear>ο</unclear><supplied reason="lost">ρ</supplied></abbr> <gap reason="lost" quantity="6" unit="character"/><supplied reason="lost"> <expan>μεγάλ<ex>α</ex></expan> κ</supplied>αὶ <expan>μικρ<ex>ὰ</ex></expan> <expan>διάφο<supplied reason="lost">ρ</supplied><ex>α</ex></expan>  <num value="29">κθ</num>, 

