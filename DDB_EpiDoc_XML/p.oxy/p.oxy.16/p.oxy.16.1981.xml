         <head n="22076" xml:lang="en">
            <date>AD612</date>
            <placeName>Oxyrhynchus</placeName>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r" type="textpart"><!--milestone unit="4"--><ab>

    <lb n="1"/><g type="stauros"/> ἐν ὀνόματι τοῦ κυρίου καὶ δεσπότ<supplied reason="lost">ου</supplied> <hi rend="diaeresis">Ἰ</hi>ησοῦ 

    <lb n="2"/>Χριστοῦ τοῦ θεοῦ καὶ σωτῆρος ἡμῶν, 

    <lb n="3"/>βασιλείας τοῦ θειοτάτου καὶ <expan>εὐσ<ex>εβεστάτου</ex></expan> ἡμῶν 

    <lb n="4"/>δεσπότου μεγίστου εὐεργέτου <expan>Φλ<ex>αουίου</ex></expan> Ἡρακλεί<supplied reason="lost">ου</supplied> 

    <lb n="5"/>τοῦ αἰωνίου Αὐγούστου καὶ <expan>Αὐτοκρ<ex>άτορος</ex></expan> ἔτους <num value="2">β</num> 

    <lb n="6"/>Φαῶφι <num value="28">κη</num> <expan><hi rend="diaeresis">ἰ</hi>νδ<ex>ικτίονος</ex></expan> <num value="1">α</num>. 

    <lb n="7"/><expan>Φλ<ex>αουίῳ</ex></expan> Ἀπίωνι τῷ πανευφήμῳ καὶ <expan>ὑπερφ<ex>υεστάτῳ</ex></expan> 

    <lb n="8"/>ἀπὸ ὑπάτων καὶ πατρικίῳ, <expan>γεουχ<ex>οῦντι</ex></expan> <gap reason="illegible" extent="unknown" unit="character"><desc>vestiges</desc></gap> 

    <lb n="9"/><gap reason="illegible" quantity="4" unit="line"/> 

    <lb n="13"/>Αὐρήλιοι Φὶβ <supplied reason="lost">υἱ</supplied>ὸς Διοσκόρου 

    <lb n="14"/>καὶ Ἀπολλῶς υἱὸς Ἀνοῦπ <subst><add place="inline"><expan><unclear>μη</unclear>τ<ex>ρὸς</ex></expan></add><del rend="corrected"><gap reason="illegible" quantity="2" unit="character"/>τ</del></subst> <expan>Φ<supplied reason="lost">ι</supplied>λοξ<ex>ένης</ex></expan> 

    <lb n="15"/>ἀπὸ κώμης Ὤφεως τοῦ <expan>Ὀξυρ<ex>υγχίτου</ex></expan> νομοῦ 

    <lb n="16"/><expan>παγαρχουμέ<ex>νης</ex></expan> παρὰ τοῦ <app type="editorial"><lem><expan>ὑμ<ex>ῶν</ex></expan> <supplied reason="lost">ἐν</supplied>δόξου</lem><rdg><gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" quantity="2" unit="character"/><gap reason="illegible" quantity="3" unit="character"/><unclear>ου</unclear></rdg></app> 

    <lb n="17"/>οἴκου. ὁμολο<supplied reason="lost">γ</supplied>ῶ τῇ ὑμετέρ<supplied reason="lost">ᾳ</supplied> <expan><hi rend="diaeresis">ὑ</hi>περ<supplied reason="lost">φ<ex>υείᾳ</ex></supplied></expan> 

    <lb n="18"/>διὰ τῶν αὐτῇ προσηκόντων ὡς εἴ 

    <lb n="19"/>ποτε καιρῷ ἢ χρόνῳ <supplied reason="omitted">φανῶμεν</supplied> κλέψαντες 

    <lb n="20"/>ἢ μηχα<supplied reason="lost">ν</supplied>ικὰ σκεύη ἢ βο<hi rend="diaeresis">ί</hi>δια 

    <lb n="21"/>ἢ οἱανδήποτε κλοπὴν <expan>π<supplied reason="lost">οι</supplied>ήσα<supplied reason="lost">ν<ex>τες</ex></supplied></expan> 

    <lb n="22"/>ἢ <hi rend="diaeresis">ὑ</hi>ποδέξασθ<unclear>αι</unclear> <choice><reg>λῃστάς</reg><orig>λιστάς</orig></choice>, <expan><supplied reason="lost">ὁ</supplied>μολογο<supplied reason="lost">ῦμε</supplied><ex>ν</ex></expan> 

    <lb n="23"/>παρασ<supplied reason="lost">χ</supplied>εῖν τῇ ὑμετέρᾳ <expan>ὑπερφ<ex>υείᾳ</ex></expan> 

    <lb n="24"/>ὑπὲρ <supplied reason="lost">ἑ</supplied>κάστου ἐγχειρήματος 

    <lb n="25"/>χρυ<supplied reason="lost">σοῦ</supplied> <expan>νομίσμ<ex>ατα</ex></expan>  εἴκοσι τέ<supplied reason="lost">σ</supplied>σερα <num value="24"/> 

    <lb n="26"/><expan>κινδ<ex>ύνῳ</ex></expan> <supplied reason="lost">ἡ</supplied>μῶν καὶ τῆς ἡμῶν 


    <lb n="27"/><hi rend="diaeresis">ὑ</hi>ποστάσεως. <expan>κυρ<ex>ία</ex></expan> ἡ <expan>ὁμολ<ex>ογία</ex></expan> <expan>ἁπλ<ex>ῆ</ex></expan> 

    <lb n="28"/><expan>γραφ<ex>εῖσα</ex></expan> κ<unclear>αὶ</unclear> <expan>ἐπερ<ex>ωτηθεὶς</ex></expan> <expan>ὡμολ<ex>όγησα</ex></expan>. <g type="stauros"/> τὸ κοιν<supplied reason="lost">ὸ</supplied><unclear>ν</unclear> 

    <lb n="29"/>τῶν ὀνομάτων <choice><reg>στοιχεῖ</reg><orig>στοιχῖ</orig></choice> ἡμῖν 

    <lb n="30"/>ὡς <expan>πρ<ex>όκειται</ex></expan>. <hi rend="diaeresis">Ἰ</hi>ωάννης <expan>ἔγρ<ex>αψα</ex></expan> <expan><ex>ὑπὲρ</ex></expan> αὐτῶν 

    <lb n="31"/><expan>ἀγρ<ex>αμμάτων</ex></expan> ὄντων. 

    <lb n="32"/><g type="chirho"/> <foreign xml:lang="la">di <expan>em<ex>u</ex></expan> <hi rend="diaeresis">I</hi>oannu eteliothh. </foreign>
</ab></div><div n="v" type="textpart"><!--milestone unit="4"--><ab>

    <lb n="33"/><g type="stauros"/> <expan>ὁμολ<ex>ογία</ex></expan> τῶν ἀπὸ <expan>κώμ<ex>ης</ex></expan> Ὤφεως το<supplied reason="lost">ῦ</supplied> <expan>Ὀξ<supplied reason="lost">υ</supplied>ρ<ex>υγχίτου</ex></expan> νομοῦ. <g type="stauros"/> </ab></div></div>
