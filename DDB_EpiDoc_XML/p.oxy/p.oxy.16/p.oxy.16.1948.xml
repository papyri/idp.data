    <lb n="1"/><expan>π<ex>αρὰ</ex></expan> 

    <lb n="2"/><g type="stauros"/> τῷ <expan>κυρ<ex>ίῳ</ex></expan> μου <expan>ἀδελφ<ex>ῷ</ex></expan> <choice><reg>Ἀφ<supplied reason="lost">οῦ</supplied>τι</reg><orig>Ἀφ<supplied reason="lost">οῦ</supplied>ς</orig></choice> <expan>προ<ex>νοητῇ</ex></expan> Πέλα <expan>π<ex>αρὰ</ex></expan> Φὶβ <expan>ναυκλ<ex>ή</ex>ρ<ex>ου</ex></expan>. 

    <lb n="3"/>παράσχοῦ τῷ <choice><reg>κυρίῳ</reg><orig>κυρυο</orig></choice> μου <choice><reg>Μαρτυρίῳ</reg><orig>Μαρτυρίου</orig></choice> <choice><reg>βοηθῷ</reg><orig>βουειθοῦ</orig></choice> σίτου ῥυπαροῦ ἀρτάβας 

