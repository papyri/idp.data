    <lb n="1"/><g type="stauros"/> <expan>ἐδόθ<ex>ησαν</ex></expan> <expan>δι<ex>ὰ</ex></expan> Μηνᾶ <choice><reg><expan>χοιρομαγείρ<ex>ου</ex></expan></reg><orig><expan>χοιρομαγίρ<ex>ου</ex></expan></orig></choice> τοῖς <num value="9">θ</num> <expan>στρα<ex>τιώταις</ex></expan> <expan>παραμένουσ<ex>ι</ex></expan> τῷ <expan>ἐνδόξ<ex>ῳ</ex></expan> <expan>οἴκ<ex>ῳ</ex></expan> <expan>λόγ<ex>ῳ</ex></expan> <expan>ἀναλώμ<ex>ατος</ex></expan> 

