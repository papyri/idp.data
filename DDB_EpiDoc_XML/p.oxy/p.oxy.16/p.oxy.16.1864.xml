         <head n="22011" xml:lang="en">
            <date>VIIspc</date>
            <placeName>Oxyrhynchus</placeName>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r" type="textpart"><!--milestone unit="4"--><ab>

    <lb n="1"/><g type="stauros"/> 

    <lb n="1"/><g type="stauros"/> παρακαλῶ τὴν ὑμετέραν <expan>ἐνδοξ<ex>οτάτην</ex></expan> καὶ 

    <lb n="2"/><choice><reg>πάνσοφον</reg><orig>πανσοφος</orig></choice> προστασίαν κελεῦσαι δοθῆναι 

    <lb n="3"/>Γεωργίῳ τῷ <expan>περιβ<ex>έπτῳ</ex></expan> <expan>χαρτουλ<ex>αρίῳ</ex></expan> τὰ νομίσματα 

    <lb n="4"/>δεκαπέντε <num value="15"/> Ἀλεξανδρείας τοῦ μι<unclear>σθ</unclear>ο<supplied reason="lost">ῦ</supplied> <unclear>μο</unclear><supplied reason="lost">υ</supplied> 

    <lb n="5"/>δωδεκάτης ἐπινεμήσεως, ἐπειδή 

    <lb n="6"/>τινα ἐπέταξα αὐτῷ ἐν Ὀξυρύγχων, <hi rend="diaeresis">ἵ</hi>να 

    <lb n="7"/>καὶ ὑπὲρ τούτου εὐχαριστήσω αὐτῇ. ἀλλʼ<g type="apostrophe"/>, ὡς 

    <lb n="8"/>εἴρηται, συνήθως μέγιστά μοι χαριζομένη 

    <lb n="9"/>ταῦτα αὐτῷ παράσχῃ, γράφουσά μοι συνεχῶς 

    <lb n="10"/>τὴν ὑγίειαν αὐτῆς καὶ περὶ τῶν αὐτῇ 

    <lb n="11"/>δοκούντων ἐνταῦθα, προθύμῳ ὄντι 

    <lb n="12"/>πάντα τὰ κελευόμενά μοι παρʼ αὐτῆς εἰς πέρας ἄξαι. <g type="stauros"/>
</ab></div><div n="v" type="textpart"><!--milestone unit="4"--><ab>

    <lb n="13"/><gap reason="illegible" quantity="13" unit="character"><desc>vestiges</desc></gap> <gap reason="lost" quantity="7" unit="character"/> <expan>δεσπό<ex cert="low">τῃ</ex></expan> Θωμᾶς <expan>σύμ<supplied reason="lost">μαχ<ex>ος</ex></supplied></expan> 

    <lb n="14"/><handShift new="m2" cert="low"/> <expan>ἀποδ<ex>ο</ex>θ<ex>ήτω</ex></expan> τῷ <expan><unclear>δεσπό</unclear><ex>τῃ</ex></expan> Μαρίνῳ τῷ <expan>ἐνδοξ<ex>οτάτῳ</ex></expan> 

    <lb n="15"/><expan>ἕνεκ<ex>α</ex></expan> τῶν <num value="15">ιε</num> <expan>νο<ex>μισμάτων</ex></expan> <expan>ὑπ<ex>ὲρ</ex></expan> <expan>μισθ<unclear>ῶ</unclear><ex>ν</ex></expan> <expan><unclear>ἰν</unclear><ex>δικτίονος</ex></expan> <num value="12">ιβ</num>. <g type="stauros"/> </ab></div></div>
