    <lb n="3"/><supplied reason="lost">ἔ</supplied>τους <num value="5">ε</num> <supplied reason="lost">ἔ</supplied>τους <num value="5">ε</num> Μάρκ<supplied reason="lost">ο</supplied>υ Α<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="4"/><gap reason="lost" extent="unknown" unit="character"/> τοῦ κυρίου, Τῦβι <num value="11">ια</num> Τῦ<supplied reason="lost">βι <num value="11">ια</num></supplied>. 

    <lb n="5"/><gap reason="illegible" quantity="22" unit="line"/> 

    <lb n="26"/>τοὺς ὑπηρετοῦντας <gap reason="lost" extent="unknown" unit="character"/> 

