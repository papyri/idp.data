    <lb n="1"/><g type="stauros"/> <expan>ἐδόθ<ex>η</ex></expan> <expan>δ<ex>ιὰ</ex></expan> τοῦ <expan>λαμπρο<ex>τάτου</ex></expan> Μηνᾶ <expan>τραπεζ<ex>ί</ex>τ<ex>ου</ex></expan> <expan>ὑ<ex>πὲρ</ex></expan> <expan>τιμ<ex>ῆς</ex></expan> <expan><hi rend="diaeresis">ἵ</hi>ππω<ex>ν</ex></expan> <num value="3">τριῶν</num> <expan>ἀγορασθ<ex>έντων</ex></expan> 

