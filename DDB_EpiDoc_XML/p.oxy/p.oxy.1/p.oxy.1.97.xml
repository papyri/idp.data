         <head n="20756" xml:lang="en">
            <date>AD 115-6</date>
            <placeName>Oxyrhynchus</placeName>
            <ref n="chr.mitt;;347" type="reprint-from">MChr347</ref>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><ab>
    <lb n="1"/><gap reason="lost" extent="unknown" unit="line"/> 

    <lb n="1"/>πα<gap reason="lost" quantity="2" unit="character"/><gap reason="illegible" quantity="1" unit="character"/>αρ<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="2" unit="character"/><unclear>α</unclear><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="2"/>αὐτόθεν συνεστακ<supplied reason="lost">έναι τὸν ἀδελ</supplied>

    <lb n="3" break="no"/>φὸν Νικάνορα κο<gap reason="illegible" quantity="2" unit="character"/>ν <app type="editorial"><lem resp="BL 1.315"><unclear>τ</unclear><supplied reason="lost">ὸν ὑπερ</supplied></lem><rdg><unclear>ὑ</unclear><supplied reason="lost">πὲρ</supplied></rdg></app><supplied reason="lost"> αὐ</supplied>

    <lb n="4" break="no"/>τοῦ λόγον ποιησόμενον ἐπί τε 

    <lb n="5"/>πάσης ἐξουσίας καὶ παντὸς κριτοῦ 

    <lb n="6"/>περὶ ὧν προφέρονται ἔχειν πρὸς 

    <lb n="7"/>Μενεσθέα Ὥρου τοῦ Μενεσθέως 

    <lb n="8"/>ἀπὸ Σερύφεως, ἢ καὶ αὐτ<supplied reason="lost">ὸ</supplied>ν πρὸς 

    <lb n="9"/>αὐτοὺς, περὶ ἧς ἀντ<supplied reason="lost">ε</supplied>κατέστη 

    <lb n="10"/>αὐτοῖς ἐπὶ τοῦ τοῦ νομοῦ <expan><supplied reason="lost">σ</supplied>τρ<ex>ατηγοῦ</ex></expan> <expan>Ἀπολλωνίο<ex>υ</ex></expan> 

    <lb n="11"/>Θαισοῦτος τῆς καὶ Θαήσιος, ἣν 

    <lb n="12"/>προηνέγκαντο εἶναι μη

    <lb n="13" break="no"/>τρικὴν αὐτῶν δούλην, 

    <lb n="14"/>περὶ ἧς τὸ ζήτημα ὑπερετέ

    <lb n="15" break="no"/>θη ἐπὶ τὸν κράτιστον ἡγεμόνα 

    <lb n="16"/>Ῥουτίλιον <supplied reason="lost">Λ</supplied>οῦπον, καὶ πάντα ἐπι

    <lb n="17" break="no"/>τελέσοντα καθὰ καὶ τῷ ὁμ<supplied reason="lost">ο</supplied>

    <lb n="18" break="no"/>λογοῦντι ἐξῆν, εὐδοκεῖν γὰρ 

    <lb n="19"/>ἐπὶ τούτοις. κυρία ἡ ὁμολογία. 
<milestone rend="paragraphos" unit="undefined"/>
    <lb n="20"/><handShift new="m2"/> Διογένης Ἀμμωνίου τοῦ 

    <lb n="21"/>Νικάνωρος συνέστησα 

    <lb n="22"/>τὸν ἀδελφὸν ἐμοῦ Νικάνωρα 

    <lb n="23"/>ἐπὶ <choice><reg>πᾶσι</reg><orig>πᾶσει</orig></choice> τοῖς προκειμένοις. 

    <lb n="24"/>Νικάνωρ ἀδελφὸς εὐδοκῶ 

    <lb n="25"/>τῇ συστάσει. 
<milestone rend="paragraphos" unit="undefined"/>
    <lb n="26"/>Διογένης ὡς <expan><ex>ἐτῶν</ex></expan> <num value="40">μ</num> <expan>ο<ex>ὐλὴ</ex></expan> <expan>π<ex>ήχει</ex></expan> <expan>ἀρ<ex>ιστερῷ</ex></expan>. 

    <lb n="27"/><supplied reason="lost">Νικάνωρ ὡς <expan><ex>ἐτῶν</ex></expan></supplied> <num value="32"><unclear>λ</unclear>β</num> <expan>ἄσημ<ex>ος</ex></expan> 

    <lb n="28"/><gap reason="lost" quantity="10" unit="character"/><unclear>φ</unclear><gap reason="lost" extent="unknown" unit="character"/> 

