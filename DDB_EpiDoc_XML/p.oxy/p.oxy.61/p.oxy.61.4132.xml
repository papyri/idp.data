    <lb n="25"/>το<unclear>ῦ</unclear> ἁγίου <unclear>ἐπι</unclear><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="3" unit="character"/><unclear>υ</unclear> ἢ <supplied reason="lost">καὶ ἐ</supplied><unclear>γὼ</unclear> 

    <lb n="26"/><unclear>ὁ</unclear> <choice><reg><unclear>τ</unclear>ούτο<supplied reason="omitted">υ</supplied></reg><orig>τούτο</orig></choice> ἐγγυη<unclear>τ</unclear><supplied reason="lost">ὴ</supplied><unclear>ς</unclear> <unclear>ο</unclear>ἴκ<unclear>οθ</unclear>εν 

    <lb n="27"/>ὑπὲρ αὐτοῦ δ<unclear>ι</unclear>δόν<supplied reason="lost">α</supplied>ι, ἐὰν ἀ<unclear>γ</unclear>νωμονή<add place="above">σῃ</add> 

    <lb n="28"/>ὁ <unclear>π</unclear>ρ<unclear>ω</unclear>τό<unclear>τυπ</unclear>ος, ἀναδεχόμενος 

    <lb n="29"/>τὴν καλλ<unclear>ο</unclear>νὴν τοῦ οἴνου μέχρι 

    <lb n="30"/>τοῦ Τῦβι μηνός. δώσω δὲ καὶ τὰ 

    <lb n="31"/>ἀναλώματα τῆς τρύγη<unclear>ς</unclear> οἴνου 

    <lb n="32"/><choice><reg>παλαιοῦ</reg><orig>παλαια</orig></choice> σηκώματα <num value="8">ὀκτὼ</num> καὶ 

    <lb n="33"/>μουστάρια <num value="8">ὀκτὼ</num> καὶ δελφάκιον 

    <lb n="34"/><num value="1">ἓν</num> τιμῆς κερατίων <num value="4">τεσσάρων</num>. 

    <lb n="35"/>ὁμολογῶ δὲ ταῦτα <choice><reg>ἀποκαταστῆσαί</reg><orig>ἀποκαταστησω</orig></choice> <add place="above">σοι</add> 

    <lb n="36"/>ἕως τῆς ἀρχοντικῆς <choice><reg cert="low">δ<supplied reason="lost">ι</supplied>ώρυχος</reg><orig>δ<supplied reason="lost">ι</supplied>ώρ<unclear>υ</unclear>κος</orig></choice>. 

    <lb n="37"/>κύριον τὸ <expan>γραμμ<ex>άτιον</ex></expan> <expan>ἁπλ<ex>οῦν</ex></expan> <expan>γραφ<ex>ὲν</ex></expan> καὶ <expan>ἐπερ<ex>ωτηθεὶς</ex></expan> 

    <lb n="38"/><expan>ὡμολ<ex>όγησα</ex></expan>. <g type="stauros"/> <handShift new="m2"/> <g type="chirho"/> <expan>Αὐρ<ex>ήλιος</ex></expan> Ἰερημίας υἱὸς Ἰωσὴφ <expan>στοι<unclear>χ</unclear><ex>εῖ</ex></expan> <expan><unclear>μ</unclear><ex>οι</ex></expan> 

    <lb n="39"/>τοῦτο τὸ <expan>γ<unclear>ρ</unclear><ex>αμμάτιον</ex></expan> ὡς <expan>πρ<unclear>όκ</unclear><ex>ειται</ex></expan>. <unclear>Μᾶρ</unclear>κος <expan><unclear>ἔγρ</unclear><ex>αψα</ex></expan> <expan>ὑ<ex>πὲρ</ex></expan> 

    <lb n="40"/>αὐτοῦ <expan>ἀγρ<ex>αμμάτου</ex></expan> ὄν<unclear>τ</unclear>ος. <g type="stauros"/> 
