    <lb n="10" break="no"/><expan>ληθ<ex cert="low">είας</ex></expan> <expan>ἐπιδεδωκ<ex>έναι</ex></expan> τὴν <app type="editorial"><lem resp="BL 3.131"><expan>προκ<ex>ειμένην</ex></expan></lem><rdg><expan>προγ<ex>εγραμμένην</ex></expan></rdg></app> ἀπο

    <lb n="11" break="no"/><expan>γρ<ex>αφὴν</ex></expan> καὶ <expan>μήτ<ex>ε</ex></expan> ἐπίξενον <expan>μήτ<ex>ε</ex></expan> 

    <lb n="12"/><expan>Ῥωμ<ex>αῖον</ex></expan> <expan>μήτ<ex>ε</ex></expan> <expan>Ἀλεξανδ<ex>ρέα</ex></expan> <expan>μήτ<ex>ε</ex></expan> <expan>Αἰγύ<ex>πτιον</ex></expan> 

    <lb n="13"/><expan>μήτ<ex>ε</ex></expan> <expan>ἀπελεύ<ex>θερον</ex></expan> <expan>μήτ<ex>ε</ex></expan> ἄλλον 

    <lb n="14"/>μηδένα <expan>οἰκ<ex>εῖν</ex></expan> ἢ <expan>ἀπογρ<ex>άφεσθαι</ex></expan> ἔ

    <lb n="15" break="no"/>ξω τῶν <expan>προγ<ex>εγραμμένων</ex></expan> ἢ ἔνοχος 

    <lb n="16"/>εἴην τῷ ὅρκῳ. <expan><ex>ἔτους</ex></expan> <num value="17">ιζ</num> 

    <lb n="17"/>Αὐτοκράτορος Καίσαρος 

