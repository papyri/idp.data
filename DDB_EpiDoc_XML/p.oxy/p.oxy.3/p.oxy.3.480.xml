    <lb n="1"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ἐπʼ ἀμφόδου</supplied> 

    <lb n="2"/><subst><add place="inline">Μυροβ<supplied reason="lost">αλάν</supplied><unclear>ου</unclear></add><del rend="corrected">μυρββ<supplied reason="lost">αλαν</supplied><unclear>ου</unclear></del></subst> <expan><supplied reason="lost">οἰ</supplied><unclear>κ</unclear><ex>ίαν</ex></expan> καὶ <expan>χρηστ<ex>ήρια</ex></expan> 

