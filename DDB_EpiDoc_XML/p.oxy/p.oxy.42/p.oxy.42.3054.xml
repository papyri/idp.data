         <head n="16449" xml:lang="en">
            <date>AD 265?</date>
            <placeName>Oxyrhynchus</placeName>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><ab>
    <lb n="1"/>ἔτους <num value="12"><unclear>ιβ</unclear></num> Αὐτοκράτορος Καίσαρος <unclear>Πουπλίου</unclear> <unclear>Λικιννίου</unclear> <supplied reason="omitted">Γαλλιηνοῦ</supplied> 

    <lb n="2"/>Γερμανικοῦ Μεγίστου Εὐσεβοῦς Εὐτυχοῦς Σεβαστοῦ 

    <lb n="3"/>Λῴου <space extent="unknown" unit="character"/> Παῦνι <space extent="unknown" unit="character"/> ἐν Ὀξυρύγχων πόλει διὰ Αὐρη

    <lb n="4" break="no"/>λίου Σερηνοθέωνος ἀσχολουμένου ὠνὴν <expan>ἀ<unclear>γορ</unclear><ex>ανομείου</ex></expan>. 

    <lb n="5"/>ἀπεγράψατο τῇ ἐνεστώσῃ ἡμέρᾳ <unclear>Αὐρηλ</unclear>ία <gap reason="illegible" quantity="4" unit="character"/>τις 

    <lb n="6"/>Σί<unclear>μ</unclear>ωνος ἀπὸ Βόστρας τῆς Συρίας φ<unclear>υλ</unclear>ῆ<unclear>ς</unclear> Α<unclear>ν</unclear><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="2" unit="character"/> 

    <lb n="7"/>ὡς <expan><ex cert="low">ἐτῶν</ex></expan> <gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" quantity="2" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="3" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="3" unit="character"/><supplied reason="lost"> ὃν ἐδήλο</supplied>υ διʼ οὗ ἐπέδωκ<supplied reason="lost">εν ὑ</supplied>

    <lb n="8" break="no"/>πομνήμ<supplied reason="lost">ατο</supplied>ς καὶ <unclear>ἧ</unclear><supplied reason="lost">ς πεποί</supplied><unclear>ητ</unclear>αι χειρογραφία<unclear>ς</unclear> ὑπάρ

    <lb n="9" break="no"/>χειν αὐτῷ <unclear>ἀ</unclear><supplied reason="lost">γ</supplied>ορασθέντα ὑπʼ αὐτοῦ κατὰ δίπλωμα 

    <lb n="10"/>Ἑλληνικὸν <unclear>γεν</unclear>όμενον ἐν Βόστρᾳ ἐπ<unclear>ὶ</unclear> <unclear>ὑ</unclear>πάτων 

    <lb n="11"/><choice><reg>Ἀλβίνου</reg><orig>Ἀλβείνου</orig></choice> τὸ <supplied reason="lost"><num value="2">β</num></supplied> καὶ Δέκστρο<supplied reason="lost">υ</supplied> εἰδ<supplied reason="lost">ο</supplied>ῖς Φεβραρίαις πα

    <lb n="12" break="no"/>ρὰ Αὐρηλίο<unclear>υ</unclear> <gap reason="illegible" quantity="1" unit="character"/>εργια Ἰουλιαν<supplied reason="lost">ο</supplied><unclear>ῦ</unclear> <unclear>Μ</unclear>αξίμου βουλευτοῦ 

    <lb n="13"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="2" unit="character"/>ι<unclear>ου</unclear> φυλῆς Ρωμ<gap reason="illegible" quantity="2" unit="character"/><unclear>ας</unclear> <unclear>δοῦ</unclear>λον ὀν<unclear>ό</unclear>

    <lb n="14" break="no"/>ματι Προκόπτοντα ὑστε<unclear>ρ</unclear><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><unclear>σ</unclear>θεντα 

    <lb n="15"/>Ἄ<unclear>πτ</unclear>ον ὡ<unclear>ς</unclear> <expan><ex>ἐτῶν</ex></expan> <gap reason="illegible" quantity="1" unit="character"/><num value="2"><unclear>β</unclear></num> <choice><reg>λευκόχρων</reg><orig>λευκόχρουν</orig></choice> ὑποπλατυπρόσω

    <lb n="16" break="no"/>πον <choice><reg>ἀραιόφρυν</reg><orig>ἀ<unclear>ρ</unclear>εόφρυν</orig></choice> κονδόρεινα οὐλαὶ μετώπῳ 

    <lb n="17"/>καὶ ὀφρύει καὶ σιαγόνει εὐωνύμοις <expan>παράστραβο<ex>ν</ex></expan>

    <lb n="18"/>ὠνηθέντα καὶ ὑπὸ τοῦ Ἰουλ<unclear>ι</unclear>ανοῦ κατʼ ὠνὴν 

    <lb n="19"/>τελειωθεῖσαν διὰ δημ<unclear>οσίου</unclear> ἐν τῇ αὐτῇ πόλει 

    <lb n="20"/>τῷ αὐτῷ ἔτει μηνὶ Δίω <unclear>παρὰ</unclear> <gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><unclear>ι</unclear>δ<g type="apostrophe"/>δ<unclear>αθ</unclear><gap reason="illegible" quantity="1" unit="character"/> 

    <lb n="21"/>μο<unclear>λ</unclear><gap reason="illegible" quantity="1" unit="character"/><unclear>α</unclear>νου βουλευτοῦ δι<gap reason="illegible" quantity="4" unit="character"/>ος γενομ<unclear>έ</unclear><supplied reason="lost">ν</supplied>ου 

    <lb n="22"/>ἐν φ<unclear>υλ</unclear>ῇ Διός, καὶ αυ<gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="2" unit="character"/><unclear>α</unclear>μεν<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="1" unit="character"/> κατʼ 

    <lb n="23"/>ὠνὴ<unclear>ν</unclear> δι<gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="3" unit="character"/>η πόλει <expan>δη<unclear>μ</unclear><supplied reason="lost">ο</supplied>σίω<ex>ν</ex></expan> 

    <lb n="24"/><supplied reason="lost">παρὰ</supplied> Σεπτιμίας <unclear>Ε</unclear><gap reason="illegible" quantity="3" unit="character"/><unclear>ν</unclear>ας Σεπτιμίου Σεουήρου 

    <lb n="25"/><gap reason="lost" quantity="4" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="1" unit="character"/>τα αυ<gap reason="illegible" quantity="4" unit="character"/><unclear>ο</unclear>γενη εκ<gap reason="illegible" quantity="2" unit="character"/>ρι 

    <lb n="26"/><gap reason="lost" quantity="4" unit="character"/><gap reason="illegible" quantity="1" unit="character"/>λης νομ<gap reason="lost" quantity="2" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><unclear>ομ</unclear><gap reason="illegible" quantity="4" unit="character"/>α<gap reason="illegible" quantity="2" unit="character"/>α<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="3" unit="character"/><gap reason="illegible" quantity="3" unit="character"/> 

