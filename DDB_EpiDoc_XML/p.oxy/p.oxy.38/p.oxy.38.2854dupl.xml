         <head n="22241" xml:lang="en">
            <date>AD 248</date>
            <placeName>Oxyrhynchus</placeName>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r" type="textpart"><ab>

    <lb n="1"/><handShift new="m2"/> Αὐρηλίῳ Πασίωνι τῷ καὶ Ἀπολλωνίῳ γυμ<unclear>ν</unclear><supplied reason="lost">α</supplied>

    <lb n="2" break="no"/>σιαρχήσαντι ἐνάρχῳ πρυτάν<add place="above">ε</add>ι βουλευτῇ τ<unclear>ῆ</unclear><supplied reason="lost">ς</supplied> 

    <lb n="3"/>Ὀξυρυγχειτῶν πόλεως. 

    <lb n="4"/>παρὰ <unclear>Σ</unclear><supplied reason="lost">ε</supplied><unclear>π</unclear>τιμίου Εὐδαίμονος γυμνασιά<supplied reason="lost">ρχου</supplied> 

    <lb n="5"/>βουλε<supplied reason="lost">υτο</supplied><unclear>ῦ</unclear> τῆς αὐτῆς πόλεως <hi rend="diaeresis">υ</hi>ἱοῦ Σεπ<supplied reason="lost">τιμίου</supplied> 

    <lb n="6"/>Σερήν<unclear>ο</unclear><supplied reason="lost">υ</supplied> <unclear>ἐ</unclear>ξηγητεύσαντος καὶ πρυτα<supplied reason="lost">νεύ</supplied>

    <lb n="7" break="no"/>σαντο<supplied reason="lost">ς</supplied> τῆς αὐτῆς πόλεως. ἐπ<supplied reason="lost">εὶ παρὰ</supplied> 

    <lb n="8"/>τὰ γε<unclear>νό</unclear>μενά μοι <hi rend="diaeresis">ὑ</hi>πὸ τῆς κρατίστης <unclear>β</unclear><supplied reason="lost">ουλῆς</supplied> 

    <lb n="9"/>ψηφίσματα ἐφʼ οἷς πεποίημαι τῆς <unclear>ἀν</unclear><supplied reason="lost">αδο</supplied>

    <lb n="10" break="no"/>χῆς τ<unclear>ῆς</unclear> γυμνασιαρχίας ὅροις <unclear>πρὸς</unclear> <unclear>τὰς</unclear> <supplied reason="lost">δυ</supplied>

    <lb n="11" break="no"/>νάμεις <app type="alternative"><lem>μου</lem><rdg>μου <g rend="extension" type="filler"/></rdg></app> ὠνόμασάς μ<unclear>ε</unclear> <unclear>σ</unclear>ήμερ<unclear>ο</unclear><supplied reason="lost">ν ἥτις</supplied> 

    <lb n="12"/>ἔστιν <num value="16"><supplied reason="lost">ι</supplied>ϛ</num> ἐν τῇ κρατίστῃ βουλῇ εἰς ε<unclear>ὐθη</unclear><supplied reason="lost">νιαρ</supplied>

    <lb n="13" break="no"/>χίας ἡμέρας <space extent="unknown" unit="character"/> π<unclear>α</unclear><supplied reason="lost">ρʼ</supplied> α<unclear>ὐ</unclear>τὸ <unclear>μ</unclear><supplied reason="lost">ὲν</supplied> 

    <lb n="14"/>ἐπὶ <unclear>τ</unclear><supplied reason="lost">ῶ</supplied>ν <hi rend="diaeresis">ὑ</hi>πομνημάτων τῆς βου<unclear>λ</unclear><supplied reason="lost">ῆς</supplied> 

    <lb n="15"/>ἔκστασίν σοι προεβ<unclear>α</unclear>λ<unclear>ό</unclear>μην καὶ νῦν δ<supplied reason="lost">ὲ</supplied> 

    <lb n="16"/>διὰ τῶνδε τῶν <choice><reg>βιβλιδίων</reg><orig>βιβ<unclear>λε</unclear>ιδίων</orig></choice> τὴν ἔκστ<unclear>α</unclear><supplied reason="lost">σίν</supplied> 

    <lb n="17"/>σοι προσφέρω παντ<supplied reason="lost">ὸ</supplied>ς τοῦ ὑπάρχον<supplied reason="lost">τός</supplied> 

    <lb n="18"/>μοι π<unclear>ό</unclear>ρου ἐπὶ τῷ <unclear>σὲ</unclear> <unclear>ἀ</unclear>ντιλαβόμεν<supplied reason="lost">ον</supplied> 

    <lb n="19"/>τ<unclear>ῶν</unclear> <hi rend="diaeresis"><unclear>ὑ</unclear></hi>π<supplied reason="lost">α</supplied><unclear>ρ</unclear>χόντων μοι ἀποπληρ<unclear>ῶ</unclear><supplied reason="lost">σαι</supplied> 

    <lb n="20"/>ὑπὲρ ἐμοῦ τήν τε γυμνα<unclear>σιαρ</unclear><supplied reason="lost">χίαν</supplied> 

    <lb n="21"/>καὶ εὐθηνιαρχίαν καὶ τὰ ἑπό<unclear>μ</unclear><supplied reason="lost">ενα</supplied> 

    <lb n="22"/>αὐτῇ ἀναλώματα πάντα <unclear>ἔ</unclear><supplied reason="lost">τι δὲ</supplied> 

    <lb n="23"/>καὶ <unclear>τὰ</unclear> ὀφ<add place="above">ε</add>ιλόμενα <hi rend="diaeresis">ὑ</hi>πʼ ἐμοῦ <choice><reg>σιτικ<supplied reason="lost">ὰ</supplied></reg><orig>σει<unclear>τ</unclear>ι<unclear>κ</unclear><supplied reason="lost">ὰ</supplied></orig></choice> 

    <lb n="24"/>καὶ ἀργ<unclear>υ</unclear>ρικὰ τ<unclear>ελέσ</unclear>ματα <hi rend="diaeresis">ὑ</hi>πέρ τε <supplied reason="lost">ὧν</supplied> 

    <lb n="25"/>κέκτημαι <hi rend="diaeresis">ὑ</hi><unclear>π</unclear><supplied reason="lost">α</supplied>ρχόντων καὶ <unclear>οὗ</unclear> <unclear>ἔχ</unclear><supplied reason="lost">ω</supplied>

    <lb n="26"/>ἐν μισθώσει μ<unclear>έρ</unclear><supplied reason="lost">ου</supplied><unclear>ς</unclear> οὐσίας πρό<supplied reason="lost">τ</supplied><unclear>ερον</unclear> 

    <lb n="27"/>Κλαυ<unclear>δ</unclear>ίου Συρ<unclear>ί</unclear>ωνος <choice><reg>αἰωνογυμνασιά<supplied reason="lost">ρ</supplied>

    <lb n="28" break="no"/>χου</reg><orig>ἐωνογυμνασ<unclear>ιά</unclear><supplied reason="lost">ρ</supplied><lb n="28" break="no"/>χου</orig></choice> τῆς λαμπροτάτης Ἀλεξαν<unclear>δρ</unclear><supplied reason="lost">εί</supplied>

    <lb n="29" break="no"/>ας. πρὸς ἅπερ τελέσματα πάντα 

    <lb n="30"/>ὁ ἐκβὰς οἶνος ἀπὸ τῶν χωρίων κα<supplied reason="lost">τε</supplied>

    <lb n="31" break="no"/>νεγυ<unclear>ήθη</unclear> <hi rend="diaeresis">ὑ</hi>πὸ τῶν δεκαπρώτω<unclear>ν</unclear>. 

    <lb n="32"/>καὶ ἐ<unclear>π</unclear>ερωτηθεὶ<unclear>ς</unclear> ὡμολόγησα. <g rend="extension" type="filler"/> 

    <lb n="33"/><expan><ex>ἔτους</ex></expan> <num value="6">ϛ</num> <unclear>Α</unclear>ὐτοκρατ<unclear>ό</unclear>ρων Καισά<supplied reason="lost">ρ</supplied>ων Μάρκ<supplied reason="lost">ων</supplied> 

    <lb n="34"/><hi rend="diaeresis">Ἰ</hi>ουλ<supplied reason="lost">ίω</supplied>ν Φιλίππων Εὐ<unclear>σ</unclear>εβῶν Εὐ<unclear>τ</unclear>υχῶ<supplied reason="lost">ν</supplied> 

    <lb n="35"/>Σεβ<supplied reason="lost">ασ</supplied><unclear>τ</unclear>ῶν Ἁθὺρ ἑκκαιδεκάτῃ. 
</ab></div><div n="v" type="textpart"><ab>

