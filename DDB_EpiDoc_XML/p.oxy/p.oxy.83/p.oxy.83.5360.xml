    <lb n="6"/><choice><reg><supplied reason="lost">Ἰσι</supplied>εῖον</reg><orig><supplied reason="lost">Ἰσι</supplied>ῆο<unclear>ν</unclear></orig></choice> <unclear>ἐκ</unclear> τοῦ Πάτρωνος κλήρου ἀρούρας <num value="3">τρεῖς</num>

    <lb n="7"/><supplied reason="lost">ὥστε</supplied> <supplied reason="lost">ξυλαμῆ</supplied><unclear>σ</unclear>αι ταύ<supplied reason="lost">τα</supplied>ς ἀράκω<unclear>ι</unclear> ἤ χόρτωι, ἀποτά

    <lb n="8" break="no"/><supplied reason="lost">κτου</supplied> <supplied reason="lost">πυροῦ <expan>ἀρτ<ex>αβῶν</ex></expan></supplied> <num value="3">τριῶ<unclear>ν</unclear></num> <gap reason="lost" quantity="14" unit="character" precision="low"/><gap reason="illegible" quantity="1" unit="character"/>β<gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" extent="unknown" unit="character"/>

