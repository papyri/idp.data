    <lb n="28" break="no"/><supplied reason="lost">ση</supplied><unclear>ς</unclear>. <supplied reason="lost">Πα</supplied><unclear>ρ</unclear><supplied reason="lost">μ</supplied><unclear>ε</unclear>νίων <expan>εἶπ<ex>εν</ex></expan>· ἀναγνωσθήτω τὰ ἐ<supplied reason="lost">π</supplied>ὶ τῶν τοι<supplied reason="lost">ού</supplied>των κ<supplied reason="lost">ε</supplied>κριμένα. ἀναγνωσθέντος <gap reason="illegible" quantity="3" unit="character"/><gap reason="lost" quantity="3" unit="character"/> 

    <lb n="29"/>δ<supplied reason="lost">ι</supplied>α<unclear>τ</unclear><supplied reason="lost">άγ</supplied>ματος Τιβερίου Ἀλεξάνδρου ἀπαγορ<supplied reason="lost">εύον</supplied>τος γυνα<unclear>ῖκα</unclear> <app type="editorial"><lem resp="BL 3.133"><unclear>γεωρ</unclear>γί<unclear>ᾳ</unclear> <unclear>προσάγ</unclear>ε<supplied reason="lost" cert="low">σ</supplied>θαι<certainty match=".." locus="value"/></lem><rdg><unclear>γεωρ</unclear>γι<unclear>α</unclear><gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" quantity="1" unit="character"/><gap reason="illegible" quantity="2" unit="character"/><unclear>ατ</unclear>ε<supplied reason="lost">σ</supplied>θαι</rdg></app> ἐπὶ τοῦ <num value="2">β</num> <expan><ex>ἔτους</ex></expan> Γάλβα <gap reason="lost" quantity="3" unit="character"/> 

