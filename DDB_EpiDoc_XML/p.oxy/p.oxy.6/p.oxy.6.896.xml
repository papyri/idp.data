    <lb n="24"/>παρὰ Αὐρηλίων Ἥρωνος <gap reason="lost" quantity="1" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="7" unit="character"/><supplied reason="lost"> καὶ Διδύμου</supplied> 

    <lb n="25"/>Διοσκόρου <expan>ἀμφ<ex>οτέρων</ex></expan> ἀπὸ τῆς <expan>λα<supplied reason="lost">μπ<ex>ρᾶς</ex></supplied></expan><supplied reason="lost"> καὶ</supplied> <expan>λαμπ<ex>ροτάτης</ex></expan> Ὀξυ<supplied reason="lost">ρυγχ</supplied>ιτῶν 

    <lb n="26"/>πόλεως δημοσίων <hi rend="diaeresis">ἰ</hi>ατρῶν. ἐπ<supplied reason="lost">εσ</supplied>τάλ<unclear>ημ</unclear>ε<unclear>ν</unclear> ὑπὸ σοῦ 

    <lb n="27"/>σήμερον ἥτις ἐστὶν Φαρμοῦθι <num value="6">ϛ</num> <supplied reason="lost">ἐκ</supplied> <unclear>β</unclear>ιβλιδίων 

    <lb n="28"/>ἐπιδοθέντων σοι <hi rend="diaeresis">ὑ</hi>πὸ Ἀπολλωνίου <expan>ὀφ<ex>φικιαλίου</ex></expan> ἡγουμένου 

    <lb n="29"/>Αἰ<unclear>γύπτ</unclear>ου Ἡρκουλείας Αὐρηλίου Ἀντωνίου ὥστε γενέσθαι 

    <lb n="30"/><supplied reason="lost">ἐπ</supplied><unclear>ὶ</unclear> <unclear>τὴ</unclear><supplied reason="lost">ν</supplied> οἰκίαν ἐν τῇ αὐτῇ πόλει καὶ τοῦτον <choice><corr>ἐπιδεῖν</corr><sic>εφιδιν</sic></choice> 

    <lb n="31"/>καὶ <supplied reason="lost">ἣ</supplied>ν ἂν καταλάβ<supplied reason="lost">ωμε</supplied><unclear>ν</unclear> περὶ αὐτὸν διάθεσιν ἐγ<g type="apostrophe"/>

    <lb n="32" type="inWord"/><unclear>γ</unclear><supplied reason="lost">ράφω</supplied>ς προσφωνῆσαι. ὅθεν γενόμενοι ἔνθα ὁ<unclear>ρ</unclear>ῶ

    <lb n="33" type="inWord"/><unclear>με</unclear><supplied reason="lost">ν αὐτὸ</supplied><unclear>ν</unclear> το<supplied reason="lost">ῦτ</supplied>ον <choice><corr>κλ<supplied reason="lost">ινή</supplied>ρην</corr><sic><unclear>κλε</unclear><supplied reason="lost">ινη</supplied><unclear>ρη</unclear>ν</sic></choice> ὄντα <choice><corr>πυρετίοις</corr><sic>πυραιτιοις</sic></choice> 

    <lb n="34"/><app type="BL"><lem resp="1.328"><unclear>ἀκ</unclear><supplied reason="lost" cert="low">ρ</supplied><unclear>ίτοι</unclear><supplied reason="lost" cert="low">ς</supplied><certainty match=".." locus="value"/></lem><rdg><unclear>α</unclear><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="1" unit="character"/><unclear>ι</unclear><gap reason="illegible" quantity="3" unit="character"/><gap reason="lost" quantity="1" unit="character"/></rdg></app> συνε<unclear>χ</unclear><supplied reason="lost">όμενον· ὅπερ</supplied> προσφωνοῦμεν. 

    <lb n="35"/>ὑπατείας Και<supplied reason="lost">κι</supplied>νίου <unclear>Σ</unclear><supplied reason="lost">α</supplied><unclear>βίν</unclear><supplied reason="lost">ου καὶ</supplied> Οὐεττίου Ῥουφίνου 

    <lb n="36"/>τῶν <expan>λαμπ<ex>ροτάτων</ex></expan> Φαρμ<supplied reason="lost">ο</supplied>ῦθι <supplied reason="lost"><num value="6">ϛ</num></supplied>. 

    <lb n="37"/><handShift new="m2"/> Αὐρήλιος Ἥρων <choice><corr>ἐπιδέδωκα</corr><sic>επειδεδωκα</sic></choice> 

    <lb n="38"/>πρ<supplied reason="lost">οσ</supplied>φωνῶν ὡς πρόκειται. 

