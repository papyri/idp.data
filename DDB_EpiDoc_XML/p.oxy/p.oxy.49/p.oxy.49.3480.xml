         <head n="15639" xml:lang="en">
            <date>ca. AD 360-90</date>
            <placeName>Oxyrhynchus</placeName>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><ab>
    <lb n="1"/><unclear>Φ</unclear><supplied reason="lost">λαο</supplied><unclear>υ</unclear>ίῳ <hi rend="diaeresis">Ἰ</hi>σιδωριανῷ <unclear>β</unclear><supplied reason="lost">ενεφικιαρίῳ τ</supplied>άξεω<supplied reason="lost">ς</supplied> 

    <lb n="2"/>ἡγεμονίας Αὐγουσταμνικῆς 

    <lb n="3"/>π<unclear>αρ</unclear>ὰ Αὐρηλίου <choice><reg>Γού<supplied reason="omitted">ν</supplied>θου</reg><orig>Γουθου</orig></choice> Πενήφιος ἀπὸ τῆς Ὀξ<supplied reason="lost">υρυγχι</supplied>

    <lb n="4" break="no"/><unclear>τῶ</unclear><supplied reason="lost">ν</supplied> πόλεως. Δίδυμο<supplied reason="lost">ς</supplied> καὶ Ἀ<hi rend="diaeresis">ϊ</hi>ῶν τέκνα τῆς <supplied reason="lost">ἀδελ</supplied>

    <lb n="5" break="no"/>φῆς τῆς ἡμετέρας γα<unclear>μ</unclear>ετῆς καὶ ἐκ προτροπῆς <supplied reason="lost">Δωρο</supplied>

    <lb n="6" break="no"/><supplied reason="lost">θ</supplied><unclear>έου</unclear> Ἀφ<unclear>υ</unclear><supplied reason="lost">γχ</supplied><unclear>ί</unclear>ου τιν<supplied reason="lost">ὸ</supplied>ς ἀπὸ τῆς αὐτῆς πόλεως <unclear>αὐτο</unclear><supplied reason="lost">ὶ</supplied> 

    <lb n="7"/><unclear>ἀπήν</unclear>τησαν <unclear>ἐ</unclear><supplied reason="lost">μο</supplied>ὶ πρὸς <choice><reg>ἀπεργασίαν</reg><orig>ἀπηργασίαν</orig></choice> τῆς ς<gap reason="illegible" quantity="4" unit="character"/> 

    <lb n="8"/><choice><reg>συνθέμενοι</reg><orig><unclear>συν</unclear>θεμενος</orig></choice> τὸν μισθὸν <choice><reg>παρασχεῖν</reg><orig>παρασχῖν</orig></choice>. <choice><reg>ἐπειδὴ</reg><orig>ἐπιδὴ</orig></choice> τ<unclear>οί</unclear>

    <lb n="9" break="no"/>ν<unclear>υ</unclear>ν ἐγώ <choice><reg>τε</reg><orig>δε</orig></choice> καὶ ἡ <choice><reg><supplied reason="omitted">ἡ</supplied>μετέρα</reg><orig>μετέρα</orig></choice> γαμετὴ <choice><reg>κατεσχέθημεν</reg><orig>καθεσχέτημεν</orig></choice> 

    <lb n="10"/><gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" quantity="1" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><unclear>ε</unclear><gap reason="illegible" quantity="4" unit="character"/> <choice><reg>ἕνεκεν</reg><orig><unclear>ἕ</unclear>ναικεν</orig></choice> χρυσαργύρου πραγματ<unclear>ευτι</unclear><supplied reason="lost" cert="low">κοῦ</supplied>

    <lb n="11"/><gap reason="lost" quantity="2" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="3" unit="character"/>τος τοῖς πρ<supplied reason="lost">ο</supplied>ειρ<unclear>ημ</unclear>ένοις <choice><reg>Διδύ<supplied reason="lost">μῳ</supplied></reg><orig>Δυδ<unclear>ύ</unclear><supplied reason="lost">μῳ</supplied></orig></choice> 

    <lb n="12"/><unclear>καὶ</unclear> Ἀ<hi rend="diaeresis">ϊ</hi>ῶνι <choice><reg>ἀπηντήσαμεν</reg><orig>ἀπεντήσαμεν</orig></choice> πρὸς τὸν προειρη<supplied reason="lost">μένον</supplied> 

    <lb n="13"/><unclear>Δωρ</unclear>όθεον τῆς ἀποδόσεως <choice><reg>ἕνεκεν</reg><orig>ἕναικεν</orig></choice> τ<unclear>ῶν</unclear> <supplied reason="lost">ὑπʼ αὐ</supplied>

    <lb n="14" break="no"/>τ<unclear>οῦ</unclear> χρεωστουμένων μισθῶν ἐξ ἀπο<gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="1" unit="character"/> 

    <lb n="15"/><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="1" unit="character"/><gap reason="illegible" quantity="3" unit="character"/>, <choice><reg>τουτέστιν</reg><orig><unclear>του</unclear>τέσσιν</orig></choice> νομισμάτιον ἕν <num value="1"/>, πρὸς τὸ χωρ<gap reason="illegible" quantity="1" unit="character"/> 

    <lb n="16"/><gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" quantity="1" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="3" unit="character"/><unclear>α</unclear>ς τῷ λόγῳ. οὐκ <choice><reg>οἴδαμεν</reg><orig>ὔδαμεν</orig></choice> δὲ τίνι τρόπῳ 

    <lb n="17"/>πρὸς τῇ ἀγν<supplied reason="lost">ω</supplied>μοσύνῃ ἐπῆλθεν ἡμῖν, ἐμοὶ <unclear>τ</unclear>ε <unclear>καὶ</unclear> 

    <lb n="18"/><choice><reg>τῇ</reg><orig>τῆ<unclear>ς</unclear></orig></choice> <choice><reg>ἡμετέρᾳ</reg><orig><supplied reason="lost">ἡ</supplied><unclear>μ</unclear>ετερα<unclear>ς</unclear></orig></choice> <choice><reg>γαμετῇ</reg><orig>γαμετ<unclear>η</unclear>ς</orig></choice> μετὰ οἰκετῶν τ<supplied reason="lost">ι</supplied>νων 

    <lb n="19"/>καὶ <choice><reg>πληγαῖς</reg><orig>πληγε͂ς</orig></choice> ἡμᾶς κατέκοψεν καθʼ ὅλου τοῦ <unclear>σ</unclear><supplied reason="lost" cert="low">ώματος</supplied> 

    <lb n="20"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="3" unit="character"/><unclear>ε</unclear>ν πρ<unclear>ο</unclear>σενε<unclear>χθε</unclear><gap reason="lost" quantity="5" unit="character"/> 

    <lb n="21"/><gap reason="illegible" quantity="3" unit="character"/><gap reason="lost" quantity="1" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><unclear>α</unclear>υτη<supplied reason="lost">ς</supplied> <unclear>γαμ</unclear>ετῆ<unclear>ς</unclear> κ<gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="8" unit="character"/> 

    <lb n="22"/><gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" quantity="2" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><unclear>ι</unclear> προσ<gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="2" unit="character"/>τοις εξε<gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" quantity="3" unit="character"/><gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" quantity="4" unit="character"/> 

    <lb n="23"/>υ<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="1" unit="character"/><gap reason="illegible" quantity="3" unit="character"/><unclear>υ</unclear> ἡμετέρο<unclear>υ</unclear> τροφίμου Ἡρωνᾶ το<gap reason="illegible" quantity="3" unit="character"/><gap reason="lost" quantity="8" unit="character"/> 

    <lb n="24"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="2" unit="character"/> ὅθεν τὰ βιβ<unclear>λία</unclear> ἐπιδίδωμι <gap reason="illegible" quantity="3" unit="character"/><gap reason="lost" quantity="10" unit="character"/> 

    <lb n="25"/>χρεωστούμενα ἀπ<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="1" unit="character"/><gap reason="illegible" quantity="3" unit="character"/>ηναι καὶ τὴν δέο<supplied reason="lost">υσαν </supplied><gap reason="lost" quantity="2" unit="character"/><gap reason="illegible" quantity="1" unit="character"/> 

    <lb n="26"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="1" unit="character"/>σιν γενέ<supplied reason="lost">σ</supplied>θαι <unclear>κ</unclear>ατὰ τῆς ὕβρ<supplied reason="lost">ε</supplied><unclear>ως</unclear>. <space extent="unknown" unit="character"/> 

    <lb n="27"/>Αὐρήλιος Γοῦνθο<supplied reason="lost">ς</supplied> ἐπιδέ<unclear>δω</unclear>κα. Αὐ<unclear>ρ</unclear>ήλιος Θέων <expan><unclear>ἔγρ</unclear><ex>αψα</ex></expan> 

    <lb n="28"/><expan><unclear>ὑ</unclear><ex>πὲρ</ex></expan> <expan><unclear>αὐτ</unclear><ex>οῦ</ex></expan> <expan><unclear>γ</unclear><supplied reason="lost">ρ</supplied><ex>άμματα</ex></expan> <unclear>μὴ</unclear> <expan><unclear>εἰδ</unclear><ex>ότος</ex></expan>. </ab></div>
