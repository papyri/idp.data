         <head n="15627" xml:lang="en">
            <date>AD 63/4</date>
            <placeName>Oxyrhynchus</placeName>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><ab>
    <lb n="1"/>Τιβερίωι Κ<unclear>λ</unclear><supplied reason="lost">αυ</supplied><unclear>δ</unclear>ίωι Ἡφαιστίωνι στρ<unclear>α</unclear><supplied reason="lost">τηγῷ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="2"/>παρὰ Φιλί<unclear>σκ</unclear><supplied reason="lost">ο</supplied><unclear>υ</unclear> <supplied reason="lost">κ</supplied>αὶ Διογένους καὶ Πτολ<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="3"/>πρὸς τὸ ἐπιστ<supplied reason="lost">αλ</supplied>ὲν <choice><reg>ἡμῖν</reg><orig>ἡμεῖν</orig></choice> ὑπὸ σοῦ ου<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="4"/>σοι τὴν ἑξῆς δη<supplied reason="lost">λου</supplied>μένην προσφών<unclear>η</unclear><supplied reason="lost">σιν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="5"/>Τιβέριος Κλαύδιος <supplied reason="lost">Ἡ</supplied>φαιστίων στρατηγὸ<unclear>ς</unclear> <gap reason="lost" extent="unknown" unit="character"/>

    <lb n="6"/>τοῖς λοιποῖς δημο<supplied reason="lost">σίοι</supplied>ς ἐπισκέπταις τοῦ <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="7"/>ἐν οἷς μετέδωκέν μοι ὁ γράφων τὸν ν<supplied reason="lost">ομὸν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="8"/><gap reason="illegible" quantity="2" unit="character"/>υ προγεγραμμένου εἶδος μετέδωκ<unclear>ε</unclear><supplied reason="lost">ν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="9"/>ται. διὸ μεταδίδωμι ὅπως εἰδῆτ<unclear>ε</unclear> <gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="10"/>εσόμενον. <expan><ex>ἔτους</ex></expan> δεκάτου Νέρωνος Κλ<unclear>αυ</unclear><supplied reason="lost">δίου </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="11"/><space extent="unknown" unit="character"/>                <gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="12"/>αἱ εὐθυμετρίαι <unclear>ἐδη</unclear>λώθησαν κοινῆ<unclear>ι</unclear> <gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="13"/>τοῦ διαλογισμοῦ περιέχουσι τῶν μὲν γεγ<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="14"/><choice><reg>μήκη</reg><orig>μηκηι</orig></choice> τῶν καταμεμετρημένων <gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost">  κα</supplied>

    <lb n="15" break="no"/>τατομαῖς καὶ τῶν ἐν τοῖς κλήροις ἀ<gap reason="illegible" quantity="3" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="16"/>λογων γεγονέναι. πάντα δὲ ἀκολούθως <gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost">  ἐπι</supplied>

    <lb n="17" break="no"/>σκέψεως <choice><reg>ἐπιδοθεῖσι</reg><orig>ἐπιδοθῖσι</orig></choice> κατὰ πορείαν κ<unclear>α</unclear><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="18"/>ἤχθη δὲ εἰς τὸ ἐπιδοθῆναι πρὸς ἐπι<unclear>γ</unclear>ρα<unclear>φὴ</unclear><supplied reason="lost">ν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="19"/>γῆς, ἔτι δὲ καὶ χέρσου καὶ τῶν ἄλλων πα<unclear>ντ</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="20"/>τὰ ἐξ αὐτῆς παραγραφέντα ἀπὸ τε π<unclear>λ</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="21"/>παραγράφωνται ἕτεροι σκεπα<supplied reason="lost">ζ</supplied><unclear>ο</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="22"/>καὶ ἄλλων ἐξ ἐπικλασμοῦ τῶν εν<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost">  γε</supplied>

    <lb n="23" break="no"/><unclear>γονέναι</unclear> καὶ ἕτερα πλεῖστα <choice><reg>ἐδάφη</reg><orig>ἐδάφηι</orig></choice> δ<unclear>η</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="24"/>παράκειται δὲ Καπίτωνος ε<unclear>πι</unclear><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="25"/>δοθῆναι καὶ <choice><reg>ἐκρίθη</reg><orig>ἐκριθηι</orig></choice>  <space extent="unknown" unit="character"/> 

    <lb n="26"/>τὰ ἐπιζητ<supplied reason="lost">ο</supplied><unclear>ύμ</unclear><supplied reason="lost">ενα </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="27"/>τάχιστα τ<unclear>ῷ</unclear> ε<unclear>π</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="28"/>καιωνηδ<unclear>η</unclear><gap reason="illegible" quantity="2" unit="character"/>α<unclear>ω</unclear><gap reason="lost" extent="unknown" unit="character"/> </ab></div>
