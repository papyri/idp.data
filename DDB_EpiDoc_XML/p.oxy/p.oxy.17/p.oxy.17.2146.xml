         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r" type="textpart"><ab>

    <lb n="1"/>δευτέρας στέγης νοτινῆς συ<unclear>μπ</unclear><gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="2"/>τοῦ <unclear>π</unclear>ε<supplied reason="lost">σσο</supplied><unclear>ῦ</unclear> <unclear>σὺ</unclear><supplied reason="lost">ν </supplied><gap reason="lost" quantity="1" unit="character"/><gap reason="illegible" quantity="3" unit="character"/><unclear>λωτη</unclear> <num value="1">α</num>, βορινῆ<supplied reason="lost">ς </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="3"/>ηλ<unclear>α</unclear><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> θυρίδες</supplied> 

    <lb n="4"/>ἐπιφωτίζουσαι τὰ ὑποκείμενα <gap reason="lost" quantity="1" unit="character"/><gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="5"/>χαλκῆ <num value="1">α</num>, καὶ <supplied reason="lost">ἐ</supplied>πὶ τοίχου τοῦ αὐτοῦ χαλκοθεσίου κλεῖδες χαλκαῖ <gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> σὺν </supplied><gap reason="lost" quantity="3" unit="character"/> 

    <lb n="6"/>καὶ ἐξομβρι<unclear>σ</unclear>τῆρσι <num value="2">β</num> χαλκαῖ, καὶ ἀφετήριαι χαλκαῖ <num value="2">β</num> ὧν <num value="1">α</num> βάλλουσα <unclear>εἰ</unclear><supplied reason="lost">ς </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="7"/>ἐν ὑπερῴοις τοῦ χαλκοθεσίου φελλὸς <num value="1">α</num> σὺν κλειδίῳ χαλκῷ καὶ ζυγώ<supplied reason="lost" cert="low">ματι </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="8"/>Ἀπιάδος βορινὴ καὶ κατόπιθεν στρογγύλη βορινὴ πρ<supplied reason="lost">ὸ</supplied><unclear>ς</unclear> ἀφετηρία<supplied reason="lost">ις </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="9"/>τῇ αὐτῇ παραδρομίδι, θύρα κέλλας σὺν κερατέᾳ καὶ πήγμασι καὶ ἐπιστά<supplied reason="lost" cert="low">θμῳ</supplied><supplied reason="lost" cert="low"> ἄλλη θύρα βάλλουσα</supplied> 

    <lb n="10"/>εἰς πλατεῖαν, ἄλλη θύρα βάλλουσα εἰς παραδρομίδα πεσσοῦ σὺν κερατέᾳ <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="11"/><gap reason="lost" quantity="3" unit="character"/><gap reason="illegible" quantity="1" unit="character"/>υκ<unclear>λ</unclear>ακησίων καὶ ὠχνων ἔχοντα δίθυρα <num value="2">β</num> σὺν κερατέαις καὶ <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="12"/><gap reason="lost" quantity="2" unit="character"/>ων καγκελλ<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="6" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><unclear>σι</unclear><gap reason="illegible" quantity="3" unit="character"/>ᾳ, στοᾶς σὺν πήγμασι κάγκελλοι <unclear>μ</unclear>ε<unclear>γ</unclear><supplied reason="lost">άλοι </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="13"/><gap reason="lost" quantity="2" unit="character"/><unclear>π</unclear><gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" quantity="2" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><unclear>κ</unclear><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="7" unit="character"/><gap reason="illegible" quantity="2" unit="character"/><unclear>κ</unclear><gap reason="illegible" quantity="2" unit="character"/>θαρη<gap reason="illegible" quantity="1" unit="character"/>ξυλοστε<unclear>γ</unclear><gap reason="lost" extent="unknown" unit="character"/> 

