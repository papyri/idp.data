         <head n="16004" xml:lang="en">
            <date>AD 325</date>
            <placeName>Oxyrhynchus</placeName>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r,ms" type="textpart"><ab>

    <lb n="1" rend="perpendicular"/> λό<unclear>γο</unclear><supplied reason="lost">ς</supplied> σίτου πραθέντος 

    <lb n="2" rend="perpendicular"/> ἐν τῇ ἐξέδρᾳ 

    <lb n="3" rend="perpendicular"/> Φαῶφι <num value="17">ιζ</num>. 
</ab></div><div n="r,ctr" type="textpart"><ab>

    <lb n="1"/>Φλαουίωι Γραφι<unclear>κιανῶ</unclear><supplied reason="lost">ι τῶι διασημοτάτωι μαγίστρωι πριουάτης</supplied> 

    <lb n="2"/>Οὐαλέριος Διοσκουρίδης <unclear>λο</unclear>γιστὴς καὶ Αὐρήλιος Ἑρμείας στρα<unclear>τη</unclear><supplied reason="lost">γὸς</supplied> 

    <lb n="3"/>Ὀξυρυγχίτου χ<supplied reason="lost">α</supplied>ίρει<unclear>ν</unclear> <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="4"/>πρόσταγμα πληροῦντες τοῦ σοῦ <unclear>μεγαλείου</unclear>, μ<unclear>άγ</unclear>ιστρε κύριε, <gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="3" unit="character"/><gap reason="lost" quantity="3" unit="character"/> 

    <lb n="5"/>καὶ <hi rend="diaeresis">Ἰ</hi>σὰκ Διοκόρ<supplied reason="lost">ου </supplied><gap reason="lost" quantity="2" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="lost" quantity="3" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="2" unit="character"/> γενομέν<unclear>ους</unclear> <gap reason="lost" quantity="8" unit="character"/> 

    <lb n="6"/>των ἐπαγομένους τὰς ῥελατωρίας τῆς γενομέν<unclear>ης</unclear> <gap reason="lost" quantity="8" unit="character"/> 

    <lb n="7"/>σεως ἀνεστείλαμεν πρὸς τὴν σὴν <unclear>ἀν</unclear>δρεί<unclear>α</unclear><supplied reason="lost">ν </supplied><gap reason="lost" quantity="11" unit="character"/>

    <lb n="8"/>παρʼ ἡμῖν <choice><reg>χειρογραφία</reg><orig>χιρογραφία</orig></choice> <gap reason="lost" quantity="2" unit="character"/><supplied reason="lost"> ἵ</supplied>νʼ εἰδέναι ἔχοι. <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="9"/><handShift new="m2"/> <choice><reg>ὑπατείας</reg><orig>ὑπ<unclear>ατί</unclear>ας</orig></choice> Π<unclear>ρόκλ</unclear>ου καὶ Π<unclear>α</unclear>υλίνου τῶν λαμπροτάτων Φαρμοῦθι <gap reason="lost" extent="unknown" unit="character"/> 
</ab></div><div n="v" type="textpart"><ab>

    <lb n="10"/><handShift new="m1"/> Φλαουίῳ Γραφικ<unclear>ιαν</unclear>ῶι τῶι διασημοτάτ<unclear>ωι</unclear> μαγί<unclear>στρω</unclear>ι πριουάτης. </ab></div></div>
