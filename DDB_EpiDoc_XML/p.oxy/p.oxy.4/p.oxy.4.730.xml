         <head n="20430" xml:lang="en">
            <date>AD 130</date>
            <placeName>Oxyrhynchus</placeName>
            <ref n="chr.mitt;;273" type="reprint-from">MChr273</ref>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r" type="textpart"><ab>

    <lb n="1"/>ἐμίσθωσεν Σαραπίων Ἡρώδου 

    <lb n="2"/>ἀπʼ Ὀξυ<supplied reason="lost">ρ</supplied>ύγχων πόλεως <choice><reg>Οὐαλερίῳ</reg><orig>Οὐαλερις</orig></choice> 

    <lb n="3"/>Ἀπολλωνίου ἀπὸ κώμης Σενέπτα 

    <lb n="4"/>Πέρσῃ <supplied reason="lost">τ</supplied>ῆς ἐπιγονῆς εἰς τὸ ἐνεσ

    <lb n="5" break="no"/>τὸς πεντεκαιδέκατον ἔτος 

    <lb n="6"/>Ἁδριανοῦ Καίσαρος τοῦ κυρίου 

    <lb n="7"/>ἀπὸ τῆς <choice><reg>ἀναγραφομένης</reg><orig>ἀναγρομένης</orig></choice> εἰς αὐ

    <lb n="8" break="no"/>τὸν βασιλικῆς γῆς ἀρούρας π<supplied reason="lost">έν</supplied>

    <lb n="9" break="no"/>τε <num value="5"/> ἐκ τοῦ Δάμωνος κλήρου, 

    <lb n="10"/>ὥστ<supplied reason="lost">ε</supplied> ταύτας ξυλαμῆσαι χόρ

    <lb n="11" break="no"/>τῳ <unclear>εἰ</unclear><supplied reason="lost">ς κοπὴν κα</supplied><unclear>ὶ</unclear> ἐ<unclear>π</unclear><supplied reason="lost">ινο</supplied><unclear>μή</unclear>ν, 

    <lb n="12"/>φόρο<supplied reason="lost">υ</supplied> ἀποτάκτου ἀργυρίου δρα

    <lb n="13" break="no"/>χμῶν ἑκατὸν εἴκοσι <num value="120"/> καὶ σπον

    <lb n="14" break="no"/>δῆς τῶν ὅλων παιδαρίοις δρα

    <lb n="15" break="no"/>χμὰς τέσσαρας <num value="4"/> ἀνκινδύνου 

    <lb n="16"/>παντὸς κινδύνου, τῶν ὑπὲρ 

    <lb n="17"/>τῆς γῆς δημοσίων ὄντων 

    <lb n="18"/>πρὸς τὸν μεμισθωκότα, ὃν καὶ 

    <lb n="19"/>κυριεύειν τῶν καρπῶν 

    <lb n="20"/>ἕως ἂν <subst><add place="inline">τὸν</add><del rend="corrected">ταν</del></subst> φόρον κομίση

    <lb n="21" break="no"/>ται. τῆς <subst><add place="inline">δὲ</add><del rend="corrected">δ<unclear>ι</unclear></del></subst> μισθώσεως βε

    <lb n="22" break="no"/>βαιουμένης ἀποδότω ὁ με

    <lb n="23" break="no"/>μισθωμέν<supplied reason="lost">ος</supplied> τὸν φόρον τῷ 

    <lb n="24"/>Παῦνι μηνὶ τοῦ αὐτοῦ ἔτους, 

    <lb n="25"/>ὃ δʼ ἂν προσοφειλέσῃ ὁ με

    <lb n="26" break="no"/>μισθωμένος ἀποτεισάτω 

    <lb n="27"/>μεθʼ ἡμιολίας, καὶ ἡ πρᾶ

    <lb n="28" break="no"/>ξις ἔστω τῷ μεμισθωκότι 

    <lb n="29"/>ἔκ τε τοῦ μεμισθωμένου 

    <lb n="30"/>καὶ ἐκ τῶν ὑπαρχόντων 

    <lb n="31"/>αὐτῷ πάντων. κυρία ἡ μί

    <lb n="32" break="no"/>σθωσις. <expan><ex>ἔτους</ex></expan> <num value="15">ιε</num> Αὐτοκράτορος 

    <lb n="33"/>Καίσαρος Τραιανοῦ Ἁδριανοῦ 

    <lb n="34"/>Σεβαστοῦ Ἁθὺρ <num value="19">ιθ</num>. <handShift new="m2"/> <choice><reg>Οὐαλέριος</reg><orig>Οὐαλερις</orig></choice> 
<milestone rend="paragraphos" unit="undefined"/>
    <lb n="35"/> Ἀπολλωνίου μεμίσθ<supplied reason="lost">ω</supplied>

    <lb n="36" break="no"/><supplied reason="lost">μαι τὴ</supplied><unclear>ν</unclear> <unclear>γῆν</unclear> <gap reason="lost" quantity="2" unit="character"/><gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" quantity="2" unit="character"/><gap reason="illegible" quantity="3" unit="character"/> 

    <lb n="37"/><gap reason="lost" quantity="3" unit="character"/><supplied reason="lost"> ἀρ</supplied><unclear>γ</unclear>υ<supplied reason="lost">ρίου δραχμαῶν ἑκα</supplied>

    <lb n="38" break="no"/><supplied reason="lost">τὸν εἴκοσι <num value="120"/> </supplied><gap reason="lost" quantity="3" unit="character"/> 

    <lb n="39"/><gap reason="lost" extent="unknown" unit="line"/> 
</ab></div><div n="v" type="textpart"><ab>

    <lb n="39"/><num value="15">ιε</num> <expan><ex>ἔτους</ex></expan> <expan>μί<ex>σθωσις</ex></expan> <unclear>ἀρ</unclear>ο<unclear>υ</unclear>ρ<supplied reason="lost">ῶ</supplied><unclear>ν</unclear> <num value="5"><unclear>ε</unclear></num> <gap reason="lost" quantity="2" unit="character"/><gap reason="illegible" quantity="4" unit="character"/> <add place="above"><expan>Σενέπ<ex>τα</ex></expan></add>. </ab></div></div>
