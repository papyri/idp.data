    <lb n="9"/><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="3" unit="character"/><choice><reg><supplied reason="lost">πα</supplied>ραδοθείσας</reg><orig><supplied reason="lost">πα</supplied>ραδοτίσας</orig></choice> τοῖς κατὰ <choice><reg>Ἀλεξάνδρειαν</reg><orig>Ἀλ<unclear>εξ</unclear>άνδριαν</orig></choice> <gap reason="illegible" quantity="5" unit="character"/><gap reason="lost" quantity="2" unit="character"/><gap reason="illegible" quantity="3" unit="character"/><gap reason="lost" extent="unknown" unit="character"/>

    <lb n="10"/><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="1" unit="character"/><gap reason="illegible" quantity="2" unit="character"/> <unclear>κ</unclear><supplied reason="lost">αὶ</supplied> <unclear>μ</unclear><supplied reason="lost">η</supplied>δὲν διεψεῦσθαι ἢ ἔνοχοι εἴημεν <unclear>τ</unclear>ῷ θ<supplied reason="lost">είῳ ὅρκῳ. </supplied><gap reason="lost" extent="unknown" unit="character"/>

