         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r" type="textpart"><ab>

    <lb n="1"/>Α<supplied reason="lost">ὐρηλ</supplied>ίῳ Νεμεσίωνι τῷ <expan>κ<ex>αὶ</ex></expan> Δ<unclear>ι</unclear><supplied reason="lost">ο</supplied><unclear>ν</unclear><supplied reason="lost">υσίῳ</supplied> <expan><unclear>βα</unclear>σιλ<ex>ικῷ</ex></expan> 

    <lb n="2"/><gap reason="lost" extent="unknown" unit="character"/> γραμματεῖ Ὀξυρυγχείτου 

    <lb n="3"/>παρὰ Ἰουλίου Ὡρίωνος οὐετρανοῦ τῶν 

    <lb n="4"/><choice><reg>ἐντίμως</reg><orig>ἐντείμως</orig></choice> ἀπολελυμένων. <expan>ἀπογρά<ex>φομαι</ex></expan> 

    <lb n="5"/>πρὸς τὸ <expan>ἐνεστ<ex>ὸς</ex></expan> <num value="5">ε</num> <expan><ex>ἔτος</ex></expan> Μάρκου Αὐρηλίου Σεουήρου 

    <lb n="6"/>Ἀλεξάνδρου Καίσαρος τοῦ κυρίου κ<supplied reason="lost">α</supplied>τὰ τὰ κε

    <lb n="7" break="no"/><expan>λευσθ<ex>έντα</ex></expan> ὑπό τε ἡγεμόνων καὶ Ἰουλίου Σω

    <lb n="8" break="no"/>πάτρου ἐπιστρατηγήσαντος ἣν ἔχω ἄβρο

    <lb n="9" break="no"/>χ<supplied reason="lost">ον</supplied> καὶ <expan>ἐπηντλ<ex>ημένην</ex></expan> περὶ κώμην Παλῶ

    <lb n="10" break="no"/>σιν ἐκ <expan>τ<ex>οῦ</ex></expan> Ἄνδρωνος σὺν τῷ <expan>Μενεσθ<ex>έως</ex></expan> <expan>κλ<ex>ήρῳ</ex></expan> 

    <lb n="11"/>εἰς Πέτσειριν Ἡρακλήου <app type="editorial"><lem resp="BL 9.186"><expan><ex>μαρτάβου</ex></expan></lem><rdg><num value="1">α</num> <expan><ex>ἀρτάβης</ex></expan></rdg></app> <expan>ἀβρόχ<ex>ου</ex></expan> <expan><ex>ἀρούρας</ex></expan> <num value="2">β</num> <num value="1/4" rend="tick">δ</num>,

    <lb n="12"/>καὶ <supplied reason="lost">ἐκ <expan>τ<ex>οῦ</ex></expan> </supplied><expan><supplied reason="lost">α</supplied><ex>ὐτοῦ</ex></expan> εἰς Ἡράκλη<supplied reason="lost">ο</supplied>ν Πετσείρι<supplied reason="lost">ο</supplied>ς <expan>βασι<supplied reason="lost">λ<ex>ικῆς</ex></supplied></expan> 

    <lb n="13"/><expan>ἀβρ<supplied reason="lost">όχ<ex>ου</ex></supplied></expan><supplied reason="lost"> <expan><ex>ἀρούρας</ex></expan> </supplied><gap reason="lost" quantity="2" unit="character"/><supplied reason="lost">, καὶ ἐ</supplied><unclear>κ</unclear> <supplied reason="lost"><expan>τ<ex>οῦ</ex></expan> </supplied><gap reason="lost" quantity="13" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="2" unit="character"/> 

    <lb n="14"/>να <unclear>Π</unclear><gap reason="lost" quantity="7" unit="character"/><gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" quantity="1" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="1" unit="character"/> <expan>ἀβρόχ<ex>ου</ex></expan> <expan><ex>ἄρουραν</ex></expan> <num value="1">α</num>, καὶ ἐκ τοῦ 

    <lb n="15"/>αὐτο<unclear>ῦ</unclear> <supplied reason="lost">εἰς </supplied><gap reason="lost" quantity="3" unit="character"/><gap reason="illegible" quantity="3" unit="character"/> <unclear>Πα</unclear>νεχώτου <num value="1">α</num> <num value="1/2">𐅵</num> <expan>ἀβρόχ<ex>ου</ex></expan> 

    <lb n="16"/><expan><ex>ἀρούρας</ex></expan> <gap reason="lost" quantity="2" unit="character"/><supplied reason="lost">, καὶ ἐκ τοῦ λοι</supplied><unclear>ποῦ</unclear> <expan>Μενεσθ<ex>έως</ex></expan> <expan>κλ<ex>ήρου</ex></expan> εἰς 

    <lb n="17"/><unclear>Π</unclear><gap reason="lost" quantity="14" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="1" unit="character"/> <expan>ἀβρόχ<ex>ου</ex></expan> <expan><ex>ἀρούρης</ex></expan> <num value="1/2">𐅵</num> <num value="1/64" rend="tick">ξδ</num>, 

    <lb n="18"/>κ<supplied reason="lost">α</supplied>ὶ <supplied reason="lost">ἐκ <expan>τ<ex>οῦ</ex></expan> <expan>α<ex>ὐτοῦ</ex></expan> εἰς </supplied><gap reason="lost" quantity="5" unit="character"/><supplied reason="lost"> Ἀλε</supplied>ξάνδρου <app type="editorial"><lem resp="BL 9.186"><expan><ex>μοναρτάβου</ex></expan></lem><rdg><num value="1">α</num> <expan><ex>ἀρτάβης</ex></expan></rdg></app> ἀβρό

    <lb n="19" break="no"/>χου <expan><ex>ἀρούρας</ex></expan> <gap reason="lost" quantity="2" unit="character"/><supplied reason="lost">, κα</supplied><unclear>ὶ</unclear> ἐ<supplied reason="lost">κ <expan>τ<ex>οῦ</ex></expan> </supplied><gap reason="lost" quantity="4" unit="character"/><supplied reason="lost"> εἰς</supplied> Θατρῆν Παυσείρι

    <lb n="20" break="no"/><supplied reason="lost">ο</supplied>ς <app type="editorial"><lem resp="BL 9.186"><expan><ex>μοναρτάβου</ex></expan></lem><rdg><num value="1">α</num> <expan><ex>ἀρτάβης</ex></expan></rdg></app> <expan>ἀβρόχ<ex>ου</ex></expan> <expan><ex>ἀρούρας</ex></expan> <gap reason="lost" quantity="2" unit="character"/><supplied reason="lost">, καὶ ἐ</supplied>κ <expan>τ<ex>οῦ</ex></expan> <expan>α<ex>ὐτοῦ</ex></expan> εἰς Σαραπιά

    <lb n="21" break="no"/>δην Ἀμμων<supplied reason="lost">ίου <app type="editorial"><lem resp="BL 9.186"><expan><ex>μοναρτάβου</ex></expan></lem><rdg><num value="1">α<certainty match="../@value" locus="value"/></num>  <expan><ex>ἀρτάβης</ex></expan></rdg></app></supplied> <expan>ἀβρόχ<ex>ου</ex></expan> <expan><ex>ἀρούρας</ex></expan> <num value="3">γ</num>, καὶ ἐκ <expan>τ<ex>οῦ</ex></expan> 

    <lb n="22"/>λοιποῦ Μενεσθέως εἰ<unclear>ς</unclear> Θατρῆν Ἀρισ

    <lb n="23" break="no"/>τάνδρου <app type="editorial"><lem resp="BL 9.186"><expan><ex>μοναρτάβου</ex></expan></lem><rdg><num value="1">α</num> <expan><ex>ἀρτάβης</ex></expan></rdg></app> <expan>ἀβρόχ<ex>ου</ex></expan> <expan><ex>ἀρούρης</ex></expan> <num value="1/2">𐅵</num>, καὶ ἐκ τοῦ <expan>α<ex>ὐτοῦ</ex></expan> εἰς 

    <lb n="24"/>Σαραπιάδην Ἀμμωνίου <num value="1">α</num> <num value="1/2">𐅵</num> καὶ <num value="1">α</num> <expan><ex>ἀρτάβης</ex></expan> ἀβρό

    <lb n="25" break="no"/>χου <expan><ex>ἀρούρας</ex></expan> <num value="5">ε</num> <num value="1/3" rend="tick">γ</num>, καὶ ἐκ τοῦ <expan>α<ex>ὐτοῦ</ex></expan> εἰς Πέ<supplied reason="lost">τσε</supplied><unclear>ι</unclear>ριν 

    <lb n="26"/>Πανεχώτου <num value="1">α</num> <num value="1/2">𐅵</num> <expan>ἀβρόχ<ex>ου</ex></expan> <expan><ex>ἀρούρης</ex></expan> <num value="1/4" rend="tick">δ</num> <num value="1/8" rend="tick">η</num> <num value="1/16" rend="tick">ιϛ</num>, καὶ ἐκ <expan>τ<ex>οῦ</ex></expan> <expan><unclear>α</unclear><supplied reason="lost"><ex>ὐτοῦ</ex></supplied></expan> 

    <lb n="27"/>εἰς Πέτσειριν Παυσείριος <app type="editorial"><lem resp="BL 9.186"><expan><ex>μον</ex><supplied reason="lost"><ex>αρτάβου</ex></supplied></expan></lem><rdg><num value="1">α</num> <supplied reason="lost"><expan><ex>ἀρτάβης</ex></expan></supplied></rdg></app> <expan>ἀβρόχ<ex>ου</ex></expan> <expan><ex>ἀρούρης</ex></expan> <num value="1/4" rend="tick">δ</num> <num value="1/8" rend="tick">η</num>, 

    <lb n="28"/>καὶ ἐκ <expan>τ<ex>οῦ</ex></expan> <expan>α<ex>ὐτοῦ</ex></expan> εἰς Θερμούθιον Ἀριστάνδρου <num value="1">α</num> <num value="1/2">𐅵</num> 

    <lb n="29"/><expan>ἀβρόχ<ex>ου</ex></expan> <expan><ex>ἀρούραν</ex></expan> <num value="1">α</num> <num value="2/3">𐅷</num>, καὶ ἐκ <expan>τ<ex>οῦ</ex></expan> λοιποῦ <expan>Μενεσθ<ex>έως</ex></expan> <expan>κλ<ex>ήρου</ex></expan> 

    <lb n="30"/>εἰς Μάρκον Πετρώνιον Πρείσκον <app type="editorial"><lem resp="BL 9.186"><expan><ex>μοναρτάβου</ex></expan></lem><rdg><num value="1">α</num> <expan><ex>ἀρτάβης</ex></expan></rdg></app> ἀβρό

    <lb n="31" break="no"/>χου <expan><ex>ἀρούρας</ex></expan> <num value="2">β</num> <num value="1/2">𐅵</num>, καὶ περὶ τὸ <expan>Νεικοστρά<ex>του</ex></expan> <expan>ἐπ<ex>οίκιον</ex></expan> ἐκ <expan>τ<ex>οῦ</ex></expan> 

    <lb n="32"/>Πτολεμαίου καὶ Φιλίππου <expan>κλ<ex>ήρου</ex></expan> εἰς Πέτσειριν

    <lb n="33"/>Ἡρακλήου <app type="editorial"><lem resp="BL 9.186"><expan><ex>μοναρτάβου</ex></expan></lem><rdg><num value="1">α</num> <expan><ex>ἀρτάβης</ex></expan></rdg></app> <expan>ἀβρόχ<ex>ου</ex></expan> <expan><ex>ἄρουραν</ex></expan> <num value="1">α</num> <num value="1/2">𐅵</num>, καὶ ἐκ <expan>τ<ex>οῦ</ex></expan> <expan>α<ex>ὐτοῦ</ex></expan> εἰς Ὀν

    <lb n="34" break="no"/>νῶφριν Πετσείριος <num value="1">α</num> <expan><ex>ἀρτάβης</ex></expan> <expan>ἀβρόχ<ex>ου</ex></expan> <expan><ex>ἄρουραν</ex></expan> <num value="1">α</num>, καὶ περὶ Σε

    <lb n="35" break="no"/>φὼ ἐκ <expan>τ<ex>οῦ</ex></expan> Παρμενίωνος <expan>κλ<ex>ήρου</ex></expan> εἰς Πέτσειριν <expan>μητ<ex>ρὸς</ex></expan> 

