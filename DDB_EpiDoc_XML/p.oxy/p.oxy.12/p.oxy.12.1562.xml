      <body>
         <head n="21928" xml:lang="en">
            <date>AD 276-82</date>
            <placeName>Oxyrhynchus</placeName>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><ab>
    <lb n="1"/><supplied reason="lost">ἔτο</supplied>υς <gap reason="lost" quantity="1" unit="character"/><supplied reason="lost"> Αὐτοκράτορος</supplied> Καίσαρ<supplied reason="lost">ος Μάρκου Αὐρηλίου Πρόβου Περσικοῦ Μεγίστου Γοθικοῦ <app type="editorial"><lem resp="BL 6.102">Μεγίστου</lem><rdg>Μεγίστου Παρθικοῦ Μεγίστου</rdg></app> Γερμανικοῦ <unclear>Μεγίστου</unclear> Εὐσεβοῦς Εὐτυχοῦς Σεβαστοῦ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="2"/><supplied reason="lost">ἐ</supplied>ν τῇ λαμπ<supplied reason="lost">ρᾷ καὶ λαμπρο</supplied>τάτῃ Ὀ<supplied reason="lost">ξυρυγχιτῶν πόλει ἐπὶ </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ἀσχολουμένου ὠνὴν ἀγορανομίου καὶ μνημονίου.</supplied> 

    <lb n="3"/><supplied reason="lost">ὁ</supplied>μολογεῖ Αὐ<supplied reason="lost">ρήλιος Δημη</supplied>τριανὸς <app type="editorial"><lem>Π<unclear>λ</unclear><supplied reason="lost">ουτίωνος</supplied></lem><rdg>Π<unclear>λ</unclear><supplied reason="lost">ουτ </supplied><gap reason="lost" extent="unknown" unit="character"/></rdg></app><supplied reason="lost"> μητρὸς </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> <unclear>βουλευτὴς</unclear> τῆς λαμπρᾶς καὶ λαμπροτάτης Ὀξυρυγχιτῶν πόλεως ἐκμαρτυρεῖσθαι ἐν ἀγυιᾷ τῷ ἑαυτοῦ τιμήματι ἣν προέθετο αὐτῷ ὁ <unclear>ἀδελφὸς</unclear> αὐ</supplied>

    <lb n="4" break="no"/><supplied reason="lost">το</supplied>ῦ Ἀμμών<supplied reason="lost">ιος βουλευ</supplied>τὴς τῆς αὐτ<supplied reason="lost">ῆς πόλεως ἰδιόγραφον διάλυσιν γενομένην τῷ </supplied><gap reason="lost" quantity="1" unit="character"/><supplied reason="lost"> ἔτει μηνὶ </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost">, ἧς <unclear>μοναχὸν</unclear> αὐθεντικὸν ἐπήνεγκεν</supplied> 

    <lb n="5"/>ὁ ὁμολογῶν τῷ πρὸς τῷ μνημ<supplied reason="lost">ονίῳ συγκαταχωρισθησόμενον εἰς τὸ ἐπὶ τόπων βιβλιοφυλάκιον, ἧς ἐστιν ἀντίγραφον. Αὐρήλιος Ἀμμώνιος </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="6"/>βουλευτὴς τῆς λαμπρᾶς καὶ λαμπρο<supplied reason="lost">τάτης Ὀξυρυγχιτῶν πόλεως Αὐρηλίῳ Δημητριανῷ <app type="editorial"><lem>Πλουτίωνος</lem><rdg>Πλουτ <gap reason="illegible" extent="unknown" unit="character"/> </rdg></app> ἀπὸ τῆς αὐτῆς πόλεως χαίρειν. ἐπεὶ </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ἔτει τῆς</supplied> 

    <lb n="7"/>Αὐρηλιανοῦ <choice><reg>βασιλείας</reg><orig>βασιλίας</orig></choice> Φαρμοῦθι <num value="19">ι<unclear>θ</unclear></num> <gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> τὸ ὑπάρχον μοι</supplied> 

    <lb n="8"/>ἐπʼ <expan>ἀμφό<ex>δου</ex></expan> Νότου Δρόμου καὶ Μυροβαλ<supplied reason="lost">άνου <unclear>τρίτον</unclear> μέρος </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> σὺν τοῖς χρη</supplied>

    <lb n="9" break="no"/>στηρίοις πᾶσι καὶ ἐπὶ τοῦ αὐτοῦ <expan>ἀμ<supplied reason="lost">φό<ex>δου</ex></supplied></expan> <gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> <unclear>Εὐδαίμονι</unclear></supplied> 

    <lb n="10"/>τῷ καὶ Διονυσίῳ ἀπολελυμέν<supplied reason="lost">ῳ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="11"/>πρὸ τῆς τοῦ ἀποτάκτου συστάσεως ο<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="12"/>τὸν τῷ Δημητριανῷ καταγραφ<supplied reason="lost">έντα </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="13"/>αὐτοῦ μὲν ἀδελφοῦ τοῦ δὲ ἀποδομ<supplied reason="lost">ένου </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="14"/>προάγει τὸ <choice><reg>δάνειον</reg><orig>δάνιον</orig></choice> τῆς ἀπολύσεως τῶ<supplied reason="lost">ν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="15"/>ἐπὶ περιγραφῇ τοῦ ταμείου καὶ τ<unclear>ο</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="16"/>μέχρι τῆς τῶν ὀφειλομένων <supplied reason="lost">ἀποδόσεως </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="17"/>διαδεχομένῳ τὴν ἐπιστρατηγί<supplied reason="lost">αν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="18"/>κέλευσον κληθῆναι Εὐδαίμον<supplied reason="lost">α </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> διαδεχόμενος</supplied> 

    <lb n="19"/>τὴν ἐπιστρατηγίαν <expan>εἶπ<ex>εν</ex></expan> μετʼ ἄ<unclear>λ</unclear><supplied reason="lost" cert="low">λα </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> <unclear>ἄκυρον</unclear> εἶναι</supplied> 

    <lb n="20"/>τὴν οὖσαν ἐν τοῖς ἀρχείοις καταγρ<supplied reason="lost">αφὴν </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> διὰ <unclear>τὸ</unclear></supplied> 

    <lb n="21"/>μὴ διὰ δίκης <choice><reg>χωρεῖν</reg><orig>χωρῖν</orig></choice>, καὶ ἔδοξεν ὡ<supplied reason="lost">ς </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost">, ὁμολογῶ</supplied> 

    <lb n="22"/>μηδεμίαν ἔφοδον ἢ ἐπέλευσιν ἔχ<supplied reason="lost">ειν πρὸς τὸν  Δημητριανὸν </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> μηδὲ <choice><reg>ἐγκαλεῖν</reg><orig><supplied reason="lost">ἐγκαλῖν</supplied></orig></choice> μηδὲ</supplied> 

    <lb n="23"/><choice><reg>ἐνκαλέσειν</reg><orig>ἐνκαλέσιν</orig></choice> μηδὲ ἐπελεύσεσθαι μη<supplied reason="lost">δὲ </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> μηδὲ</supplied> 

    <lb n="24"/>προσχρήσασθαι τὸν Ἀμμώνιον <gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> διὰ τὸ ἄκυρον εἶναι τὴν πρᾶ</supplied>

    <lb n="25" break="no"/>σιν. κύρια τὰ τῆς διαλύσεως γράμματα <gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> γραφέντα, ἥνπερ ὁπόταν αἱρῇ ἀνοίσεις διὰ δημοσίου οὐ προσδεόμενος ἑτέρας μου εὐδοκήσεως διὰ τὸ ἐντεῦθεν εὐ</supplied>

    <lb n="26" break="no"/>δοκεῖν τὸν Ἀμμώνιον τῇ ἐσομένῃ <supplied reason="lost">δημοσιώσει, περὶ δὲ τοῦ ταῦτα ὀρθῶς καλῶς πεπρᾶχθαι ἐπερωτηθεὶς ὑπὸ σοῦ ὡμολόγησα. <expan><ex>ἔτους</ex></expan> </supplied><gap reason="lost" quantity="1" unit="character"/><supplied reason="lost"> Αὐτοκράτορος Καίσαρος Μάρκου Αὐρηλίου Πρόβου</supplied> 

    <lb n="27"/>Περσικοῦ Μεγίστου <choice><reg>Γοθικοῦ</reg><orig>Γοθθικοῦ</orig></choice> <app type="editorial"><lem resp="BL 6.102">Μεγίστο<supplied reason="lost">υ</supplied></lem><rdg>Μεγίστο<supplied reason="lost">υ Παρθικοῦ Μεγίστου</supplied></rdg></app><supplied reason="lost"> Γερμανικοῦ Μεγίστου Εὐσεβοῦς Εὐτυχοῦς Σεβαστοῦ </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> Αὐρήλιος Ἀμμώνιος <unclear>εὐδοκῶ</unclear></supplied> 

    <lb n="28"/>περὶ τοῦ προκειμένου τρίτου μέρους <supplied reason="lost" cert="low">οἰκίας καὶ ψιλοῦ τόπου </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="29"/>ἕως τούτου τὸ <expan>ἀντίγρα<ex>φον</ex></expan>. κυρία <supplied reason="lost">ἡ <expan>ὁμολ<ex>ογία</ex></expan>, περὶ δὲ τοῦ ταῦτα ὀρθῶς καλῶς πεπρᾶχθαι <expan>ἐπερ<ex>ωτηθεὶς</ex></expan> ὁ <expan>ὁμολ<ex>ογῶν</ex></expan> <expan>ὡμολ<ex>όγησεν</ex></expan> ἐν ἀγυιᾷ τῇ αὐτῇ.</supplied> </ab></div>
      </body>
