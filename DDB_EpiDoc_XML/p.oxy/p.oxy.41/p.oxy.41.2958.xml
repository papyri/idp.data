    <milestone rend="paragraphos" unit="undefined"/> 

    <lb n="20"/>Γερμανικοῦ, <handShift new="m2"/> <subst><add place="inline">Χοιὰχ</add><del rend="corrected"><gap reason="illegible" quantity="1" unit="character"/>οιαχ</del></subst> <num value="5">ε</num>. <handShift new="m3"/> Ἀπολ

