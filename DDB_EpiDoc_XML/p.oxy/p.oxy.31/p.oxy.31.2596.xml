         <head n="30435" xml:lang="en">
            <date>III spc</date>
            <placeName>Oxyrhynchus</placeName>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r,ctr" type="textpart"><ab>

    <lb n="1"/>Σαραπάμμων <choice><reg>Ἀνδρονίκῳ</reg><orig>Ἀνδρονικός</orig></choice> ἀδελφῷ 

    <lb n="2"/>πολλὰ χαίρειν. 

    <lb n="3"/>ἐκομισάμην σου γράμματα διὰ Ψαεις. <expan>ἔγν<hi rend="superscript">ω</hi><ex>ν</ex></expan> 

    <lb n="4"/><choice><reg>σε</reg><orig>σαι</orig></choice> ὁλοκληροῦντα μετὰ τῶν ἡμῶν <choice><reg>πάντων</reg><orig>παντες</orig></choice>. 

    <lb n="5"/>ἐκομισάμην δὲ παρὰ τοῦ αὐτοῦ Ψαεις τὸ σφυ

    <lb n="6" break="no"/>ρίδιον <choice><reg>ἔχον</reg><orig>ἔχων</orig></choice> ταρίχους <num value="5">ε</num> καὶ καθαροὺς <num value="4">δ</num> καὶ μυ

    <lb n="7" break="no"/>ράφιον <supplied reason="omitted">καὶ</supplied> χαρτάρια <num value="2">β</num>. κόμισον οὖν παρὰ τοῦ αὐ

    <lb n="8" break="no"/>τοῦ Ψαεις οἴ<add place="above">νου</add> <choice><reg>προπαλαίου</reg><orig>προπαλέου</orig></choice> χρηστο<add place="above">ῦ</add> βωξίον <add place="above"> <num value="1">α</num> </add> μετὰ 

    <lb n="9"/>ἀξίωσιν τοῦ πατρός μου Λαμυστου. πολλά 

    <lb n="10"/>μοι <choice><reg>βαρήσει<supplied reason="lost">ς</supplied></reg><orig>βαρή<unclear>σι</unclear><supplied reason="lost">ς</supplied></orig></choice> <unclear>πε</unclear>ρὶ τ<unclear>ῶ</unclear>ν <unclear>τ</unclear>ρ<unclear>ι</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="11"/>ζις ὅτι ὀκνήσω. πέμπ<unclear>ω</unclear> σοι<gap reason="lost" quantity="3" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="12"/>σω ὅτι οὐχ εὕρηκα το<unclear>ναπ</unclear><gap reason="illegible" quantity="2" unit="character"/><unclear>ωσι</unclear><gap reason="lost" quantity="1" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="13"/>ψά σοι καὶ ἅπαξ καὶ δὶς περὶ τούτο<supplied reason="lost">υ</supplied> <gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="14"/>ενα<del rend="erasure"><gap reason="illegible" quantity="1" unit="character"/></del> καμηλάτη<unclear>ν</unclear> παρ<add place="above">ά</add> σοι καὶ α<gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="15"/>μοι<gap reason="illegible" quantity="2" unit="character"/><unclear>α</unclear><gap reason="illegible" quantity="1" unit="character"/><unclear>α</unclear><gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="16"/>νον <num value="3">γ</num> αλλ<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="17"/>κεκμηκ<unclear>αμ</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="18"/><gap reason="illegible" quantity="1" unit="character"/>νην καὶ απο<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="19"/>γράψον τ<add place="above">ῷ</add> πατρί <gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="20"/>ταχα δυνη<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="21"/>ἑορτήν πεμψο<gap reason="lost" extent="unknown" unit="character"/> 
</ab></div><div n="r,ms" type="textpart"><ab>

    <lb n="22"/>πολ<unclear>λὰ</unclear> προσαγ<supplied reason="lost">ό</supplied><unclear>ρ</unclear>ευε μετὰ καὶ τῶ<unclear>ν</unclear> <unclear>ἡ</unclear>μῶ<gap reason="lost" quantity="1" unit="character"/>ν <expan>πάντω<ex>ν</ex></expan> 

    <lb n="23"/>ἐρρ<unclear>ω</unclear><gap reason="lost" quantity="7" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="1" unit="character"/><unclear>ε</unclear><gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" quantity="1" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="1" unit="character"/><gap reason="illegible" quantity="2" unit="character"/> 
</ab></div><div n="v" type="textpart"><ab>

