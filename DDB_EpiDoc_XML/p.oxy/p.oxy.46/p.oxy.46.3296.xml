         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r" type="textpart"><ab>

    <lb n="1"/>Αὐρηλίῳ Ἀντιφάτῃ τῷ διασημο<unclear>τ</unclear><supplied reason="lost">άτῳ  </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="2"/><space extent="unknown" unit="character"/>   <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="2"/>παρὰ κληρονόμων Αὐρηλίας <choice><reg>Στρατονίκη<unclear>ς</unclear></reg><orig>Στρατονείκης</orig></choice> <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="3"/>νοέως γυμνασιάρχου βουλευτοῦ τῆς λαμπρᾶς καὶ λα<supplied reason="lost">μπροτάτης Ὀξυρυγχιτῶν πόλεως</supplied> 

    <lb n="4"/>Λουκίλλης τῆς καὶ <choice><reg>Θεωνίδος</reg><orig>Θεωνείδος</orig></choice> θυγατρὸς Θέωνο<supplied reason="lost">ς </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="5"/>χιτῶν πόλεως. ἐπειδὴ τῇ ἐνεστώσῃ ἡμ<supplied reason="lost">έρᾳ </supplied><gap reason="lost" extent="unknown" unit="character"/>

    <lb n="6"/>ἐπιτρόπου ἐπήνεγ<g type="apostrophe"/>κεν ἐπιστολὴν ἐξ ὀνόμα<supplied reason="lost">τος </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="7"/>εἰς Μεχεὶρ μῆνα τοῦ ἐνεστῶτος <num value="7">ζ</num> <expan><ex>ἔτους</ex></expan> καὶ <num value="6">ϛ</num> <expan><ex>ἔτους</ex></expan> <gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="8"/>στουμένων <hi rend="diaeresis">ὑ</hi>φʼ ἡμῶν, τῆς δὲ ἐπισ<unclear>τ</unclear><supplied reason="lost">ο</supplied><unclear>λῆ</unclear><supplied reason="lost">ς</supplied> <gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="9"/>δέπω τοῦ πράγματος παρὰ σοὶ γνωσ<unclear>θέν</unclear><supplied reason="lost">τος </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="10"/>διὰ τοῦ <hi rend="diaeresis">υ</hi><hi rend="diaeresis">ἱ</hi>οῦ Ἡρώδου καί τινα διελάλησας δι<gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="11"/>ἡμᾶς ἀκολούθως τοῖς νόμοις ἐκ<g type="apostrophe"/>καλουμε<unclear>ν</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="12"/>Ὁνωρᾶτον, ᾧ παραθησόμεθα τὰ προσόντ<supplied reason="lost">α </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="13"/><supplied reason="lost">ἀξι</supplied><unclear>οῦ</unclear>ντες, ὡς ἐκελεύσθη, τὴν συνήθη υ<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="14"/><supplied reason="lost">στρ</supplied>ατηγὸν <choice><reg>ἐπιστολιμαῖα</reg><orig>ἐπιστολιμε͂α</orig></choice> γράμματα πρὸς τὸ <unclear>α</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="15"/><gap reason="lost" quantity="3" unit="character"/> τοῦ τῆς ἐκ<g type="apostrophe"/>κλήτου προστίμ<supplied reason="lost">ο</supplied>υ ὄντος συν<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="16"/><supplied reason="lost">τε</supplied>τρακισχιλίαις ἑξακοσίαις εἴκοσι πέντε <num value="4625"/> του<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="17"/>καὶ καθʼ ἕνα ὡμολογήσαμεν. <expan><ex>ἔτους</ex></expan> <num value="7">ζ</num> Αὐτοκράτ<supplied reason="lost">ορος Καίσαρος Γαΐου Αὐρηλίου Οὐαλερίου Διοκλητιανοῦ</supplied> 

    <lb n="18"/>καὶ <expan><ex>ἔτους</ex></expan> <num value="6">ϛ</num> Αὐτοκράτορος Καίσαρος Μάρκου Αὐ<supplied reason="lost">ρηλίου Οὐαλερίου Μαξιμιανοῦ Γερμανικῶν Μεγίστων</supplied> 

    <lb n="19"/>Εὐσεβῶν Εὐτυχῶν Σεβαστῶν, Παῦνι <num value="16">ιϛ</num> <gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 
<milestone rend="paragraphos" unit="undefined"/>
    <lb n="20"/><supplied reason="lost"><handShift new="m2"/> διʼ ἐ</supplied>μοῦ Αὐρηλίου <unclear>Σ</unclear>αρᾶ του<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="21"/><supplied reason="lost">ἐπ</supplied>ιδεδώκαμεν καὶ ε<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="22"/><supplied reason="lost"><handShift new="m3"/> Αὐρη</supplied><unclear>λ</unclear>ία Λουκίλλα ἡ κ<supplied reason="lost">αὶ Θεωνίς </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="23"/><supplied reason="lost">ἐπερ</supplied>ωτηθεῖσα ὡμολ<supplied reason="lost">όγησα. </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="24"/><supplied reason="lost"><handShift new="m4"/> </supplied><gap reason="lost" quantity="5" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><unclear>ων</unclear> τὴν ἐκ<unclear>λ</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="25"/><gap reason="lost" quantity="5" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="3" unit="character"/> προσέλθατ<unclear>ε</unclear> <gap reason="lost" extent="unknown" unit="character"/> 

