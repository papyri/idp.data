    <lb n="1"/><handShift new="m2" cert="low"/>  <num value="26">κϛ</num> 

    <lb n="2"/><supplied reason="lost"><handShift new="m1"/> Κλαύδιος</supplied> Ιουλιανὸς Ὡρίω<supplied reason="lost">νι</supplied> στρατηγῷ Προσ<supplied reason="lost">ωπίτου χαίρειν</supplied>. 

    <lb n="3"/><supplied reason="lost">τὸ ἀντίγραφον</supplied> τῶν δοθέντων μοι <choice><reg>βιβλιδίων</reg><orig>βιβλειδίων</orig></choice> <unclear>ὑ</unclear><supplied reason="lost" cert="low">πὸ </supplied><gap reason="lost" quantity="18" unit="character"/> 

    <lb n="4"/><supplied reason="lost">ἐπέστειλά σ</supplied>οι καὶ εἰ ὀφείλεται αὐτοῖς τῶν ἐμῶν χρόν<supplied reason="lost">ων </supplied><gap reason="lost" quantity="18" unit="character"/> 

    <lb n="5"/><supplied reason="lost">αὐτὸν φρό</supplied><unclear>ν</unclear>τισον κα<unclear>τὰ</unclear> <unclear>τ</unclear>ὸν τύπον τὸν ὡ<unclear>ρισ</unclear>μέ<unclear>νο</unclear><supplied reason="lost">ν </supplied><gap reason="lost" quantity="19" unit="character"/> 

    <lb n="6"/><gap reason="lost" quantity="8" unit="character"/> ὑπὸ τοῦ διασημοτάτου Μαικίου Λαίτου <gap reason="lost" quantity="20" unit="character"/> 

    <lb n="7"/><gap reason="lost" quantity="8" unit="character"/><gap reason="illegible" quantity="1" unit="character"/>ως καὶ καθαρῶς τοῦτο γενέσθαι <unclear>μ</unclear><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="20" unit="character"/> 

    <lb n="8"/><gap reason="lost" quantity="5" unit="character"/><supplied reason="lost" cert="low">τοῖ</supplied><unclear>ς</unclear> τετελευτηκόσι. ἐρρῶσθαί σε βούλομ<supplied reason="lost" cert="low">αι </supplied><gap reason="lost" quantity="18" unit="character"/><supplied reason="lost"> <space extent="unknown" unit="character"/> </supplied> 

    <lb n="9"/><supplied reason="lost">ἔτους </supplied><gap reason="lost" quantity="2" unit="character"/><supplied reason="lost"> Λ</supplied>ουκίου Σεπτιμίου Σεουήρου Εὐσεβοῦς Π<supplied reason="lost" cert="low">ερτίνακος καὶ Μάρκου</supplied> 

    <lb n="10"/><supplied reason="lost">Αὐρηλίου Ἀντωνί</supplied>νου Εὐσεβοῦς Σεβαστῶν, Παχὼν <num value="11">ια</num>. <supplied reason="lost" cert="low"><space extent="unknown" unit="character"/></supplied> 

    <lb n="11"/><gap reason="lost" quantity="6" unit="character"/><supplied reason="lost"> ἐ</supplied><unclear>π</unclear>ι<unclear>στ</unclear>αλήσεται τῷ στρατηγῶι τ<gap reason="illegible" quantity="2" unit="character"/><unclear>β</unclear><gap reason="lost" quantity="22" unit="character"/> 

    <lb n="12"/><gap reason="lost" quantity="7" unit="character"/><gap reason="illegible" quantity="2" unit="character"/> <unclear>κ</unclear>αὶ ὑ<unclear>π</unclear>ὸ Κλαυδίου Ἰουλιανοῦ π<unclear>ε</unclear><supplied reason="lost" cert="low">ρὶ </supplied><gap reason="lost" quantity="20" unit="character"/> 

    <lb n="13"/><supplied reason="lost">Κλαύδιο</supplied>ς Ἰουλιανὸς Ἀνδροσθένει στρα<unclear>τ</unclear><supplied reason="lost">ηγῷ Ὀξυρυγχείτου χαίρειν.</supplied> 

    <lb n="14"/><supplied reason="lost">τὸ ἀντίγραφο</supplied>ν τῶν δοθέντων μοι <choice><reg>βιβλιδίων</reg><orig>βιβλειδίων</orig></choice> <supplied reason="lost" cert="low">ὑπὸ </supplied><gap reason="lost" quantity="18" unit="character"/> 

