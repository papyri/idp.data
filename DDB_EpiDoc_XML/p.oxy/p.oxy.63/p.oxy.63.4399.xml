    <lb n="2"/><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="7" unit="character"/><supplied reason="lost"> τοῦ </supplied><expan><supplied reason="lost">ἐνδο</supplied><unclear>ξ</unclear><supplied reason="lost"><ex>οτάτου</ex></supplied></expan>
