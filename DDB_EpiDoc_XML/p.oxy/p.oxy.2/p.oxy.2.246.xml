         <head n="20515" xml:lang="en">
            <date>AD 66</date>
            <placeName>Oxyrhynchus</placeName>
            <ref n="chr.wilck;;247" type="reprint-from">WChr247</ref>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><ab>
    <lb n="1"/>Παπίσκωι <expan>κοσμητεύ<unclear>σ</unclear><supplied reason="lost">α<ex>ντι</ex></supplied></expan> 

    <lb n="2"/>τῆς πόλεως καὶ <expan>στρα<ex>τηγῷ</ex></expan> <expan>Ὀξυ<supplied reason="lost">ρυγχ<ex>ίτου</ex></supplied></expan> 

    <lb n="3"/>καὶ <expan>Πτολεμα<ex>ίῳ</ex></expan> βασιλικῶ<supplied reason="lost">ι <expan>γρα<ex>μματεῖ</ex></expan></supplied> 

    <lb n="4"/>καὶ τοῖς γράφουσι τὸν νο<supplied reason="lost">μὸν</supplied> 

    <lb n="5"/>παρὰ Ἁρμιύσιος τοῦ Πε<supplied reason="lost">το</supplied>

    <lb n="6" break="no"/>σίριος τοῦ Πετοσίριος μ<supplied reason="lost">η</supplied>

    <lb n="7" break="no"/>τρὸς Διδύμης τῆς Διογέ<supplied reason="lost">νους</supplied> 

    <lb n="8"/>τῶν ἀπὸ κώμης Φθώχ<supplied reason="lost">ιος</supplied> 


    <lb n="9"/>τῆς πρὸς ἀπηλιώτην <expan>τ<unclear>ο</unclear><supplied reason="lost">π<ex>αρχίας</ex></supplied></expan>. 

    <lb n="10"/>ἀπεγραψάμην τῶι ἐν<supplied reason="lost">εσ</supplied>

    <lb n="11" break="no"/>τῶτι <num value="12">ιβ</num> <expan><ex>ἔτει</ex></expan> Νέρωνο<supplied reason="lost">ς</supplied> 

    <lb n="12"/>Κλαυδίου Καίσαρος 

    <lb n="13"/>Σεβαστοῦ Γερμανικοῦ 

    <lb n="14"/>Αὐτοκράτορος περὶ τὴν 

    <lb n="15"/>αὐτὴν Φθῶχιν, ἀπὸ <unclear>γ</unclear><supplied reason="lost">ο</supplied>

    <lb n="16" break="no"/>νῆς ὧν ἔχω θρεμμάτω<supplied reason="lost">ν</supplied> 

    <lb n="17"/>ἄρνας δέκα δύο <num value="12"/>, καὶ νῦ<supplied reason="lost">ν</supplied> 

    <lb n="18"/>ἀπογράφομαι τοὺς ἐπ<supplied reason="lost">ιγε</supplied>

    <lb n="19" break="no"/>γονότας εἰς τὴν ἐνεστ<supplied reason="lost">ῶσαν</supplied> 

    <lb n="20"/>δευτέραν ἀπογραφὴν ἀ<supplied reason="lost">πὸ</supplied> 

    <lb n="21"/>γονῆς τῶν αὐτῶν θρεμ<supplied reason="lost">μά</supplied>

    <lb n="22" break="no"/>των ἄρνας ἑπτά, γίνον<supplied reason="lost">ται</supplied> 

    <lb n="23"/>ἄρνες ἑπτά <num value="7"/>· καὶ ὀμν<supplied reason="lost">ύω</supplied> 

    <lb n="24"/>Νέρωνα Κλαύδιον Καίσαρ<supplied reason="lost">α</supplied> 

    <lb n="25"/>Σεβαστὸν Γερμανικὸν 

    <lb n="26"/>Αὐτοκράτορα μὴ <expan>ὑπεστ<unclear>ά</unclear><supplied reason="lost">λθ<ex>αι</ex></supplied></expan>. 
<milestone rend="paragraphos" unit="undefined"/>
    <lb n="27"/><app type="editorial"><lem resp="BL 1.319">ε<supplied reason="lost">ὐτυχεῖτε</supplied></lem><rdg><expan>ἔ<supplied reason="lost">ρρω<ex>σθε</ex></supplied></expan></rdg></app>. 

    <lb n="28"/><handShift new="m2"/> Ἀπολλώνιος ὁ <expan>π<ex>αρὰ</ex></expan> Παπ<supplied reason="lost">ίσκου</supplied> 

    <lb n="29"/>στρατηγοῦ <expan>σεση<ex>μείωμαι</ex></expan> <expan>ἄρν<ex>ας</ex></expan> <num value="7">ζ</num>. 

    <lb n="30"/><expan><ex>ἔτους</ex></expan> <num value="12">ιβ</num> Νέρωνος τοῦ <choice><reg>κυρ<supplied reason="omitted">ί</supplied>ου</reg><orig>κυρου</orig></choice>, 

    <lb n="31"/>Ἐπεὶφ <num value="30">λ</num>. 

    <lb n="32"/><handShift new="m3"/> Ὡρίων ὁ <expan>π<ex>αρὰ</ex></expan> <expan>Πτολ<ex>εμαίου</ex></expan> <expan>βα<ex>σιλικοῦ</ex></expan> <expan>γρ<ex>αμματέως</ex></expan> 

    <lb n="33"/><expan><unclear>σεση</unclear><ex>μείωμαι</ex></expan> <expan>ἄρ<supplied reason="lost">ν<ex>ας</ex></supplied></expan><supplied reason="lost"> <num value="7">ζ</num></supplied>. 

    <lb n="34"/><expan><ex>ἔτους</ex></expan> <num value="12">ιβ</num> Νέρωνος Καίσαρος 

    <lb n="35"/>τοῦ κυρίου, Ἐπεὶφ <supplied reason="lost"><num value="30">λ</num></supplied>. 

    <lb n="36"/><handShift new="m4"/> Ζήνων ὁ <expan>π<ex>αρὰ</ex></expan> <expan>τ<ex>ῶν</ex></expan> τὸν <expan>νομ<ex>ὸν</ex></expan> <expan>γρ<ex>αφόντων</ex></expan> <expan>σεση<ex>μείωμαι</ex></expan> 

    <lb n="37"/><expan>ἄρν<ex>ας</ex></expan> <num value="7">ζ</num>. <expan><ex>ἔτους</ex></expan> <num value="12">ιβ</num> Νέρων<supplied reason="lost">ο</supplied>ς Καίσαρος 

    <lb n="38"/><unclear>τοῦ</unclear> <unclear>κυρίο</unclear><supplied reason="lost">υ</supplied>, Ἐπ<supplied reason="lost">εὶ</supplied>φ <num value="30">λ</num>. </ab></div>
