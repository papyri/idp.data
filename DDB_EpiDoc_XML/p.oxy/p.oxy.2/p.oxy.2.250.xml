         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r" type="textpart"><ab>

    <lb n="1"/><gap reason="lost" extent="unknown" unit="line"/> 

    <lb n="1"/><gap reason="lost" quantity="5" unit="character"/><gap reason="illegible" quantity="1" unit="character"/> ἀπογράφομαι κατὰ τὰ ὑπ<supplied reason="lost">ὸ τοῦ κρατίστου</supplied> 

    <lb n="2"/><supplied reason="lost">ἡγεμόνος</supplied> Λευκίου Ἰουλίου Οὐηστείν<supplied reason="lost">ου προστεταγ</supplied>

    <lb n="3" break="no"/><supplied reason="lost">μένα χ</supplied>ωρὶς ὧν προαπεγραψάμην <gap reason="lost" quantity="12" unit="character"/> 

    <lb n="4"/><gap reason="lost" quantity="7" unit="character"/><gap reason="illegible" quantity="2" unit="character"/> παρὰ Ἀρσινόης τῆς <unclear>Κ</unclear>ο<gap reason="lost" quantity="12" unit="character"/> 

    <lb n="5"/><gap reason="lost" quantity="10" unit="character"/>ου ἐν τῶι <app type="editorial"><lem resp="BL 1.319">Παγγᾶ Ἰσιείω<supplied reason="lost">ι </supplied><gap reason="lost" quantity="11" unit="character"/></lem><rdg>Παιταισιείω<supplied reason="lost">ι </supplied><gap reason="lost" quantity="11" unit="character"/></rdg></app> 

    <lb n="6"/><gap reason="lost" quantity="10" unit="character"/>υ τῶι <num value="3">γ</num> <expan><ex>ἔτει</ex></expan> Νέρων<supplied reason="lost">ος Κλαυδίου Καί</supplied>

    <lb n="7" break="no"/><supplied reason="lost">σαρος Σεβαστο</supplied>ῦ <subst><add place="inline">Γερμανικοῦ</add><del rend="corrected">γαρμανικου</del></subst> Αὐτο<supplied reason="lost">κράτορος περὶ</supplied> 

    <lb n="8"/>τὴν α<unclear>ὐτ</unclear><supplied reason="lost">ὴν κ</supplied>ώμην ἐκ <choice><reg>τῶν</reg><orig>του</orig></choice> Νικάνορος <supplied reason="lost">καὶ Δριμάκου</supplied> 

    <lb n="9"/>κλήρων λεγομένων Δωροθέου ἀρ<supplied reason="lost">ούρας </supplied><gap reason="lost" quantity="7" unit="character"/> 

    <lb n="10"/>τέταρτον <num value="1/4"/> <choice><reg>ἑνδέκα<supplied reason="omitted">τον</supplied></reg><orig>ἑνδεκα</orig></choice>, τὰ κατηντηκ<supplied reason="lost">ότα εἰς ἐμὲ</supplied> 

    <lb n="11"/>ἐξ ὀνόματος τοῦ μετηλλαχότος <unclear>π</unclear><supplied reason="lost">ατρός μου</supplied> 

    <lb n="12"/>Ἀμμωνίου τοῦ Σαραπίωνος ταῖ<unclear>ς</unclear> <supplied reason="lost">ἐπαγομέναις</supplied> 

    <lb n="13"/>τοῦ αὐτοῦ <num value="3">γ</num> <expan><ex>ἔτους</ex></expan> ἀφʼ ἧς ἔθετο ἰδιο<supplied reason="lost">γράφου ὁμολογί</supplied>

    <lb n="14" break="no"/>ας τῶι <num value="13">ιγ</num> <expan><ex>ἔτει</ex></expan> θεοῦ Κλαυδίου καὶ δι<unclear>ὰ</unclear> <supplied reason="lost">τῆς πρὸς τὴν</supplied> 

    <lb n="15"/>γυναῖκά μου Τααγρελλσὰ Ἀπελ<unclear>λ</unclear><supplied reason="lost">ᾶτος </supplied><gap reason="lost" quantity="8" unit="character"/> 

    <lb n="16"/>συνοικεσίου συνγραφῆς γεγονυία<supplied reason="lost">ς διὰ τοῦ ἐν</supplied> 

    <lb n="17"/>Ὀξυρύγχων πόλει ἀγορανομίου τῶ<supplied reason="lost">ι </supplied><gap reason="lost" quantity="6" unit="character"/><supplied reason="lost"> μηνὶ</supplied> 

    <lb n="18"/>τοῦ <num value="14">ιδ</num> <expan><ex>ἔτους</ex></expan> θεοῦ Κλαυδίου, ἐν μὲν Ὀξυρ<supplied reason="lost">ύγχων πόλει</supplied> 

    <lb n="19"/>ἐν τῆι τῶν Λυκίων παρεμβολῇ ο<supplied reason="lost">ἰκίαν καὶ αὐλὴν</supplied> 

    <lb n="20"/>καὶ ἕτερα χρηστήρια, καὶ περὶ τὸ ν<gap reason="lost" quantity="12" unit="character"/> 

    <lb n="21"/>ἐκ τοῦ Νικάνορος καὶ Δριμάκου κλήρ<supplied reason="lost">ου </supplied><gap reason="lost" quantity="10" unit="character"/> 

    <lb n="22"/>ὑπάρχο<supplied reason="lost">ν</supplied>τος αὐτῶι ἐποικίου το<gap reason="lost" quantity="12" unit="character"/> 

    <lb n="23"/>ἐκ τοῦ ἀπὸ βορρᾶ μέρους εἰς ὃ ἐνλο<supplied reason="lost">γίζεται καὶ</supplied> 

    <lb n="24"/>ὁ ἀπὸ βορρᾶ περιστερεὼν καὶ τὰ <gap reason="lost" quantity="12" unit="character"/> 

    <lb n="25"/>γου<unclear>χα</unclear> πρότερον <app type="editorial"><lem resp="BL 8.234"><supplied reason="lost">Ἀφ</supplied><unclear>ύν</unclear>χ<supplied reason="lost">ι</supplied><unclear>ο</unclear>ς</lem><rdg><supplied reason="lost">Ἀπ</supplied><unclear>ύν</unclear>χ<supplied reason="lost">ι</supplied><unclear>ο</unclear>ς</rdg></app> Α<gap reason="lost" quantity="12" unit="character"/> 

    <lb n="26"/>ει δὲ ἐλαιῶνι κατα<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="22" unit="character"/> 

