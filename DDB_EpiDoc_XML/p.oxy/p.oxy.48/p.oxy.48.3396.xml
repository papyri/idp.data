         <head n="33708" xml:lang="en">
            <date>IV spc</date>
            <placeName>Oxyrhynchus</placeName>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r,ctr" type="textpart"><ab>

    <lb n="1"/>κυρίῳ μου πατρὶ Ἀφυγχίῳ καὶ τῇ κυρίᾳ μου μητρὶ 

    <lb n="2"/><choice><reg>Μαρίᾳ</reg><orig>Μαριας</orig></choice> <choice><reg>Παπνοῦθις</reg><orig>Παπνοθιου</orig></choice> χαίρειν. 

    <lb n="3"/><choice><reg>προηγουμένως</reg><orig>προηγουμένους</orig></choice> εὔχομαι τῇ <choice><reg>θείᾳ</reg><orig>θίᾳ</orig></choice> προνοίᾳ <choice><reg>ὑγιαίνοντά</reg><orig><unclear>ο</unclear>ἱ<supplied reason="lost">γ</supplied><unclear>ε</unclear>νοντά</orig></choice> <choice><reg>σε</reg><orig>σαι</orig></choice> καὶ <choice><reg>ὁλοκληροῦν<supplied reason="omitted">τα</supplied></reg><orig>ὁλοκληρουν</orig></choice> 

    <lb n="4"/>καὶ <choice><reg>εὐθυμοῦντα</reg><orig>εὐθυμο͂ντα</orig></choice> <surplus>καὶ</surplus> ἀποδοθῆναι <del rend="erasure">σαι</del> <choice><reg>ὑμῖν</reg><orig><add place="above">ὑμεῖν</add></orig></choice> τὰ παρʼ ἐμοῦ γράμματα. <choice><reg>ποσάκις</reg><orig>ποσάκεις</orig></choice> 

    <lb n="5"/>ἔγραψα <choice><reg>ὑμῖν</reg><orig>ὑμεῖν</orig></choice> περὶ τῶν <choice><reg>ἐνεχύρων</reg><orig>ἐναιχύρων</orig></choice> ἐκείνων καὶ οὐδὲ 

    <lb n="6"/>φάσις οὐδὲ βάσις. περὶ τῶν αὐτῶν <choice><reg>ἐνεχύρων</reg><orig>ἐναιχύρων</orig></choice> καὶ νῦν γράφω 

    <lb n="7"/><choice><reg>ὑμῖν</reg><orig><hi rend="diaeresis">ὑ</hi>μεῖν</orig></choice>· ὁ γὰρ <surplus><unclear>ς</unclear></surplus> γεοῦχος Σαραπάμμωνος ἔγραψεν πρὸς <subst><add place="inline"><choice><reg>Εὐδαί

    <lb n="8" break="no"/>μονα</reg><orig>Εὐδαι<lb n="8" break="no"/>μων</orig></choice></add><del rend="corrected">θεοδαιμων</del></subst> Θεοδώρου τραπ<unclear>εζ</unclear><supplied reason="lost">ίτην</supplied> παρασχεθῆναι <choice><reg>ὑμῖν</reg><orig>ὑμεῖν</orig></choice> <choice><reg>τὸ</reg><orig>τω</orig></choice> <choice><reg>ἀργύριον</reg><orig>ἀργύριων</orig></choice>. 

    <lb n="9"/>εἰ μὲν <choice><reg>οἴδατε</reg><orig>οἴδαται</orig></choice> ὅτι <choice><reg>ἐλάβετε</reg><orig>ἐλάβαιτε</orig></choice> <choice><reg>τὸ</reg><orig>τω</orig></choice> <choice><reg>ἀργύρων</reg><orig>ἀργυριων</orig></choice> παρὰ <choice><reg>Εὐδαίμονος</reg><orig>Εὐδαίμωνος</orig></choice>, <choice><reg>ἀντι

    <lb n="10" break="no"/>γράψατέ</reg><orig>ἀντι<lb n="10" break="no"/>γράψαταί</orig></choice> μοι· εἰ μὲν <choice><reg>οἴδατε</reg><orig>οἴδαται</orig></choice> <choice><reg>πάλιν</reg><orig>πάλειν</orig></choice> ὅτι οὐκ <choice><reg>ἔδωκεν</reg><orig>αἴδωκεν</orig></choice> <choice><reg>ὑμῖν</reg><orig><hi rend="diaeresis">ὑ</hi>μεῖν</orig></choice> 

    <lb n="11"/><choice><reg>τὸ</reg><orig>τω</orig></choice> <choice><reg>ἀργύριον</reg><orig>ἀργύριων</orig></choice>, <choice><reg>γράψατέ</reg><orig>γράψαταί</orig></choice> μοι καὶ <subst><add place="inline">ποιήσει</add><del rend="corrected">ποσος</del></subst> ἓν <num value="1"/> <choice><reg>νομισμάτιον</reg><orig>νομισμάθιων</orig></choice> 

    <lb n="12"/>ἤδη ἐν ἀργυρίοις <choice><reg>ἐνεχθῆναι</reg><orig>ἐναιχθῆναι</orig></choice> <choice><reg>ὑμῖν</reg><orig>ὑμεῖν</orig></choice>, ἀλλʼ ὅρα μὴ <choice><reg>ἀμελήσητε</reg><orig>ἀμελήσηται</orig></choice> 

    <lb n="13"/>μὴ γράψασ<unclear>θαί</unclear> μοι. <choice><reg>γινώσκειν</reg><orig>γινοσκε</orig></choice> δὲ ὑμᾶς <choice><reg>θέλω</reg><orig>θέλο</orig></choice> ὅτι ἐν τῇ Ἀλεξανδρείᾳ 

    <lb n="14"/><choice><reg>εἰμὶ</reg><orig>εἰμεὶ</orig></choice> μόνος μετὰ δηναρίων μυρι<unclear>άδο</unclear>ς ταλάντων εἴκοσι 

    <lb n="15"/><choice><reg>πέντε</reg><orig>πένται</orig></choice> <num value="25"/>. ὁ ἡγεμὼν ἔγραψεν πρὸς <choice><reg>τὸν</reg><orig>των</orig></choice> <choice><reg>γεοῦχον</reg><orig>γεοῦχων</orig></choice> 

    <lb n="16"/>Σαραπάμμωνος καὶ <choice><reg>τὸ<supplied reason="omitted">ν</supplied></reg><orig>τω</orig></choice> <choice><reg>κοινωνὸ<supplied reason="omitted">ν</supplied></reg><orig>κοινονω</orig></choice> αὐτοῦ ἀρθῆναι <choice><reg>ἐπὶ</reg><orig>ἐπεὶ</orig></choice> τῆς 

    <lb n="17"/>Ἀλεξανδρείας <choice><reg>ἀργυρίου</reg><orig>ἀργυριων</orig></choice> δηναρίων μυριάδας μυριάδων 

    <lb n="18"/>ἑπτὰ <choice><reg>ἡμίσους</reg><orig>ἡμείσους</orig></choice> <num value="750000000"/> καὶ <choice><reg>ἀπέστειλόν</reg><orig>ἀπέστιλών</orig></choice> <choice><reg>με</reg><orig>μοι</orig></choice> μετὰ τοῦ <choice><reg>μέρους</reg><orig>μέρως</orig></choice> ἡμῶν 

    <lb n="19"/>καὶ παραμενῶμαι <choice><reg>ἐπὶ</reg><orig>ἐπεὶ</orig></choice> τῆς Ἀλεξανδρείας ἕως <choice><reg>ἐξοδιασθῶ</reg><orig>ἐξωδιασθῶ</orig></choice> αὐτὰ 

    <lb n="20"/>καὶ <choice><reg>πάλιν</reg><orig>πάλειν</orig></choice> <choice><reg>ἀπέρχομαι</reg><orig>ἀπέρχωμαι</orig></choice> <choice><reg>ἐπὶ</reg><orig>ἐπεὶ</orig></choice> τῆς Ἀθριβειτῶν πόλεως. 

    <lb n="21"/><choice><reg>ἀσπάζομαι</reg><orig>ἀσπάζωμαι</orig></choice> τὴν <choice><reg>κυρία<supplied reason="omitted">ν</supplied></reg><orig>κυρια</orig></choice> μου <choice><reg>σύμβιον</reg><orig>σύμβιων</orig></choice> <choice><reg>Ἀπιάδα</reg><orig>Ἀ<unclear>π</unclear>ιας</orig></choice>, ἀσπάζομαι τὴν 

    <lb n="22"/><choice><reg>κυρία<supplied reason="omitted">ν</supplied></reg><orig>κυρια</orig></choice> μου ἀδελφὴν Τερηοῦς, ἀσπάζομαι <choice><reg>τὸν</reg><orig>των</orig></choice> <choice><reg>κύριό<supplied reason="omitted">ν</supplied></reg><orig>κυριω</orig></choice> μου <choice><reg>ἀδελφὸν</reg><orig>ἀδελφὼν</orig></choice> 

    <lb n="23"/><choice><reg>Γερόντιον</reg><orig>Γερόντιος</orig></choice>, ἀσπάζομαι <choice><reg>τὸν</reg><orig>των</orig></choice> <choice><reg>δεσπότη<supplied reason="omitted">ν</supplied></reg><orig>δε<unclear>σ</unclear>πότη</orig></choice> μου <choice><reg>ἀδελφὸν</reg><orig>ἀδελφὼν</orig></choice> <choice><reg>Δωρόθεον</reg><orig>Δωροθέως</orig></choice>, 

    <lb n="24"/>ἀσπάζομαι τὴν ἀδελφήν μου <choice><reg>Εὔηθιν</reg><orig>Εὐηθις</orig></choice>, ἀσπ<unclear>ά</unclear><supplied reason="lost">ζ</supplied>ομαι τὴν 

    <lb n="25"/><choice><reg>κυρία<supplied reason="omitted">ν</supplied></reg><orig>κυρια</orig></choice> μου ἀδελφὴν τὴν Μικ<unclear>κ</unclear>ήν, ἀσπάζομαι Χωοῦς καὶ <supplied reason="omitted">τὴν</supplied> <choice><reg>σύμβιον</reg><orig>σύμβιων</orig></choice> 

    <lb n="26"/>αὐτοῦ Ἡράκλειαν καὶ τὰ ἀβάσκαντα αὐτῆς τέκνα, ἀσπάζομαι 

    <lb n="27"/><choice><reg>τὸν</reg><orig>των</orig></choice> <choice><reg>πατέρα</reg><orig>πατέραν</orig></choice> μου <choice><reg>Ἀμμώνιον</reg><orig>Ἀμμώνιων</orig></choice>, ἀσπάζομαι Τασιλβάνεις 

    <lb n="28"/>καὶ τὰ ἀβάσκαντα αὐτῆς τέκ<unclear>να</unclear>, <unclear>ἀσπάζομ</unclear>αι <choice><reg>τὸν</reg><orig>των</orig></choice> <choice><reg>πατέρα</reg><orig>πατέρα<unclear>ν</unclear></orig></choice> 

    <lb n="29"/>μου Πετεμούνιος καὶ τὴν <choice><reg>μητέρα</reg><orig>μητέραν</orig></choice> μ<unclear>ου</unclear> Ταήσ<supplied reason="lost">ει</supplied>ς 
</ab></div><div n="r,ms" type="textpart"><ab>

    <lb n="30"/>καὶ τὰ ἀβάσκαντα αὐτῆς τέκνα, ἀσπάζομαι τὴν <choice><reg>κυρία<supplied reason="omitted">ν</supplied></reg><orig>κυρια</orig></choice> μου καὶ ἀδελφὴν Παλλαδίης ἀσπάζομαι πάντας 

    <lb n="31"/>κατʼ ὄνομα τοὺς <choice><reg>φιλοῦντας</reg><orig>φιλο͂ντας</orig></choice> ἡμᾶς. 
</ab></div><div n="v" type="textpart"><ab>

    <lb n="32"/><figure><figDesc>drawing</figDesc></figure>  <expan>ἀπόδ<ex>ος</ex></expan> τῷ κυρίῳ μου <g type="xs"/> πατρὶ Ἀφυγχίῳ <expan>π<ex>αρὰ</ex></expan> Παπνουθίου <figure><figDesc>drawing</figDesc></figure> </ab></div></div>
