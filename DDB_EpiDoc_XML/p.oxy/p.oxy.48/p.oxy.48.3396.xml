         <head n="33708" xml:lang="en">
            <date>IVspc</date>
            <placeName>Oxyrhynchus</placeName>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r,ctr" type="textpart"><!--milestone unit="4"--><ab>

    <lb n="1"/>κυρίῳ μου πατρὶ Ἀφυγχίῳ καὶ τῇ κυρίᾳ μου μητρὶ 

    <lb n="2"/><choice><reg>Μαρίᾳ</reg><orig>μαριας</orig></choice> <choice><reg>Παπνοῦθις</reg><orig>παπνοθιου</orig></choice> χαίρειν. 

    <lb n="3"/><choice><reg>προηγουμένως</reg><orig>προηγουμενους</orig></choice> εὔχομαι τῇ <choice><reg>θείᾳ</reg><orig>θια</orig></choice> προνοίᾳ <choice><reg>ὑγιαίνοντά</reg><orig><unclear>ο</unclear>ι<supplied reason="lost">γ</supplied><unclear>ε</unclear>νοντα</orig></choice> <choice><reg>σε</reg><orig>σαι</orig></choice> καὶ <choice><reg>ὁλοκληροῦν<supplied reason="omitted">τα</supplied></reg><orig>ολοκληρουν</orig></choice> 

    <lb n="4"/>καὶ <choice><reg>εὐθυμοῦντα</reg><orig>ευθυμοντα</orig></choice> <surplus>καὶ</surplus> ἀποδοθῆναι <del rend="erasure">σαι</del> <choice><reg>ὑμῖν</reg><orig><add place="above">υμειν</add></orig></choice> τὰ παρʼ ἐμοῦ γράμματα. <choice><reg>ποσάκις</reg><orig>ποσακεις</orig></choice> 

    <lb n="5"/>ἔγραψα <choice><reg>ὑμῖν</reg><orig>υμειν</orig></choice> περὶ τῶν <choice><reg>ἐνεχύρων</reg><orig>εναιχυρων</orig></choice> ἐκείνων καὶ οὐδὲ 

    <lb n="6"/>φάσις οὐδὲ βάσις. περὶ τῶν αὐτῶν <choice><reg>ἐνεχύρων</reg><orig>εναιχυρων</orig></choice> καὶ νῦν γράφω 

    <lb n="7"/><choice><reg>ὑμῖν</reg><orig><hi rend="diaeresis">υ</hi>μειν</orig></choice>· ὁ γὰρ <surplus><unclear>ς</unclear></surplus> γεοῦχος Σαραπάμμωνος ἔγραψεν πρὸς <subst><add place="inline"><choice><reg>Εὐδαί

    <lb n="8" type="inWord"/>μονα</reg><orig>ευδαιμων</orig></choice></add><del rend="corrected">θεοδαιμων</del></subst> Θεοδώρου τραπ<unclear>εζ</unclear><supplied reason="lost">ίτην</supplied> παρασχεθῆναι <choice><reg>ὑμῖν</reg><orig>υμειν</orig></choice> <choice><reg>τὸ</reg><orig>τω</orig></choice> <choice><reg>ἀργύριον</reg><orig>αργυριων</orig></choice>. 

    <lb n="9"/>εἰ μὲν <choice><reg>οἴδατε</reg><orig>οιδαται</orig></choice> ὅτι <choice><reg>ἐλάβετε</reg><orig>ελαβαιτε</orig></choice> <choice><reg>τὸ</reg><orig>τω</orig></choice> <choice><reg>ἀργύρων</reg><orig>αργυριων</orig></choice> παρὰ <choice><reg>Εὐδαίμονος</reg><orig>ευδαιμωνος</orig></choice>, <choice><reg>ἀντι

    <lb n="10" type="inWord"/>γράψατέ</reg><orig>αντιγραψαται</orig></choice> μοι· εἰ μὲν <choice><reg>οἴδατε</reg><orig>οιδαται</orig></choice> <choice><reg>πάλιν</reg><orig>παλειν</orig></choice> ὅτι οὐκ <choice><reg>ἔδωκεν</reg><orig>αιδωκεν</orig></choice> <choice><reg>ὑμῖν</reg><orig><hi rend="diaeresis">υ</hi>μειν</orig></choice> 

    <lb n="11"/><choice><reg>τὸ</reg><orig>τω</orig></choice> <choice><reg>ἀργύριον</reg><orig>αργυριων</orig></choice>, <choice><reg>γράψατέ</reg><orig>γραψαται</orig></choice> μοι καὶ <subst><add place="inline">ποιήσει</add><del rend="corrected">ποσος</del></subst> ἓν <num value="1"/> <choice><reg>νομισμάτιον</reg><orig>νομισμαθιων</orig></choice> 

    <lb n="12"/>ἤδη ἐν ἀργυρίοις <choice><reg>ἐνεχθῆναι</reg><orig>εναιχθηναι</orig></choice> <choice><reg>ὑμῖν</reg><orig>υμειν</orig></choice>, ἀλλʼ ὅρα μὴ <choice><reg>ἀμελήσητε</reg><orig>αμελησηται</orig></choice> 

    <lb n="13"/>μὴ γράψασ<unclear>θαί</unclear> μοι. <choice><reg>γινώσκειν</reg><orig>γινοσκε</orig></choice> δὲ ὑμᾶς <choice><reg>θέλω</reg><orig>θελο</orig></choice> ὅτι ἐν τῇ Ἀλεξανδρείᾳ 

    <lb n="14"/><choice><reg>εἰμὶ</reg><orig>ειμει</orig></choice> μόνος μετὰ δηναρίων μυρι<unclear>άδο</unclear>ς ταλάντων εἴκοσι 

    <lb n="15"/><choice><reg>πέντε</reg><orig>πενται</orig></choice> <num value="25"/>. ὁ ἡγεμὼν ἔγραψεν πρὸς <choice><reg>τὸν</reg><orig>των</orig></choice> <choice><reg>γεοῦχον</reg><orig>γεουχων</orig></choice> 

    <lb n="16"/>Σαραπάμμωνος καὶ <choice><reg>τὸ<supplied reason="omitted">ν</supplied></reg><orig>τω</orig></choice> <choice><reg>κοινωνὸ<supplied reason="omitted">ν</supplied></reg><orig>κοινονω</orig></choice> αὐτοῦ ἀρθῆναι <choice><reg>ἐπὶ</reg><orig>επει</orig></choice> τῆς 

    <lb n="17"/>Ἀλεξανδρείας <choice><reg>ἀργυρίου</reg><orig>αργυριων</orig></choice> δηναρίων μυριάδας μυριάδων 

    <lb n="18"/>ἑπτὰ <choice><reg>ἡμίσους</reg><orig>ημεισους</orig></choice> <num value="750000000"/> καὶ <choice><reg>ἀπέστειλόν</reg><orig>απεστιλων</orig></choice> <choice><reg>με</reg><orig>μοι</orig></choice> μετὰ τοῦ <choice><reg>μέρους</reg><orig>μερως</orig></choice> ἡμῶν 

    <lb n="19"/>καὶ παραμενῶμαι <choice><reg>ἐπὶ</reg><orig>επει</orig></choice> τῆς Ἀλεξανδρείας ἕως <choice><reg>ἐξοδιασθῶ</reg><orig>εξωδιασθω</orig></choice> αὐτὰ 

    <lb n="20"/>καὶ <choice><reg>πάλιν</reg><orig>παλειν</orig></choice> <choice><reg>ἀπέρχομαι</reg><orig>απερχωμαι</orig></choice> <choice><reg>ἐπὶ</reg><orig>επει</orig></choice> τῆς Ἀθριβειτῶν πόλεως. 

    <lb n="21"/><choice><reg>ἀσπάζομαι</reg><orig>ασπαζωμαι</orig></choice> τὴν <choice><reg>κυρία<supplied reason="omitted">ν</supplied></reg><orig>κυρια</orig></choice> μου <choice><reg>σύμβιον</reg><orig>συμβιων</orig></choice> <choice><reg>Ἀπιάδα</reg><orig>α<unclear>π</unclear>ιας</orig></choice>, ἀσπάζομαι τὴν 

    <lb n="22"/><choice><reg>κυρία<supplied reason="omitted">ν</supplied></reg><orig>κυρια</orig></choice> μου ἀδελφὴν Τερηοῦς, ἀσπάζομαι <choice><reg>τὸν</reg><orig>των</orig></choice> <choice><reg>κύριό<supplied reason="omitted">ν</supplied></reg><orig>κυριω</orig></choice> μου <choice><reg>ἀδελφὸν</reg><orig>αδελφων</orig></choice> 

    <lb n="23"/><choice><reg>Γερόντιον</reg><orig>γεροντιος</orig></choice>, ἀσπάζομαι <choice><reg>τὸν</reg><orig>των</orig></choice> <choice><reg>δεσπότη<supplied reason="omitted">ν</supplied></reg><orig>δε<unclear>σ</unclear>ποτη</orig></choice> μου <choice><reg>ἀδελφὸν</reg><orig>αδελφων</orig></choice> <choice><reg>Δωρόθεον</reg><orig>δωροθεως</orig></choice>, 

    <lb n="24"/>ἀσπάζομαι τὴν ἀδελφήν μου <choice><reg>Εὔηθιν</reg><orig>ευηθις</orig></choice>, ἀσπ<unclear>ά</unclear><supplied reason="lost">ζ</supplied>ομαι τὴν 

    <lb n="25"/><choice><reg>κυρία<supplied reason="omitted">ν</supplied></reg><orig>κυρια</orig></choice> μου ἀδελφὴν τὴν Μικ<unclear>κ</unclear>ήν, ἀσπάζομαι Χωοῦς καὶ <supplied reason="omitted">τὴν</supplied> <choice><reg>σύμβιον</reg><orig>συμβιων</orig></choice> 

    <lb n="26"/>αὐτοῦ Ἡράκλειαν καὶ τὰ ἀβάσκαντα αὐτῆς τέκνα, ἀσπάζομαι 

    <lb n="27"/><choice><reg>τὸν</reg><orig>των</orig></choice> <choice><reg>πατέρα</reg><orig>πατεραν</orig></choice> μου <choice><reg>Ἀμμώνιον</reg><orig>αμμωνιων</orig></choice>, ἀσπάζομαι Τασιλβάνεις 

    <lb n="28"/>καὶ τὰ ἀβάσκαντα αὐτῆς τέκ<unclear>να</unclear>, <unclear>ἀσπάζομ</unclear>αι <choice><reg>τὸν</reg><orig>των</orig></choice> <choice><reg>πατέρα</reg><orig>πατερα<unclear>ν</unclear></orig></choice> 

    <lb n="29"/>μου Πετεμούνιος καὶ τὴν <choice><reg>μητέρα</reg><orig>μητεραν</orig></choice> μ<unclear>ου</unclear> Ταήσ<supplied reason="lost">ει</supplied>ς 
</ab></div><div n="r,ms" type="textpart"><!--milestone unit="4"--><ab>

    <lb n="30"/>καὶ τὰ ἀβάσκαντα αὐτῆς τέκνα, ἀσπάζομαι τὴν <choice><reg>κυρία<supplied reason="omitted">ν</supplied></reg><orig>κυρια</orig></choice> μου καὶ ἀδελφὴν Παλλαδίης ἀσπάζομαι πάντας 

    <lb n="31"/>κατʼ ὄνομα τοὺς <choice><reg>φιλοῦντας</reg><orig>φιλοντας</orig></choice> ἡμᾶς. 
</ab></div><div n="v" type="textpart"><!--milestone unit="4"--><ab>

    <lb n="32"/><figure><figDesc>draςing</figDesc></figure>  <expan>ἀπόδ<ex>ος</ex></expan> τῷ κυρίῳ μου <g type="xs"/> πατρὶ Ἀφυγχίῳ <expan>π<ex>αρὰ</ex></expan> Παπνουθίου <figure><figDesc>draςing</figDesc></figure> </ab></div></div>
