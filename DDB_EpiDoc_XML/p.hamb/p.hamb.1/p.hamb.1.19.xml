         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r" type="textpart"><ab>

    <lb n="1"/>Αὐρ<unclear>ηλί</unclear>ωι Ν<unclear>εμ</unclear><supplied reason="lost">εσίωνι</supplied> <unclear>τῶι</unclear> καὶ Διονυσίωι <supplied reason="lost">βασ</supplied>ι<supplied reason="lost">λι</supplied><unclear>κ</unclear>ῶ<unclear>ι</unclear> 

    <lb n="2"/>γραμματεῖ Ὀξυρυγχείτου 

    <lb n="3"/>παρὰ Αὐρηλ<unclear>ίο</unclear><supplied reason="lost">υ</supplied> <app type="editorial"><lem><unclear>Ἀ</unclear><supplied reason="lost" cert="low">σκ</supplied><unclear>λᾶς</unclear><certainty match=".." locus="value"/></lem><rdg><unclear>Α</unclear><gap reason="lost" quantity="3" unit="character"/><unclear>λ</unclear><gap reason="illegible" quantity="1" unit="character"/></rdg></app> <unclear>Χ</unclear>αρμί<unclear>ων</unclear>ος <supplied reason="lost">ἀ</supplied>πʼ <unclear>Ὀ</unclear>ξ<unclear>υ</unclear>ρύ<supplied reason="lost">γχων</supplied> 

    <lb n="4"/>πόλεως κατα<unclear>γ</unclear><supplied reason="lost">ε</supplied>ι<supplied reason="lost">νο</supplied><unclear>μ</unclear>ένου ἐν <gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" quantity="1" unit="character"/><gap reason="illegible" quantity="2" unit="character"/>ει<unclear>λο</unclear><gap reason="lost" quantity="11" unit="character"/><gap reason="illegible" quantity="1" unit="character"/> 

    <lb n="5"/>αἰτοῦμαι ἐπισταλῆναι εἰς σπέρ<unclear>μ</unclear><supplied reason="lost">ατ</supplied>α δά<unclear>ν</unclear><supplied reason="lost">ε</supplied><unclear>ι</unclear><supplied reason="lost">α</supplied> 

    <lb n="6"/>ἀπὸ πυροῦ γενήματος τοῦ διελθόντος <num value="4"><unclear>δ</unclear></num> <expan><ex>ἔτους</ex></expan> <unclear>εἰς</unclear> <supplied reason="lost">τ</supplied><unclear>ὴ</unclear><supplied reason="lost">ν</supplied> <unclear>τοῦ</unclear> 

    <lb n="7"/>ἐνεστῶτος <supplied reason="lost"><num value="5">ε</num></supplied> <expan><ex>ἔτους</ex></expan> κ<unclear>ατα</unclear>σπορὰν εἰ<unclear>ς</unclear> ἣν γεω<supplied reason="lost">ργῶ περὶ</supplied> 

