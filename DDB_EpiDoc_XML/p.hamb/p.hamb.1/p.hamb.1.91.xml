    <lb n="7"/>σπάσα<supplied reason="lost">ς </supplied><gap reason="lost" quantity="6" unit="character"/>υχατω<unclear>ν</unclear> <gap reason="lost" quantity="6" unit="character"/> ὑπὸ 

    <lb n="8"/>τιμῶν ἐ<supplied reason="lost">πηρ</supplied>ώτησε. τῆ<supplied reason="lost">ς δὲ ἀν</supplied><unclear>α</unclear>ζυγῆς 

    <lb n="9"/>ἐνστάσης <supplied reason="lost">μ</supplied>ετενεγυή<supplied reason="lost">θη </supplied><gap reason="lost" quantity="3" unit="character"/> διʼ ἐμοῦ. 

    <lb n="10"/>οἱ συνστρα<supplied reason="lost">τι</supplied>ῶται δ<unclear>ι</unclear>α τ<gap reason="lost" quantity="4" unit="character"/> 

    <lb n="11"/><gap reason="lost" quantity="3" unit="character"/><unclear>α</unclear>τη<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="1" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="2" unit="character"/><gap reason="illegible" quantity="1" unit="character"/>ριτου <gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="4" unit="character"/>θα<gap reason="illegible" quantity="2" unit="character"/><unclear>ε</unclear> 

    <lb n="12"/>ἀποκαταστῆσαι. ὕστερ<supplied reason="lost">ον ἐμο</supplied>ῦ 

    <lb n="13"/>εὑρό<unclear>ντ</unclear>ος μίαν <num value="1"/> αὐτῶν πωλουμένη<unclear>ν</unclear> 

    <lb n="14"/>ἐν τῆι Μέμφει ἐκομισάμην, ἣ κ<unclear>αὶ</unclear> 

    <lb n="15"/>ἐπηγγείλατό μοι, ἐὰν ἀποκαταστ<supplied reason="lost">α</supplied><unclear>θ</unclear>ῆ<unclear>ι</unclear> 

    <lb n="16"/>ἐνταῦθα ἑαυτῆς τε λύτρ<unclear>α</unclear> μοι 

    <lb n="17"/>καταβαλεῖν <unclear>κ</unclear>αὶ τὰ λ<supplied reason="lost">ο</supplied><unclear>ι</unclear><supplied reason="lost">πὰ αἰχμά</supplied><unclear>λ</unclear>ωτα 

    <lb n="18"/>παραδώσειν <gap reason="lost" quantity="1" unit="character"/>ν<gap reason="illegible" quantity="2" unit="character"/> αποστατ<gap reason="lost" quantity="6" unit="character"/> 

    <lb n="19"/>εἰς Πῶιν διε<supplied reason="lost">πέ</supplied><unclear>μ</unclear>ψ<unclear>α</unclear><supplied reason="lost">τ</supplied>ο πρὸ<supplied reason="lost">ς τὸν</supplied> 

    <lb n="20"/>πατέρα, οὐκ σ<supplied reason="lost">υνεχ</supplied>ώρησαν τ<unclear>α</unclear><supplied reason="lost">ῦτα</supplied> 

    <lb n="21"/>ἀποδώσειν <supplied reason="lost">μοι τὰ</supplied> λύτρα η<unclear>π</unclear>ρ<gap reason="lost" quantity="2" unit="character"/> 

    <lb n="22"/>γρασας, ἐν<unclear>θ</unclear>ʼ <supplied reason="lost">ἡ αἰχμάλ</supplied>ωτος ψυχα

    <lb n="23" break="no"/>γωγηθε<unclear>ῖσα</unclear> ὑπὸ τῶν ἐκ τῆς 

    <lb n="24"/>κώμης <choice><reg>ἱερέων</reg><orig>ἱερείων</orig></choice> <gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="3" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="25"/><gap reason="illegible" quantity="2" unit="character"/>το <unclear>ἐν</unclear> τῆι Πῶ<unclear>ι</unclear>. <supplied reason="lost">διὸ ἀ</supplied><unclear>ξ</unclear>ι<supplied reason="lost">ῶ</supplied> 

    <lb n="26"/>κατα<unclear>φθ</unclear>ειρόμενος ἐπὶ ξένης 

    <lb n="27"/>μὴ περιιδεῖν μ<unclear>ε</unclear> <supplied reason="lost">καὶ ἐά</supplied>ν σοι 

    <lb n="28"/>φαίνηται γράψαι Δ<supplied reason="lost">ημητρίωι τῶι</supplied> 

    <lb n="29"/>ἐπιστάτηι, ἐὰν ἦι οἷ<supplied reason="lost">α προφέρ</supplied><unclear>ομ</unclear>αι, 

    <lb n="30"/>παραδοῦναί μοι τὰ σ<supplied reason="lost">κῦλ</supplied><unclear>α</unclear>, 

    <lb n="31"/>περὶ δὲ τῶν ταῦτα δια<unclear>π</unclear>ε<unclear>π</unclear>ρα

    <lb n="32" break="no"/>γμ<supplied reason="lost">ένω</supplied>ν δι<unclear>α</unclear>λαβεῖν κα<supplied reason="lost">τ</supplied>ὰ τὸ φανέν σ<supplied reason="lost">οι</supplied>. 

    <lb n="33"/>εὐτύχει. 

