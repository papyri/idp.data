         <head n="17471" xml:lang="en">
            <date>III spc</date>
            <placeName>Herakleopolite?</placeName>
            <ref n="psi;9;1052" type="reprint-from">PSI 9 1052</ref>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><ab>
    <lb n="1"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> τῶν κυρ</supplied><unclear>ί</unclear>ων ἡμῶν Αὐτοκρατόρων Σεουήρου καὶ <choice><reg>Ἀντωνίνου</reg><orig>Ἀντωνίου</orig></choice> <gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="2"/><gap reason="lost" extent="unknown" unit="character"/>ακολουθήσει ἐν <add place="above">ωι</add> γραμματεῖον. προετέθη ἐν Ἀλε<supplied reason="lost">ξανδρείᾳ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="3"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> Οὐ</supplied>ι<unclear>κ</unclear>τωρίνῳ ὑπάτοις. Σουβατιανῷ ἐπάρχῳ Αἰγύπτου <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="4"/><gap reason="lost" extent="unknown" unit="character"/>υ. <choice><reg>Αἴλιος</reg><orig>Αἴλις</orig></choice> Σαραπάμμων <choice><reg>οὐετρανὸς</reg><orig>οὐτερανὸς</orig></choice> ἐκ πλεον<supplied reason="lost">εξίας </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="5"/><gap reason="lost" extent="unknown" unit="character"/><gap reason="illegible" quantity="1" unit="character"/>σης τῇ στρ<unclear>α</unclear><supplied reason="lost">τ</supplied>είᾳ διαφέρουσα σχολάζειν <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="6"/><gap reason="lost" extent="unknown" unit="character"/>οτις ὡς δὲ ἔ<unclear>χ</unclear>ει τὰ τοῦ πράγματος διὰ συντομ<supplied reason="lost">ίας </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="7"/><gap reason="lost" extent="unknown" unit="character"/><gap reason="illegible" quantity="1" unit="character"/>ήνεγκεν οὐ μόνον τοῖς ἐμοῖς ἀδελφοῖς, ἀλλ<supplied reason="lost">ὰ καὶ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="8"/><gap reason="lost" extent="unknown" unit="character"/>ρος ὑπὲρ φόρων τάλαντα <num value="4"><choice><reg>τέσσαρα</reg><orig>τέσαρα</orig></choice></num> καὶ δραχμὰ<supplied reason="lost">ς </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="9"/><gap reason="lost" extent="unknown" unit="character"/>των νομίμων <choice><reg>ἀποτομίᾳ</reg><orig><subst><add place="inline">ἀποδομείᾳ</add><del rend="corrected">αφοδομεια</del></subst></orig></choice> χρήσασθαι πανταχ<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="10"/><gap reason="lost" extent="unknown" unit="character"/>ου <add place="above">τινος</add> <del rend="erasure">εἰπεῖν</del> ἁπλῶς <add place="above">μὴ</add> εἰπεῖν μα<gap reason="illegible" quantity="3" unit="character"/>σται καὶ τοῦ θρυλουμ<supplied reason="lost">ένου </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="11"/><gap reason="lost" extent="unknown" unit="character"/>δοθῆναι ὀφείλοντος ὅλ<unclear>ω</unclear>ν  <app type="editorial"><lem resp="D. Hagedorn, P. Sänger, Tyche 27 (2012) 220 (KorrTyche 721)"><app type="alternative"><lem>εἴκοσει δύ<unclear>ο</unclear> <gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/></lem><rdg>εἰκὸς εἰ δυ<unclear>ν</unclear><gap reason="lost" extent="unknown" unit="character"/></rdg></app></lem><rdg>εἰκὸς εἰ δυ<unclear>ν</unclear><gap reason="lost" extent="unknown" unit="character"/></rdg></app> 

    <lb n="12"/><gap reason="lost" extent="unknown" unit="character"/><gap reason="illegible" quantity="2" unit="character"/>εμοι<gap reason="illegible" quantity="3" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="13"/><gap reason="lost" extent="unknown" unit="character"/><unclear>ω</unclear>ν νομοθετησάντων μηδὲ <unclear>γὰρ</unclear> <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="14"/><gap reason="lost" extent="unknown" unit="character"/>αυτον η <unclear>απον</unclear> εἶναί μοι εν ἐὰν δο<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="15"/><gap reason="lost" extent="unknown" unit="character"/>τη στρατείᾳ διαφέρουσα προσευ<gap reason="illegible" quantity="3" unit="character"/>ρειν <gap reason="illegible" quantity="4" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="16"/><space extent="unknown" unit="character"/> ὁ ἀξιολογώτατος ἀρχιδι<unclear>κ</unclear><supplied reason="lost">αστής </supplied><gap reason="lost" extent="unknown" unit="character"/>
</ab></div>
