    <lb n="1"/><gap reason="lost" extent="unknown" unit="line"/> 

    <lb n="1"/>Ἡρακλείδου ἐποικίῳ <gap reason="illegible" quantity="2" unit="character"/><unclear>ς</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="2"/>οὐδὲν αὐτῶν διαπε<unclear>σ</unclear><supplied reason="lost">εῖτ</supplied>αι <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="3"/><expan>κ<ex>αὶ</ex></expan> ὅτι τοῦ παραπεσουμένου 

    <lb n="4"/><choice><reg><supplied reason="lost">ἔ</supplied>κτεισις</reg><orig><supplied reason="lost">ἔ</supplied>κτισις</orig></choice> ἑκάστου <expan><ex>χαλκοῦ</ex></expan> <num value="2"><expan><ex>διώβολον</ex></expan></num>, ἃ 

    <lb n="5"/><supplied reason="lost">κ</supplied>αὶ μὴ ἀνασχῇ <supplied reason="lost">Μ</supplied>αρρήους· 

    <lb n="6"/>οὐ γὰρ αὐτῷ <choice><reg>παραδέδωκα</reg><orig>παραδέδωικα</orig></choice> ἀλλά 

    <lb n="7"/>σοι καὶ τὰ τέταρτα τῶν παι

    <lb n="8" break="no"/>δαρίων ὡς ἀπόδοσις αὐτοῖς. 

    <lb n="9"/>σὺ τόδε <choice><reg>ποιήσας</reg><orig>ποήσας</orig></choice> ἀντέχου <gap reason="lost" quantity="2" unit="character"/> 

    <lb n="10"/><gap reason="illegible" quantity="2" unit="character"/>σουτερα τὰ ἑ<unclear>α</unclear>υ<unclear>τ</unclear><gap reason="illegible" quantity="2" unit="character"/> 

