    <lb n="2"/><expan>νο<ex>μισμάτιον</ex></expan> <expan>εἰ<ex>ς</ex></expan> <expan><ex>κεράτια</ex></expan> <num value="22">κβ</num> <num value="1/2">𐅵</num> σίτου <expan>ἀρτ<ex>άβαι</ex></expan> <add rend="sling" place="margin">κ</add><gap reason="lost" extent="unknown" unit="character"/> </ab></div></div>
