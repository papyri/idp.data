         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r" type="textpart"><ab>

    <lb n="1"/>βασιλεύοντος Πτολεμαίου τοῦ Πτολεμαίου <unclear>Σω</unclear><supplied reason="lost">τῆρος</supplied> 

    <lb n="2"/><expan><ex>ἔτους</ex></expan> <num value="29">κθ</num>, ἐφʼ ἱερέως Ἀντιόχου τοῦ Κέββα Ἀλεξ<unclear>άνδ</unclear><supplied reason="lost">ρου καὶ</supplied> 

    <lb n="3"/>θεῶν Ἀδελφῶν, κανηφόρου Ἀρσινόης Φιλαδέ<supplied reason="lost">λφου Δημονίκης</supplied> 

    <lb n="4"/>τῆς Φίλωνος, μηνὸς Δύστρου <add place="above"><num value="7">ζ</num> <del rend="erasure">ἑ<unclear>βδόμηι</unclear></del></add> <unclear>Αἰγυπτίων</unclear> <unclear>δὲ</unclear> <supplied reason="lost">Φαμενὼθ</supplied> 

    <lb n="5"/><add place="above">ἑβδόμηι.</add> ὀμνύουσιν <del rend="erasure">Ἀμενεμῆσος <app type="editorial"><lem>Ἁράχθιος</lem><rdg>Ἁράχδιος</rdg></app></del> Σεῶς <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="6"/>Ἀμενεμῆσος <app type="editorial"><lem>Ἁράχθιος</lem><rdg>Ἁράχδιος</rdg></app>, Σαμῶυς <unclear>Ὥρο</unclear>υ, Π<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="7"/>οἱ <num value="4">δ</num> Μεμφῖται πλινθουλκοὶ καὶ Φαρ<gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="8"/><add place="above">Τρωίτης πλινθουλκὸς,</add> <del rend="erasure">Ἑλληνομεμφίτης</del> <unclear>βα</unclear>σιλέα Π<unclear>το</unclear>λεμαῖον <supplied reason="lost">καὶ Ἀρσινόην</supplied> 

    <lb n="9"/>Φιλάδελφον θεοὺς Ἀδελφοὺς καὶ θεοὺς Σ<unclear>ω</unclear><supplied reason="lost">τῆρας τοὺς</supplied> 

    <lb n="10"/>τούτων γονεῖς Ζήνωνι Ἀγρεοφῶντος Καυνίωι <unclear>τ</unclear><supplied reason="lost">ῶι παρʼ Ἀ</supplied>

    <lb n="11" break="no"/>πολλωνίου· παραμενοῦμεν ἐν Φιλαδελ<unclear>φ</unclear><supplied reason="lost">είαι τῆι ἐν τῶι</supplied> 

    <lb n="12"/>Ἀρσινοίτηι νομῶι ἐμφανεῖς ὄντες κα<supplied reason="lost">ὶ ἀπεργώμεθα</supplied> 

    <lb n="13"/>πλίνθον ὅσην ἂν ἐγλάβω<unclear>μεν</unclear> <gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="3" unit="character"/><gap reason="lost" quantity="8" unit="character"/><supplied reason="lost"> ἀποστη</supplied>

    <lb n="14" break="no"/>σόμεθα καταλιπόντες τὰ ἔργα οὐ<unclear>δ</unclear>ὲ α<unclear>λ</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="15"/>ἐπὶ τὰς ἰδίας χρείας οὐδαμοῦ ἀν<supplied reason="lost">αχωρ</supplied>ή<unclear>σ</unclear><supplied reason="lost">ομεν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="16"/>ἐὰν δὲ μὴ ποιῶμεν κατὰ τὰ γεγ<unclear>ρα</unclear><supplied reason="lost">μμ</supplied><unclear>έν</unclear><supplied reason="lost">α, ἀποτείσομεν</supplied> 

    <lb n="17"/>ὃ ἂν λαβόν<unclear>τε</unclear>ς παρὰ Ζήνωνος <unclear>απε</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="18"/><gap reason="lost" extent="unknown" unit="character"/>ε<gap reason="illegible" quantity="1" unit="character"/>γ<unclear>λ</unclear><gap reason="lost" extent="unknown" unit="character"/><unclear>χικο</unclear><gap reason="lost" extent="unknown" unit="character"/> 

