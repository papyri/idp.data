    <lb n="1"/><gap reason="lost" extent="unknown" unit="line"/> 

    <lb n="1"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ᾠκοδο</supplied>μηκέναι λαβοῦσα τόπον <unclear>ψιλ</unclear><supplied reason="lost">ὸν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="2"/><del rend="erasure"><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ἀρχιτέ</supplied><unclear>κτ</unclear><supplied reason="lost">τονο</supplied><unclear>ς</unclear> <unclear>Θε</unclear>οδώρου</del> καὶ εἰσανηλωκέναι εἰς τὴν οἰκοδομ<unclear>ὴ</unclear><supplied reason="lost">ν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="3"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> Φιλ</supplied><unclear>α</unclear><supplied reason="lost">δ</supplied><unclear>ε</unclear>λφείαι ἐπʼ Ἀγήνορος τοῦ σ<supplied reason="lost">τ</supplied>ρατηγοῦ καὶ Ἀρσινόη <unclear>οὔ</unclear><supplied reason="lost">τε</supplied> τοὺ<unclear>ς</unclear> <unclear>ε</unclear><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="1" unit="character"/><gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="4"/><gap reason="lost" extent="unknown" unit="character"/><gap reason="illegible" quantity="2" unit="character"/> <unclear>ο</unclear>ὔτε ὡς <choice><reg>ᾠκοδομήκει</reg><orig>ὠικοδομήκει</orig></choice> τὴν οἰκίαν, τοῦ τε Εὐτύχου φαμένο<supplied reason="lost">υ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="5"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> οἰκο</supplied>δ<supplied reason="lost">ο</supplied>μὴν καὶ περὶ τούτ<unclear>ων</unclear> <unclear>β</unclear><supplied reason="lost">αλομ</supplied><unclear>έν</unclear><supplied reason="lost">ο</supplied><unclear>υ</unclear> εἴ<supplied reason="lost">ς τ</supplied><unclear>ε</unclear> <unclear>τ</unclear>οὺς οἰκοδόμους καὶ <add place="above"><unclear>τοὺ</unclear><supplied reason="lost">ς </supplied><gap reason="lost" extent="unknown" unit="character"/></add> <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="6"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> οἰκοδ</supplied>όμος καὶ ὁ τὰ ξύλα ἀποδόμενος κα<gap reason="lost" quantity="1" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="7"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> τὴ</supplied>ν τιμὴν καὶ τοὺς μισθοὺς εἰληφέναι παρʼ Εὐτύχου τοῦ παιδός μου κο<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="8"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ἀνα</supplied>χωρήσαντ<supplied reason="lost">ο</supplied>ς περὶ τῆς βολῆς ταύτης, διὸ παραλογισθεὶς Ἀγήνω<supplied reason="lost">ρ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="9"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ἐμ</supplied><unclear>ὲ</unclear> ὡρίσθαι ταῦτα, γράφει Κλειτορίωι <choice><reg>ἀνακαλεσάμενόν</reg><orig>ἀνακαλεσάμενόμ</orig></choice> με συν<supplied reason="lost">τάξαι </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="10"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> βασι</supplied><unclear>λεῦ</unclear>, προστάξα<supplied reason="lost">ι Ἀ</supplied>γή<supplied reason="lost">νορι τ</supplied>ῶι στρατηγῶι ἐπισκεψάμεν<unclear>ον</unclear> <gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="11"/><gap reason="lost" extent="unknown" unit="character"/>αι ὡς εἴ<supplied reason="lost">λη</supplied>φεν τὸν τόπον ἐκ τοῦ <supplied reason="lost">βασιλικοῦ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="12"/><gap reason="lost" extent="unknown" unit="character"/> τὴν πλίνθον καὶ τὰ ξύλα καὶ τοὺς οἰκοδόμους <add place="above">παρασχέσθαι</add> μαρτυροῦντας αὐτῆι <add place="above">εν<unclear>α</unclear>μ<gap reason="lost" extent="unknown" unit="character"/></add> <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="12a"/>ἐ<supplied reason="lost">ρη</supplied>μοῦμαι δὲ καὶ ἐγὼ μαρτυρίας περὶ τούτων, ὅπως πάλιν δι<supplied reason="lost">ακούσας </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="13"/><gap reason="lost" extent="unknown" unit="character"/>ους, ἐὰν δὲ μὴ ἐνδείξηται, εἶναι τὴν οἰκίαν <subst><add place="inline">ἐμήν</add><del rend="corrected">ημετεραν</del></subst>, ἵνα κ<supplied reason="lost">αὶ ἐγὼ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="13a"/><gap reason="lost" extent="unknown" unit="character"/>θέντα κρίνηι ὡς ἂν αὐτῶι φαίνηται, ἵνα καὶ ἐγὼ διὰ σέ, βασιλ<supplied reason="lost">εῦ, τοῦ δικαίου τύχω</supplied>. </ab></div>
