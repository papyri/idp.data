         <head n="1045" xml:lang="en">
            <date>III sac</date>
            <placeName>Philadelphia</placeName>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><ab>
    <lb n="1"/>Ζήνωνι χαίρειν Ἁρυώτη<unclear>ς</unclear> <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="2"/>φείας γίνωσκε <add place="above">τὸ <surplus><unclear>ζ</unclear></surplus> <unclear>ζ</unclear>ῦτος οὐθ<gap reason="lost" extent="unknown" unit="character"/></add> <del rend="erasure">τὰ χρημα<gap reason="illegible" quantity="1" unit="character"/> <gap reason="lost" extent="unknown" unit="character"/> </del> 

    <lb n="3"/><add place="above">ἀπὸ <num value="28">κη</num> τοῦ Παχὼνς διὰ τὸ τὸν</add> <del rend="erasure">το</del> <choice><reg>ἐν</reg><orig>ἐγ</orig></choice> 

Κερκῆι <subst><add place="inline">τὸ<add place="above">ν</add></add><del rend="corrected">το<del rend="erasure">υς</del></del></subst> Κροκοδιλ<supplied reason="lost" cert="low">οπολίτην </supplied><gap reason="lost" extent="unknown" unit="character"/>  

    <lb n="4"/>γάσθην <add place="above">γὰρ ἂν</add> ἑ<unclear>π</unclear>τὰ <num value="7"/> <choice><reg cert="low">σωλῆνα<unclear>ς</unclear></reg><orig>σηλῆνα<unclear>ς</unclear></orig></choice> <gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> <add place="above">ζυ<gap reason="lost" extent="unknown" unit="character"/></add> 

    <lb n="5"/><add place="above">αλλε</add> <del rend="erasure">διὰ ταύτην τὴν αἰτί<unclear>αν</unclear> <gap reason="illegible" quantity="2" unit="character"/> <gap reason="lost" extent="unknown" unit="character"/> </del> 

    <lb n="6"/>ἐργαστήριον καὶ οἱ ἀδικοῦν<unclear>τ</unclear><supplied reason="lost">ες </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="7"/>οὔτε παρὰ τοῦ βασιλέως <gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> <add place="above"><unclear>ε</unclear><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/></add> 

    <lb n="8"/>οὔτε ἱππεῖς εἰσιν, ἀλλὰ ταυ<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="9"/>διατρέφονται. καλῶς οὖν <supplied reason="lost">ποιήσεις </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="10"/>ἐπιστροφὴν ποίησαι περὶ τ<unclear>ῶν</unclear> <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="11"/>καὶ γράψας Διοτίμωι τῶι ὑποδιοικητῆι καὶ Φι<supplied reason="lost" cert="low">λίσκωι </supplied><gap reason="lost" extent="unknown" unit="character"/>  

    <lb n="12"/><del rend="erasure">εξει ἐπʼ Ἀπολλώνιον. σὺ γὰρ <gap reason="illegible" quantity="1" unit="character"/> <gap reason="lost" extent="unknown" unit="character"/> </del> 

    <lb n="13"/><del rend="erasure">βελτίω</del>. </ab></div>
