    <lb n="29"/><supplied reason="lost">ὧν</supplied> μεμέτρηται ἐκ τοῦ θ<unclear>ησα</unclear>υροῦ 

    <lb n="30"/><supplied reason="lost">τοῦ</supplied> <num value="34"><unclear>λδ</unclear></num> <expan><ex>ἔτους</ex></expan> Φαρμοῦθι <del rend="erasure"><gap reason="illegible" quantity="1" unit="character"/></del> <num value="3"><unclear>γ</unclear></num> 

    <lb n="31"/><del rend="erasure"><supplied reason="lost"><expan>ἀρ<ex>ταβῶν</ex></expan> <num value="60">ξ</num></supplied> <gap reason="illegible" quantity="3" unit="character"/></del> <expan>ἀν<ex>ὰ</ex></expan> <num value="3"><expan><ex>τριώβολον</ex></expan></num> <num value="2"><expan><ex>διώβολον</ex></expan></num> γίνονται <expan><ex>δραχμαὶ</ex></expan> <num value="50">ν</num> 

    <lb n="32"/><supplied reason="lost">ἐρίων ὧ</supplied><unclear>ν</unclear> δεῖ αὐτὸν λαβεῖν παρὰ 

    <lb n="33"/><supplied reason="lost">Κα</supplied>λλίππου <expan>μν<ex>ῶν</ex></expan> <num value="10">ι</num> <expan>ἀν<ex>ὰ</ex></expan> <num value="1">α</num> <num value="2"><expan><ex>διώβολον</ex></expan></num> <expan><ex>γίνονται</ex></expan> <expan><ex>δραχμαὶ</ex></expan> <num value="13">ιγ</num> <num value="2"><expan><ex>διώβολον</ex></expan></num> 

    <lb n="34"/>Ἀραβίων <expan>μν<ex>ῶν</ex></expan> <num value="10">ι</num> <expan>ἀν<ex>ὰ</ex></expan> <expan><ex cert="low">δραχμὰς</ex></expan> <num value="2"><unclear>β</unclear></num> <supplied reason="lost"><expan><ex>γίνονται</ex></expan></supplied> <expan><ex cert="low">δραχμαὶ</ex></expan> <num value="20"><unclear>κ</unclear></num> 

    <lb n="35"/>καὶ ὃ ἔλαβεν διὰ σοῦ εἰς Μ<supplied reason="lost" cert="low">έ</supplied><unclear>μ</unclear><supplied reason="lost" cert="low">φιν</supplied> 

    <lb n="36"/>ἀποστελλόμενος ἐφόδιον <supplied reason="lost"><expan><ex>δραχμὰς</ex></expan> <num value="8">η</num></supplied> 

    <lb n="37"/><supplied reason="lost">καὶ ὃ ἔ</supplied>λαβε Νικόλαος διὰ 

    <lb n="38"/><supplied reason="lost">Σατ</supplied>ύρου <space extent="unknown" unit="character"/> <expan><ex>δραχμὰς</ex></expan> <num value="3">γ</num> 

    <lb n="39"/><supplied reason="lost">καὶ</supplied> ὑπὲρ Ἀμμωνίου ὃ προσοφείλει 

    <lb n="40"/><supplied reason="lost">αὐτ</supplied>ῶι πρὸς τοὺς <expan>χ<ex>όας</ex></expan> <num value="8">η</num> τοῦ οἴνου <expan><ex>δραχμὰς</ex></expan> <num value="5">ε</num> 

