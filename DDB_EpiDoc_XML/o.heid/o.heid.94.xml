    <lb n="5"/><expan>γενήμ<ex>ατος</ex></expan> <num value="26">κϛ</num> <expan><ex>ἔτους</ex></expan> <expan><ex>δραχμὰς</ex></expan> ἕνδ<unclear>ε</unclear>

