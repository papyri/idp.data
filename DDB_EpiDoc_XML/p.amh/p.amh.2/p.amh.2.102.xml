         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r" type="textpart"><ab>

    <lb n="1"/>ἔτους εἰκ<supplied reason="lost">ο</supplied>στοῦ Αὐτ<supplied reason="lost">ο</supplied>κρατόρων Καισάρων Μάρκου 

    <lb n="2"/>Αὐρηλίου Ἀντωνίνου καὶ Λουκίου Αὐρηλίου 

    <lb n="3"/>Κομμόδου Σεβαστ<supplied reason="lost">ῶ</supplied>ν Ἀ<supplied reason="lost">ρ</supplied>μενιακῶν Μηδικῶν 

    <lb n="4"/>Παρθικῶν Γερμανικῶν Σαρματικῶν Μεγίστων 

    <lb n="5"/>μηνὸς Δύστρου Τῦβ<supplied reason="lost">ι</supplied> <num value="30">λ</num>, ἐν Πτολεμαίδι Εὐεργέ

    <lb n="6" break="no"/>τιδι τοῦ Ἀ<supplied reason="lost">ρ</supplied>σινοείτου νομοῦ.  ὁμολογεῖ Θασῆς 

    <lb n="7"/>Ἀγχώφεως <expan>ὡ<ex>ς</ex></expan> <expan><ex>ἐτῶν</ex></expan> <num value="35">λε</num> εὔσημος ὀφθαλμὸν δεξιὸν 

    <lb n="8"/>μετὰ κυρίου τοῦ ἀν<supplied reason="lost">δ</supplied>ρὸς Στοτοήτεως τοῦ Στοτο

    <lb n="9" break="no"/>ήτεως <expan>ὡ<ex>ς</ex></expan> <expan><ex>ἐτῶν</ex></expan> <num value="50">ν</num> οὐλὴ <choice><reg>π<supplied reason="lost">ή</supplied>χει</reg><orig>π<supplied reason="lost">ή</supplied>χι</orig></choice> δεξιῷ Στοτοήτι Ἀγχώ

    <lb n="10" break="no"/>φεως ὡς ἐτῶν τρ<supplied reason="lost">ιάκο</supplied>ντα δύο <num value="32"/> οὐλὴ μήλῳ ἀρισ

    <lb n="11" break="no"/>τερῷ, πάντες ἀπ<supplied reason="lost">ὸ</supplied> κώμης Σοκ<supplied reason="lost">νο</supplied>παίου Νή

    <lb n="12" break="no"/>σου πεπρακέναι τ<supplied reason="lost">ὴ</supplied>ν Θ<supplied reason="lost">α</supplied>σῆν τῷ Στοτοήτι 

    <lb n="13"/>ἀφʼ ὧν ἔχει ἐν <expan>ἀπογρ<ex>αφῇ</ex></expan> ἐπὶ τῆς κώμης καμήλων 

    <lb n="14"/>ἓξ <num value="6"/> <supplied reason="lost">κ</supplied>άμ<supplied reason="lost">η</supplied>λον ἕνα <num value="1"/>, καὶ <choice><reg>ἀπέχειν</reg><orig>ἀπέχιν</orig></choice> τὴν <choice><reg>τιμὴν</reg><orig>τειμὴν</orig></choice> 

    <lb n="15"/>ἀρ<supplied reason="lost">γυ</supplied>ρίου δραχμὰς <gap reason="lost" quantity="2" unit="character"/><unclear>τ</unclear><supplied reason="lost">ακοσ</supplied>ίας, τῆς βεβ<supplied reason="lost">α</supplied><unclear>ι</unclear>ώσεως 

    <lb n="16"/>αὐτ<supplied reason="lost">ῇ ἐ</supplied>ξακολ<supplied reason="lost">ουθούσης, καὶ</supplied> εἶναι <supplied reason="lost">ἀν</supplied><unclear>ά</unclear>γ<supplied reason="lost">κην Στοτ</supplied>ο

    <lb n="17" break="no"/>ήτι ἀπογράφ<supplied reason="lost">εσθαι τῇ τ</supplied>ῶν θρεμμάτων <supplied reason="lost"><expan>ἀπογρ<ex>αφῇ</ex></expan>.</supplied> 

    <lb n="18"/><handShift new="m2"/> Στοτοῆτις <choice><reg>ἐ<supplied reason="lost">πιγ</supplied>έγραμμαι</reg><orig>ἐ<supplied reason="lost">πιγ</supplied>έγραμμε</orig></choice> <choice><reg>κύριος</reg><orig>κῦρις</orig></choice>. 

    <lb n="19"/><handShift new="m3"/> Θασῆς Ἀγχώφε<supplied reason="lost">ω</supplied>ς μετὰ κυρίου τοῦ ἀν

    <lb n="20" break="no"/>δ<supplied reason="lost">ρ</supplied>ὸς <supplied reason="lost">Στο</supplied>τοήτ<supplied reason="lost">εω</supplied>ς τοῦ <choice><reg><supplied reason="lost">Στ</supplied>ο<supplied reason="omitted">το</supplied><supplied reason="lost">ήτ</supplied>εως</reg><orig><supplied reason="lost">Στ</supplied>ο<supplied reason="lost">ήτ</supplied>εως</orig></choice> πέπρα

    <lb n="21" break="no"/>κα <unclear>κ</unclear><supplied reason="lost">αὶ ἀπ</supplied><unclear>έχ</unclear><supplied reason="lost">ω παρὰ</supplied> <unclear>Σ</unclear><supplied reason="lost">τοτο</supplied>ήτεως Ἀγχώ

    <lb n="22" break="no"/>φ<supplied reason="lost">ε</supplied><unclear>ως</unclear> <unclear>δρα</unclear><supplied reason="lost">χμὰς </supplied><gap reason="lost" quantity="4" unit="character"/><supplied reason="lost"> κο</supplied><unclear>σία</unclear>ς ὡς <expan>πρ<ex>όκειται</ex></expan>.  Τα<unclear>ρ</unclear>

    <lb n="23" break="no"/><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="1" unit="character"/><gap reason="illegible" quantity="2" unit="character"/><unclear>ς</unclear> Δ<gap reason="illegible" quantity="4" unit="character"/><gap reason="lost" quantity="2" unit="character"/><supplied reason="lost"> ἔγρα</supplied>ψα <supplied reason="lost">ὑ</supplied><unclear>πὲρ</unclear> <unclear>αὐ</unclear>τῆς <expan>Θασῆτο<ex>ς</ex></expan> 

    <lb n="24"/><expan>ἀγρ<ex>αμμάτου</ex></expan>, <expan>αὐτο<ex>ῦ</ex></expan> <unclear>Στ</unclear><supplied reason="lost">οτότεως βρ</supplied>αδέα γράφοντος. 

    <lb n="25"/><handShift new="m4"/> <gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" quantity="2" unit="character"/><gap reason="illegible" quantity="2" unit="character"/> <expan><ex>ἔτους</ex></expan> <num value="20">κ</num> Τῦβι <num value="30">λ</num>. 
