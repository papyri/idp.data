    <lb n="1"/>Διόσκορος καὶ <choice><reg>μέτοχοι</reg><orig>μέτοχ<unclear>υ</unclear></orig></choice> <app type="editorial"><lem><choice><reg>ἐπιτη <lb n="2" break="no"/>ρηταὶ</reg><orig>ἐπετε<lb n="2" break="no"/>ρετε</orig></choice></lem><rdg>ἐπιτ<unclear>η</unclear><lb n="2" break="no"/>ρετε</rdg></app> <choice><reg>πορθμείου</reg><orig>προθμίου</orig></choice> <app type="editorial"><lem>Καροῦρι</lem><rdg><choice><reg>Καροῦρι</reg><orig>Καροῦρις</orig></choice></rdg></app>
<app type="editorial"><lem><choice><reg>Ἰνα<lb n="3" break="no"/>ροίτος</reg><orig>Εἰνα<lb n="3" break="no"/>ρό<unclear>ι</unclear>τος</orig></choice></lem><rdg><choice><reg>Ἰνα<lb n="3" break="no"/>ρῶτος</reg><orig>Ἰνα<lb n="3" break="no"/>ροίτος</orig></choice></rdg></app> καὶ <app type="editorial"><lem><choice><reg>μετόχοις</reg><orig>μεδ<unclear>ω</unclear>χον</orig></choice></lem><rdg><choice><reg>μετόχοις</reg><orig>μεδ<unclear>υο</unclear>χον</orig></choice></rdg></app> <choice><reg>χαίρειν</reg><orig>χαίριν</orig></choice>. 

