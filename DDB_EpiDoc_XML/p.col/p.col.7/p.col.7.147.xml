    <lb n="27"/><handShift new="m4" cert="low"/><gap reason="lost" quantity="3" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="lost" quantity="2" unit="character"/> 

    <lb n="28"/><handShift new="m3"/> καὶ τῇ <num value="21">κα</num> ὁμοίω<unclear>ς</unclear> ὁ αὐτὸς Ἀν<supplied reason="lost">τιοῦρις Ἀβοῦ ὑπὲρ</supplied> 

    <lb n="29"/>ἑνδεκάτης ἰνδικτίονος <unclear>κωμητῶν</unclear> <unclear>Καρ</unclear><supplied reason="lost">ανίδος</supplied> 

    <lb n="30"/>πυροῦ καθαροῦ ἀ<supplied reason="lost">ρτάβας δύο</supplied> <unclear>ἥμ</unclear>ισυ <unclear>τρίτον</unclear> <supplied reason="lost">δωδέ</supplied>

    <lb n="31" break="no"/>κατον, <expan><ex>ἀρτάβας</ex></expan> <num value="2">β</num> <num value="1/2">𐅵</num> <num value="1/3" rend="tick">γ</num> <num value="1/12" rend="tick">ιβ</num> <expan>μ<ex>όνας</ex></expan>. <handShift new="m4"/> Ἄ<unclear>γαθος</unclear> <expan><unclear>σεσ</unclear><ex>ημείωμαι</ex></expan>. 

    <lb n="32"/><handShift new="m5"/> Παῦνι <num value="21">κα</num>. <expan>παρ<ex>ήνεγκεν</ex></expan> <choice><reg>Ἀντιούριος</reg><orig>Ἀντιουριου</orig></choice> Ἀβοῦ ὑπὲρ πεντε<supplied reason="lost">και</supplied><choice><reg><lb n="33" break="no"/>δεκάτης</reg><orig>π<unclear>εντ</unclear>η<supplied reason="lost">και</supplied>δεκάτης</orig></choice> <expan>ἰνδικτί<ex>ονος</ex></expan> <expan>κωμ<ex>ητῶν</ex></expan> <expan>Καρ<ex>ανίδος</ex></expan> πυροῦ <expan>ἀρτάβ<ex>ας</ex></expan> δεκαπ<supplied reason="lost">έντε</supplied>, 

    <lb n="34"/><expan><ex>ἀρτάβας</ex></expan> <num value="15">ιε</num> μόνας. καὶ Φαμε<unclear>ν</unclear>ὼθ <num value="7">ζ</num>, ὁ αὐτὸς <supplied reason="lost">ὑπὲρ</supplied> 

    <lb n="35"/><choice><reg>πεντεκαιδεκάτης</reg><orig>πεντηκαιδ<unclear>ε</unclear>κάτης</orig></choice> <expan>ἰνδικτί<ex>ονος</ex></expan> <expan>κωμ<ex>ητῶν</ex></expan> <expan>Καρ<ex>ανίδος</ex></expan> π<unclear>υροῦ</unclear> <expan><unclear>ἀρτάβ</unclear><ex>ας</ex></expan> <supplied reason="lost">δε</supplied>

    <lb n="36" break="no"/>καοκτὼ <choice><reg>ἥμισυ</reg><orig>ἥμισοι</orig></choice> τρίτον, <expan><ex>ἀρτάβας</ex></expan> <num value="18">ιη</num> <num value="1/2">𐅵</num> <num value="1/3" rend="tick">γ</num> μόνας. 

    <lb n="37"/>καὶ Φαρμοῦθι <num value="11">ια</num>, ὁ αὐτὸς ἄλλας σίτου <expan>ἀ<unclear>ρτά</unclear>β<ex>ας</ex></expan><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> μόνας</supplied>. 

    <lb n="38"/><handShift new="m4"/> α<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="5" unit="character"/><supplied reason="lost"> <expan>σεσ<ex>ημείωμαι</ex></expan></supplied>. 

    <lb n="39"/><handShift new="m6"/> Παῦνι <num value="21">κα</num>. κατέβαλεν κωμητῶ<unclear>ν</unclear> 

    <lb n="40"/>Καρανίδος διὰ Ἀντιουρίου Ἀβοῦ <unclear>ὑπὲρ</unclear> <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="41"/>δεκάτης <expan>ἰνδικ<ex>τίονος</ex></expan> ἀχύρου λίτρα<unclear>ς</unclear> διακ<supplied reason="lost">οσίας</supplied> 

    <lb n="42"/>τεσσαράκοντα, <expan><ex>γίνονται</ex></expan> <expan>λί<ex>τραι</ex></expan> <num value="240">σμ</num> μ<unclear>ό</unclear>νας. <gap reason="illegible" quantity="2" unit="character"/><supplied reason="lost"><expan>σεσ<ex>ημείωμαι</ex></expan></supplied>. </ab></div></div>
