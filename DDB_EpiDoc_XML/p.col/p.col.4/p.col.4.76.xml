    <lb n="21"/>καὶ <add place="above">τῶν</add> <num value="4">δ</num> <num value="2"><expan><ex>διωβόλου</ex></expan></num> <expan><ex>δραχμῶν</ex></expan> τῶ<add place="above">ν</add> διαγραφει

