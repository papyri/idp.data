         <head n="31083" xml:lang="en">
            <date>III spc</date>
            <placeName>Memphis</placeName>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="ctr" type="textpart"><ab>

    <lb n="1"/><gap reason="lost" extent="unknown" unit="character"/>κ<gap reason="illegible" quantity="1" unit="character"/>ν νῦν παντω<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="2"/><gap reason="lost" extent="unknown" unit="character"/>ει ὡς κερμάτιον <gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="3"/>ἀλλὰ καὶ ἃς ἔχει ἀσφαλείους κ<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="4"/>Σαραπίωνι ἐξελθεῖν ἀπαιτ<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="5"/>αυτ<gap reason="lost" extent="unknown" unit="character"/>εχει λεπτάριον εὐλογ<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="6"/>τω<gap reason="lost" extent="unknown" unit="character"/>οτων περὶ στρωματ<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="7"/>εχ<gap reason="lost" extent="unknown" unit="character"/> καὶ τὰ ἄλλα πάντα <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="8"/>σα<gap reason="lost" extent="unknown" unit="character"/> φορείσθω ἵνα ἐὰν <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="9"/>τὸ κατοίκιον κα<supplied reason="omitted">τὰ</supplied> <choice><reg cert="low">μῆνα</reg><orig>καμῆνα</orig></choice> διδότω <gap reason="lost" extent="unknown" unit="character"/> 
</ab></div><div n="ms" type="textpart"><ab>

    <lb n="1" rend="perpendicular"/> <gap reason="lost" extent="unknown" unit="character"/>ν μωμον καὶ συντιμηθη<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="2" rend="perpendicular"/> <gap reason="lost" extent="unknown" unit="character"/>τας τοὺς σημε<gap reason="lost" extent="unknown" unit="character"/>ειμεθεν<gap reason="lost" extent="unknown" unit="character"/> </ab></div></div>
