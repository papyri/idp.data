         <head n="31089" xml:lang="en">
            <date>III spc</date>
            <placeName>Memphis</placeName>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><ab>
    <lb n="1"/><gap reason="lost" extent="unknown" unit="character"/>ρομαι ὡς οἶδας ου<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="2"/><gap reason="lost" extent="unknown" unit="character"/>ατος ἐμὴ δὲ χαίρειν μοι εἶπας <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="3"/><gap reason="lost" extent="unknown" unit="character"/> γράμματα <gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="1" unit="character"/> ἄλλας <expan>γνω<ex>μ  </ex></expan> <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="4"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ἐ</supplied>πιστολή μου ὑπὸ ταρίχους ἢ ἄλλου <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="5"/><gap reason="lost" extent="unknown" unit="character"/>ηρω καίπερ οὐκ ἐξετάζεις περὶ ε<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="6"/><gap reason="lost" extent="unknown" unit="character"/>ω καὶ πάντα οἶδα εἴπερ καὶ τὰ ω<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="7"/><gap reason="lost" extent="unknown" unit="character"/> τῶν σῶν ἐμοὶ οὐ μέλει τούτων <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="8"/><gap reason="lost" extent="unknown" unit="character"/>αι λυπεῖσθαι ὅτι ἐξερχόμενος <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="9"/><gap reason="lost" extent="unknown" unit="character"/> κατά σοι <abbr>καμη</abbr> ραν μύστην <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="10"/><gap reason="lost" extent="unknown" unit="character"/>κωμάτων καὶ ἐρχόμενος εἰς <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="11"/><gap reason="lost" extent="unknown" unit="character"/> αὐτὸ καὶ τὴν δαπάνην σου καὶ <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="12"/><gap reason="lost" extent="unknown" unit="character"/>υτης δούλης ἐξ οὗ πραπεις μοι <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="13"/><gap reason="lost" extent="unknown" unit="character"/> ταῦτα δὲ ἐποίησα ἵνα σὲ μὴ λυ<supplied reason="lost">πῶ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="14"/><gap reason="lost" extent="unknown" unit="character"/>ν ἐπιτρεπόντων πωλήσω τὰ ὀλι<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="15"/><gap reason="lost" extent="unknown" unit="character"/>ναριδια ἐν Κόπτῳ γίνωμαι πορ<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="16"/><gap reason="lost" extent="unknown" unit="character"/>ριν οὗπερ μο<gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="1" unit="character"/> τηλικαύτην <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="17"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> συ</supplied>ν τοῖς γονεῦσιν.  ἔρρωσο. <gap reason="lost" extent="unknown" unit="character"/> </ab></div>
