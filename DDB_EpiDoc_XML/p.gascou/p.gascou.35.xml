<ab>
    <lb n="1"/><g type="stauros"/> ὡς <choice><reg>παρήγγειλε</reg><reg>ἐπαρήγγειλε</reg><orig>ἐπαρήλλι<del rend="erasure">α</del><add place="above">λέ</add></orig></choice> <add place="above">μ<unclear>ο</unclear>ι</add> ὁ 'μὸς δεσπότης διὰ τὴν ληνόν, ὅτι φι<unclear>λ</unclear><gap reason="lost" extent="unknown" unit="character"/>

    <lb n="2"/><unclear>εισ</unclear>τους ἐξενεγκεῖν τοὺς λίθους, <choice><reg>ἵνα</reg><orig>εἵ<unclear>ν</unclear>α</orig></choice> φροντίζω<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/>

    <lb n="3"/>πλοίων, καὶ ταχέως ἐξενέγκουσιν αὐτά. κ<unclear>αὶ</unclear> <subst><add place="inline">φιλ</add><del rend="corrected">πιλ <certainty match=".." locus="value"/></del></subst> <gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/>

    <lb n="4"/>ὁ 'μὸς δεσπότης, ὅτι πόσα κοῦφα κρια <choice><reg>ἔχομ<supplied reason="lost">εν</supplied></reg><orig>ἔχωμ<supplied reason="lost">εν</supplied></orig></choice> <gap reason="lost" extent="unknown" unit="character"/>

    <lb n="5"/>καὶ <choice><reg>ἐν</reg><orig>ε<unclear>ἰ</unclear>ς</orig></choice> <choice><reg>τῷ</reg><orig>το͂ͅ</orig></choice> <choice><reg>πύργῳ</reg><orig>πύργῳ</orig></choice> <choice><reg>τῷ</reg><orig>τοῦ</orig></choice> σὺν <choice><reg>τῷ</reg><orig>το͂ͅ</orig></choice> Ψάει χι<supplied reason="omitted">λί</supplied>ας <num value="300">τριακ<unclear>ο</unclear><supplied reason="lost">σίας</supplied></num> <gap reason="lost" extent="unknown" unit="character"/>

    <lb n="6"/><subst><add place="inline">ἔπεμψεν</add><del rend="corrected">ἔπεμψα</del></subst> λαβεῖν τοὺς ὁμήρους τῶν ἀμπελο<unclear>υρ</unclear><supplied reason="lost">γῶν</supplied> <gap reason="lost" extent="unknown" unit="character"/>

    <lb n="7"/>τὰ ἀμπελικά, <subst><add place="inline">ἐὰν</add><del rend="corrected">ιαν <certainty match=".." locus="value"/></del></subst> <choice><reg>κελεύῃ</reg><orig>κελεύει</orig></choice> ὁ ἐμὸς δεσπότη<supplied reason="lost">ς</supplied> <gap reason="lost" extent="unknown" unit="character"/>

    <lb n="8"/>τὴν φυλακὴν, καὶ ἐὰν <choice><reg>κελεύῃ</reg><orig>κελεύει</orig></choice> ὁ 'μ<supplied reason="lost">ὸ</supplied>ς δεσπότης <gap reason="lost" extent="unknown" unit="character"/>

