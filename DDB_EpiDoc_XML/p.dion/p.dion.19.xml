         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r,ext" type="textpart"><ab>

    <lb n="1"/>βα<unclear>σιλευόν</unclear>τω<unclear>ν</unclear> <unclear>βασιλίσσης</unclear> <supplied reason="lost">Κλεοπάτρας θεᾶς Φιλομήτορος Σωτείρας καὶ</supplied> 

    <lb n="2"/>βα<unclear>σιλ</unclear>έως Πτολεμαίου <supplied reason="lost">τοῦ ἐπικαλουμένου Ἀλεξάνδρου θεοῦ Φιλομήτορος</supplied> 

    <lb n="3"/><supplied reason="lost">ἔτ</supplied><unclear>ο</unclear>υς <unclear>δω</unclear>δε<unclear>κ</unclear>άτου τ<unclear>οῦ</unclear> <supplied reason="lost">καὶ ἐνάτου, ἐφʼ ἱερέως βασιλέως Πτολεμαίου τοῦ καὶ</supplied> 

    <lb n="4"/><unclear>Ἀ</unclear><supplied reason="lost">λ</supplied><unclear>εξ</unclear>άνδρ<unclear>ου</unclear> ἐ<unclear>ν</unclear> <supplied reason="lost">Ἀλεξανδ</supplied><unclear>ρ</unclear>εί<supplied reason="lost">αι Ἀλεξάνδρου καὶ θεῶν Σωτήρων καὶ θεῶν</supplied> 

    <lb n="5"/>Ἀ<supplied reason="lost">δ</supplied><unclear>ε</unclear>λφῶν καὶ θε<unclear>ῶ</unclear>ν Εὐ<supplied reason="lost">εργετῶν καὶ θεῶν Φιλοπατόρων καὶ θεῶν</supplied> 

    <lb n="6"/>Ἐπιφανῶν καὶ θεοῦ <unclear>Ε</unclear><supplied reason="lost">ὐπάτορος καὶ θεοῦ Φιλομήτορος καὶ θεοῦ Νέου</supplied> 

    <lb n="7"/>Φιλοπάτορος <unclear>καὶ</unclear> <unclear>θεοῦ</unclear> <unclear>Ε</unclear>ὐ<supplied reason="lost">εργέτου καὶ θεῶν Φιλομητόρων Σωτήρων</supplied> 

    <lb n="8"/>καὶ τῶν ὄντων καὶ οὐ<unclear>σῶ</unclear><supplied reason="lost">ν ἐν Ἀλεξανδρείαι, μηνὸς Περιτίου τρίτηι</supplied> 

    <lb n="9"/>καὶ εἰκάδι, Χοιὰχ <unclear>τ</unclear>ρ<unclear>ί</unclear><supplied reason="lost">τηι καὶ εἰκάδι, ἐν κώμηι Τήνει τῆι καὶ</supplied> 

    <lb n="10"/><unclear>Ἀ</unclear>κώριος τοῦ Μωχίτ<supplied reason="lost">ου τόπου, νομοῦ δʼ Ἑρμοπο</supplied><unclear>λί</unclear>του. <supplied reason="lost">ἐδάνεισεν</supplied> 

    <lb n="11"/><unclear>Ἱππ</unclear>όνικ<unclear>ο</unclear><supplied reason="lost">ς Πτολεμαίου Μακεδὼν ἑκατόν</supplied><unclear>ταρ</unclear>χος <supplied reason="lost">Διονυσίωι</supplied> 

    <lb n="12"/>Κ<unclear>εφ</unclear>αλᾶτος Π<supplied reason="lost">έ</supplied><unclear>ρ</unclear>σηι τ<unclear>ῆ</unclear><supplied reason="lost">ς ἐπιγονῆς πυροῦ</supplied> στερεοῦ <unclear>ἀ</unclear><supplied reason="lost">ρτάβας</supplied> 

    <lb n="13"/>δ<unclear>έ</unclear>κα ἕξ <num value="16"/>, ἃς ἐ<unclear>κμ</unclear>εμέ<unclear>τ</unclear><supplied reason="lost">ρηται παρʼ αὐτ</supplied>οῦ, τόκω<unclear>ν</unclear> <supplied reason="lost">ἡμιολίων,</supplied> 

    <lb n="14"/>ἀρτά<unclear>β</unclear>αις ὀκτώ <num value="8"/>· τὸν <unclear>δ</unclear><supplied reason="lost">ὲ πυρὸν καὶ τὸν τόκο</supplied><unclear>ν</unclear>, τὰς πάσα<unclear>ς</unclear> <supplied reason="lost">πυροῦ ἀρτάβας</supplied> 

    <lb n="15"/>εἴκοσι τέσσαρες <num value="24"/>, ἀπο<unclear>δ</unclear><supplied reason="lost">ότω Διονύσιος Ἱππο</supplied>νίκωι ἢ <unclear>τ</unclear><supplied reason="lost">οῖς παρʼ αὐτοῦ</supplied> 

    <lb n="16"/>ἐν μηνὶ Λωίωι ὅς ἐ<supplied reason="lost">στιν Παῦνι τοῦ προγεγρ</supplied>αμμένου <unclear>ἔ</unclear><supplied reason="lost">τους,</supplied> 

    <lb n="17"/>πυρὸν νέον καθαρὸ<unclear>ν</unclear> <supplied reason="lost">ἄδολον, μέτρωι τῶι πρὸς τὸ</supplied> <unclear>χαλ</unclear>κοῦν <supplied reason="lost">καὶ</supplied> 

    <lb n="18"/>σκυτάλῃ δικαίαι καὶ <unclear>κ</unclear><supplied reason="lost">αταστησάτω ἐπὶ τὸν κατ</supplied>ὰ τὴν Ἀ<unclear>κ</unclear><supplied reason="lost">ώριος</supplied> 

    <lb n="19"/>ὅ<unclear>ρμ</unclear>ον τοῖς ἰδίοις <unclear>ἀν</unclear><supplied reason="lost">ηλώμασι, ἄνευ δίκη</supplied><unclear>ς</unclear> καὶ κρίσεως <unclear>κ</unclear><supplied reason="lost">αὶ</supplied> 

    <lb n="20"/>πάσης κακοτεχνίας· <unclear>ἐ</unclear><supplied reason="lost">ὰν δὲ μὴ ἀποδῷ καθʼ ἃ γέγ</supplied>ραπται, ἀπ<supplied reason="lost">οτεισάτω</supplied> 

    <lb n="21"/>τιμὴν ἑκάστης ἀρτ<unclear>ά</unclear><supplied reason="lost">βης χαλκοῦ δραχμὰς τ</supplied>ρισχιλία<unclear>ς</unclear> <num value="3000"/> <supplied reason="lost">καὶ μηθὲν</supplied> 

    <lb n="22"/>ἧσσον· ἡ πρᾶξις ἔστω <unclear>Ἱπ</unclear><supplied reason="lost">πονίκωι καὶ τοῖς π</supplied><unclear>α</unclear>ρʼ αὐτοῦ τ<unclear>ο</unclear><supplied reason="lost">ῦ πυροῦ</supplied> 

    <lb n="23"/>καὶ τῶν κατὰ τὴν συγγρ<unclear>α</unclear><supplied reason="lost">φὴν πάντων ἔκ τ</supplied><unclear>ε</unclear> αὐτοῦ Διο<supplied reason="lost">νυσίου</supplied> 

    <lb n="24"/>καὶ ἐκ τῶν ὑπαρχόντω<unclear>ν</unclear> <unclear>α</unclear><supplied reason="lost">ὐ</supplied><unclear>τ</unclear>ῶ<supplied reason="lost">ι πάντ</supplied><unclear>ων</unclear> <supplied reason="lost">κα</supplied><unclear>θ</unclear>ʼ ἅπερ ἐκ δί<unclear>κ</unclear><supplied reason="lost">ης·</supplied> 

    <lb n="25"/>ἡ συγγραφὴ ἥδε κυρία ἔσ<supplied reason="lost">τω</supplied> <unclear>παντα</unclear>χῆι ἐπ<unclear>ι</unclear>φ<supplied reason="lost">ερο</supplied><unclear>μ</unclear>έ<unclear>ν</unclear>η. μάρτυρ<supplied reason="lost">ες·</supplied> 

    <lb n="26"/>Ἀπολλώνιος Πολυκρ<unclear>ά</unclear><supplied reason="lost">του</supplied> Πέρσης, Ἀρίμμας <supplied reason="lost">Διο</supplied><unclear>νυ</unclear>σίου Χαρισ<supplied reason="lost">τήριος,</supplied> 

    <lb n="27"/><supplied reason="lost">Πτ</supplied>ολεμαῖος Ἀναξαγόρο<unclear>υ</unclear> <unclear>Μ</unclear><supplied reason="lost">ι</supplied>λήσιος, Ἀγή<unclear>ν</unclear>ωρ Β<unclear>α</unclear><supplied reason="lost">ρ</supplied><unclear>κ</unclear>αίου Λακε<unclear>δ</unclear>αι<unclear>μ</unclear><supplied reason="lost">όνιος,</supplied> 

    <lb n="28"/><supplied reason="lost">Διο</supplied><unclear>νύσ</unclear>ιο<unclear>ς</unclear> <unclear>Ἀπολλ</unclear>ω<unclear>ν</unclear><supplied reason="lost">ι</supplied><unclear>ου</unclear>, <unclear>Ἀπολλώ</unclear><supplied reason="lost">νιος </supplied><gap reason="lost" quantity="5" unit="character"/> <unclear>ο</unclear>ἱ δύο <unclear>Μ</unclear><supplied reason="lost">α</supplied><unclear>κ</unclear><supplied reason="lost">εδόνες</supplied> 

    <lb n="29"/><supplied reason="lost">τῶν</supplied> φερομέ<unclear>νω</unclear>ν ἐν Κλεοπάτραι. 

    <lb n="30"/>σ<unclear>υγγ</unclear><supplied reason="lost">ραφο</supplied><unclear>φ</unclear>ύλαξ· Ἀπολλώ<unclear>ν</unclear><supplied reason="lost">ιος</supplied>. 

    <lb n="31"/><handShift new="m2"/> <supplied reason="lost">Διο</supplied><unclear>ν</unclear>ύσιος Κεφαλᾶτος Πέρση<unclear>ς</unclear> τῆς <unclear>ἐ</unclear><supplied reason="lost">πιγο</supplied><unclear>ν</unclear>ῆς ἀπέ<supplied reason="lost">χ</supplied><unclear>ω</unclear> τὰς τοῦ <unclear>π</unclear><supplied reason="lost">υροῦ ἀρτά</supplied>

    <lb n="32" break="no"/><supplied reason="lost">βας εἴκ</supplied><unclear>ο</unclear>σι τ<unclear>έ</unclear>σσαρες <num value="24"/> καὶ πο<unclear>ιήσ</unclear>ω <unclear>κ</unclear><supplied reason="lost">α</supplied><unclear>θ</unclear>ʼ ὅτι <unclear>π</unclear><supplied reason="lost">ρόκει</supplied><unclear>τ</unclear>αι καὶ δέδω<supplied reason="lost">κα κυρίαν</supplied> 

    <lb n="33"/><unclear>Ἀ</unclear><supplied reason="lost">πολλ</supplied>ωνίωι. 

    <lb n="34"/><handShift new="m3"/> <unclear>Ἀπ</unclear>ολλώνιος ἔχω κυρίαν. 

