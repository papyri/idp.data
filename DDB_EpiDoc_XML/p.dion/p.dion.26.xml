         <head n="3110" xml:lang="en">
            <date>116 BC</date>
            <placeName>Hermoupolis Magna</placeName>
            <ref n="p.rein;1;31" type="reprint-from">PRein1,31</ref>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r" type="textpart"><ab>

    <lb n="1"/>ἔτους <num value="2"><unclear>β</unclear></num>, <supplied reason="lost">μηνὸς Δίου <num value="1">α</num></supplied><num value="6">ϛ</num>, Θῶυθ <num value="16">ιϛ</num>, 

    <lb n="2"/>ἐν Ἑρμο<supplied reason="lost">ῦ πόλει τῆς Θηβαΐδ</supplied>ος. 

    <lb n="3"/>ἐδάνεισ<supplied reason="lost">εν Ἑρμίας </supplied><gap reason="lost" quantity="5" unit="character"/><gap reason="illegible" quantity="1" unit="character"/>ου Πέρσης τῶν φερο

    <lb n="4" break="no"/>μένων ἐ<supplied reason="lost">ν τοῖς τῶν ἱππ</supplied><unclear>έ</unclear>ων μισθοφόρων υἱοῖς 

    <lb n="5"/>Διονυσίω<supplied reason="lost">ι Κεφαλᾶτος Πέρ</supplied><unclear>σ</unclear>ηι τῆς ἐπιγονῆς πυροῦ 

    <lb n="6"/>στερεοῦ ἀρ<unclear>τ</unclear><supplied reason="lost">άβας ἑκατὸν πεν</supplied>τήκοντα <num value="150"/>· τοῦτο 

    <lb n="7"/>δʼ ἐστὶν τὸ <unclear>δ</unclear><supplied reason="lost">άνειον ὃ προσ</supplied><unclear>ω</unclear>φείληκεν Διονύσιος 

    <lb n="8"/>Ἑρμίαι ἀπ<supplied reason="lost">ὸ συγγραφῆς δαν</supplied>είου οὗ συνῆρται αὐτῶ<unclear>ι</unclear> 

    <lb n="9"/>ἅμα τῆι συ<unclear>γ</unclear><supplied reason="lost">γραφῆι ταύτηι ἀ</supplied>ναφερομένηι· 

    <lb n="10"/>τὸν δὲ πυ<unclear>ρ</unclear><supplied reason="lost">ὸν ἄτοκον ἀπο</supplied>δότω Διονύσιος 

    <lb n="11"/>Ἑρμίαι ἢ το<supplied reason="lost">ῖς παρʼ αὐτοῦ ἐν</supplied> μηνὶ Λωίωι 

    <lb n="12"/>ὅς ἐστιν Πα<supplied reason="lost">ῧνι τοῦ δευτέρ</supplied>ου ἔτους, πυρὸν 

    <lb n="13"/>στερεὸν ν<unclear>έ</unclear><supplied reason="lost">ον καθαρὸν ἄδολ</supplied>ον μέτ<unclear>ρ</unclear>ωι τῶι 

    <lb n="14"/>πρὸς τὸ χα<unclear>λ</unclear><supplied reason="lost">κοῦν καὶ κατασ</supplied>τησάτω ἐπὶ τὸν 

    <lb n="15"/><unclear>κ</unclear>ατὰ <unclear>τ</unclear>ὴν <supplied reason="lost">Ἀκώριος ὅ</supplied><unclear>ρ</unclear><supplied reason="lost">μον τοῖς</supplied> ἰδίοις ἀν<unclear>η</unclear>λώμασι· 

    <lb n="16"/>ἐὰν δὲ μὴ <supplied reason="lost">ἀποδ</supplied>ῷ καθʼ <unclear>ἃ</unclear> <supplied reason="lost">γέγρα</supplied><unclear>π</unclear>ται, ἀ<unclear>π</unclear><supplied reason="lost">ο</supplied>τεισάτω 

    <lb n="17"/>Διονύσιος <unclear>Ἑ</unclear><supplied reason="lost">ρμί</supplied>αι ἢ <unclear>τοῖ</unclear><supplied reason="lost">ς παρʼ αὐτοῦ τιμὴν</supplied> 

    <lb n="18"/>ἑκάστης <unclear>ἀ</unclear><supplied reason="lost">ρτάβ</supplied><unclear>ης</unclear> <expan>χα<unclear>λ</unclear><supplied reason="lost"><ex>κοῦ</ex></supplied></expan><supplied reason="lost"> δρ</supplied>αχμὰς τ<unclear>ρ</unclear>ισχιλίας <num value="3000"/>· 

    <lb n="19"/>καὶ ἡ πρᾶξ<supplied reason="lost">ις ἔσ</supplied>τω Ἑ<unclear>ρ</unclear><supplied reason="lost">μία</supplied><unclear>ι</unclear> κ<unclear>α</unclear>ὶ <unclear>τ</unclear><supplied reason="lost">οῖ</supplied><unclear>ς</unclear> <unclear>παρ</unclear>ʼ <unclear>αὐ</unclear>τοῦ 

    <lb n="20"/><supplied reason="lost">τ</supplied><unclear>ῶ</unclear>ν <unclear>κ</unclear>ατὰ <unclear>τ</unclear><supplied reason="lost">ὴ</supplied>ν συ<unclear>γγρ</unclear><supplied reason="lost">αφὴ</supplied><unclear>ν</unclear> <supplied reason="lost">πάντων ἔκ τε αὐτοῦ</supplied> 

    <lb n="21"/>Διονυσίου κα<unclear>ὶ</unclear> <supplied reason="lost">ἐ</supplied><unclear>κ</unclear> τῶν ὑ<supplied reason="lost">παρχό</supplied><unclear>ν</unclear>τω<unclear>ν</unclear> αὐτ<unclear>ῶ</unclear><supplied reason="lost">ι</supplied> 

    <lb n="22"/>πάντων κ<unclear>α</unclear><supplied reason="lost">θʼ ἅ</supplied>περ <choice><reg>ἐκ</reg><orig>ἐγ</orig></choice> <unclear>δ</unclear><supplied reason="lost">ίκης· ἡ σ</supplied><unclear>υ</unclear><supplied reason="lost">γγρα</supplied><unclear>φ</unclear>ὴ ἥ<supplied reason="lost">δε</supplied> 

    <lb n="23"/><unclear>κ</unclear>υρία ἔστω. 
</ab></div><div n="v" type="textpart"><ab>

    <lb n="24"/><supplied reason="lost">δάνειον Διονυσίωι</supplied> 

    <lb n="25"/>πρὸς Ἑρμί<unclear>ου</unclear> <unclear>π</unclear>υρ<unclear>ο</unclear>ῦ <expan><ex cert="low">ἀρταβῶν</ex></expan> <num value="150"><unclear>ρν</unclear></num>. </ab></div></div>
