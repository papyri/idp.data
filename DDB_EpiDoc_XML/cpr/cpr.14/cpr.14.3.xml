         <head n="9896" xml:lang="en">
            <date>ca. AD 604</date>
            <placeName>Arsinoite</placeName>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r" type="textpart"><ab>

    <lb n="1"/><gap reason="lost" extent="unknown" unit="line"/> 

    <lb n="1"/><gap reason="lost" quantity="8" unit="character"/><gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="2"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="lost" quantity="2" unit="character"/><gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" quantity="9" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ὀφειλο</supplied>

    <lb n="3" break="no"/>μένας δοθῆναι παρὰ σοῦ τῷ α<gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ἐπὶ</supplied> 

    <lb n="4"/>τῷ <choice><reg>πάσης</reg><orig>πάσας</orig></choice> <hi rend="diaeresis">ἰ</hi>χθύος <hi rend="diaeresis">ὑ</hi>πηρεσίας <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="5"/>λα καὶ π<unclear>οῖο</unclear>ν ἐκ προσώπου σ<supplied reason="lost">ου </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="6"/><gap reason="lost" quantity="9" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><unclear>αι</unclear> τῆς <choice><reg>συνηθείας</reg><orig>συνηθίας</orig></choice> τ<unclear>ο</unclear><gap reason="lost" quantity="2" unit="character"/><gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="7"/><gap reason="lost" quantity="9" unit="character"/><gap reason="illegible" quantity="1" unit="character"/>ς <subst><add place="inline">συνηθείας</add><del rend="corrected">συν<del rend="erasure">ει</del>ηθειας</del></subst> ὀφείλομέν σοι ᾡδήποτ<unclear>ε</unclear> <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="8"/><gap reason="illegible" quantity="3" unit="character"/><gap reason="lost" quantity="2" unit="character"/><supplied reason="lost"> δοθ</supplied><unclear>ῆ</unclear>ναι παρὰ σοῦ. προσεπὶ τούτοις δὲ παρασχ<unclear>εῖ</unclear><supplied reason="lost">ν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="9"/>ἡμᾶς καὶ τῇ σῇ εὐλαβείᾳ μηνιαίως ἐπὶ πάντα τὸν εἰρημένον χ<supplied reason="lost">ρό</supplied>

    <lb n="10" break="no"/>νον χρυσίου νομισματίου ἥμισυ πα<unclear>ρ</unclear>ὰ κεράτια τρία ἥμισυ <expan>τ<unclear>έ</unclear><supplied reason="lost">τ<ex>αρτον</ex></supplied></expan> 

    <lb n="11"/><choice><reg>ὄγδοον</reg><orig>ὄγδον</orig></choice> <expan>χρ<ex>υσίου</ex></expan> <expan>νο<ex>μισματίου</ex></expan> <num value="1/2">𐅵</num> <expan>π<unclear>αρ</unclear><ex>ὰ</ex></expan> <expan><unclear>κε</unclear><supplied reason="lost">ρ<ex>άτια</ex></supplied></expan> <num value="3"><unclear>γ</unclear></num> <num value="1/2"><unclear>𐅵</unclear></num> <num value="1/4" rend="tick"><unclear>δ</unclear></num> <num value="1/8" rend="tick"><unclear>η</unclear></num> <unclear>εἰ</unclear><supplied reason="lost">ς</supplied> <unclear>χρεία</unclear>ν σου τοῦ ἰχθ<supplied reason="lost">υο </supplied><gap reason="lost" quantity="5" unit="character"/> 

    <lb n="12"/>ὁπόταν ἔλθῃς πρὸς ἡμᾶς ἐν τῇ <supplied reason="lost">ἡ</supplied><unclear>μ</unclear>ῶν ἀποθήκῃ τῆ<supplied reason="lost">ς αὐτῆς πό</supplied>

    <lb n="13" break="no"/>λεως, καὶ μὴ ἐξεῖναι <del rend="erasure">μοι</del> ἡμῖν <choice><reg>ὑπαναχωρεῖν</reg><orig>ὑπαναχωρε͂ν</orig></choice> ταύτης <supplied reason="omitted">τῆς</supplied> ὁμολ<supplied reason="lost">ο</supplied>γί<supplied reason="lost">ας</supplied> 

    <lb n="14"/>πρὸ συμπληρώσ<unclear>εως</unclear> <unclear>τ</unclear>ο<unclear>ῦ</unclear> <unclear>χ</unclear>ρόνου μήτε σοι ἀποβαλέσθαι ἡμ<unclear>ᾶς</unclear> <unclear>ἐ</unclear><supplied reason="lost">ν</supplied>

    <lb n="15" break="no"/>τὸς τούτου. εἰ δὲ ὑ<unclear>πα</unclear>ναχωρήσωμεν εἴσω τοῦ χρόνου ἐνέχεσθ<unclear>αι</unclear> 

    <lb n="16"/>ἡμᾶς παραχρῆμα <subst><add place="inline">δοῦναι</add><del rend="corrected">δουν<del rend="erasure">ν</del>αι</del></subst> λόγῳ προστίμο<supplied reason="lost">υ</supplied> χρυσίου νομισμάτια ἕξ, 

    <lb n="17"/><expan>νο<ex>μισμάτια</ex></expan> <num value="6">ϛ</num> ἐξ <hi rend="diaeresis">ὑ</hi>παρχόντων μο<unclear>υ</unclear> πάντων. εἰ δὲ καὶ σὺ <choice><reg>ἀποβάλῃς</reg><orig>ἀποβαλη</orig></choice> ἡμᾶς πρὸ τῆς <unclear>τ</unclear><supplied reason="lost">ού</supplied>

    <lb n="18" break="no"/>του συμπληρώσεως ἐ<unclear>ν</unclear>έχεσθαι καί σε δοῦναι παρʼ εὐθὺ<unclear>ς</unclear> <unclear>ἡμῖ</unclear><supplied reason="lost">ν</supplied> 

    <lb n="19"/>λόγῳ προστίμου τὰ ἴσα <unclear>τ</unclear>οῦ χρυσίου νομισμάτια ἕξ, <expan>χρ<ex>υσίου</ex></expan> <expan>νο<ex>μισμάτια</ex></expan> <num value="6">ϛ</num>, καὶ <expan>ἐπ<unclear>ερ</unclear><ex>ωτηθέντες</ex></expan> <expan><supplied reason="lost">ὡμ</supplied><ex>ολογήσαμεν</ex></expan>. 

    <lb n="20"/><handShift new="m2"/> Αὐρήλιοι Σερῆνος <expan><ex>καὶ</ex></expan> Ἀμαῖος <choice><reg>υἱοὶ</reg><orig><hi rend="diaeresis">υ</hi>ἱος</orig></choice> Ἀπολλῶ ἰχθυοπ<unclear>ρ</unclear><supplied reason="lost">ᾶται</supplied> 

    <lb n="21"/>οἱ προκείμενοι <unclear>στοιχεῖ</unclear> ἡμῖν τὸ γραμματεῖον ὡς 

    <lb n="22"/>πρόκειται. <expan>Αὐρ<ex>ήλιος</ex></expan> <expan><unclear>Ἡράκλ</unclear><ex>ειος</ex></expan> Ἰωάννου <expan>ἔγρ<ex>αψα</ex></expan> <expan><ex>ὑπὲρ</ex></expan> <subst><add place="inline">αὐτῶν</add><del rend="corrected">αυτου</del></subst> <expan>παρ<ex>όντων</ex></expan> 

    <lb n="23"/>ἀγραμμάτων ὄντων. <g type="stauros"/> 

    <lb n="24"/><handShift new="m3"/><g type="stauros"/> δι εμυ <add place="above"><g type="stauros"/></add> <unclear>η</unclear>λια <g type="monogram"/> 

    <lb n="24"/>διʼ ἐμοῦ <add place="above"><g type="stauros"/></add> Ἠλία 
</ab></div><div n="v" type="textpart"><ab>

    <lb n="25"/><supplied reason="lost"><expan>ὁμ<ex>ολογία</ex></expan> <expan>γενομ<ex>ένη</ex></expan> ὑπὸ</supplied> <expan>Αὐρ<ex>ηλίων</ex></expan> Ἀμαίου <expan>υἱο<ex>ῦ</ex></expan> Ἀπολλῶ 

    <lb n="26"/><supplied reason="lost">καὶ Σερήνου υἱοῦ</supplied> <expan>Ἀπολλ<ex>ῶ</ex></expan> εἰς τὸν εὐλαβέστατον Κυρικὸν <expan>διάκ<ex>ονον</ex></expan> <g type="stauros"/> </ab></div></div>
