         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r" type="textpart"><ab>

    <lb n="1"/><gap reason="lost" extent="unknown" unit="line"/> 

    <lb n="1"/><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="2" unit="character"/>σι<gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="2"/>Σαρμάτας υἱὸς <gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="5" unit="character"/><supplied reason="lost"> μητρὸς</supplied> 

    <lb n="3"/>Ἡραε<hi rend="diaeresis">ί</hi>δος καὶ Ταχ<supplied reason="lost">άων υἱὸς</supplied> 

    <lb n="4"/>Ἀνοῦπ μητρὸς <gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="4" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="5" unit="character"/><gap reason="illegible" quantity="1" unit="character"/> 

    <lb n="5"/>ἀπὸ ἐποικίο<supplied reason="lost">υ</supplied> Πεε<unclear>ν</unclear><supplied reason="lost">άμεω</supplied>ς τοῦ Ἡρα

    <lb n="6" break="no"/>κλεοπολίτου νομ<supplied reason="lost">ο</supplied>ῦ, τῷ θαυμασι<add place="above">ωτά</add>

    <lb n="7" break="no"/>τῳ Μακαρίῳ ἀρχισυμ<add place="above">μ</add>άχῳ 

    <lb n="8"/>υἱῷ τοῦ μακαρίου Σιλβανοῦ 

    <lb n="9"/>ἀφʼ Ἡρακλέους πόλεως, <expan>χ<ex>αίρειν</ex></expan>. 

    <lb n="10"/>ὁμολογοῦμεν ἐξ <choice><reg>ἀλληλεγγύης</reg><orig>ἀλλυνηκης</orig></choice> 

    <lb n="11"/>ἐσχηκέναι παρὰ σοῦ διὰ χειρὸ<supplied reason="lost">ς</supplied> 

    <lb n="12"/>ἐξ οἴκου καὶ πεπληρῶσθαι 

    <lb n="13"/>τὴν τιμ<supplied reason="lost">ὴν </supplied><gap reason="lost" quantity="3" unit="character"/><gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

