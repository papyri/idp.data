         <div xml:lang="grc" type="edition" xml:space="preserve"><ab>
    <lb n="1"/><gap reason="lost" extent="unknown" unit="line"/> 

    <lb n="1"/><gap reason="lost" quantity="4" unit="character"/><supplied reason="lost"> μην</supplied>ὸς Ξανθ<supplied reason="lost">ίκου </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> διʼ ἐπιτηρητῶν</supplied> 

    <lb n="2"/>ἀγοραν<supplied reason="lost">ομίας</supplied> περὶ Τέ<supplied reason="lost">κμει τοῦ ὑπὲρ Μέμφιν Ἡρακλεοπολίτου</supplied>· 

    <lb n="3"/>ὁμολ<supplied reason="lost">ογεῖ Αὐ</supplied>ρήλιος Δίδυ<supplied reason="lost">μος </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="4"/>καὶ Θεο<supplied reason="lost">δο</supplied>σία ἀφʼ Ἡρακλ<supplied reason="lost">έους πόλεως </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="5"/><gap reason="illegible" quantity="1" unit="character"/>ιος Ἀπολλωνίου αμ<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="6"/>παρακεχωρηκέναι <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="7"/>ης ἀφʼ Ἡρακλέους πόλ<supplied reason="lost">εως </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="8"/><supplied reason="lost">το</supplied>ῦ <subst><add place="inline">Αὐρηλίου</add><del rend="corrected">αρρηλιου</del></subst> Ἡρα<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="9"/>ὡς ἐτῶν εἴκοσι πέντ<supplied reason="lost">ε <num value="25"/> </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="10"/>αὐτῷ ὑπὸ τῆς μ<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="11"/>λαμμωνος Ἀντιν<supplied reason="lost">οέως </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="12"/>Δωρίωνος κλήρου <supplied reason="lost"><app type="editorial"><lem resp="BL 1.452">κατοικικοῦ</lem><rdg><gap reason="illegible" quantity="9" unit="character"/></rdg></app> ἀρουρῶν</supplied> 

    <lb n="13"/>πέντε τετάρτου <num value="5"/><num value="1/4"/> το<gap reason="lost" quantity="13" unit="character"/><supplied reason="lost"> ἥμι</supplied>

    <lb n="14" break="no"/>συ ὄγδοον <num value="1/2"/><num value="1/8"/> π<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="15"/>κληρονόμων <app type="editorial"><lem resp="BL 1.452">Σταλβ<supplied reason="lost">ᾶ</supplied></lem><rdg>Σταλβ<supplied reason="lost">ᾶ υἱὸς</supplied></rdg></app><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="16"/>ἀπηλιώτου Ὡρίονο<supplied reason="lost">ς </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="17"/>τὸ δὲ ἑστάμενον τοῦ <app type="editorial"><lem resp="BL 1.452"><supplied reason="lost">κλήρου παραχωρητικὸν</supplied></lem><rdg><gap reason="lost" extent="unknown" unit="character"/></rdg></app> 

    <lb n="18"/>ἅπαν ὅ ἐστιν ἀργυρίου <supplied reason="lost">δραχμὰς </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="19"/>προσομολογεῖ ὁ παρα<supplied reason="lost">χωρῶν ἀπεσχηκέναι </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="20"/>ἐκ πλήρους διὰ χειρ<supplied reason="lost">ὸς ἐξ οἴκου καὶ παρέξεται αὐτὰς</supplied> 

    <lb n="21"/>καθαρὰς ἀπό τε ἄλλων <supplied reason="lost"><app type="editorial"><lem resp="BL 1.452">παραχωρήσεων</lem><rdg>πράσεων</rdg></app> ἢ ἑτέρων οἰκονομι</supplied>

    <lb n="22" break="no"/>ῶν καὶ παντὸς τοῦ ἐπ<supplied reason="lost">ελευσομένου ἢ ἐμποιησομένου</supplied> 

    <lb n="23"/><supplied reason="lost">ἐπὶ τὸν ἅπαντα</supplied> χρόνο<supplied reason="lost">ν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

