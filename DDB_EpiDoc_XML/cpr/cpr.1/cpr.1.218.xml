         <head n="29082" xml:lang="en">
            <date>II spc</date>
            <placeName>Soknopaiou Nesos</placeName>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><ab>
    <lb n="1"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> Εὐσεβ</supplied>οῦς Εὐτυχοῦς Σεβαστοῦ μηνὸς Ξανθίκου <app type="editorial"><lem resp="BL 1.121"><supplied reason="lost">Μεχεὶρ</supplied></lem><rdg><gap reason="lost" extent="unknown" unit="character"/></rdg></app><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="2"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ὡ</supplied>ς ἐτῶν ἑξήκοντα <num value="60"/> <subst><add place="inline">εὔσημος</add><del rend="corrected">αυσημος</del></subst> ὄφθαλμον <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="3"/><gap reason="lost" extent="unknown" unit="character"/> πέντε <hi rend="diaeresis">ὑ</hi>πόπτιλ<g type="apostrophe"/>λος παρόντος καὶ εὐδοκ<supplied reason="lost">οῦντος </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="4"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> Ἀ</supplied>πιάδος ἀπὸ ἀμφόδου Μακεδόνων ὡς ἐτῶν <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="5"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> Ἀρσινο</supplied>ιτῶν πόλεως τέλειος καὶ Ἀμμωνιανὸς <gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ὡς</supplied> 

    <lb n="6"/><supplied reason="lost">ἐτῶ</supplied>ν <subst><add place="inline">τεσσαράκοντα</add><del rend="corrected">ωσσαρακοντα</del></subst> πέντε <num value="45"/> οὐλὴ ἀντικνημίῳ ἀρ<supplied reason="lost">ιστερῷ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="7"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> Π</supplied>λουτάμμωνι ἀφήλικι μετὰ κυρίου τοῦ πα<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="8"/><gap reason="lost" extent="unknown" unit="character"/>δε ταῦτα τῷ ἐνεστῶτι <num value="4">δ</num> <expan><ex>ἔτει</ex></expan> μηνὶ Τῦβι <num value="19">ιθ</num> <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="9"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> μὴ</supplied> ἐπελ<g type="apostrophe"/>θεῖν ἐπὶ τὸν Ἡρωνεῖνον μηδʼ <app type="editorial"><lem resp="BL 1.121">ἐπ<supplied reason="lost">ὶ τοὺς παρʼ αὐτοῦ </supplied><gap reason="lost" extent="unknown" unit="character"/></lem><rdg>ἐπ<supplied reason="lost">ὶ </supplied><gap reason="lost" extent="unknown" unit="character"/></rdg></app> 

    <lb n="10"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> περ</supplied>ὶ κώμην Ἀλαβανθίδα τὰ νῦν κατὰ τήνδ<supplied reason="lost">ε </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="11"/><gap reason="lost" extent="unknown" unit="character"/>χωρεῖ τῇ Φιλουμένῃ τὰς ὑπαρχούσας ἀ<supplied reason="lost">πὸ κληρονομίας </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="12"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> κλή</supplied>ρου κατοικικοῦ ἀρούρας τρεῖς <num value="3"/> ἐν τόπῳ Κερ<gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> λεγομένῳ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="13"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ἀρούρα</supplied>ς τρεῖς <num value="3"/> γεωργουμένας ὑπὸ Γαλάτου ὧν <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="14"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> οἰκονο</supplied>μεῖν περὶ αὐτῶν ω῾ς ἐὰν αἱρῆται μὴ ἐνκ<supplied reason="lost">αλεῖν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="15"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> τοὺ</supplied>ς παρʼ αὐτῶν περὶ ὧν ἐωνήσατο καὶ τὸ <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="16"/><gap reason="lost" extent="unknown" unit="character"/>υθεν παρεχωρήθη ἐπὶ τὸ ὡς πρόκειτ<supplied reason="lost">αι </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="17"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> τὸ</supplied>ν ἐπελευσόμενον ἢ ἐμποιησόμενον ἀφισ<supplied reason="lost">τάνειν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="18"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ἐνεστ</supplied>ώσης ἡμέρας τρόπῳ μηδενὶ ὁπόταν <gap reason="lost" extent="unknown" unit="character"/> </ab></div>
