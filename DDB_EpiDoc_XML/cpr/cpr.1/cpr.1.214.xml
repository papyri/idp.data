         <head n="29081" xml:lang="en">
            <date>II spc</date>
            <placeName>Arsinoite</placeName>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><ab>
    <lb n="1"/><gap reason="lost" extent="unknown" unit="line"/> 

    <lb n="1"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ἐνθέ</supplied>σμου ἀπογραφῆς <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="2"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> καὶ τοὺς π</supplied>αρʼ αὐ<supplied reason="lost">το</supplied>ῦ κυριεύοντας <supplied reason="lost">καὶ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="3"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> πω</supplied>λοῦντας καὶ ὑποτιθεμένους <supplied reason="lost">καὶ οἰκονο</supplied>μο<supplied reason="lost">ῦντ</supplied>ας π<supplied reason="lost">ερὶ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="4"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> τῷ</supplied> Διονυσίῳ παραχρῆμα ἥν τε εἴ<supplied reason="lost">ληφε τιμὴ</supplied>ν μεθʼ ἡμι<supplied reason="lost">ολίας </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="5"/><app type="editorial"><lem resp="BL 1.121">καὶ εἰ<supplied reason="lost">ς τ</supplied>ο</lem><rdg><supplied reason="lost">καὶ εἰς τ</supplied>ὸ</rdg></app> δημόσιον τὰς <supplied reason="lost">ἴσας καὶ μηδὲν ἧσσον τὰ</supplied> προγεγραμ<supplied reason="lost">μένα </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="6"/><handShift new="m2"/> <gap reason="lost" extent="unknown" unit="character"/> Ἑρμογένους τοῦ Ἡρακλε<supplied reason="lost">ίδου</supplied> ὁμολογῶ <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="7"/><app type="editorial"><lem resp="BL 1.121"><supplied reason="lost">τῆς</supplied></lem><rdg><gap reason="lost" extent="unknown" unit="character"/></rdg></app> ὁμοπατρίας ἡμῶν ἀδελφῆς <app type="editorial"><lem resp="BL 1.121">Σαμβοῦ<supplied reason="lost">τος </supplied><gap reason="lost" extent="unknown" unit="character"/></lem><rdg>Σαμμοῦ<supplied reason="lost">τος </supplied><gap reason="lost" extent="unknown" unit="character"/></rdg></app> 

    <lb n="8"/><app type="editorial"><lem resp="BL 1.121"><unclear>μηνὶ</unclear> Χοιὰκ</lem><rdg><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> Χοι</supplied>ὰκ</rdg></app> δευτέρᾳ καὶ εἰκάδι ἥμισυ μέρος κοι<supplied reason="lost">νὸν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="9"/><gap reason="lost" extent="unknown" unit="character"/> ἐποίκου <supplied reason="lost">καὶ αὐλῆς</supplied> καὶ τῶν συνκυρο<supplied reason="lost">ύντων </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="10"/><app type="editorial"><lem resp="BL 1.121"><gap reason="lost" quantity="7" unit="character"/></lem><rdg><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ὧν</supplied></rdg></app> τὰ μέτρα <app type="editorial"><lem resp="BL 1.121"><unclear>ὧν</unclear></lem><rdg><supplied reason="lost">κα</supplied>ὶ</rdg></app> γείτνιαι καθὼς ἡ εἰς μ<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="11"/><gap reason="lost" extent="unknown" unit="character"/> τραπέζης καὶ <choice><reg>βεβαιώσω</reg><orig>βεβαιώσωι</orig></choice> πάσῃ βεβαιώσει <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="12"/><gap reason="lost" extent="unknown" unit="character"/> καὶ ἀπὸ λαογραφίας τῶν φανησομένων διʼ αὐτῶ<supplied reason="lost">ν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="13"/><handShift new="m3"/> <gap reason="lost" extent="unknown" unit="character"/> ἠγόρασα ὡς πρόκιται.  ἔγραψε<gap reason="lost" extent="unknown" unit="character"/> </ab></div>
