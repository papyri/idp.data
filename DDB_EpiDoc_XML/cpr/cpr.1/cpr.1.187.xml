         <head n="9835" xml:lang="en">
            <date>I/II spc</date>
            <placeName>Arsinoite</placeName>
            <ref type="reprint-from">cfCPR1,173‡7`195</ref>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><ab>
    <lb n="1"/><gap reason="lost" extent="unknown" unit="line"/> 

    <lb n="1"/><gap reason="lost" extent="unknown" unit="character"/>νου ως<gap reason="lost" extent="unknown" unit="character"/>τατ<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="2"/><supplied reason="lost">ἐν τῇ π</supplied>όλει ἀγορ<supplied reason="lost">αν</supplied><gap reason="lost" quantity="3" unit="character"/> ἀπὸ <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="3"/><gap reason="lost" quantity="7" unit="character"/>ης Ἀμμων<gap reason="lost" quantity="3" unit="character"/><supplied reason="lost"> τέτ</supplied>αρτον μ<supplied reason="lost">έρος </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="4"/>τρι<gap reason="lost" quantity="5" unit="character"/> πάντων ἐπʼ ἀμ<supplied reason="lost">φ</supplied>όδου Φανησίο<supplied reason="lost">υ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="5"/>σὺν κ<gap reason="lost" quantity="3" unit="character"/><supplied reason="lost"> νό</supplied>του δημοσία πλατεῖα, βορρᾶ ἑτέρων ομο<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="6"/>τετ<supplied reason="lost">άχ</supplied>θαι καθὼς αἱ περὶ αὐτῶν προκτήσεις περιέχουσι <supplied reason="lost">καὶ ἔχειν τὴν τιμὴν</supplied> 

    <lb n="7"/><supplied reason="lost">τὴν πρὸς ἀλλήλους συμ</supplied>πεφωνημένην τοῦ πεπραμένου καθὼς πρόκειτα<supplied reason="lost">ι ἀργυρίου δραχμὰς</supplied> 

    <lb n="8"/><gap reason="lost" quantity="3" unit="character"/> ἑκατὸν <num value="100"/> διὰ τῆς Διδύμου τραπέζης Φανησίου καὶ βεβ<supplied reason="lost">αιώσει τῇ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="9"/>καὶ τοῖς παρʼ α<supplied reason="lost">ὐτῆς</supplied> τὸ πεπραμένον τέταρτον μέρος τ<supplied reason="lost">ῆς οἰκίας </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="10"/><supplied reason="lost">ἀνέπα</supplied>φον καὶ ἀνενεχύραστον καὶ <choice><reg>ἀνεπιδάνειστον</reg><orig>ἀνεπιδάνιστον</orig></choice> καὶ καθα<supplied reason="lost">ρὸν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="11"/>ἀπό <choice><reg>τε</reg><orig>δε</orig></choice> λαογραφιῶν μεχρὶ ἑτέρας κατʼ οἰκίαν ἀπογρ<supplied reason="lost">αφῆς </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="12"/>μηδέ<supplied reason="lost">να ἐ</supplied>μποιούμενον αὐτοῦ μηδὲ μέρους μηδε<supplied reason="lost">νὸς </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> εἰσοδεύ</supplied>

    <lb n="13" break="no"/>οντας καὶ ἐξοδεύοντας καὶ καθαιροῦντας καὶ <app type="editorial"><lem resp="BL 8.99">ἀνοικοδ<supplied reason="lost">ομοῦντας</supplied></lem><rdg><choice><reg>ἀντικατ<supplied reason="lost">αλάσσοντας</supplied></reg><orig>ἀντικαδ<supplied reason="lost">αλάσσοντας</supplied></orig></choice></rdg></app><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="14"/>ἃς καὶ πρὸς ταῦτα τὸν Ἀμμώνιον μὴ ἐνκαλεῖν μηδὲ ἀ<supplied reason="lost">μφισβητεῖν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="15"/><choice><reg>τιμὴν</reg><orig>τειμὴν</orig></choice> μεθʼ ἡμιολίας καὶ τὰ βλάβη καὶ ἀνηλώματα διπλᾶ καὶ <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="16"/>τῶν <expan>προγεγρ<ex>αμμένων</ex></expan> <expan>ὑπογρ<ex>αφεῖς</ex></expan> τοῦ μὲν Ἀμμωνίου Χαιρήμων Χαιρήμονος <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="17"/><handShift new="m2"/> Ἀλ<supplied reason="lost">έξ</supplied>α<supplied reason="lost">νδρ</supplied>ος Διοσκόρου ὁμολογῶ πεπρακέναι τὸ προκ<supplied reason="lost">είμενον </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="18"/>Ἀμμωνίου τέταρτον μέρος οἰκίας κοινὸν καὶ ἀδιαίρετον πρὸς <gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> μετὰ παντὸς αὐτῶν</supplied> 

    <lb n="19"/>τοῦ δικαίου ὧν καὶ αἱ γειτνίαι πρόκινται καὶ ἀπέχω τὴν <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="20"/>ἀμφόδου Φανησίου καὶ βεβαιώσω πάσῃ <choice><reg>βεβαιώσει</reg><orig>βεβαιώσι</orig></choice> καὶ καθαρὰ <supplied reason="lost">παρέξω ὡς</supplied> πρόκι<supplied reason="lost">ται </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="21"/>σα Νικάριον Ἀμμωνίου μετὰ κυρίου τοῦ ἀδελφοῦ Σαρ<gap reason="lost" quantity="4" unit="character"/> <choice><reg>ἐώνημαι</reg><orig>ἐώνημε</orig></choice> <gap reason="illegible" quantity="1" unit="character"/>ν<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="22"/>ἔγραψα ὑπὲρ αὐτῶν Τιμόθεως Μάρωνος μὴ <choice><reg>εἰδότων</reg><orig>ἰδότων</orig></choice> γράμματα. </ab></div>
