         <head n="31854" xml:lang="en">
            <date>III spc</date>
            <placeName>Arsinoite</placeName>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><ab>
    <lb n="1"/>ἔτους πέμπτου Αὐτοκράτορος Καίσαρος Μάρκου <supplied reason="lost" cert="low">Αὐρηλίου </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ἱερέ</supplied>

    <lb n="2" break="no"/>ων τῶν ὄντων ἐν Ἀλεξανδρείᾳ καὶ τῶν ἄλλων <gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ἐπι</supplied>

    <lb n="3" break="no"/>τηρητῶν ἀγορανομίας <space extent="unknown" unit="character"/> τοῦ ὑπὲρ Μέμφι<supplied reason="lost" cert="low">ν Ἡρακλεοπολίτου· ὁμολογεῖ ἡ δεῖνα </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="4"/>μητρὸς Νεμεσιαίνης ἀφʼ Ἡρακλέους πόλεως <gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> πέν</supplied>

    <lb n="5" break="no"/>τε οὐλὴ μετώπῳ πεπρακέναι Αὐρηλίᾳ Αρουση<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="6"/>μέσῃ <choice><reg>μελίχρῳ</reg><orig>μελιχρος</orig></choice> μετὰ κυρίου Αὐρηλίου <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="7"/>ἀπὸ τῶν κατηντηκότων <supplied reason="lost">εἰ</supplied>ς <unclear>αὐτὸν</unclear> ἀπὸ <app type="editorial"><lem resp="BL cf. 2.2.51"><supplied reason="lost">κληρονομίας </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ἀπὸ τῆς</supplied></lem><rdg><gap reason="lost" extent="unknown" unit="character"/></rdg></app> 

    <lb n="8"/>αὐτῆς πόλεως τροφίμης λόγου μητρὸς <add place="above">τοῦ παν<supplied reason="lost">τὸς </supplied><gap reason="lost" extent="unknown" unit="character"/></add> ἀκολ<supplied reason="lost">ούθως </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> κατὰ διαθήκην <app type="editorial"><lem resp="BL cf. 2.2.51">τῆσδε</lem><rdg/></app> τῆς</supplied> 

    <lb n="9"/><choice><reg>βασιλείας</reg><orig>βασιλίας</orig></choice> λυθεῖσαν ἐπὶ τόπων διʼ ἧς κληρον<supplied reason="lost" cert="low">όμον αὐτὴν <app type="editorial"><lem resp="BL 2.2.51">κατέλιπε <gap reason="illegible" extent="unknown" unit="character"/></lem><rdg>κατέστησε <gap reason="illegible" extent="unknown" unit="character"/></rdg></app></supplied> 

    <lb n="10"/>ἃ ἐνεβάδευσεν ἡ προγεγραμμένη Ἀρτεμ<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="11"/>Ἀρτεμιδώρου Ἁρποκρατίωνος τοῦ καὶ Π<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="12"/>ἀπὸ οἰκίας καὶ αὐλῆς <add place="above">ἐν ᾗ φρέαρ</add> καὶ αἰθρί<supplied reason="lost">ου </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="13"/><gap reason="lost" quantity="3" unit="character"/>ης ἀφʼ ἧς οἰκίας μετὰ ἐμβαδευθε<supplied reason="lost">ίσης </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="14"/>τῇ στέγῃ <app type="editorial"><lem resp="cpr 1.298">συμπόσιον</lem><rdg>συμπόδιον</rdg></app> καὶ προστάδα <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="15"/><add place="above">καὶ φρέαρ</add> καὶ τοῖς ἄλλοις χρηστηρίοις πᾶσι <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="16"/>τιῳ τῷ καὶ Ἡρακλείδῃ ἀπὸ τ<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="17"/><supplied reason="lost">προγεγραμμ</supplied>ένα ἅπαντα <supplied reason="lost">ἀπὸ ἐδ</supplied>άφους <supplied reason="lost">μέχρι παντὸς ὕψους </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> γείτονες </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="18"/><supplied reason="lost">νότου</supplied> δημοσία ῥύμη, β<supplied reason="lost">ορρ</supplied>ᾶ τειχο<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="19"/><gap reason="lost" extent="unknown" unit="line"/> </ab></div>
