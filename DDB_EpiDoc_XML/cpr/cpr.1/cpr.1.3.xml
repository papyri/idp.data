    <lb n="2"/><gap reason="lost" extent="unknown" unit="character"/><gap reason="illegible" quantity="3" unit="character"/>ομολ<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="3"/><gap reason="lost" quantity="5" unit="character"/><supplied reason="lost"> καὶ ἐσχηκέναι</supplied> παρὰ τῆς Αὐρηλίας <app type="editorial"><lem resp="BL 1.111"><supplied reason="lost">Ἀρτέμιτος </supplied><gap reason="lost" extent="unknown" unit="character"/></lem><rdg><supplied reason="lost">τὸ παραχωρητικὸν κεφάλαιον ἅπαν</supplied></rdg></app> 

    <lb n="4"/><app type="editorial"><lem resp="BL 1.111"><gap reason="lost" extent="unknown" unit="character"/>ξ<gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><unclear>τη</unclear> Ἀρ<supplied reason="lost" cert="low">σινοίτου </supplied><gap reason="lost" extent="unknown" unit="character"/>  

    <lb n="5"/><gap reason="lost" extent="unknown" unit="character"/> μερ<unclear>ίδ</unclear>ι το<gap reason="illegible" quantity="2" unit="character"/> <unclear>Διο</unclear>σκόρου α<gap reason="lost" extent="unknown" unit="character"/></lem><rdg><supplied reason="lost">ὃ ἔστιν ἀργυρίου σεβαστοῦ νο</supplied>μίσματος δρα<supplied reason="lost">χμαὶ δισχίλιαι <num value="2000"/> προσομολογεῖ δὲ ἡ παραχ</supplied>ωροῦσα <supplied reason="lost">Αὐρηλία Ἁρποκρατίαινα</supplied></rdg></app> 

    <lb n="6"/><app type="editorial"><lem resp="BL 1.111"><gap reason="lost" extent="unknown" unit="character"/>α<gap reason="illegible" quantity="3" unit="character"/> Αὐρηλιο<gap reason="lost" extent="unknown" unit="character"/></lem><rdg><supplied reason="lost">ἀπεσχηκέναι τὴν αὐτὴν τιμὴν παρὰ τοῦ κυ</supplied>ρίου Αὐρηλίο<supplied reason="lost">υ Κεφαλᾶ</supplied></rdg></app> 

    <lb n="7"/><supplied reason="lost">ἐκ πλήρους διὰ χειρὸς</supplied> <app type="editorial"><lem resp="BL 1.111"><unclear>ἐξ</unclear> <unclear>οἴκου</unclear></lem><rdg>ἐξ <supplied reason="lost">οἴκου</supplied></rdg></app> <supplied reason="lost">καὶ πα</supplied>ρέξεται αὐτῇ <supplied reason="lost">ταύτην καθαρὰν</supplied> 

    <lb n="8"/><supplied reason="lost">ἀπό τε ἄλλων παραχωρήσ</supplied>εων ἢ ἑτέρων οἰκονομιῶν κ<supplied reason="lost">αὶ ἀπὸ παντὸς ἐπε</supplied>

    <lb n="9" break="no"/><supplied reason="lost">λευσομένου ἢ ἐμποιησο</supplied>μένου ἀπὸ τοῦ νῦν ἐπ<supplied reason="lost">ὶ τὸν ἅπαντα <app type="editorial"><lem resp="BL 1.111">χρόνον</lem><rdg>καὶ ἀεὶ χρόνον</rdg></app></supplied>, 

    <lb n="10"/>βεβα<supplied reason="lost">ιώσει δὲ πάσῃ βεβαιώσει ἐνέ</supplied>χεσθαι αὐτὴν π<supplied reason="lost">αραβᾶσαν τῷ</supplied> 

    <lb n="11"/>ὡρισ<supplied reason="lost">μένῳ κατὰ τῶν παρασυ</supplied>νγραφούντων ἐ<supplied reason="lost">πιτίμῳ καὶ μηδὲν</supplied> 

    <lb n="12"/>ἧσσον <supplied reason="lost">τὰ διωμολογ</supplied>ημένα κύρια εἶναι.  περὶ δὲ τοῦ τ<supplied reason="lost">αῦτα ὀρθῶς καὶ </supplied><app type="editorial"><lem resp="BL 1.111"><supplied reason="lost">κα</supplied>

    <lb n="13" break="no"/>λῶς</lem><rdg>καλῶσ</rdg></app> γεγενῆσθ<supplied reason="lost">αι ἐπε</supplied>ρωτη<supplied reason="lost">θέντες</supplied> ὡμολόγησαν. ὁ δὲ <app type="editorial"><lem resp="BL 1.111">χ<supplied reason="lost">ρηματισμὸς </supplied><gap reason="lost" quantity="2" unit="character"/><unclear>α</unclear><gap reason="illegible" quantity="2" unit="character"/> <unclear>ἐτε</unclear>

    <lb n="14" break="no"/>λειώθη</lem><rdg>χ<supplied reason="lost">ρηματισμὸς</supplied> ἐτελείωθη</rdg></app> κινδύνῳ αὐτῶν <supplied reason="lost">πρωτο</supplied>πραξίας οὔσης τῷ δη<supplied reason="lost">μοσίῳ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="15"/><handShift new="m2"/> Αὐρηλία <app type="editorial"><lem resp="BL 1.111">Σαρ<gap reason="lost" quantity="10" unit="character"/><supplied reason="lost"> χωρὶς</supplied></lem><rdg>Ἁρ<supplied reason="lost">ποκρατίαινα μετὰ</supplied></rdg></app> κυρίου χρηματίζουσα 

    <lb n="16"/>πέπρακα τῇ Αὐρ<supplied reason="lost">ηλίᾳ</supplied> Ἀρτέμιτι τὸ προκίμε

    <lb n="17" break="no"/>νον τρίτον μ<unclear>έ</unclear>ρο<supplied reason="lost">ς οἰκί</supplied>ας <choice><reg>καὶ</reg><orig>κὲ</orig></choice> αὐλῆς <choice><reg>καὶ</reg><orig>κὲ</orig></choice> τὰ συν

    <lb n="18" break="no"/>κύροντα πάντα <supplied reason="lost">καὶ ἀπ</supplied>έσχον τὴν τιμὴν 

    <lb n="19"/><choice><reg>ἀργυρίου</reg><orig>αὐγυρίου</orig></choice> δραχμ<supplied reason="lost">ὰς δι</supplied>σχιλίας <num value="2000"/> <supplied reason="omitted">ἃς</supplied> <choice><reg>καὶ</reg><orig>κὲ</orig></choice> <choice><reg>αὐτόθι</reg><orig>αὐτότι</orig></choice> 

    <lb n="20"/>μετεβαλόμην Αὐρηλίῳ <supplied reason="omitted">Ἡρακλείῳ τῷ καὶ Ἀρτεμιδώρῳ</supplied> <choice><reg>δανειστῇ</reg><orig>δανιστῇ</orig></choice> μου δι

    <lb n="21" break="no"/>ὰ τῆς τραπέζης <choice><reg>καὶ</reg><orig>κὲ</orig></choice> <choice><reg>βεβαιώσω</reg><orig>βαιβεώσω</orig></choice> ὡς <subst><add place="inline">πρόκιται</add><del rend="corrected">προαιται</del></subst> 

    <lb n="22"/><handShift new="m1"/> <expan>Αὐρ<ex>ήλιος</ex></expan> Ἡράκλειος ὁ καὶ Ἀρτε<supplied reason="lost">μί</supplied>δωρος <app type="editorial"><lem resp="BL 1.111">ψιλὸς<certainty match=".." locus="value"/></lem><rdg>ψ<gap reason="illegible" quantity="1" unit="character"/>λισ</rdg></app> <choice><reg>δανειστὴς</reg><orig>δανιστὴς</orig></choice> μετεβλή

    <lb n="23" break="no"/>θησαν μοι διὰ τῆς <choice><reg>τραπέζης</reg><orig>δραπ<supplied reason="lost">έζη</supplied>ς</orig></choice> αἱ δραχμαὶ 

    <lb n="24"/><supplied reason="lost">δισχίλιαι <num value="2000"/> ὡς πρό</supplied>

    <lb n="25" break="no"/>κιται. <expan>Αὐρήλ<ex>ιος</ex></expan> Ἁρποκρατίων <gap reason="lost" quantity="6" unit="character"/> <expan><supplied reason="lost">μ</supplied>ετεβλήθ<ex>ησαν</ex></expan> διʼ ἐμοῦ αἱ τοῦ <expan>ἀργ<ex>υρίου</ex></expan> 

    <lb n="26"/><expan>δραχ<ex>μαὶ</ex></expan> <choice><reg>δισχίλιαι</reg><orig>δισχίλειαι</orig></choice> <num value="2000"/> ὡς <expan>πρ<ex>ό</ex>κ<ex>ειται</ex></expan>.  Αὐρηλία Ἄρτεμεις μετὰ <expan>κυρίο<ex>υ</ex></expan> 

    <lb n="27"/>ἐμοῦ Αὐρηλίου Κεφα<supplied reason="lost">λᾶ</supplied> ἐώνημαι ὡς <choice><reg>πρόκειται</reg><orig>πρόχειται</orig></choice>. </ab></div>
