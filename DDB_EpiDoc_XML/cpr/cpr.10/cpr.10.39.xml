         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r" type="textpart"><!--milestone unit="4"--><ab>

    <lb n="1"/><supplied reason="lost">ὑπ</supplied><unclear>ατεί</unclear><supplied reason="lost">ας</supplied> <expan>Φλ<ex>αουίων</ex></expan> <supplied reason="lost">Μα</supplied>ξ<supplied reason="lost">ίμου τὸ <num value="2">β</num> καὶ Πατερίου τῶν</supplied> 

    <lb n="2"/>λαμπροτάτων Ἁθὺρ <num value="16">ιϛ</num> τῆ<unclear>ς</unclear> <supplied reason="lost">εὐτυχοῦς δωδεκάτης</supplied> 

    <lb n="3"/><expan>ἰνδικ<ex>τίωνος</ex></expan> ἐν Ἡρακλέους πόλει Φλαυ<supplied reason="lost">ίῳ </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> καθοσιωμέ</supplied>

    <lb n="4" break="no"/>νῳ μαγιστριανῷ τῶν θείω<unclear>ν</unclear> <supplied reason="lost">ὀφφικίων ἐν τῷ</supplied> 

    <lb n="5"/>Ἡρακλεοπολίτῃ Αὐρήλιο<unclear>ς</unclear> <supplied reason="lost">Ἀνδρόνικος Νείλου</supplied> 

    <lb n="6"/><unclear>ἀπ</unclear><supplied reason="lost">ὸ τ</supplied>ῆς μεγ<unclear>άλη</unclear>ς πόλεως Ἀλεξ<supplied reason="lost">ανδρείας</supplied> 

    <lb n="7"/><supplied reason="lost">ἐν Ἡ</supplied>ρακλέους πό<supplied reason="lost">λει</supplied> κ<unclear>ον</unclear>διτάριος ἑξ<supplied reason="lost">ῆς ὑπογράφων ἰδίοις</supplied> 

    <lb n="8"/><supplied reason="lost">γράμ</supplied>μασιν <space extent="unknown" unit="character"/> χαίρειν. ὁ<supplied reason="lost">μο</supplied>λ<supplied reason="lost">ο</supplied>γ<supplied reason="lost">ῶ</supplied> με <choice><reg>ἔχειν</reg><orig><unclear>ἔ</unclear>χιν</orig></choice> 

    <lb n="9"/><gap reason="lost" extent="unknown" unit="character"/><gap reason="illegible" quantity="1" unit="character"/> παρὰ σοῦ τὴν μεταξὺ συμφωνηθεῖ

    <lb n="10" break="no"/><supplied reason="lost">σαν </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> τιμὴ</supplied>ν κούφων εὐαρέστων 
<choice><reg>δισχιλί<lb n="11" break="no"/><supplied reason="lost">ων</supplied></reg><orig>δισχειλί<lb n="11" break="no"/><supplied reason="lost">ων</supplied></orig></choice> <supplied reason="lost">διακοσίων <num value="2200"/> </supplied><gap reason="lost" extent="unknown" unit="character"/><gap reason="illegible" quantity="1" unit="character"/>ν<gap reason="illegible" quantity="1" unit="character"/> ἀπὸ λόγου ῥύσεως δωδεκά

    <lb n="12" break="no"/><supplied reason="lost">της <expan>ἰνδικ<ex>τίωνος</ex></expan> </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="13"/><gap reason="lost" extent="unknown" unit="character"/> καὶ <unclear>ὁ</unclear>μ<supplied reason="lost">οίως </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="14"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ἕ</supplied>τ<supplied reason="lost">ερα</supplied> <unclear>κ</unclear>οῦφα διακόσια <num value="200"/> μό<supplied reason="lost">να <expan>γ<ex>ίνεται</ex></expan> κοῦφα <num value="2400">Βυ</num> ἅπερ ἐπάναγ</supplied>

    <lb n="15" break="no"/>κες ἀποδώσω σοι μηνὶ Μεσορὴ ἀρχῇ τῆς εὐτυχοῦς <choice><reg>τρεῖς</reg><orig>τρῖς</orig></choice> 

    <lb n="16"/>καὶ δεκάτης <expan>ἰνδικ<ex>τίωνος</ex></expan> εἰς ῥῦσιν τῆς αὐτῆς <num value="13">ιγ</num> <expan>ἰνδικ<ex>τίωνος</ex></expan> ἀνυπερθέτως 

    <lb n="17"/>καὶ ἄνευ πάσης ἀντιλογίας καὶ οὐκ ἔξεσταί μοι ἄλλην αἰτήσασθαι 

    <lb n="18"/>προθεσμίαν περὶ τὴν <choice><reg>ἀπόδοσιν</reg><orig>ἀπόδωσιν</orig></choice> τῆς πράξεώς σοι οὔσης καὶ 

    <lb n="19"/>τοῖς παρὰ σοῦ ἔκ τε ἐμοῦ καὶ ἐκ τῶν ὑπαρχόντων μοι πάντων 

    <lb n="20"/>παντοίων παντὸς εἴδο<supplied reason="lost">υς</supplied> πράσσοντι τὸ <choice><reg>γραμματεῖον</reg><orig>γραμματῖον</orig></choice> κύριον καὶ 

    <lb n="21"/><expan>ἐπερ<ex>ωτηθεὶς</ex></expan> <expan>ὡ<ex>μολόγησα</ex></expan> <handShift new="m2"/> Αὐρήλιος Ἀνδρόνικος Νίλου 

    <lb n="22"/>ὁ προκίμενος ἔσχον τὴν τιμὴν κούφων 

    <lb n="23"/>δισχιλίων διακοσίων <num value="2200"/> <subst><add place="inline">εὐαρέστων</add><del rend="corrected">εαραρεστων</del></subst> ἔχω δὲ καὶ 

    <lb n="24"/>ἀπὸ λόγου ἕτερα κοῦφα διακόσια <num value="200"/> καὶ ἀπο

    <lb n="25" break="no"/>δῶ σοι τῇ προθεσμ<supplied reason="lost">ίᾳ ὡς πρόκειται.</supplied> 
