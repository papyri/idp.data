         <head n="40964" xml:lang="en">
            <date>VI/VII spc</date>
            <placeName>Arsinoiton Polis, Krokodilopolis, Ptolemais Euergetis</placeName>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><ab>
    <lb n="1"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost">                  κα</supplied>τὰ <unclear>ἑξ</unclear><supplied reason="lost">ήκοντα <num value="60"/></supplied> 

    <lb n="2"/><gap reason="lost" extent="unknown" unit="character"/>βεβαίως το<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="3"/><gap reason="lost" extent="unknown" unit="character"/><gap reason="illegible" quantity="4" unit="character"/>μου ἐπὶ τόκῳ δεδ<supplied reason="lost">αν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="4"/><gap reason="lost" extent="unknown" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="4" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="2" unit="character"/> ἕκαστος ἡμῶν τ<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/><gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" extent="unknown" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="5"/>τοῦ γενομένου μοι παρʼ αὐτῶν ἐγγράφου γραμματείου ὅπερ καὶ λα<unclear>βοτ</unclear><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="6"/>αὐτοῦ τὴν δύναμιν περιέχοντος ὡς ἀνωτέρω λέλεκται. ἠγόρασαν οὖν <gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="7"/>τὸ <choice><reg>λογάριον</reg><orig>λογαριν</orig></choice> στιππύα ἐν ταύτῃ τῇ πόλει κατὰ ἑξήκοντα <num value="60"/> λίτρας τοῦ ν<supplied reason="lost">ομ</supplied><unclear>ί</unclear><supplied reason="lost">σματος</supplied> 

    <lb n="8"/>οἱ αὐτοὶ Χριστοφόρος καὶ Νααραῦς εἰς Ταμιᾶθιν καὶ ἐκεῖσε γεγεν<supplied reason="lost">η</supplied><unclear>μ</unclear><supplied reason="lost">ένα σ</supplied><unclear>τ</unclear><supplied reason="lost">ιππύα</supplied> 

    <lb n="9"/>νομίσματα εἰκοσιπέντε τρίτον <num value="25"/><num value="1/3"/> κατὰ εἰκοσιμίαν <num value="21"/> λίτρας τοῦ νομίσματος καὶ ταῦτα λ<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="10"/>παρὰ τοῦ Χριστοφόρου ἤνεγκεν ἐν ταύτῃ τῇ Ἀρσινοιτῶν πόλει μετʼ ἐπιστολῆς τοῦ Χριστ<supplied reason="lost">οφόρου</supplied> 

    <lb n="11"/>ἐπιτραπεὶς παρʼ αὐτοῦ ἐργάσασθαι αὐτὸν τὸν Νααραῦν εἰς τὰ αὐτὰ εἰκοσιπέντε τρίτον <num value="25"/><num value="1/3"/> <expan>νομίσμ<ex>ατα</ex></expan> 

    <lb n="12"/>ἐν Θηβαίδι. αὐτὸς δὲ ὁ Χριστοφόρος ἵνα παραλάβῃ τὰ ὑπόλοιπα στιππύα τῆς αὐτῆς <expan>πραγμ<ex>ατείας</ex></expan> 

    <lb n="13"/>καὶ ἀπέλθῃ εἰς τὴν ἔξω χώραν κἀκεῖσε διαπολῆσαι αὐτὰ προσδοκῶν πολὺν  κέρδος εὑρεῖν ὅστις 

    <lb n="14"/>τ<unclear>άχ</unclear>α <choice><reg>παραχειμασθεὶς</reg><orig>παραχιμασθεὶς</orig></choice> ἐν <choice><reg>θαλάσσῃ</reg><orig>θαλάσῃ</orig></choice> ἐναυάγησεν καὶ τὸ ἀλλότριον ἤτοι πραγματεῖα <choice><reg>ἀνωτέρω</reg><orig>ἀνοτέρω</orig></choice> δὲ <expan>εἰρημέ<ex>να</ex></expan> 

    <lb n="15"/><gap reason="lost" extent="unknown" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/>ν ἐπὶ τὰ ἐνταῦθα ὑπέδειξεν τῷ αὐτῷ Μηνᾷ τα<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="16"/><gap reason="lost" extent="unknown" unit="character"/><unclear>ε</unclear>νφ<gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

