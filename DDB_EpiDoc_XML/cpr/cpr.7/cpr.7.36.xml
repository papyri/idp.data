         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r" type="textpart"><ab>

    <lb n="1"/>Αὐρήλιος Γοῦν<supplied reason="lost">θος Παμούνιος μητρὸς </supplied><gap reason="lost" quantity="14" unit="character"/><supplied reason="lost">           ὡς ἐτῶν </supplied><gap reason="lost" quantity="1" unit="character"/> 

    <lb n="2"/>οὐλὴ δακτύλῳ λιχα<unclear>ν</unclear><supplied reason="lost">ῷ ἀριστερᾶς χειρὸς ἀπὸ κώμης Σκιν</supplied>

    <lb n="3" break="no"/>επώεω<unclear>ς</unclear> τοῦ <unclear>Λ</unclear>υκοπ<unclear>ολ</unclear>ί<supplied reason="lost" cert="low">του νομοῦ Αὐρηλίῳ </supplied><gap reason="lost" quantity="5" unit="character"/> 

    <lb n="4"/><hi rend="diaeresis">Ἰ</hi>σιδώρου ἀπὸ Ἑρμοῦ πόλεω<unclear>ς</unclear> <supplied reason="lost">τῆς λαμπροτάτης χαίρειν.</supplied> 

    <lb n="5"/>ὁμολογῶ πεπρ<unclear>ακ</unclear><supplied reason="lost">ένα</supplied><unclear>ι</unclear> <supplied reason="lost">σοι ἐπὶ τὸν ἀεὶ χρόνον ὄνον</supplied> 

    <lb n="6"/>μου ἄρρενα ἀμμόχρωμον <unclear>π</unclear><supplied reason="lost">ρωτοβόλον ὃν παραδέδω</supplied>

    <lb n="7" break="no"/><supplied reason="lost">κ</supplied>ά σοι ἐν τῇ αὐτῇ Ἑρμοῦ <unclear>π</unclear>όλει τιμ<unclear>ῆ</unclear><supplied reason="lost">ς τῆς πρὸς ἀλλήλους</supplied> 

    <lb n="8"/>συμπ<unclear>ε</unclear>φωνημένης ἀργυρίου Σεβα<supplied reason="lost">στοῦ νομίσματος</supplied> 

    <lb n="9"/>ταλάντων τεσσεράκοντα <expan>γί<ex>νεται</ex></expan> <expan>ἀργυ<ex>ρίου</ex></expan> <expan><ex>τάλαντα</ex></expan> <num value="40">μ</num>, ἅ<unclear>π</unclear><supplied reason="lost">ερ αὐτόθι δέ</supplied><unclear>χ</unclear><supplied reason="lost">ο</supplied>

    <lb n="10" break="no"/>μαι παρὰ σοῦ διὰ χειρὸς ἐκ πλήρους καὶ βεβ<unclear>α</unclear><supplied reason="lost">ιώσω σοι</supplied> 

    <lb n="11"/>περὶ αὐτοῦ πάσῃ βεβαιώ<unclear>σ</unclear>ει ἀπὸ παντὸς <unclear>δ</unclear><supplied reason="lost">ιὰ π</supplied><unclear>αντὸ</unclear><supplied reason="lost">ς</supplied> 

    <lb n="12"/>τοῦ ἐπελευσομένου ἢ ἀντιπ<supplied reason="lost">ο</supplied><unclear>ιησ</unclear>ομέ<unclear>νου</unclear>. ἡ πρᾶσις 

    <lb n="13"/>κυρία καὶ ἐπερωτηθεὶς <unclear>ὡμ</unclear><supplied reason="lost">ολ</supplied>όγησα. <hi rend="diaeresis">ὑ</hi>πατείας 

    <lb n="14"/><hi rend="diaeresis">Ἰ</hi>ουνίου Βάσ<unclear>σ</unclear>ου καὶ Φλαυ<hi rend="diaeresis">ί</hi>ου Ἀ<unclear>β</unclear><supplied reason="lost">λα</supplied><unclear>βίου</unclear> <unclear>τῶ</unclear>ν λαμπροτά

    <lb n="15" break="no"/>των ἐπάρ<unclear>χ</unclear><supplied reason="lost">ων</supplied> Χοιὰκ <num value="25"><unclear>κε</unclear></num>. Αὐρήλι<unclear>ος</unclear> <supplied reason="lost">Γο</supplied><unclear>ῦν</unclear>θος 

    <lb n="16"/>Παμούνιος π<unclear>έ</unclear><supplied reason="lost">π</supplied><unclear>ρακ</unclear>α τ<unclear>ὸ</unclear>ν ὄνο<supplied reason="lost">ν</supplied> <unclear>κ</unclear>αὶ <unclear>ἀ</unclear><supplied reason="lost">πέ</supplied><unclear>σ</unclear>χον 

    <lb n="17"/>τὴν τιμὴν κα<supplied reason="lost">ὶ</supplied> <unclear>β</unclear><supplied reason="lost">εβαι</supplied><unclear>ώ</unclear>σω <unclear>ὡς</unclear> <unclear>πρ</unclear><supplied reason="lost">όκ</supplied><unclear>ε</unclear>ιται. <unclear>Αὐ</unclear><supplied reason="lost">ρήλι</supplied><unclear>ο</unclear><supplied reason="lost">ς</supplied> 

    <lb n="18"/>Θεόδωρος Δ<unclear>ί</unclear>ω<unclear>ν</unclear><supplied reason="lost" cert="low">ος</supplied><supplied reason="lost"> ἔ</supplied>γραψα <subst><add place="inline">ὑπὲρ</add><del rend="corrected"><hi rend="diaeresis">υ</hi>ιερ</del></subst> α<unclear>ὐ</unclear><supplied reason="lost">τ</supplied><unclear>οῦ</unclear> <unclear>γ</unclear><supplied reason="lost">ράμμα</supplied>

    <lb n="19" break="no"/>τα μὴ εἰδότος. 

