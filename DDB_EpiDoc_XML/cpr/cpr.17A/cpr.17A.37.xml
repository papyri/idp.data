         <div xml:lang="grc" type="edition" xml:space="preserve"><ab>
    <lb n="1"/><supplied reason="lost">Αὐρήλιος</supplied> <unclear>Ἀ</unclear>μμ<supplied reason="lost">ώνιος </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="2"/>Ἀσκ<supplied reason="lost">λη</supplied>πιάδῃ <supplied reason="lost"><expan>πραιπ<ex>οσίτῳ</ex></expan> <num value="15">ιε</num> πάγου χαίρειν.</supplied> 

    <lb n="3"/>προσέταξ<unclear>εν</unclear> ἡ ἀνδρεί<supplied reason="lost">α τοῦ κυρίου μου διασημοτάτου ἡγεμόνος</supplied> 

    <lb n="4"/>Φλαυίου Ἠουασίου διʼ ὧν <supplied reason="lost">ἔγραψεν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="5"/><gap reason="illegible" quantity="3" unit="character"/><gap reason="lost" quantity="3" unit="character"/>ει καὶ τὰς κριθὰ<supplied reason="lost">ς </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="6"/>ουμ<unclear>αι</unclear> <supplied reason="lost">τὸ</supplied><unclear>ν</unclear> κανόνα Ν<unclear>ο</unclear>ε<unclear>μ</unclear><supplied reason="lost" cert="low">βρι </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="7"/>Καλαν<unclear>δῶ</unclear>ν  <choice><reg>Ὀκτωβρίω<supplied reason="lost">ν</supplied></reg><orig>Ὀκτωμβρί<unclear>ω</unclear><supplied reason="lost">ν</supplied></orig></choice> <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="8"/>ὡς ἐκ θείου προστάγμ<supplied reason="lost">ατος καὶ γραμμάτων τῶν</supplied> 

    <lb n="9"/>κυρίων μ<supplied reason="lost">ου</supplied> τῶν λαμ<supplied reason="lost">προτάτων </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="10"/><expan><ex>τάλαντον</ex></expan> ἕν <num value="1"/>, τ<unclear>ὸ</unclear>ν <unclear>ἀμελ</unclear>οῦντα <unclear>δ</unclear><supplied reason="lost">ὲ τῶν γραμμάτων </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="11"/>τῶν αὐτῶν κυρίων μ<supplied reason="lost">ου </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="12"/>ἐπιστέλλεταί σοι διὰ τ<supplied reason="lost">οῦδε τοῦ ἐπιστάλματος </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="13"/>γειν <subst><add place="inline">βουλη<unclear>σ</unclear>κη<unclear>π</unclear></add><del rend="corrected">βου<del rend="erasure">λ</del>λη<unclear>σ</unclear>κη<unclear>π</unclear></del></subst> <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="14"/>ὁρισθείσης προθεσμ<supplied reason="lost">ίας </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="15"/>καὶ τῶν κριθῶν τῶν <gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="16"/>ὡς προστέτακται <unclear>δ</unclear><supplied reason="lost">ιὰ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="17"/>ἑκάστοτε περι<unclear>ποηθω</unclear><gap reason="illegible" quantity="3" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

