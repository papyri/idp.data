    <lb n="1"/><expan>Ἀναστασαρ<ex>ίῳ</ex></expan> 

    <lb n="2"/>τῇ κυρ<add place="above">ί</add>ᾳ μου <g type="middot"/> <expan>μητρ<ex>ὶ</ex></expan> 

    <lb n="3"/><subst><add place="inline">Σιλβανὸς</add><del rend="corrected">σιλβακος</del></subst> <expan>χ<ex>αίρειν</ex></expan>. 

    <lb n="4"/>τὸ <choice><reg>κολόβιον</reg><orig>κολοβιω</orig></choice>, 

    <lb n="5"/><choice><reg>ὃ</reg><orig>ἃ</orig></choice> <choice><reg>ἔχεις</reg><orig>ἔχις</orig></choice> <choice><reg>αὐτὸ</reg><orig>αὐτὰ</orig></choice> 

    <lb n="6"/>ἐνεχύρου οἴνου 

    <lb n="7"/><expan>δι<ex>πλοῦ</ex></expan> <num value="1">α</num> <choice><reg>τοῦτο</reg><orig>τω</orig></choice> 

    <lb n="8"/>λαβὲ αὐτὴ 

    <lb n="9"/>παρὰ Παχύμιο<add place="above">ς</add> 

    <lb n="10"/>καὶ δὸς <add place="above">τῷ Πασῖρι</add> αὐτῷ 

    <lb n="11"/><choice><reg>αὐτὸ</reg><orig>αὐτὰ</orig></choice>. 

    <lb n="12"/><space extent="unknown" unit="character"/> 

    <lb n="12"/>ἐρρῶσ

    <lb n="13" break="no"/>θαί σε εὔχο

    <lb n="14" break="no"/>μαι πολλοῖς 

