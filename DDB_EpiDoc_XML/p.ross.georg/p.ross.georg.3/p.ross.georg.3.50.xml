         <head n="40992" xml:lang="en">
            <date>AD 613/628</date>
            <placeName>Arsinoiton Polis, Krokodilopolis, Ptolemais Euergetis</placeName>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r" type="textpart"><ab>

    <lb n="1"/><gap reason="lost" extent="unknown" unit="line"/> 

    <lb n="1"/><unclear>η</unclear><gap reason="lost" quantity="11" unit="character"/>ων τ<gap reason="lost" quantity="14" unit="character"/> 

    <lb n="2"/>Ἐ<supplied reason="lost">π</supplied>εὶφ <num value="26"><unclear>κ</unclear>ϛ</num> δευτέρας <expan>ἰν<ex>δικτίονος</ex></expan> ἐν <expan>Ἀρ<ex>σινόῃ</ex></expan> <g type="stauros"/> 

    <lb n="3"/><expan>Φλ<ex>αυίῳ</ex></expan> Θεοδωρακίῳ τῳ ἐνδοξοτάτῳ <expan>στρατηλ<ex>ά</ex>τ<ex>ῃ</ex></expan> 

    <lb n="4"/>καὶ παγάρχῳ ταύτης τῆς Ἀρσινοιτῶν 

    <lb n="5"/>πόλεως Αὐρήλιος Κοσμᾶς <expan>σταβλ<ex>ί</ex>της</expan> 

    <lb n="6"/>ἀλλαγῆς ἐποικίου Παντικοῦ υἱὸς Ποῦσι 

    <lb n="7"/>ἀπὸ τῆς αὐτῆς πόλεως <expan>χ<ex>αίρειν</ex></expan>. ὁμολογῶ 

    <lb n="8"/>εἰληφέναι καὶ πεπληρῶσθαι παρʼ ὑμῶν 

    <lb n="9"/>διὰ <add place="above"><g type="stauros"/></add> Ζαχαρίου καὶ <add place="above"><g type="stauros"/></add> Ἰωαννακίου τῶν <expan>λαμπρο<ex>τάτων</ex></expan> 

    <lb n="10"/>ὑμετέρων χαρτουλαρίων· τὴν τροφὴ<unclear>ν</unclear> 

    <lb n="11"/>τῶν ὑπʼ ἐμὲ ἀλόγων, <expan><hi rend="diaeresis">ἱ</hi>ππαρίω<ex>ν</ex></expan> 

    <lb n="12"/>ἑπτὰ <num value="7"/> καὶ γα<hi rend="diaeresis">ι</hi>δαρίων <choice><reg>δύο</reg><orig>δύου</orig></choice> <num value="2"/>, τῆς αὐτῆς 

    <lb n="13"/>ἀλλαγῆς <supplied reason="lost">μ</supplied>ην<unclear>ῶν</unclear> δύ<supplied reason="lost">ο</supplied> <num value="2"/> Μεσορὴ καὶ Θὼθ 

    <lb n="14"/>τῆς παρούσης δευτέρας <expan>ἰν<ex>δικτίονος</ex></expan> εἰς πλήρη 

    <lb n="15"/>ὅ ἐστιν κριθῶν ἀρτάβας τεσσαράκοντα 

    <lb n="16"/>δύο <expan>κρ<ex>ι</ex>θ<ex>ῶν</ex></expan> <expan>ἀρτ<ex>άβας</ex></expan> <num value="42">μβ</num> καὶ πρὸς <expan>ὑμε<ex>τέραν</ex></expan> 

    <lb n="17"/>ἀσφάλειαν ταύτην πεποίημαι <expan>αὐτ<ex>ὸς</ex></expan> 

    <lb n="18"/>τὴν ἀπόδειξιν <expan>κυρ<ex>ίαν</ex></expan> οὖσαν <expan><ex>καὶ</ex></expan> <expan>ἐπερ<ex>ωτηθεὶς</ex></expan> <expan>ὡμ<ex>ο</ex>λ<ex>όγησα</ex></expan> <g type="stauros"/> 

    <lb n="19"/><g type="stauros"/> Κοσμᾶς <expan>σταβλ<ex>ί</ex>τ<ex>ης</ex></expan> στοιχεῖ μοι ὡς <expan>πρό<ex>κειται</ex></expan> <g type="stauros"/> 

    <lb n="20"/><handShift new="m2"/> <g type="stauros"/> Ἰωαννάκιος υἱὸς τοῦ <expan>μαρακ<ex>ί</ex>τ<ex>ου</ex></expan> Μηνᾶ μαρτυρῶ 

    <lb n="21"/>τῇδε τῇ <choice><reg>ἀποδείξει</reg><orig>ἀποδείξι</orig></choice> ὡς <expan>πρόκ<ex>ειται</ex></expan> <g type="stauros"/> <handShift new="m3"/> <g type="stauros"/> <hi rend="diaeresis">Ἰ</hi>ωάννης 

    <lb n="22"/>ταβουλάριος μαρτυρῶ τῇδε τῇ 

    <lb n="23"/>ἀποδείξει ὡς <expan>πρόκ<ex>ειται</ex></expan> <g type="stauros"/> <handShift new="m4"/> <g type="stauros"/> Φιλόξενος υἱὸς τοῦ τῆς 

    <lb n="24"/>μακαρίας μνήμης Θεοδωρακίου 

    <lb n="25"/><supplied reason="lost">μ</supplied><unclear>α</unclear>ρτυρῶ τῇδε τῇ <choice><reg>ἀποδείξει</reg><orig>ἀποδείξι</orig></choice> ὡς πρόκειται <g type="stauros"/> 

    <lb n="26"/><space extent="unknown" unit="character"/> 

    <lb n="26"/><gap reason="illegible" extent="unknown" unit="character"><desc>vestiges</desc></gap> υ 
</ab></div><div n="v" type="textpart"><ab>

    <lb n="27"/><expan><supplied reason="lost">ἀπόδ</supplied>ειξ<ex>ις</ex></expan> <expan>κρ<ex>ι</ex>θ<ex>ῶν</ex></expan> <expan>ἀρτ<ex>αβῶν</ex></expan> <num value="4">δ</num><supplied reason="lost"><num value="2">β</num> </supplied><expan><supplied reason="lost">γ</supplied>ενομέ<ex>νη</ex></expan> ὑπὸ Κοσμᾶ <expan>σταβλ<ex>ί</ex>τ<ex>ου</ex></expan> <expan>ἀλλαγῆ<ex>ς</ex></expan> <expan>Παντικ<ex>οῦ</ex></expan> <expan>εἰ<ex>ς</ex></expan> <expan>Φλ<ex>αύιον</ex></expan> Θεοδωράκιον τὸ<supplied reason="lost">ν </supplied><gap reason="lost" extent="unknown" unit="character"/> </ab></div></div>
