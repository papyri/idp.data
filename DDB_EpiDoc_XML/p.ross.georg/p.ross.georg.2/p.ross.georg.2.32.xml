    <lb n="1"/><gap reason="lost" extent="unknown" unit="character"/><abbr>δ</abbr> 

    <lb n="2"/><space extent="unknown" unit="character"/> 

    <lb n="1"/><supplied reason="lost">ἔτου</supplied>ς ἐνάτου Αὐτοκράτορο<supplied reason="lost">ς Καίσαρος </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> μηνὸς </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="2"/><supplied reason="lost">ἐν Πτ</supplied>ολεμα<hi rend="diaeresis">ί</hi>δι Εὐεργέτιδι τ<supplied reason="lost">οῦ Ἀρσινοείτου νομοῦ. ὁμολογεῖ Ἰσιδώρα τοῦ δεῖνος </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ὡς <expan><ex>ἐτῶν</ex></expan> </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> οὐλὴ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="3"/><gap reason="lost" quantity="3" unit="character"/><supplied reason="lost"> ἀ</supplied>ριστερᾶι μετὰ κυρίου τ<supplied reason="lost">οῦ δεῖνος </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ὡς <expan><ex>ἐτῶν</ex></expan> </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> οὐ</supplied>

    <lb n="4" break="no"/><supplied reason="lost">λὴ πή</supplied>χει δεξιῶι Μαρεῖτι Λευ<gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> πεπρακέναι τὴν Ἰσιδώραν κατὰ τήν</supplied>

    <lb n="5" break="no"/><supplied reason="lost">δε τ</supplied>ὴν ὁμολογίαν ἀπὸ τοῦ <supplied reason="lost">νῦν ἐπὶ τὸν ἅπαντα χρόνον ἀπὸ τοῦ ὑπάρχοντος τῆι</supplied> 

    <lb n="6"/><supplied reason="lost">Ἰσι</supplied>δώραι ἀγοραστοῦ ἡμίσο<supplied reason="lost" cert="low">υς μέρους οἰκίας </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> καὶ τῶν</supplied> 

    <lb n="7"/><supplied reason="lost">συν</supplied>κυρόντων πάντων ἐν <supplied reason="lost">μίᾳ <num value="1"/> σφραγῖδι </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="8"/><gap reason="lost" quantity="2" unit="character"/><supplied reason="lost"> τ</supplied>οῦ δρόμου τοῦ Σούχου βου<gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="9"/><gap reason="lost" quantity="3" unit="character"/> ἐπʼ ἀμφοτέρων δὲ τῶν <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="10"/><gap reason="lost" quantity="3" unit="character"/><gap reason="illegible" quantity="1" unit="character"/> Ἡρώδου, μετάξυ δὲ τῶ<supplied reason="lost">ν </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ἀπέχειν δὲ τὴν ισιδώ</supplied>

    <lb n="11" break="no"/><supplied reason="lost">ραν</supplied> τὴν συνκεχωρημ<unclear>έ</unclear>ν<supplied reason="lost">ην τιμὴν ἀργυρίου δραχμὰς </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="12"/><gap reason="lost" quantity="3" unit="character"/><supplied reason="lost"> ἐ</supplied>κ πλήρους διὰ τῆ<supplied reason="lost">ς ἐν </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> τραπέζης καὶ βεβαιώσειν τὴν ὁμολογοῦ</supplied>

    <lb n="13" break="no"/><supplied reason="lost">σαν κ</supplied>αὶ τοὺς παρʼ αὐτῆ<supplied reason="lost">ς πάσῃ βεβαιώσει </supplied><gap reason="lost" extent="unknown" unit="character"/> 

