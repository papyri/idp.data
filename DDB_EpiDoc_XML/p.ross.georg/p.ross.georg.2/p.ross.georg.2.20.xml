         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r" type="textpart"><ab>

    <lb n="1"/><supplied reason="lost">Λουκίωι Οὐαλε</supplied>ρ<supplied reason="lost">ί</supplied>ωι Πρό<supplied reason="lost">κ</supplied>λωι ἐπάρχω <app type="editorial"><lem resp="G. Bastianini, ANRW II.10.1 (1988) 589 n. 42, see also R. Mascellari, ZPE 207 (2018) 167 (photo)"><supplied reason="lost">Αἰγύπτου</supplied></lem><rdg><gap reason="lost" extent="unknown" unit="character"/></rdg></app> 

    <lb n="2"/><supplied reason="lost">παρὰ </supplied><gap reason="lost" quantity="8" unit="character"/>σ<gap reason="lost" quantity="2" unit="character"/>ης Σαραπίωνος ἀσ<unclear>τ</unclear>ῆς <gap reason="lost" quantity="17" unit="character"/> 

    <lb n="3"/><gap reason="lost" quantity="3" unit="character"/><supplied reason="lost"> πρὸς τοὺς ἀ</supplied>δ<supplied reason="lost">ε</supplied>λφούς μου καὶ πρὸς Ἑρμίαν ἀγ<supplied reason="lost">οραστὴν </supplied><gap reason="lost" quantity="7" unit="character"/> 

    <lb n="4"/><gap reason="lost" quantity="12" unit="character"/> πράσεως αὐτοῦ πλεῖσταί μου ἀ<unclear>ν</unclear><supplied reason="lost">τικαταστάσεις</supplied> 

    <lb n="5"/><supplied reason="lost">ἐγένοντο ἐπὶ </supplied><gap reason="lost" quantity="1" unit="character"/><unclear>ω</unclear>ρηιου <choice><reg>Κελεάριος</reg><orig>Κελεαρις</orig></choice> γενομένου ὑπ<supplied reason="lost">ομνηματογράφου</supplied> 

    <lb n="6"/><supplied reason="lost">καὶ ἐπὶ τῶν κε</supplied>κριμένων, ὃς ἀνάδαστον τὸν ἀ<supplied reason="lost">γορασμὸν ἐποίη</supplied>

    <lb n="7" break="no"/><supplied reason="lost">σεν τῆς τιμῆς</supplied> <unclear>φ</unclear>ανείσης ἀδότου κατὰ τὴν γενο<supplied reason="lost">μένην ἐξέτασιν. ὁ</supplied> 

    <lb n="8"/><supplied reason="lost">γράψας τὴν </supplied><supplied reason="lost" cert="low">οἰκ</supplied><unclear>ο</unclear>νομίαν νομικὸς Σαραπίων, ὃς <gap reason="lost" quantity="12" unit="character"/> 

    <lb n="9"/><gap reason="lost" quantity="3" unit="character"/><supplied reason="lost"> ἀκολούθως</supplied> τοῖς γενομένοις ὑπομνηματ<supplied reason="lost">ισμοῖς </supplied><gap reason="lost" quantity="8" unit="character"/> 

    <lb n="10"/><gap reason="lost" quantity="11" unit="character"/><gap reason="illegible" quantity="4" unit="character"/>ντων καὶ ἐπὶ τοῦ κρατίστου ὑπ<supplied reason="lost">ομνηματογρά</supplied>

    <lb n="11" break="no"/><supplied reason="lost">φου </supplied><gap reason="lost" quantity="8" unit="character"/>ων αὐτῶν γενομένης μου <unclear>ἄλ</unclear><supplied reason="lost">λης ἀντικατα</supplied>

    <lb n="12" break="no"/><supplied reason="lost" cert="low">στάσεως</supplied><gap reason="lost" quantity="4" unit="character"/> τοῦ ἀγοραστοῦ Ἑρμία ἐκ προκηρύξε<supplied reason="lost">ως </supplied><gap reason="lost" quantity="9" unit="character"/> 

    <lb n="13"/><gap reason="lost" quantity="6" unit="character"/><supplied reason="lost"> βιβλί</supplied>διον εἰς τὸ <supplied reason="lost">δί</supplied>κας <choice><reg>ἡμῖν</reg><orig>ἡμεῖν</orig></choice> εἰσάγειν <gap reason="lost" quantity="11" unit="character"/> 

    <lb n="14"/><gap reason="lost" quantity="11" unit="character"/>σθαι καὶ κυρ<supplied reason="lost">ω</supplied>θεὶς ὁ Ἑρμίας κατας<gap reason="lost" quantity="11" unit="character"/> 

    <lb n="15"/><gap reason="lost" quantity="8" unit="character"/><supplied reason="lost"> ἐξ ἧ</supplied>ς ἔσχομ<unclear>εν</unclear> πρὸς αὐτὸν ἀντικατα<supplied reason="lost">στάσεως τῇ δε</supplied>

    <lb n="16" break="no"/><supplied reason="lost">δομένῃ προθ</supplied>εσμίᾳ τὴν <choice><reg>τιμὴν</reg><orig><supplied reason="lost">τ</supplied>ειμὴν</orig></choice> πᾶσαν οὐκ ἐθέ<supplied reason="lost" cert="low">λων δοῦναι </supplied><gap reason="lost" quantity="2" unit="character"/> 

    <lb n="17"/><gap reason="lost" quantity="11" unit="character"/> ἑξῆς κατὰ <unclear>τ</unclear><supplied reason="lost">ὰ</supplied> <unclear>κ</unclear>ριθέντα καὶ διὰ τοῦτο μ<gap reason="lost" quantity="11" unit="character"/> 

    <lb n="18"/><gap reason="lost" quantity="5" unit="character"/><supplied reason="lost"> ἐνέτυχ</supplied>ον πλειστάκις τῷ αὐτῷ ὑπομ<unclear>ν</unclear><supplied reason="lost">ηματογράφῳ</supplied> 

    <lb n="19"/><gap reason="lost" quantity="11" unit="character"/> ὃς ἐκέλευσεν τοῖς περὶ αὐτὸν ὑπ<supplied reason="lost">ηρέταις </supplied><gap reason="lost" quantity="4" unit="character"/> 

    <lb n="20"/><supplied reason="lost">ἐπειδὴ ὁ Ἑρμία</supplied>ς καταφρονήσας τῶν κριθέντων α<gap reason="lost" quantity="11" unit="character"/> 

    <lb n="21"/><gap reason="lost" quantity="10" unit="character"/><supplied reason="lost"> ἐ</supplied><unclear>π</unclear>οίησε, μ<unclear>ὴ</unclear> ἀρκεσθεὶς δὲ ἐπὶ τούτοι<unclear>ς</unclear> <gap reason="lost" quantity="6" unit="character"/><supplied reason="lost"> οὐ</supplied> 

    <lb n="22"/><supplied reason="lost">τὰς τυχούσα</supplied>ς ὕβρεις μοι παρέχει σὺν τοῖς συνερ<supplied reason="lost">γοῦσιν αὐτῷ</supplied> 

    <lb n="23"/><supplied reason="lost">ἀδελφοῖς καὶ</supplied> ἑτέροις, ἀξιῶ σε τὸν κύριον ἐλεῆσ<supplied reason="lost">αί με ἀδικουμέ</supplied>

    <lb n="24" break="no"/><supplied reason="lost">νην ὑπὸ τῶν κακ</supplied>οπρ<unclear>α</unclear>γ<unclear>μ</unclear><supplied reason="lost">ό</supplied><unclear>νω</unclear>ν ἀ<supplied reason="lost">ν</supplied><unclear>θ</unclear>ρώπων δι<gap reason="lost" quantity="11" unit="character"/> 

    <lb n="25"/><gap reason="lost" quantity="4" unit="character"/><supplied reason="lost"> οὐκ ἐμμε</supplied><unclear>ν</unclear>όντ<unclear>ων</unclear> <unclear>τ</unclear><supplied reason="lost">οῖς κ</supplied><unclear>ρ</unclear>ιθεῖσι καὶ κελεῦσ<unclear>α</unclear><supplied reason="lost">ι </supplied><gap reason="lost" quantity="10" unit="character"/> 

    <lb n="26"/><gap reason="lost" quantity="13" unit="character"/><supplied reason="lost"> ἱνʼ ὦ εὐερ</supplied>γετημένη. 

    <lb n="27"/><space extent="unknown" unit="character"/> <handShift new="m2"/> διὰ Ἀπίωνος <app type="editorial"><lem resp="R. Mascellari, ZPE 207 (2018) 169">ἐπι<supplied reason="lost" cert="low">δέδωκα</supplied></lem><rdg>ἐπι<supplied reason="lost">δέδοται</supplied></rdg></app>. 

    <lb n="28"/><handShift new="m3"/><gap reason="lost" extent="unknown" unit="character"/>ε ὁ ἐπὶ τῶν <app type="editorial"><lem resp="R. Mascellari, ZPE 207 (2018) 166-167 (from photo)">κεκριμένω<supplied reason="lost">ν</supplied></lem><rdg>κεκριμέν<supplied reason="lost">οις</supplied></rdg></app><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="29"/><gap reason="lost" extent="unknown" unit="character"/> <unclear>ἐ</unclear>πεξελ<supplied reason="lost">ε</supplied>ύσεται τοῖς κεκριμέν<supplied reason="lost">οις </supplied><gap reason="lost" extent="unknown" unit="character"/> 
