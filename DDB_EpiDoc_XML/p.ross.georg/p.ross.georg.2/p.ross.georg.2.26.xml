         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="1" type="textpart"><ab>

    <lb n="1"/><gap reason="lost" extent="unknown" unit="character"/>επιστο 

    <lb n="2"/><gap reason="lost" extent="unknown" unit="character"/>ολ<gap reason="illegible" quantity="3" unit="character"/> 

