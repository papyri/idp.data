    <lb n="12"/>καὶ λαβὲ τὸ κρ<unclear>ο</unclear><gap quantity="1" reason="lost" unit="character"/><gap quantity="2" reason="illegible" unit="character"/><gap quantity="1" reason="lost" unit="character"/>των τὸ <choice><reg>ἐλεφάντινον</reg><orig>αἰλε<unclear>φ</unclear>άν<unclear>τ</unclear>ινον</orig></choice>

    <lb n="13"/>παρὰ δορ<unclear>εας</unclear> η<unclear>ψ</unclear>υ<gap quantity="1" reason="illegible" unit="character"/><gap quantity="1" reason="lost" unit="character"/><gap quantity="1" reason="illegible" unit="character"/><unclear>θ</unclear>εων κ<unclear>α</unclear>ὶ <choice><reg>κράτει</reg><orig>κρ<unclear>ά</unclear>τι</orig></choice> α<unclear>ὐ</unclear>τ<unclear>ω</unclear>

    <lb n="14"/>διαντων <choice><reg>ἱματίων</reg><orig>ἡματίων</orig></choice> <unclear>τ</unclear>ων ηρκ<gap quantity="3" reason="illegible" unit="character"/> προ<supplied reason="omitted">σ</supplied>δωκῶ ς<gap extent="unknown" reason="lost" unit="character"/>

    <lb n="15"/>θεῖν μετα <choice><reg>τὸν θεόν</reg><orig>τὼ<unclear>ν</unclear>ν  θεών</orig></choice> παρʼ ἐμὲ καὶ αικρατ<unclear>η</unclear><gap extent="unknown" reason="lost" unit="character"/>

    <lb n="16"/>καὶ αὐτ<unclear>η</unclear><gap quantity="4" reason="lost" unit="character"/><unclear>ε</unclear>ρε τὰ <choice><reg>ἱμάτια</reg><orig>αἱμάτια</orig></choice> <gap quantity="3" reason="illegible" unit="character"/><gap quantity="2" reason="lost" unit="character"/><gap quantity="1" reason="illegible" unit="character"/><unclear>π</unclear>α<unclear>ν</unclear><gap quantity="1" reason="lost" unit="character"/><gap quantity="1" reason="illegible" unit="character"/> <expan><ex>δραχμ  </ex></expan> <app type="alternative"><lem><num value="208">ση</num></lem><rdg><num value="200">σ</num> ἡ</rdg></app>

    <lb n="17"/>οἰκία μου <gap quantity="1" reason="illegible" unit="character"/><gap quantity="1" reason="lost" unit="character"/><unclear>α</unclear>ι<gap quantity="2" reason="illegible" unit="character"/>τιμε ὅτι <choice><reg>ἐὰν</reg><orig>αἰὰν</orig></choice> μὴ πα<gap quantity="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/>

    <lb n="18"/>τὸ μέρος τῆς οἰκίας ου <choice><reg>πρᾶσιν</reg><orig>πρᾶσην</orig></choice> ὅτι <choice><reg>ἐὰν</reg><orig>αἰὰν</orig></choice>

    <lb n="19"/>εἰς τὰ ἴδιά <unclear>σ</unclear>ου <choice><reg><app type="alternative"><lem>αἴρω</lem><rdg>ἐρῶ</rdg></app></reg><orig>αἰρῶ</orig></choice> τὴν οἰκία<unclear>ν</unclear> <gap quantity="2" reason="lost" unit="character"/><unclear>α</unclear><gap quantity="2" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/>

