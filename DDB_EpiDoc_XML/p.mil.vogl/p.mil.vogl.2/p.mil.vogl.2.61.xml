    <lb n="33"/><abbr><unclear>τ</unclear>ελ</abbr> καινὴν <expan><supplied reason="lost">ἄ</supplied>ρτω<ex>ν</ex></expan> <expan><supplied reason="lost">τ</supplied>έλ<ex>εσμα</ex></expan> <expan><ex>γίνεται</ex></expan> <num value="6">ϛ</num> ἐπὶ <num value="4">δ</num> 

