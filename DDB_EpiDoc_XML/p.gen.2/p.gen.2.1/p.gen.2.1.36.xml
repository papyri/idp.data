         <head n="11230" xml:lang="en">
            <date>AD 170</date>
            <placeName>Soknopaiou Nesos</placeName>
            <ref n="chr.wilck;;85" type="reprint-from">P.Gen. 1,36; Chrest.Wilck. 85</ref>
