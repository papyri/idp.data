         <head xml:lang="en">
            <date>AD88</date>
            <placeName>Oxyrhynchus</placeName>
            <ref n="p.oxy;1;174" type="reprint-from">POxy1,174</ref>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><ab>
    <lb n="1"/>Πλούταρχος ὁ καθεσταμένος 

    <lb n="2"/>ἐπιτηρητὴς καὶ χειριστὴς 

    <lb n="3"/>καταλοχισμῶν Ὀξυρυγχείτου 

    <lb n="4"/>τοῖς ἀγορανόμοις χαίρειν. 

    <lb n="5"/>ἀπελευθέρας Φιλουταρίου με

    <lb n="6" break="no"/><unclear>τ</unclear>ὰ κυρίου Ἰσιδώρου τοῦ Πτολεμαί

    <lb n="7" break="no"/><unclear>ου</unclear> τοῦ Πτολεμαίου ἐρημο

    <lb n="8" break="no"/><expan>φύλακ<ex>ος</ex></expan> ἐκ τῆς Μικρᾶς Ὀάσεως 

    <lb n="9"/>παρακεχωρημένης παρὰ <gap reason="illegible" quantity="3" unit="character"/> 

    <lb n="10"/>ση<gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="4" unit="character"/>θ<gap reason="illegible" quantity="1" unit="character"/>ρω 

    <lb n="11"/><unclear>μ</unclear>ισ<unclear>τ</unclear>η<unclear>σ</unclear>θ<gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="3" unit="character"/> 

    <lb n="12"/><gap reason="lost" quantity="6" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="1" unit="character"/> 

    <lb n="13"/><gap reason="lost" quantity="6" unit="character"/><gap reason="illegible" quantity="2" unit="character"/><unclear>ρπ</unclear><gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" quantity="6" unit="character"/> 

    <lb n="14"/><gap reason="lost" quantity="6" unit="character"/>ωνκωμ<gap reason="lost" quantity="6" unit="character"/> 

    <lb n="15"/><gap reason="lost" quantity="8" unit="character"/><gap reason="illegible" quantity="1" unit="character"/>εδ<gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="2" unit="character"/> 

    <lb n="16"/><gap reason="lost" quantity="8" unit="character"/>μεν<gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="2" unit="character"/> 

    <lb n="17"/><gap reason="lost" quantity="8" unit="character"/>εμα<gap reason="illegible" quantity="2" unit="character"/><unclear>ε</unclear>ντου 

    <lb n="18"/><gap reason="lost" quantity="8" unit="character"/>σαλ<gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="1" unit="character"/> <supplied reason="lost">ἐ</supplied><unclear>κ</unclear> <unclear>τὰς</unclear> 

    <lb n="19"/><supplied reason="lost">ὑ</supplied>παρχούσας <supplied reason="lost">αὐ</supplied>τ<unclear>ῶι</unclear> <unclear>π</unclear><supplied reason="lost">ερὶ</supplied> <unclear>Ψ</unclear><supplied reason="lost">ῶβ</supplied>

    <lb n="20" break="no"/>θιν τῆς ἄνω τοπαρχί<unclear>ας</unclear> 

    <lb n="21"/>ἐκ τοῦ Φιλιστίωνος 

    <lb n="22"/>κλήρου ἀπὸ κοινῶν καὶ ἀ

    <lb n="23" break="no"/>διαιρέτων ἀρουρῶν δεκα

    <lb n="24" break="no"/>πέντε ἥμισυ <num value="15"/><num value="1/2"/> κατοικικῆς 

    <lb n="25"/>γῆς σιτοφόρου σπορίμου ἐξ ὀρ

    <lb n="26" break="no"/>θογωνίου <gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="4" unit="character"/> 

    <lb n="27"/>διὸ γράφω <choice><reg>ὑμῖν</reg><orig>ὑμεῖν</orig></choice> ἵν εἰ<supplied reason="lost">δῆτ</supplied>ε. 

    <lb n="28"/><space extent="unknown" unit="character"/> <expan>ἔρρω<ex>σο</ex></expan>. <expan><ex>ἔτους</ex></expan> <num value="8">η</num> Αὐτοκράτορος 

    <lb n="29"/><unclear>Κ</unclear><supplied reason="lost">αίσ</supplied>αρος Δομιτιανοῦ Σεβαστοῦ 

    <lb n="30"/>Γερμανικοῦ, μηνὸς Νέου Σεβ<supplied reason="lost">αστοῦ</supplied> 

    <lb n="31"/><num value="8">η</num> Σεβαστῆι. 

    <lb n="32"/><handShift new="m2"/> Πλούταρχος <expan>σεση<ex>μείωμαι</ex></expan>. 

    <lb n="33"/><gap reason="illegible" quantity="1" unit="line"/> </ab></div>
