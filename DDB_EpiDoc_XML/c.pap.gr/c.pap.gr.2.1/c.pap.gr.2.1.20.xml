    <lb n="1"/>Ἰσιδώ<supplied reason="lost">ρῳ </supplied><expan><supplied reason="lost">βασι</supplied>λ<ex>ικῷ</ex></expan> <expan>γραμμα<supplied reason="lost">τ<ex>εῖ</ex></supplied></expan> 

