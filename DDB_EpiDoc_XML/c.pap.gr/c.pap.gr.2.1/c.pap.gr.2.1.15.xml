         <head n="15658" xml:lang="en">
            <date>AD 78/9</date>
            <placeName>Oxyrhynchus</placeName>
            <ref n="p.oxy;49;3510" type="reprint-from">POxy49,3510</ref>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><ab>
    <lb n="1"/>Ἀπολλοφάνει καὶ Διογ<unclear>έ</unclear>νει <expan>γρα<ex>μματεῦσι</ex></expan> πόλεως 

    <lb n="2"/>παρὰ Ἡρώδου τοῦ Ἀπίωνος τῶν ἀπʼ Ὀ

    <lb n="3" break="no"/>ξυρύγχων πόλεως. οἱ ἀπογραφόμενοι 

    <lb n="4"/>ἐφʼ ἧς ἔχω οἰκίας ἐπʼ ἀμφόδου Λυκίων 

    <lb n="5"/>Παρεμβολῆς Ἀμαράντος δοῦλος τοῦ 

    <lb n="6"/>ἀδελφοῦ μου Ἡρακλείδου καὶ <subst><add place="inline"><expan>Διογένη<ex>ς</ex></expan></add><del rend="corrected">διογ<gap reason="illegible" quantity="1" unit="character"/>νη</del></subst> 

    <lb n="7"/>δοῦλος τῆς γυναικός <unclear>μ</unclear>ου <subst><add place="inline">Βερενείκης</add><del rend="corrected">πρενεικης</del></subst> 

    <lb n="8"/>τῆς Βιθαρίωνος ἀφῆλιξ ἀμφότεροι 

    <lb n="9"/>ἄτεχνοι ἐτελεύτησαν τῷ ἐνεστῶτι 

    <lb n="10"/>ἑνδεκάτῳ ἔτει Αὐτοκράτορος Καίσαρος 

    <lb n="11"/>Οὐε<unclear>σ</unclear>πασιανοῦ Σεβαστοῦ, καὶ <choice><reg>ὀμνύω</reg><orig>ὀμνύωι</orig></choice> <g rend="extension" type="filler"/> 

    <lb n="12"/>Αὐτοκράτορα Καίσαρα Οὐεσπασιανὸν 

    <lb n="13"/>Σεβαστὸν ἐπʼ ἀληθείας αὐτοὺς τετελευ

    <lb n="14" break="no"/>τηκέναι ὡς πρόκειται. ἀξιῶ οὖν <g rend="extension" type="filler"/> 

    <lb n="15"/>ἀναγράψεσθαι <subst><add place="inline">τούτ<unclear>ου</unclear>ς</add><del rend="corrected">αουτ<unclear>ου</unclear>ς</del></subst> ἐν τῇ τάξει 

    <lb n="16"/><unclear>τῶ</unclear>ν τετ<unclear>ε</unclear><supplied reason="lost">λ</supplied>ευτηκό<unclear>τω</unclear>ν τοῦ α<unclear>ὐ</unclear><supplied reason="lost">τ</supplied><unclear>ο</unclear>ῦ 


    <lb n="17"/><unclear>ἔ</unclear>τους ὡς ἐπὶ τῶν <supplied reason="lost">ὁ</supplied><unclear>μ</unclear>οίων. 
<milestone rend="paragraphos" unit="undefined"/>
    <lb n="18"/><handShift new="m2"/> Διογέ<unclear>ν</unclear>η<supplied reason="lost">ς </supplied><expan><supplied reason="lost">σεσ</supplied><unclear>ημ</unclear><ex>είωμαι</ex></expan>. <expan><ex>ἔτους</ex></expan> <num value="11"><unclear>ια</unclear></num> Αὐτοκ<supplied reason="lost">ρ</supplied>ά<unclear>το</unclear><supplied reason="lost">ρο</supplied><unclear>ς</unclear> 

    <lb n="19"/><unclear>Κ</unclear><supplied reason="lost">αίσαρος Οὐεσ</supplied><unclear>α</unclear>σιαν<unclear>ο</unclear>ῦ 

    <lb n="20"/><supplied reason="lost">Σεβαστοῦ </supplied><gap reason="lost" extent="unknown" unit="character"/> <num value="4"><unclear>δ</unclear></num>. </ab></div>
