         <head n="16229" xml:lang="en">
            <date>AD 55/6</date>
            <placeName>Oxyrhynchus</placeName>
            <ref n="sb;16;12383|sb;14;11336" type="reprint-from">SB 16.12383; SB 14.11336</ref>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><ab>
    <lb n="1"/>Τιβερίωι Κ<supplied reason="lost">λαυδίωι Θέωνι μισ</supplied>

    <lb n="2" break="no"/>θωτῆι τῆ<supplied reason="lost">ς Λουκίου Ἀνναίου</supplied> 

    <lb n="3"/>Σενέκα ο<supplied reason="lost">ὐσίας παρὰ </supplied><gap reason="lost" quantity="5" unit="character"/> 

    <lb n="4"/>Ἀντων<gap reason="lost" quantity="15" unit="character"/> 

    <lb n="5"/>πα<gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="14" unit="character"/> 

    <lb n="6"/>ὁ πατήρ <supplied reason="lost">μου </supplied><gap reason="lost" quantity="7" unit="character"/><supplied reason="lost"> ἄτεχ</supplied>

    <lb n="7" break="no"/>νος <foreign xml:lang="la">?,</foreign> ἀναγραφόμεν<supplied reason="lost">ος ἐπὶ</supplied> 

    <lb n="8"/>τῆς αὐτῆς οὐσί<supplied reason="lost">ας</supplied>, <unclear>ἔτ</unclear>ι <supplied reason="lost" cert="low">δὲ</supplied> 

    <lb n="9"/>ἐπὶ λαύρ<supplied reason="lost">ας </supplied><gap reason="lost" quantity="11" unit="character"/> 

    <lb n="10"/>καταγει<supplied reason="lost">νόμενος δὲ ἐν</supplied> 

    <lb n="11"/>κώμη<supplied reason="lost">ι </supplied><gap reason="lost" quantity="12" unit="character"/><supplied reason="lost"> τῆς</supplied> 

    <lb n="12"/>μέσης <supplied reason="lost">τοπαρχίας, ἐτελεύ</supplied>

    <lb n="13" break="no"/>τησεν τ<supplied reason="lost">ῶι ἐνεστῶτι δευ</supplied>

    <lb n="14" break="no"/>τέρωι <unclear>ἔ</unclear><supplied reason="lost">τει Νέρωνος Κλαυδίου</supplied> 

    <lb n="15"/>Καίσαρ<supplied reason="lost">ος Σεβαστοῦ Γερμανικοῦ</supplied> 

    <lb n="16"/>Αὐτοκρ<supplied reason="lost">άτορος. διὸ ἐπιδίδω</supplied>

    <lb n="17" break="no"/>μι τὸ ἀ<supplied reason="lost">ναφόριον ἀξιῶν</supplied> 

    <lb n="18"/>ἀναγρα<supplied reason="lost">φῆναι τοῦτον ἐν</supplied> 

    <lb n="19"/>τῇ τῶ<supplied reason="lost">ν ὁμοίων τάξει</supplied> 

    <lb n="20"/><supplied reason="lost">ὡς καθήκει. </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="21"/><gap reason="lost" extent="unknown" unit="line"/> </ab></div>
