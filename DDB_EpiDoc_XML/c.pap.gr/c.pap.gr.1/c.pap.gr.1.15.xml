         <head n="23023" xml:lang="en">
            <date>ca. AD 26</date>
            <placeName>Oxyrhynchus</placeName>
            <ref n="p.rein;2;104" type="reprint-from">PRein2,104</ref>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><ab>
    <lb n="1"/><supplied reason="lost">ἔτους δωδεκάτου Τιβερίου Καίσαρος Σεβαστοῦ </supplied><gap reason="lost" quantity="5" unit="character"/><gap reason="illegible" quantity="3" unit="character"/> ἐν Ὀξυρύγχων πόλει τῆς 

    <lb n="2"/><supplied reason="lost">Θ</supplied><unclear>ηβαίδο</unclear>ς. <unclear>ὁμολογε</unclear><supplied reason="lost">ῖ</supplied> Θε<unclear>ρ</unclear>β<gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ἀπὸ κώμης </supplied><gap reason="lost" quantity="5" unit="character"/><supplied reason="lost"> τῆς </supplied><gap reason="lost" quantity="3" unit="character"/> 

    <lb n="3"/><supplied reason="lost">το</supplied>παρχίας <choice><reg>Περσίνη</reg><orig>Περσεϊνῆι</orig></choice> μετὰ κυρί<supplied reason="lost">ου καὶ ἐγγύου πάντων τῶν κατὰ</supplied> 

    <lb n="4"/><supplied reason="lost">τ</supplied>ὴν συγγραφὴν ταύτην εἰς <choice><reg>ἔκτισι<supplied reason="lost">ν</supplied></reg><orig>ἔκτεισι<supplied reason="lost">ν</supplied></orig></choice> <supplied reason="lost" cert="low">τοῦ ἑαυτῆς ἀνδρὸς </supplied><gap reason="lost" quantity="4" unit="character"/> 

    <lb n="5"/><gap reason="lost" quantity="2" unit="character"/><gap reason="illegible" quantity="1" unit="character"/> τοῦ Δημητρίου τῶν ἀπὸ τῆς αὐ<supplied reason="lost">τῆς κώμης Πέρσου τῆς ἐπιγο</supplied>

    <lb n="6" break="no"/><supplied reason="lost">ν</supplied>ῆς Πανεχώτηι Παυσίριος ἐν ἀ<supplied reason="lost">γυιᾷ παρειληφέναι παρʼ αὐτοῦ ἀ</supplied>

    <lb n="7" break="no"/><supplied reason="lost">πὸ</supplied> τῆς <choice><reg>ἐνεστώση<supplied reason="omitted">ς</supplied></reg><orig>ἐνεστωση</orig></choice> ἡμέρας ὃ <choice><reg>ἀνῄρ<supplied reason="lost">ηται</supplied></reg><orig>ἀνείρ<supplied reason="lost">ηται</supplied></orig></choice> <supplied reason="lost">ἀπὸ κοπρίας εἰς δουλείαν</supplied> 

    <lb n="8"/><supplied reason="lost">θ</supplied>ηλυκὸν σωμάτιον ᾧ ἐπ<unclear>έ</unclear><supplied reason="lost">θηκεν ὄνομα </supplied><gap reason="lost" quantity="4" unit="character"/><supplied reason="lost"> ὥστε</supplied> 

    <lb n="9"/>ἐπάναγκον τὴν ὁμολογοῦσ<supplied reason="lost">αν τοῦτο θρέψαι καὶ θηλάσαι τῷ</supplied> 

    <lb n="10"/><supplied reason="lost">ἰδ</supplied>ίῳ αὐτῆς γάλακτι, ἔτι δὲ κ<supplied reason="lost">αὶ τιθηνῆσαι ἐπὶ χρόνον ἔτη δύ</supplied>

    <lb n="11" break="no"/><supplied reason="lost">ο</supplied> <num value="2"/> ἀπὸ τῆς αὐτῆς ἐνεστώσ<supplied reason="lost">ης ἡμέρας ἀντὶ τῶν ἑσταμένων</supplied> 

    <lb n="12"/><supplied reason="lost">ἐπ</supplied>ιδοθῆναι αὐτῇ ὑπὸ τοῦ Π<supplied reason="lost">ανεχώτου εἰς τροφεῖα καὶ ἱ</supplied>

    <lb n="13" break="no"/><supplied reason="lost">μα</supplied>τισμὸν καὶ τὴν ἄλλην τὴ<unclear>ν</unclear> <supplied reason="lost">ὑπὲρ τοῦ σωματίου δαπάνην πᾶσαν</supplied> 

    <lb n="14"/><gap reason="lost" quantity="1" unit="character"/><gap reason="illegible" quantity="1" unit="character"/>ην καὶ <gap reason="illegible" quantity="3" unit="character"/>ιου λόγον κατʼ ἔτ<supplied reason="lost">ος </supplied><gap reason="lost" quantity="7" unit="character"/><supplied reason="lost"> ἀργυρί</supplied>

    <lb n="15" break="no"/><supplied reason="lost">ο</supplied>υ νομίσματος δραχμῶν <gap reason="lost" quantity="5" unit="character"/><supplied reason="lost"> καὶ δὴ αὐτόθεν</supplied> 

    <lb n="16"/><supplied reason="lost">ἀ</supplied>πέχειν <add place="above"><unclear>θ</unclear><gap reason="illegible" quantity="2" unit="character"/></add> αὐτήν τε καὶ <unclear>τ</unclear><supplied reason="lost">ὸν κύριον καὶ ἔγγυον </supplied><gap reason="lost" quantity="5" unit="character"/><supplied reason="lost"> πα</supplied>

    <lb n="17" break="no"/><supplied reason="lost">ρ</supplied>ὰ Πανεχώτου δ<supplied reason="lost">ρ</supplied>αχ<supplied reason="lost">μὰς </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="18"/><gap reason="lost" extent="unknown" unit="line"/> </ab></div>
