         <head n="78014" xml:lang="en">
            <date>85 BC</date>
            <placeName>Herakleopolis</placeName>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="1" type="textpart"><ab>

    <lb n="1"/><expan><unclear>μετ</unclear>αγρ<ex>αφὲν</ex></expan> <expan><ex>ἔτους</ex></expan> <num value="32">λβ</num> <expan>Τυβ<ex>ὶ</ex></expan> <num value="20"><unclear>κ</unclear></num>. <g type="x"/>

    <lb n="2"/><handShift new="m2"/> Σεμθεῖ <expan>ἀντιγρα<ex>φεῖ</ex></expan> <expan><supplied reason="lost">θ</supplied>η<ex>σαυροῦ</ex></expan> περὶ <expan>Πό<ex>λιν</ex></expan>.

    <lb n="3"/>ἐὰν οἱ παρὰ Λεω<supplied reason="lost">νί</supplied>δου τοῦ <unclear>τ</unclear>ετα<unclear>γ</unclear>μένου πρὸς τῆ<unclear>ι</unclear>

    <lb n="4"/>σιτολογίαι μετρῶσι κατὰ τὸν <supplied reason="lost">ἐ</supplied><unclear>π</unclear>εσταλμένον

    <lb n="5"/>αὐτῶι παρὰ Σαραπίωνος τοῦ ἐπ<supplied reason="lost">ὶ τ</supplied>ῶν

    <lb n="6"/>προσόδων <expan>χρη<ex>ματισμὸν</ex></expan> Πτολεμαί<unclear>ωι</unclear> τῶν ἐ<unclear>κ</unclear> τῆς

    <lb n="7"/>διοικη<unclear>τ</unclear><supplied reason="lost">ικῆς</supplied> <unclear>ὑ</unclear>πηρεσίας ἐληλ<unclear>υ</unclear>θότι

    <lb n="8"/>ἐπὶ τοὺς πρὸς εἰσαγωγὴν λ<unclear>όγο</unclear><supplied reason="lost">υ</supplied><unclear>ς</unclear> τοῦ <num value="31">λα</num> <expan><ex>ἔτους</ex></expan>

    <lb n="9"/>πυροῦ ἀρτάβας δέκα, γίνονται <expan><ex>πυροῦ</ex></expan> <expan><ex>ἀρτάβαι</ex></expan> <num value="10">ι</num>,

    <lb n="10"/><choice><reg><supplied reason="lost">συ</supplied>μπροο<supplied reason="lost">ῦ</supplied></reg><orig><supplied reason="lost">συ</supplied><unclear>νπροο</unclear><supplied reason="lost">ῦ</supplied></orig></choice><supplied reason="lost"> ὡ</supplied><unclear>ς</unclear> <unclear>ε</unclear>ἴθι<supplied reason="lost">σ</supplied>τ́αι καὶ <expan>σ<unclear>ύ</unclear><ex>μβολον</ex></expan> <unclear>καὶ</unclear> <expan><unclear>ἀντισύ</unclear><ex>μβολον</ex></expan>

    <lb n="11"/>ποί<unclear>ησ</unclear>αι πρὸς <expan>αὐ<ex>τοὺς</ex></expan> ὡς καθήκει.</ab></div></div>
