    <lb n="1"/><expan>ἀ<unclear>ν</unclear><supplied reason="lost">έ<ex>γνωσται</ex></supplied></expan> <expan><ex>ἔτους</ex></expan> <num value="32">λβ</num> <expan>Χοι<ex>ὰχ</ex></expan> <num value="7">ζ</num> <g type="x"/>

