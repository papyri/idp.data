    <lb n="1"/><choice><reg>Σοκμᾶς</reg><orig>Σοκμᾶ<del rend="erasure">α</del>ς</orig></choice> <choice><reg>Ναινάιδος</reg><orig>Ναιναις</orig></choice> <choice><reg>Σύρων</reg><orig>Σύρον</orig></choice> <gap reason="illegible" quantity="2" unit="character"/> 

    <lb n="2"/>τι <abbr>στ<unclear>ρ</unclear>α</abbr> 

    <lb n="3"/><num value="22">κ<unclear>β</unclear></num> <expan><ex>ἐτῶν</ex></expan>. εἰς τὸ <choice><reg>βαστάξα<supplied reason="omitted">ι</supplied></reg><orig>βαστάξα</orig></choice> 

    <lb n="4"/><choice><reg>δεῖγμα</reg><orig>δῖγμα</orig></choice> ἀπὸ ἁλώνων πυροῦ 

    <lb n="5"/><expan>γενή<ex>ματος</ex></expan> τοῦ <choice><reg>ἐνεστῶτος</reg><orig>ἐνοστο͂τος</orig></choice> <num value="24">κδ</num> <expan><ex>ἔτους</ex></expan> 

    <lb n="6"/>καὶ <choice><reg>ἀποκαταστῆσα<supplied reason="omitted">ι</supplied></reg><orig>ἀποκαταστῆσα</orig></choice> <choice><reg>εἰς</reg><orig><hi rend="diaeresis">ἰ</hi>ς</orig></choice> Ἀλεξάν

    <lb n="7" break="no"/>δριαν <choice><reg>δίδωμι</reg><orig>δίδομι</orig></choice>  <add place="above">κατὰ τὸ ἔθος</add> τὸν <expan><supplied reason="lost">ὑπ</supplied>ογρα<ex>φόμενον</ex></expan> 

    <lb n="8"/>ὄντα εὔ<supplied reason="lost">πορ</supplied>ον κα<supplied reason="lost">ὶ</supplied> ἐπιτ<supplied reason="lost">ήδειο</supplied>ν 

    <lb n="9"/><choice><reg>τῷ</reg><orig>το</orig></choice> <choice><reg>ἐμῷ</reg><orig>ἐμο</orig></choice> κ<supplied reason="lost">ιν</supplied>δύνῳ. </ab></div>
