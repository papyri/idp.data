         <head n="8755" xml:lang="en">
            <date>AD 184/5</date>
            <placeName>Ptolemais Hormou</placeName>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="1" type="textpart"><ab>

    <lb n="1"/>Ἀπολλωνίῳ <expan>στρα<ex>τηγῷ</ex></expan> <expan>Ἀρσι<ex>νοίτου</ex></expan> <expan>Ἡρακλ<ex>είδου</ex></expan> <expan>μερίδ<ex>ος</ex></expan> 

    <lb n="2"/>παρὰ Πεταῦτος <expan>κωμογρα<ex>μματέως</ex></expan> <expan>Πτολ<ex>εμαίδος</ex></expan> <supplied reason="lost">Ὅ</supplied>ρμου 

    <lb n="3"/>καὶ      ἄλλων κωμῶν. 

    <lb n="4"/>αἰτούμενος ὑπὸ σοῦ ὀνόματα εἰ<supplied reason="lost">ς</supplied> τὸ 

    <lb n="5"/>παραφυλάξαι τὰ ἀπὸ τῶν ἀγρῶν 

    <lb n="6"/>ἀναφερόμενα γενήματα καὶ ἐπα<supplied reason="lost">ν</supplied><unclear>αγ</unclear>

    <lb n="7" break="no"/>κάζειν εἰς τὰς ἅλω παραφέρεσθαι 

    <lb n="8"/>καὶ ἐπιτηρ<supplied reason="lost">ε</supplied>ῖν αὐτὰ ἐπὶ τῶν ἁλώνων ὄντα 

    <lb n="9"/>ἕως ἂν τὰ <choice><reg>ὀφειλόμενα</reg><orig>ὀφιλόμενα</orig></choice> τῷ ταμείῳ 

    <lb n="10"/>μετρηθῇ, καὶ πρόνοιαν ποιεῖσθαι τοῦ 

    <lb n="11"/>κρι<supplied reason="lost">θ</supplied>ο<supplied reason="lost">λ</supplied>ογηθῆναι τὸν πυρόν, καὶ ἐπὶ τῆς 

    <lb n="12"/>λικ<supplied reason="lost">μ</supplied>ήσεως καὶ τῆς <choice><reg>εἰσπράξεως</reg><orig>ἰσπράξεως</orig></choice> τοῦ πυροῦ, 

    <lb n="13"/>καὶ <choice><reg>ἐπισφραγίζειν</reg><orig>ἐπισφρακίζειν</orig></choice> τοὺς θησαυροὺς καὶ 

    <lb n="14"/><choice><reg>ἐπείγειν</reg><orig>ἐπίγειν</orig></choice> τὰς ἐμβολὰς καὶ ἐπακολ<supplied reason="lost">ο</supplied>υ

    <lb n="15" break="no"/>θεῖν τῇ καταγωγῇ          <choice><reg>δίδωμι</reg><orig>δείδωμει</orig></choice> τοὺς 

    <lb n="16"/><expan>ὑπογ<supplied reason="lost">εγ</supplied>ρα<ex>μμένους</ex></expan> ὄντας εὐπόρους καὶ ἐπιτη<supplied reason="lost">δεί</supplied>

    <lb n="17" break="no"/>ους ἐπὶ τῷ αὐτοὺς καὶ τῆς <choice><reg>εἰρήνης</reg><orig><hi rend="diaeresis">ἰ</hi>ρήνης</orig></choice> 

    <lb n="18"/>πρόνοιαν ποιεῖσθαι.      ἔστι δέ· 

    <lb n="19"/>Πτολ<supplied reason="lost">ε</supplied>μα<hi rend="diaeresis">ί</hi>δος Ὅρμου 

    <lb n="20"/>Πεθεὺς Ἁτρῆτος <expan>μη<ex>τρὸς</ex></expan> <expan>Διδύμ<ex>ης</ex></expan> <expan>ἐπ<ex>ικαλούμενος</ex></expan> Πκαν

    <lb n="21" break="no"/>πεσοῦλ ἔχων πόρον <expan><ex>δραχμῶν</ex></expan> <num value="800">ω</num> 

    <lb n="22"/><supplied reason="lost">Ἀ</supplied>πύγχις Παβῶτος <expan>μη<ex>τρὸς</ex></expan> <gap reason="lost" quantity="2" unit="character"/><gap reason="illegible" quantity="2" unit="character"/> <supplied reason="lost"><expan>ὁ<ex>μοίως</ex></expan></supplied> <expan><ex>δραχμῶν</ex></expan> <gap reason="illegible" quantity="1" unit="character"/> 

    <lb n="23"/><gap reason="lost" quantity="3" unit="character"/><gap reason="illegible" quantity="1" unit="character"/>ας <expan>ἐπικαλ<ex>ούμενος</ex></expan> Ἀλεκτορᾶς <expan><unclear>ὁ</unclear><ex>μοίως</ex></expan> <expan><ex>δραχμῶν</ex></expan> <num value="800">ω</num> 

    <lb n="24"/><supplied reason="lost">Φάσ</supplied>ει<unclear>ς</unclear> ἀπάτωρ <expan>μη<ex>τρὸς</ex></expan> Τα<hi rend="diaeresis">ύ</hi>ρεως <expan>ἐπ<ex>ικαλούμενος</ex></expan> τ<unclear>έ</unclear><supplied reason="lost">κτ</supplied><unclear>ω</unclear>ν   <supplied reason="lost"><expan>ὁ<ex>μοίως</ex></expan> <expan><ex>δραχμῶν</ex></expan> </supplied><gap reason="lost" quantity="1" unit="character"/> 

    <lb n="25"/><gap reason="lost" quantity="1" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="4" unit="character"/><gap reason="lost" quantity="2" unit="character"/> <expan><unclear>μη</unclear><ex>τρὸς</ex></expan> <supplied reason="lost">Ἰσχ</supplied>υριαίνη<supplied reason="lost">ς</supplied> <expan>ὁ<ex>μοίως</ex></expan> <expan><ex>δραχμῶν</ex></expan> <num value="800">ω</num> 

    <lb n="26"/><supplied reason="lost">Ἰ</supplied>σχυρ<unclear>ίων</unclear> Α<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="3" unit="character"/><gap reason="illegible" quantity="1" unit="character"/>ς τοῦ <gap reason="lost" quantity="2" unit="character"/><unclear>υ</unclear><gap reason="lost" quantity="2" unit="character"/> <expan><supplied reason="lost">ὁ</supplied><ex>μοίως</ex></expan> <expan><ex>δραχμῶν</ex></expan> <num value="700">ψ</num> 

    <lb n="27"/>Σύρων κώμη<supplied reason="lost">ς ὁμοίω</supplied><unclear>ς</unclear> 

    <lb n="28"/><supplied reason="lost">Σε</supplied>ξστίλλις <supplied reason="lost">Μιύσεως ὁ</supplied>μ<supplied reason="lost">οί</supplied><unclear>ως</unclear> <expan><ex>δραχμῶν</ex></expan> <num value="700">ψ</num> 

