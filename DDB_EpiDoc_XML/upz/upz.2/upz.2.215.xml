         <head n="3617" xml:lang="en">
            <date>130BC</date>
            <placeName>Thebes</placeName>
            <ref type="reprint-from">Actenstuecke8</ref>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><ab>
    <lb n="1"/>Προῖτος <gap reason="illegible" quantity="3" unit="character"/><unclear>α</unclear>ι<gap reason="lost" quantity="1" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="3" unit="character"/><supplied reason="lost"> τοῖ</supplied>ς 

    <lb n="2"/>ταγματικοῖς ὑπ<supplied reason="lost">η</supplied>ρ<supplied reason="lost">έται</supplied>ς χαίρειν. 

    <lb n="3"/>ἐπεὶ προσπέπτωκ<supplied reason="lost">εν</supplied> ἡμῖν 

    <lb n="4"/>Κόνωνα πρόσλη<supplied reason="lost">ψίν</supplied> τινων 

    <lb n="5"/>ποιεῖσθαι εἰ<supplied reason="lost">ς τὸ ταγμ</supplied>ατικὸν 

    <lb n="6"/>ἄνευ τῆς ἡ<supplied reason="lost">μετέρας</supplied> γνώμης 

    <lb n="7"/>παρὰ τὰς γε<supplied reason="lost">νομένα</supplied>ς ὑπὸ 

    <lb n="8"/>Παῶι τοῦ στ<supplied reason="lost">ρατηγοῦ</supplied> διαστολὰς 

    <lb n="9"/>περὶ τοῦ σὺν <supplied reason="lost">Ἑρμίαι τ</supplied>ῶι ὑπο

    <lb n="10" type="inWord"/>στρατήγωι γε<supplied reason="lost">νόμενον</supplied> τὸ προκείμενον 

    <lb n="11"/><subst><add place="inline">ἐπιτελεῖν</add><del rend="corrected">επιστελειν</del></subst> π<supplied reason="lost">ρᾶγμα, ὃ κ</supplied>αὶ τούτωι 

    <lb n="12"/>δι<supplied reason="lost">ασ</supplied>εσαφήκαμ<supplied reason="lost">εν διὰ γραμμά</supplied>των, 

    <lb n="13"/>καλῶς <choice><corr>ποιήσετε</corr><sic>ποησετε</sic></choice> κατὰ <supplied reason="lost">τ</supplied>ὸ παρὸν 

    <lb n="14"/>ἐπισχόντες μέχρι τοῦ τὰ παρὰ τοῦ 

    <lb n="15"/>Ἑρμίου πρὸς ταῦτα ἀντιφωνηθῆναι, 

    <lb n="16"/>ὅπως μετὰ τῆς πάσ<supplied reason="lost">η</supplied>ς προσοχῆς 

    <lb n="17"/>οἰκονομηθ<supplied reason="lost">ῆι</supplied> ἕκαστα, <supplied reason="lost">ἀ</supplied>κολούθως 

    <lb n="18"/>ἧι <choice><corr>ποιεῖται</corr><sic>ποειται</sic></choice> ὁ στρατηγὸς σπουδῆι. 
<milestone rend="paragraphos" unit="undefined"/>
