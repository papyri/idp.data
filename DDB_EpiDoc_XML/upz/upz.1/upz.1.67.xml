         <head n="3458" xml:lang="en">
            <date>153/152 BC</date>
            <placeName>Memphis</placeName>
            <ref n="p.paris;;58r" type="reprint-from">P.Paris 58</ref>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r" type="textpart"><ab>

    <lb n="1"/>Πτολεμαῖος 

    <lb n="2"/>Ἱππάλωι <choice><reg>χαίρειν</reg><orig>χαίριν</orig></choice>. 

    <lb n="3"/><choice><reg>τετίμηκα<supplied reason="omitted">ς</supplied></reg><orig>τετιμηκα</orig></choice> 

    <lb n="4"/>τὴν βοῦν ταλάν

    <lb n="5" break="no"/>των τρία <choice><reg>ἥμισυ</reg><orig>ἥμυσυ</orig></choice> <num value="3"/><num value="1/2"/>. 

    <lb n="6"/>ἀπέχει <expan><ex>δραχμὰς</ex></expan> <num value="1000">Α</num>. 

    <lb n="7"/>ἀπόδος αὐτῇ 

    <lb n="8"/>τάλαντα 

    <lb n="9"/><choice><reg>τρία</reg><orig>τρῖς</orig></choice> <choice><reg>ἥμισυ</reg><orig>ἥμυσυ</orig></choice> <num value="3"/><num value="1/2"/>. 

    <lb n="10"/><choice><reg>ἐάν</reg><orig>ἠάν</orig></choice> μὴ θέλῃ 

    <lb n="11"/><choice><reg>τοσαύτη<supplied reason="omitted">ς</supplied></reg><orig>δοσαυτη</orig></choice>, 

    <lb n="12"/><choice><reg>ἀποδῷ</reg><orig>ἀποδο</orig></choice> <unclear>τὸ</unclear>ν 

    <lb n="13"/>ἀρραβῶνα, καὶ 

    <lb n="14"/>παραγίνου μοι 

    <lb n="15"/>εἰς τὸ <choice><reg>Σαραπιεῖον</reg><orig>Σαραπιην</orig></choice> 

    <lb n="16"/><gap reason="lost" extent="unknown" unit="line"/> 
</ab></div><div n="v,ms" type="textpart"><ab>

    <lb n="1"/>Ἱππά-
</ab></div><div n="v,md" type="textpart"><ab>

    <lb n="1"/>λωι </ab></div></div>
