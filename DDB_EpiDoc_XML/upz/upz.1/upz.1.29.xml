         <head n="3420a 3420b" xml:lang="en">
            <date>162 BC</date>
            <placeName>Memphis</placeName>
            <ref n="p.lond;1;31,p15" type="reprint-from">P.Lond. 1 31 p15; P.Forshall 8</ref>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r" type="textpart"><ab>

    <lb n="1"/><expan><ex>ἔτους</ex></expan> <num value="20">κ</num> Ἁθὺρ <num value="25">κε</num> ὁμολογεῖ Δημήτριος 

    <lb n="2"/>Σώσου Κρὴς μεμετρῆσθαι παρὰ Διονυσίου 

    <lb n="3"/>καὶ Ἀσκληπιάδου τῶν πρὸς τῆι ἐλαικῆι ὑπὲρ 

    <lb n="4"/>τῶν ἐν τῶι μεγάλωι <choice><reg>Σα<supplied reason="omitted">ρα</supplied>πιείωι</reg><orig>Σαπιείωι</orig></choice> διδυμῶν ἐλαίου 

    <lb n="5"/>σησαμίνου μετρητὰς δύο γίνονται <choice><reg>δύο</reg><orig>δύου</orig></choice> <num value="2"/>. 

    <lb n="6"/>ἔτους <num value="20">κ</num> <choice><reg>Ἁθὺρ</reg><orig>Ἁθρὺρ</orig></choice> <num value="25">κε</num> <choice><reg>πέπτωκεν</reg><orig><subst><add place="inline">μετρησον</add><del rend="corrected">μετρητον</del></subst></orig></choice> <choice><reg>Κρατέρωι</reg><orig>Κρατερῶν</orig></choice> 

    <lb n="7"/>χειριστῇ τῷ παρὰ Δωρίωνος τοῦ 
<choice><reg>τρα<lb n="8" break="no"/>πεζίτου</reg><orig>τρα<lb n="8" break="no"/>πεζείτου</orig></choice> <choice><reg>συνπαρόντων</reg><orig>συνπαροντες</orig></choice> Χρυσίππου 

    <lb n="9"/>καὶ Ἀρήου <add place="above">παρὰ</add> Ταυῆτος καὶ Ταῦτος 

    <lb n="10"/>ἐλαικῆς ἱερῶν σησαμίνου διδυμῶν 

    <lb n="11"/>Σαραπιείου <del rend="erasure"><num value="19">ιθ</num> <expan><ex>ἔτους</ex></expan></del> ἔτους <num value="19">ιθ</num>  <num value="21">κα</num> <num value="2"><expan><ex>διώβολον</ex></expan></num> καὶ <num value="18">ιη</num> <expan><ex>ἔτους</ex></expan> 

    <lb n="12"/><num value="21">κα</num> <num value="2"><expan><ex>διώβολον</ex></expan></num> <del rend="erasure"> <expan><ex>γίνονται</ex></expan> <num value="42">μβ</num> <num value="4"><expan><ex>τετρώβολον</ex></expan></num> </del> <expan><ex>γίνονται</ex></expan> <num value="43">μγ</num> <num value="4"><expan><ex>τετρώβολον</ex></expan></num>. 
</ab></div><div n="v" type="textpart"><ab>

    <lb n="1"/><handShift new="m2"/> <app type="editorial"><lem resp="BL 2.2.172"><gap reason="illegible" quantity="1" unit="character"/><unclear>μυ</unclear>κω</lem><rdg><gap reason="illegible" quantity="2" unit="character"/><unclear>υ</unclear>κω</rdg></app> 

    <lb n="2"/><handShift new="m1"/> συμβουλε<unclear>υ</unclear><gap reason="lost" quantity="5" unit="character"/>ην ὑπογραφὴν </ab></div></div>
