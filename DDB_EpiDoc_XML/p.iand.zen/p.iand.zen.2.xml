    <lb n="1"/><gap reason="lost" extent="unknown" unit="character"/> <expan><ex>ἔτους</ex></expan> <num value="31">λα</num> ἱερέως Γλαύκωνος τοῦ Ἐτεο<supplied reason="lost">κλέους</supplied> 

