    <lb n="1a"/><num value="6"><unclear>ϛ</unclear></num> 

    <lb n="1"/>ἔτους ἑκκαιδεκάτου Αὐτοκράτορος Καίσαρος <expan><unclear>Τ</unclear>ρ<supplied reason="lost">α</supplied>ια<unclear>ν</unclear><ex>οῦ</ex></expan> 

