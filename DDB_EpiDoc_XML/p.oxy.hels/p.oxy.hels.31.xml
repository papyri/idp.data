         <head n="15813" xml:lang="en">
            <date>AD 86</date>
            <placeName>Oxyrhynchus</placeName>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r" type="textpart"><ab>

    <lb n="1"/><expan>ἀντίγρ<ex>αφον</ex></expan>. ἔτους πέμπτου <supplied reason="lost">Αὐτοκράτορος Καίσαρος Δομιτιανοῦ</supplied> 

    <lb n="2"/>Σεβαστ<supplied reason="lost">ο</supplied><unclear>ῦ</unclear> Γερμ<unclear>α</unclear>ν<supplied reason="lost">ι</supplied><unclear>κ</unclear>οῦ Φα<unclear>ρ</unclear>μ<supplied reason="lost">οῦθι </supplied><gap reason="lost" quantity="2" unit="character"/><supplied reason="lost"> ἐν Ὀξυρύγχων πόλει τῆς Θηβαίδος.</supplied> 

    <lb n="3"/>ἐδάνεισεν Πετοσῖρις ὁ καὶ Πλ <gap reason="lost" quantity="20" unit="character"/><supplied reason="lost"> μητρὸς  </supplied><gap reason="lost" quantity="10" unit="character"/> 

    <lb n="4"/>τ<unclear>ῆ</unclear>ς Λοβάιτος Ταῶτι Ὥρου <supplied reason="lost">μητρὸς </supplied><gap reason="lost" quantity="6" unit="character"/><supplied reason="lost"> τῆς </supplied><gap reason="lost" quantity="6" unit="character"/><supplied reason="lost"> Πέρσῃ τῆς ἐπιγονῆς με</supplied>

    <lb n="5" break="no"/>τὰ κυρίου τοῦ ἑαυτῆς υἱοῦ <unclear>Ὀ</unclear><gap reason="lost" quantity="34" unit="character"/><supplied reason="lost"> παν</supplied>

    <lb n="6" break="no"/>τες τῶν ἀπʼ Ὀξυρύγχ<unclear>ω</unclear>ν πόλ<supplied reason="lost">εως ἐν ἀγυιᾷ ἀργυρίου Σεβαστοῦ καὶ Πτολεμαικοῦ</supplied> 

    <lb n="7"/><unclear>νο</unclear>μίσματος δραχμὰς π<supplied reason="lost">εντακοσίας <expan><ex>δραχμὰς</ex></expan> <num value="500">φ</num> κεφαλαίου αἷς οὐδὲν τῷ καθόλου</supplied> 

    <lb n="8"/>προσῆ<unclear>κ</unclear>ται, ἃ<unclear>ς</unclear> κ<unclear>αὶ</unclear> ἀ<unclear>πο</unclear>δότω <unclear>ἡ</unclear> <supplied reason="lost">δεδανεισμένη τῷ δεδανεικότι τριακάδι μηνὸς</supplied> 

    <lb n="9"/>Φαμενὼθ τοῦ ἑβδόμου ἔτου<supplied reason="lost">ς Αὐτοκράτορος Καίσαρος Δομιτιανοῦ Σεβαστοῦ</supplied> 

    <lb n="10"/>Γερμανι<unclear>κ</unclear>οῦ χωρὶς πάσης ὑπερ<supplied reason="lost">θέσεως. ἐὰν δὲ μὴ ἀποδῷ καθὰ γέγραπται, συγχωρεῖ</supplied> 

    <lb n="11"/>ἡ δεδα<unclear>ν</unclear>εισμένη μένειν πε<supplied reason="lost">ρὶ τὸν δεδανεικότα καὶ τοὺς παρʼ αὐτοῦ μεταληψο</supplied>

    <lb n="12" break="no"/>μένους ἀντὶ τοῦ κεφαλαίου <unclear>ἀ</unclear><supplied reason="lost">πὸ τοῦ τῆς ἀποδόσεως χρόνου τὴν κράτησιν καὶ </supplied><choice><reg><supplied reason="lost">κυ</supplied>

    <lb n="13" break="no"/>ρίαν</reg><orig><supplied reason="lost">κυ</supplied><lb n="13" break="no"/>ρείαν</orig></choice> ε<unclear>ἰς</unclear> <unclear>τ</unclear>ὸν ἀεὶ <unclear>χ</unclear>ρόνον τοῦ <unclear>ὑ</unclear><supplied reason="lost">πάρχοντος αὐτῇ μέρους οἰκίας ἐν τῇ αὐτῇ μη</supplied>

    <lb n="14" break="no"/>τροπό<unclear>λ</unclear>ει τοῦ <choice><reg>Ὀξυρυγχίτου</reg><orig>Ὀξυρυγχείτου</orig></choice> κα<supplied reason="lost">ὶ χρηστηρίων καὶ εἰσόδων καὶ ἐξόδων πασῶν καὶ</supplied> 

    <lb n="15"/>ἐξουσίαν ἔχειν <unclear>τ</unclear>ὸν δεδανει<supplied reason="lost">κότα τάξασθαι τὰ τέλη καὶ δεσπόζειν αὐτοῦ καὶ τὰ</supplied> 

    <lb n="16"/>περιε<unclear>σ</unclear>όμενα ἀπ<supplied reason="lost">ο</supplied>φέρεσθ<unclear>α</unclear>ι <unclear>π</unclear><supplied reason="lost">άντα καὶ ἑτέροις πωλεῖν καὶ χρᾶσθαι καὶ διοικεῖν</supplied> 

    <lb n="17"/>περὶ αὐτοῦ ὡς ἐὰν αἱρῆ<unclear>ται</unclear> μ<unclear>η</unclear><supplied reason="lost">δεμιᾶς τῇ δεδανεισμένῃ ἢ τοῖς παρʼ αὐτῆς καταλειπο</supplied>

    <lb n="18" break="no"/>μένης ἐφόδου κατὰ μ<unclear>ηδ</unclear>ένα <supplied reason="lost">τρόπον, ἀλλὰ ἐπάναγκον αὐτὴν παρέξεσθαι τῷ δεδα</supplied>

    <lb n="19" break="no"/>νεικότι καὶ τοῖς <unclear>π</unclear>αρʼ αὐτοῦ τὸ <unclear>μ</unclear><supplied reason="lost">έρος τῆς οἰκίας διὰ παντὸς βέβαιον</supplied>

    <lb n="20"/><supplied reason="lost">ἀπὸ πάντων</supplied> 

    <lb n="21"/>πάσῃ βεβαιώσει. ἀπὸ δὲ τοῦ <unclear>νῦ</unclear><supplied reason="lost">ν ἕως ἂν ἀποδώσει ἡ δεδανεισμένη τῷ δεδανεικότι</supplied> 

    <lb n="22"/>τὰς τοῦ <expan>ἀργ<ex>υρίου</ex></expan> <expan><ex>δραχμὰς</ex></expan> <num value="500"><unclear>φ</unclear></num> <supplied reason="lost">μὴ ἐξ</supplied><unclear>εῖ</unclear>ναι αὐ<supplied reason="lost">τῇ τῇ δεδανεισμένῃ τὸ μέρος τῆς οἰκίας πωλεῖν</supplied> 

    <lb n="23"/>μηδʼ ὑποτίθ<unclear>ε</unclear><supplied reason="lost">σθαι μηδʼ</supplied> <unclear>ἄ</unclear>λ<supplied reason="lost">λ</supplied><unclear>ω</unclear>ς κ<supplied reason="lost">αταχρηματίζειν μηδʼ ἀπογράφεσθαί τινα ἐπʼ αὐτοῦ,</supplied> 

    <lb n="24"/><choice><reg>ἐκλογῆς</reg><orig>ἐγλο<unclear>γῆς</unclear></orig></choice> <supplied reason="lost">ο</supplied>ὔσης <supplied reason="lost">τῷ δεδ</supplied><unclear>α</unclear>νει<unclear>κ</unclear><supplied reason="lost">ό</supplied>τ<supplied reason="lost">ι διὰ ἑαυτοῦ μετὰ τὸν χρόνον μὴ ἀπολαβόντι ἐὰν</supplied> 

    <lb n="25"/><unclear>β</unclear>ούληται κυ<unclear>ρ</unclear><supplied reason="lost">ιεύειν το</supplied><unclear>ῦ</unclear> μ<unclear>έ</unclear>ρου<supplied reason="lost">ς ἀντὶ τοῦ κεφαλαίου ἢ τὴν πρᾶξιν ποιεῖσθαι ἐκ τῆς δεδα</supplied>

    <lb n="26" break="no"/><unclear>ν</unclear>ει<supplied reason="lost">σμέ</supplied>νη<supplied reason="lost">ς καὶ τοῦ μέρους τῆς οἰκίας καὶ τῶν ἄλλων ὑπαρχόντων αὐτῇ πάντων κα</supplied>

    <lb n="27" break="no"/><unclear>θ</unclear><supplied reason="lost">άπερ ἐ</supplied><unclear>κ</unclear> <unclear>δ</unclear><supplied reason="lost">ίκης  </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="28"/><gap reason="lost" extent="unknown" unit="line"/> 
</ab></div><div n="v" type="textpart"><ab>

