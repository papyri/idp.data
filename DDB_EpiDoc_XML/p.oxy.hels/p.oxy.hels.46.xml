         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r" type="textpart"><ab>

    <lb n="1"/><gap reason="lost" quantity="20" unit="character"/> τῶι ἀδελφῶι <space extent="unknown" unit="character"/> 

    <lb n="2"/><gap reason="lost" quantity="20" unit="character"/> <space extent="unknown" unit="character"/>   χαίρειν <space extent="unknown" unit="character"/> 

