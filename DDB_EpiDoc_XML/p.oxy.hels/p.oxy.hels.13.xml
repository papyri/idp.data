         <head n="24970" xml:lang="en">
            <date>I spc?</date>
            <placeName>Oxyrhynchus</placeName>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><ab>
    <lb n="1"/>Ἀπελλᾶτι βασιλικῶι γραμματ<unclear>εῖ</unclear> <supplied reason="lost"><expan>Ἀπολλωνοπ<ex>ολίτου</ex></expan></supplied> 

    <lb n="2"/>καὶ <choice><reg><expan>Εἰλειθυιοπ<ex>ολίτου</ex></expan></reg><orig><expan>Ἰλιθυιοπ<ex>ολίτου</ex></expan></orig></choice> παρὰ Ψενεχόις τ<unclear>οῦ</unclear> Θε<gap reason="lost" quantity="13" unit="character"/> 

    <lb n="3"/>ἀρχιγ<supplied reason="lost">ε</supplied><unclear>ω</unclear>ργὸς Τεβῆ<unclear>ι</unclear> ἀπὸ βορρᾶ πέ<unclear>ρ</unclear><supplied reason="lost">υσι κατασταθεὶς</supplied> 

    <lb n="4"/>νῦν ἐπαναφέρω σοι περὶ τῆς αὐτ<supplied reason="lost">ῆς γεωργίας ἐν</supplied> 

    <lb n="5"/>τῶι καιρῶι τῆς κατασπορᾶς, προσδ<supplied reason="lost">εόμενος λαβεῖν</supplied> 

    <lb n="6"/>μετόχους γεωργοὺς εἰς τὴν <unclear>α</unclear><supplied reason="lost">ὐτὴν γεωργίαν</supplied> 

    <lb n="7"/><supplied reason="lost">ἐπεὶ</supplied> ἀνεχώρησαν οἱ <choice><reg>περυσινοὶ</reg><orig>περουσινοὶ</orig></choice> <unclear>γ</unclear><supplied reason="lost">εωργοὶ</supplied> 

    <lb n="8"/><supplied reason="lost">ὥστε μόνο</supplied>ς εἰμι, ἵνʼ ἰσχύσω τὰ δημ<supplied reason="lost">όσια τελεῖν</supplied> 

    <lb n="9"/><supplied reason="lost">τῶι ταμεί</supplied>ωι πρὸς τὸ μηδὲν ἔλασσον το<unclear>ῖς</unclear> <supplied reason="lost">κυρια</supplied>

    <lb n="10" break="no"/><supplied reason="lost">κοῖς λόγ</supplied>οις <choice><reg>ἐπακολουθῆσαι</reg><orig>ἐπακολλουθησι</orig></choice>. ἔτι ἐπαν<unclear>α</unclear><supplied reason="lost">φέρω σοι</supplied> 

    <lb n="11"/><gap reason="lost" quantity="15" unit="character"/><gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="5" unit="character"/>χασε<gap reason="illegible" quantity="4" unit="character"/><gap reason="illegible" quantity="2" unit="character"/>χ<gap reason="lost" quantity="6" unit="character"/> 

