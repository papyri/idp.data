    <lb n="5"/>πρὸς μόνον τ<unclear>ὸ</unclear> <supplied reason="lost">ἐνεσ</supplied><unclear>τ</unclear>ὸς <supplied reason="omitted">ἔτος</supplied> τῆς εὐ<unclear>τ</unclear><supplied reason="lost">υχοῦς </supplied><gap reason="lost" quantity="8" unit="character"/> 

    <lb n="6"/><choice><reg>ἕκτης</reg><orig><unclear>ἕ</unclear>κκτης</orig></choice> <choice><reg>ἰνδικτίωνος</reg><orig>ἰντ<unclear>ικ</unclear><supplied reason="lost">τίω</supplied><unclear>ν</unclear>ος</orig></choice> τὰς ὑπαρχο<unclear>ύ</unclear><supplied reason="lost">σας σοι περὶ </supplied><gap reason="lost" quantity="5" unit="character"/> 

    <lb n="7"/>ἐκ <subst><add place="inline">τοῦ</add><del rend="corrected"><gap reason="illegible" quantity="1" unit="character"/>ου</del></subst> Ἀσκ<unclear>λη</unclear><supplied reason="lost">πιά</supplied><unclear>δ</unclear>ου κλήρου ἀρ<supplied reason="lost">ούρας </supplied><gap reason="lost" quantity="5" unit="character"/><supplied reason="lost"> εἰς</supplied> 

    <lb n="8"/><supplied reason="lost">σ</supplied><unclear>π</unclear>ορὰν πυροῦ καὶ κατάθεσιν <choice><reg>χορτασ<supplied reason="lost">μάτων</supplied></reg><orig>χορτοσ<supplied reason="lost">μάτων</supplied></orig></choice> <supplied reason="lost">κατὰ</supplied> 

    <lb n="9"/>τὸ <choice><reg>ἥμισυ</reg><orig>ἥμυσι</orig></choice> <num value="1/2"/>, φόρου τῆς μὲν ἐν πυρῷ <supplied reason="lost">ἡμισείας ἑκάστης</supplied> 

    <lb n="10"/><supplied reason="lost">ἀ</supplied><unclear>ρ</unclear>ούρης ἀνὰ πυροῦ ἀρταβῶν <unclear>τ</unclear><supplied reason="lost">εσσάρων <num value="4"/>, τῆς δὲ ἐν</supplied> 

