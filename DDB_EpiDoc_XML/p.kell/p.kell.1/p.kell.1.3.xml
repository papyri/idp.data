    <lb n="1"/><supplied reason="lost">Αὐρήλιοι </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> καὶ </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ἀπ</supplied>ὸ κώμης Κέλλεως τοῦ Μωθίτου νομοῦ <unclear>Α</unclear><supplied reason="lost">ὐρηλίῳ</supplied> Τι<unclear>β</unclear>ερίῳ <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="2"/><supplied reason="lost">βουλόμενοι ἐπίδοσιν λιβέλλ</supplied>ων ποιήσασθαι παρὰ τῇ ἀνδρείᾳ τοῦ κυρ<supplied reason="lost">ίου διαση</supplied>μ<supplied reason="lost">ο</supplied><unclear>τά</unclear>τ<supplied reason="lost">ου </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="3"/><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ὅτι μηδὲν ἀδι</supplied>κοῦμεν μετριώτατοι ὄντες καὶ μηδὲ<unclear>ν</unclear> <gap reason="lost" quantity="5" unit="character"/><supplied reason="lost"> με</supplied><unclear>ν</unclear>οι καὶ ὅ<unclear>τ</unclear><supplied reason="lost">ι </supplied><gap reason="lost" extent="unknown" unit="character"/> 

