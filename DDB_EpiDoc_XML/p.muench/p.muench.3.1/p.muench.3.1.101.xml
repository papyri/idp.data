         <head n="12456" xml:lang="en">
            <date>AD 150</date>
            <placeName>Arsinoiton Polis, Krokodilopolis, Ptolemais Euergetis</placeName>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><ab>
    <lb n="1"/>ἔτους τεσσαρεσκαιδεκάτ<supplied reason="lost">ο</supplied><unclear>υ</unclear> Αὐτοκράτορ<supplied reason="lost">ος Καίσαρος Τίτου Αἰλίου Ἁδριανοῦ</supplied> 

    <lb n="2"/>Ἀντωνίνου Σεβαστοῦ Εὐσεβοῦς Θὼ<supplied reason="lost">θ </supplied><gap reason="lost" quantity="1" unit="character"/><supplied reason="lost"> διὰ τῆς Διδύμου κεκοσ</supplied>

    <lb n="3" break="no"/>μητευκότος τραπέζης Φρεμεί. <supplied reason="lost">Στοτοήτιος </supplied><gap reason="lost" quantity="6" unit="character"/><supplied reason="lost"> τοῦ Στο</supplied>

    <lb n="4" break="no"/>τοήτιος ὡς <expan><ex>ἐτῶν</ex></expan> <num value="28">κη</num> <expan>οὐλ<ex>ὴ</ex></expan> δακτύλῳ <num value="3">γ</num> χ<supplied reason="lost">ειρὸς δεξιᾶς, ἐπίτροπος </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="5"/>ἀφήλικος Ὥρου το<unclear>ῦ</unclear> <supplied reason="lost">Ὀ</supplied>ννώφρεω<supplied reason="lost">ς ἐπακολουθούσης τῆς τοῦ</supplied> 

    <lb n="6"/>ἀφήλικος μητρὸς <supplied reason="lost">Σ</supplied>αγάθιος Α<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" quantity="13" unit="character"/><supplied reason="lost"> τοῦ Ὀν</supplied>

    <lb n="7" break="no"/>νώφρεως ὡς <expan><ex>ἐτῶν</ex></expan> <num value="58">νη</num> <expan>οὐλ<ex>ὴ</ex></expan> <choice><reg>ὀφρύι</reg><orig>ὀφρύει</orig></choice> ἀρισ<supplied reason="lost">τερᾷ. ἔχειν αὐτὸν παρὰ</supplied> 

    <lb n="8"/>τοῦ Στοτοήτιος <choice><reg>τροφεῖα</reg><orig>τροφῖα</orig></choice> τοῦ προκ<supplied reason="lost">ειμένου ἀφήλικος μηνῶν</supplied> 

    <lb n="9"/>ὀκτὼ <num value="8"/> τῶν ἀπὸ Μεχεὶρ μηνὸς το<supplied reason="lost">ῦ ἑνδεκάτου ἔτους μέχρι</supplied> 

    <lb n="10"/>Θὼθ <num value="30">λ</num> τοῦ δωδεκάτου ἔτους <unclear>ὡ</unclear><supplied reason="lost">ς ἑκάστου μηνὸς δραχμὰς</supplied> 

    <lb n="11"/>ἓξ <num value="6"/> τὰς ἐπὶ τ<supplied reason="lost">ὸ</supplied> <choice><reg>αὐτὸ</reg><orig>αὐτω</orig></choice> δραχμὰς τ<supplied reason="lost">εσσεράκοντα ὀκτώ</supplied> <num value="48"/>, 

    <lb n="12"/>τ<unclear>ῶ</unclear>ν δ<supplied reason="lost">ὲ</supplied> ἀπὸ <supplied reason="lost">τ</supplied>οῦ Φαῶφι τοῦ αὐτο<supplied reason="lost">ῦ δωδεκάτου ἔτους</supplied> 

    <lb n="13"/>μέχρι ἐπαγομένων <choice><reg>πέμπτης</reg><orig>πέπτης</orig></choice> <unclear>τ</unclear><supplied reason="lost">οῦ τρεισκαιδεκάτου</supplied> 

    <lb n="14"/>ἔτους μηνῶν <supplied reason="lost">εἴ</supplied>κοσι τριῶν <num value="23"/> τὰς σ<supplied reason="lost">υναγομένας ἐπὶ τὸ αὐτὸ</supplied> 

    <lb n="15"/>ἀργυρίου δραχμὰς ἑκατὸν τριάκ<supplied reason="lost">οντα ὀκτὼ</supplied> <num value="138"/> <gap reason="lost" quantity="8" unit="character"/><supplied reason="lost">, ἃς</supplied> 

    <lb n="16"/>ἀποδώσει ἕως τῆς δευτέρας <supplied reason="lost">καὶ εἰκάδος τοῦ ὄντος</supplied> 

    <lb n="17"/>μην<unclear>ὸ</unclear>ς Θώθ, μένοντος τοῦ λ<supplied reason="lost">όγου τῷ Α</supplied><gap reason="lost" quantity="7" unit="character"/><supplied reason="lost"> περὶ</supplied>

    <lb n="18"/>ὧν ὀ<supplied reason="lost">φ</supplied>είλει αὐτῷ ὁ Στοτοήτιος <supplied reason="lost">ἄλλων δραχμῶν  </supplied><gap reason="lost" quantity="6" unit="character"/> 

    <lb n="19"/>καθʼ ἃς ἔχει ἐνγράπτους ἀσφ<supplied reason="lost">αλείας  </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="20"/>ναι τοὺς προκειμένους τόκους <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="21"/><expan>προκείμεν<ex>α</ex></expan> τρ<supplied reason="lost">ο</supplied>φεῖα. </ab></div>
