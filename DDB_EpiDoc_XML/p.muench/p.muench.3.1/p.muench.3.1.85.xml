    <lb n="2"/><gap reason="lost" extent="unknown" unit="character"/>νο<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/>τρ<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/><gap reason="illegible" quantity="1" unit="character"/>ολοι<gap reason="illegible" quantity="2" unit="character"/><gap reason="lost" quantity="1" unit="character"/><gap reason="illegible" quantity="4" unit="character"/>ει ἐπ<unclear>ι</unclear><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> ἐπὶ τοῖς</supplied> 

    <lb n="3"/>οὖσι αὐτ<unclear>ῶ</unclear><supplied reason="lost">ν ὁρ</supplied><unclear>ί</unclear>οις, ε<supplied reason="lost">ἰσό</supplied><unclear>δ</unclear>ο<unclear>ι</unclear>ς καὶ ἐξόδοις καὶ ποτιστρέαις <unclear>κ</unclear><supplied reason="lost">αὶ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="4"/>δων ὁμοί<unclear>ω</unclear><supplied reason="lost">ς ε</supplied>ἰσόδοι<supplied reason="lost">ς κ</supplied><unclear>αὶ</unclear> ἐξόδοις καὶ <choice><reg>φωσφορίαις</reg><orig>φοσφορίαις</orig></choice> καὶ <unclear>θ</unclear><supplied reason="lost">εμελίοις </supplied><gap reason="lost" extent="unknown" unit="character"/><supplied reason="lost"> γείτονες, καθὼς ἐκ συμφώνου ὑπη</supplied>

