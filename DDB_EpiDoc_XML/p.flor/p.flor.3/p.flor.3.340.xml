         <head n="8864" xml:lang="en">
            <date>III spc</date>
            <placeName>Arsinoite</placeName>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r" type="textpart"><ab>

    <lb n="1"/>καὶ ἐν ἄλλῳ <expan>τόπ<ex>ῳ</ex></expan> <choice><reg>ψιλὸς</reg><orig>ψειλὸς</orig></choice> τόπος πρὸς τῷ βαλ<supplied reason="lost">ανείῳ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="2"/>καὶ ἐν ἄλλῳ τόπῳ οἰκία δίστεγος <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="3"/>καὶ περὶ <expan>κώ<ex>μην</ex></expan> Ἐλευσῖνα <expan>σι<ex>τικαὶ</ex></expan> <expan><ex>ἄρουραι</ex></expan> <num value="24">κδ</num> διὰ μισθω<supplied reason="lost">τῶν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="4"/>Εὐπόρου <expan>κ<ex>αὶ</ex></expan> Ἱρακλείδου μισθωτῶν <g type="unintelligible"/> <expan>φό<ex>ρου</ex></expan> <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="5"/>ἄλλης <expan><ex>ἀρούρης</ex></expan> <num value="1">α</num> <num value="1/2">𐅵</num> <abbr>τόπ</abbr> Ναρκίσσων ἐκ <expan>φό<ex>ρου</ex></expan> <num value="1">α</num> <expan><ex>ἔτους</ex></expan> <expan><ex>πυροῦ ἀρτάβαι</ex></expan> <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="6"/>ἄλλων <expan><ex>ἀρουρῶν</ex></expan> <num value="10">ι</num> διὰ Νείλου ἐκ <expan>φό<ex>ρου</ex></expan> <num value="1">α</num> <expan><ex>ἔτους</ex></expan> <expan><ex>πυροῦ ἀρτάβαι</ex></expan> <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="7"/>ἄλλων <expan><ex>ἀρουρῶν</ex></expan> <num value="20">κ</num> <num value="1/2">𐅵</num> ἀσπόρου 

    <lb n="8"/>κήπου ἐν τόπῳ Τέστω <expan>λεγομ<ex>ένῳ</ex></expan> <expan><ex>τέταρτον</ex></expan> 

    <lb n="9"/>μέρος <expan><ex>ἄρουραι</ex></expan> <num value="20">κ</num> <num value="1/2">𐅵</num> <gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="10"/>διὰ μισθωτοῦ Σουχάμμωνος ἀπὸ τῆς <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="11"/>φόρου <num value="1">α</num> <expan><ex>ἔτους</ex></expan> <expan><ex>δραχμαὶ</ex></expan> <num value="40">μ</num> καὶ οἰκίαν ἐν τῇ κώ<supplied reason="lost">μῃ </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="12"/>διὰ τοῦ αὐτοῦ <num value="1">α</num> <expan><ex>ἔτους</ex></expan> <expan><ex>δραχμαὶ</ex></expan> <num value="48">μη</num> <expan><ex>γίνονται</ex></expan> <expan><ex>δραχμαὶ</ex></expan> <num value="88">πη</num> 

    <lb n="13"/>καὶ <choice><reg>ἐκ</reg><orig>ἐγ<g type="apostrophe"/></orig></choice> νότου τῆς προκειμένης οἰκίας <gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="14"/>καὶ ἐκ τοῦ πρὸς βορρᾶ μέρους τοῦ προκειμέν<supplied reason="lost">ου </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="15"/>τόπου μύλαιον <choice><reg>ἐκ</reg><orig>ἐγ<g type="apostrophe"/></orig></choice> μέρους κατὰ πεντ<add place="above"><unclear>α</unclear></add><supplied reason="lost" cert="low">μοιρίαν </supplied><gap reason="lost" extent="unknown" unit="character"/> 

    <lb n="16"/><unclear>κα</unclear><supplied reason="lost">ὶ </supplied><gap reason="lost" extent="unknown" unit="character"/> 
</ab></div><div n="v" type="textpart"><ab>

