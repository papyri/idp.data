    <lb n="24"/>αἱ <expan>π<ex>ροκείμεναι</ex></expan> <expan><ex>ἄρουραι</ex></expan> <num value="668">χξη</num> <num value="1/2">𐅵</num> <num value="1/16" rend="tick">ιϛ</num> <num value="1/32" rend="tick">λβ</num> <num value="1/64" rend="tick">ξδ</num>, 

    <lb n="25"/>ὧν <expan>βεβρεγμέ<ex>ναι</ex></expan> <expan><ex>ἄρουραι</ex></expan> <num value="402">υβ</num> <num value="1/4" rend="tick">δ</num> <num value="1/32" rend="tick">λβ</num>, 

    <lb n="26"/><expan>δυνάμε<ex>ναι</ex></expan> <expan>αὐλακισθ<ex>ῆναι</ex></expan> <expan><ex>ἄρουραι</ex></expan> <num value="260">σξ</num> <num value="1/4" rend="tick">δ</num> <num value="1/64" rend="tick">ξδ</num>, 

    <lb n="27"/><expan>ἄβροχ<ex>οι</ex></expan> <expan><ex>ἄρουραι</ex></expan> <num value="6">ϛ</num> <num value="1/16" rend="tick">ιϛ</num> <expan><ex>γίνονται</ex></expan> αἱ <expan><ex>προκείμεναι</ex></expan>. 

    <lb n="28"/>ὧν ἐστιν τὸ κατʼ ἄνδρα· 

    <lb n="29"/><expan>ἀρχό<ex>μενον</ex></expan> ἀπὸ <expan>δυσ<ex>μ  </ex></expan> τῆς κώμης 

    <lb n="30"/>Θατρῆς <expan>Ὀννώφριο<ex>ς</ex></expan> <abbr>πολ</abbr> <num value="3">γ</num> <num value="1/2">𐅵</num>, <expan>ἀν<ex>ὰ</ex></expan> <num value="2">β</num> <num value="1/12" rend="tick">ιβ</num>  <num value="1">α</num> <num value="1/2">𐅵</num>, 

