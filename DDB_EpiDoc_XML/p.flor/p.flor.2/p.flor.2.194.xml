    <lb n="18"/><supplied reason="lost">ἀ</supplied>λλὰ καὶ <app type="editorial"><lem resp="BL 1.152"><choice><reg>μάθε</reg><orig>μά<unclear>θ</unclear>αι</orig></choice>
