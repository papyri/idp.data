    <lb n="5" break="no"/>του <supplied reason="lost">κτ</supplied>ηνῶν τεσσά

    <lb n="6" break="no"/>ρω<supplied reason="lost">ν</supplied> <num value="4"/> <unclear>κ</unclear>αὶ Ἀ<hi rend="diaeresis">ι</hi>ώνιος 

