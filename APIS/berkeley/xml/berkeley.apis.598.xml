         <titleStmt>
            <title>Field-by-field register of land at Kerkeosiris</title>
         </titleStmt>
         <publicationStmt>
            <authority>APIS</authority>
            <idno type="apisid">berkeley.apis.598</idno>
            <idno type="controlno">(CU)598</idno>
