    <lb n="3"/><supplied cert="high" reason="lost">παρὰ</supplied> Ὀννώφ<supplied cert="high" reason="lost">ρ</supplied>εω<supplied cert="high" reason="lost">ς </supplied><gap extent="4" reason="lost" unit="character"/> 

    <lb n="4"/><gap extent="3" reason="lost" unit="character"/>τος τῶν ἀπ᾽ Εὐημ<supplied cert="high" reason="lost">ερίας</supplied> 

    <lb n="5"/>τῆς Θεμίστου μερίδος <supplied cert="high" reason="lost">γεωρ</supplied>-


    <lb n="6"/>γοῦ τῆς Ἰουλίας Σεβ<supplied cert="high" reason="lost">αστῆς</supplied> 

    <lb n="7"/>οὐσίας τῶν πρότερον <supplied cert="high" reason="lost">Γαίου</supplied> 

    <lb n="8"/>Ἰουλ<supplied cert="high" reason="lost">ί</supplied>ου Ἀλεξάνδρου <supplied cert="high" reason="lost"><expan>ἐδαφ<ex cert="high">ῶν</ex></expan>.</supplied> 

    <lb n="9"/>τῶι <supplied cert="high" reason="lost">ἐ</supplied>νεστῶτι μηνὶ <gap extent="4" reason="lost" unit="character"/> 

    <lb n="10"/>τοῦ <num value="15">ιε</num> <expan><ex cert="high">ἔτους</ex></expan> Τιβερίου Καίσα<supplied cert="high" reason="lost">ρος</supplied> 

    <lb n="11"/>Σεβαστοῦ Δημᾶς <unclear cert="high" reason="undefined">Ψ</unclear>αήσι<supplied cert="high" reason="lost">ος</supplied> 

    <lb n="12"/>καταγεινόμενος ἐν τῶι <supplied cert="high" reason="lost">περὶ</supplied> 

    <lb n="13"/>τὴν κώμην ἐποικίωι <expan>λε<unclear cert="high" reason="undefined">γ</unclear><supplied cert="high" reason="lost">ο<ex cert="high">μένῳ</ex></supplied></expan> 

    <lb n="14"/>Δρομέως ἐπαφεὶς τὰ <choice><corr>ἑαυτοῦ</corr><sic>εατου</sic></choice> 

    <lb n="15"/>πρόβατα καὶ βοϊκὰ κτήνη 

    <lb n="16"/>εἰς ἃ γεωργῶ τῆς <expan><unclear cert="high" reason="undefined">μ</unclear>η<ex cert="high">τρὸς</ex></expan> μ<supplied cert="high" reason="lost">ο</supplied>υ <expan>ἐδάφ<ex cert="high">η</ex></expan> 

    <lb n="17"/>κατενέμησάν μου πυροῦ 

    <lb n="18"/><expan>σπόρο<ex cert="high">υ</ex></expan> <expan>ἀρο<ex cert="high">ύρας</ex></expan> <num value="2">β</num> καὶ <expan>κριθ<ex cert="high">ῆς</ex></expan> <expan>ἀρο<ex cert="high">ύρης</ex></expan> <num value="1/2">𐅵</num> 

    <lb n="19"/>ἐξ οὗ βλάβος μοι <choice><corr><expan>ἐπηκ<supplied cert="high" reason="omitted">ο</supplied>λούθ<ex cert="high">ησεν</ex></expan></corr><sic>επηκλουθ</sic></choice> 

    <lb n="20"/>οὐκ ὀλίγον. ὁ δὲ <expan>ἐνκαλούμ<ex cert="high">ενός</ex></expan> 

    <lb n="21"/>ἐστιν μετὰ <expan>Ἁρπαήσιο<ex cert="high">ς</ex></expan> τοῦ 

    <lb n="22"/>Ἡρᾶτος χλωροφαγῶν. 

    <lb n="23"/>διὸ ἀξιῶ ἀχθῆναι τὸν 

    <lb n="24"/><expan>ἐνκαλούμεν<supplied cert="high" reason="lost">ο<ex cert="high">ν</ex></supplied></expan> ἐπὶ σὲ πρ<supplied cert="high" reason="lost">ὸς</supplied> 

    <lb n="25"/>τὴν ἐσομένην <expan>ἐπέξοδ<ex cert="high">ον</ex></expan>. 

    <lb n="26"/><expan>εὐτύχ<ex cert="high">ει</ex></expan>. 

