    <lb n="1"/>βασιλεὺς Πτολεμ<unclear reason="undefined" cert="high">α</unclear>ῖ<unclear reason="undefined" cert="high">ος</unclear> <supplied reason="omitted" cert="high">καὶ βασίλισσα Κλεοπάτρα ἡ ἀδελφὴ</supplied> <unclear reason="undefined" cert="high">κ</unclear><supplied reason="lost" cert="high">αὶ</supplied> <unclear reason="undefined" cert="high">βα</unclear>σίλ<unclear reason="undefined" cert="high">ισσ</unclear>α Κλεοπάτρα ἡ <choice><corr>γυνὴ</corr><sic>γυνηι</sic></choice> 

    <lb n="2"/>ἀφιᾶσ<unclear reason="undefined" cert="high">ι</unclear> <sic>δὲ</sic> τοὺς ὑπὸ <unclear reason="undefined" cert="high">τ</unclear>ὴν <choice><corr>βασιλείαν</corr><sic>βασιληαν</sic></choice> πάντας ἀγνοημάτων 

    <lb n="3"/>ἁμα<unclear reason="undefined" cert="high">ρτ</unclear>ημάτων <sic>ἀγνοημάτων</sic> ἐγκλημάτων καταγνωσ-

    <lb n="4"/><unclear reason="undefined" cert="high">μ</unclear>άτω<unclear reason="undefined" cert="high">ν</unclear> αἰτιῶν πασῶν τῶν ἕως <num value="9">θ</num> τοῦ Φαρμοῦθι τοῦ <num value="52">νβ</num> <expan><ex cert="high">ἔτους</ex></expan> 

    <lb n="5"/><unclear reason="undefined" cert="high">π</unclear>λὴν τω῀ν <choice><corr>φόνοις</corr><sic>φονεις</sic></choice> ἑκουσίοις καὶ ἱερο<unclear reason="undefined" cert="high">σ</unclear>υλίαις συνεχομένων. 

    <lb n="6"/><supplied reason="lost" cert="high">προ</supplied><unclear reason="undefined" cert="high">σ</unclear>τετάχασι δὲ καὶ τοὺς ἀνακεχωρ<unclear reason="undefined" cert="high">η</unclear>κότας διὰ τὸ ἐνέχεσθαι 

    <lb n="7"/><choice><corr>λείαις</corr><sic>ληαις</sic></choice> καὶ ἑτέραις αἰτίαις καταπορευομ<supplied reason="lost" cert="high">έ</supplied>νους εἰς τὰς ἰδίας 

    <lb n="8"/><unclear reason="undefined" cert="high">γίνε</unclear>σθαι πρὸς αἷς καὶ πρότερον ἦσα<unclear reason="undefined" cert="high">ν</unclear> ἐργασίαις καὶ κομίζεσθαι 

    <lb n="9"/>τὰ ἔτι ὑπάρχοντα ἄπρατα ἀπὸ τῶν δι<unclear reason="undefined" cert="high">ὰ</unclear> ταῦ<unclear reason="undefined" cert="high">τα</unclear> ἠνεχυρασμένων. 

    <lb n="10"/>ἀπολύουσι δὲ πάντας τῶν <choice><corr>ἐνοφ<supplied reason="omitted" cert="high">ε</supplied>ιλομένων</corr><sic>ενοφιλομενων</sic></choice> πρὸς τοὺς ἕως 

    <lb n="11"/>τοῦ <num value="50">ν</num> <expan><ex cert="high">ἔτους</ex></expan> χρόνους πρό<unclear reason="undefined" cert="high">ς</unclear> τε τὴν σιτικὴν <expan>φο<ex cert="high">ρολογίαν</ex></expan> καὶ τὰς <expan>ἀργυ<ex cert="high">ρικὰς</ex></expan> <expan>πρ<ex cert="high">οσόδους</ex></expan> πλὴν 

    <lb n="12"/>τῶν μεμισθωμ<unclear reason="undefined" cert="high">έν</unclear><supplied reason="lost" cert="high">ω</supplied>ν εἰς τ<unclear reason="undefined" cert="high">ὸ</unclear> ρικὸν καὶ ὧν διεγγρ<unclear reason="undefined" cert="high">αφ</unclear><gap reason="lost" extent="3" unit="character"/>. 

    <lb n="13"/><supplied reason="lost" cert="high">ὁ</supplied><unclear reason="undefined" cert="high">μ</unclear>οίως δὲ καὶ τοὺς ὀφ<supplied reason="lost" cert="high">είλοντας</supplied> <unclear reason="undefined" cert="high">πρός</unclear>  τὸ <expan><ex cert="high">ἡμιαρτάβιον</ex></expan> κα<unclear reason="undefined" cert="high">ὶ</unclear> <gap reason="illegible" extent="3" unit="character"/> τὴ<supplied reason="lost" cert="high">ν</supplied> <expan><ex cert="high">διαρταβίαν</ex></expan> 

    <lb n="14"/><unclear reason="undefined" cert="high">κ</unclear>α<unclear reason="undefined" cert="high">ὶ</unclear> τὸ φυλακιτικὸν καὶ τὸ <expan>ναύ<ex cert="high">βιον</ex></expan> καὶ τὰ <choice><corr>παραπλήσια</corr><sic>παραταπλησια</sic></choice> καὶ τὸ <expan>χω<ex cert="high">ματικὸν</ex></expan> 

    <lb n="15"/>μέχρι τῶν αὐτῶν χρόνων. ἀπολύουσι δὲ καὶ τῶν παρεχομένων 

    <lb n="16"/>πρὸς τὴν ἀπόμοιραν καὶ τὸ ἐπαρούριον καὶ τοὺς φόρους <unclear reason="undefined" cert="high">καὶ</unclear> <choice><corr>τὰ ἄλλα</corr><sic>ταλλα</sic></choice> 

    <lb n="17"/><gap reason="lost" extent="13" unit="character"/> ἕως τοῦ αὐτοῦ χ<supplied reason="lost" cert="high">ρόνο</supplied><unclear reason="undefined" cert="high">υ</unclear>. 

