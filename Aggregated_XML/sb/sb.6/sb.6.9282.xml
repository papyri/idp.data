    <lb n="4"/>γενημάτων ἐπάναγκες ἀπ<supplied cert="high" reason="lost">οδώσω</supplied> 

    <lb n="5"/>ἐφ᾽ ἅλω τῆς εὐτυχοῦς δωδε<supplied cert="high" reason="lost">κάτης <expan>ἰνδ<ex cert="high">ικτίονος</ex></expan></supplied> 

