    <lb n="1"/><expan>σιτολ<ex cert="high">όγοις</ex></expan> <expan>Κ<ex cert="high">α</ex>ρανίδ<ex cert="high">ος</ex></expan>. <expan>ἔσχ<ex cert="high">ον</ex></expan> <app type="BL"><lem resp="1.45"><expan>προ<ex cert="high">σφωνήσας</ex></expan> <expan>χειρογρ<ex cert="high">α</ex>φ<ex cert="high">ίᾳ</ex></expan></lem><rdg><expan>προ<ex cert="high">χρείαν</ex></expan> <expan>χειρόγρ<ex cert="high">α</ex>φ<ex cert="high">ον</ex></expan></rdg></app> 

    <lb n="2"/><expan>σπερμάτ<ex cert="high">ων</ex></expan> <num value="22">κβ</num> <expan><ex cert="high">ἔτους</ex></expan> Ἀντωνείνου Καίσαρος 

