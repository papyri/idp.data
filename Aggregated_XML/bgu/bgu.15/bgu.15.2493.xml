    <lb n="1"/>α<gap extent="1" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="2"/>ατων <unclear cert="high" reason="undefined">ε</unclear><gap extent="1" reason="illegible" unit="character"/><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="3"/>του διὰ τὴν <gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="4"/>χην τῆς κώμη<supplied cert="high" reason="lost">ς   </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="5"/>καὶ <choice><corr>γινώσκειν</corr><sic>γεινωσκε<unclear cert="high" reason="undefined">ν</unclear></sic></choice> <supplied cert="low" reason="lost">σὲ</supplied> 

    <lb n="6"/>ὅτι ἐμέ<unclear cert="high" reason="undefined">τ</unclear>ρησαν <supplied cert="high" reason="lost">τὸ</supplied>-

    <lb n="7"/>ν σῖτον τ<unclear cert="high" reason="undefined">οῖ</unclear><supplied cert="high" reason="lost">ς</supplied> σιτο-

    <lb n="8"/><choice><corr>λόγοις</corr><sic><unclear cert="high" reason="undefined">σιτο</unclear>λογων</sic></choice> ἐ<unclear cert="high" reason="undefined">π</unclear>᾽ <supplied cert="high" reason="lost">ὀνόμα</supplied>-

    <lb n="9"/>τος σ<unclear cert="high" reason="undefined">ε</unclear><gap extent="3" reason="illegible" unit="character"/><gap extent="1" reason="lost" unit="character"/><gap extent="1" reason="illegible" unit="character"/><unclear cert="high" reason="undefined">κ</unclear><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="10"/>οὔπω μετα<supplied cert="high" reason="lost">λα</supplied>βέσ-

    <lb n="11"/><choice><corr>θαι</corr><sic>μετ<unclear cert="high" reason="undefined">α</unclear><supplied cert="high" reason="lost">λα</supplied>βε<unclear cert="high" reason="undefined">στ</unclear>αι</sic></choice> ἐπ᾽ ὀνόματος 

    <lb n="12"/>σοῦ, ἔχων αὐτῶ-

    <lb n="13"/>ν τὴν διαστολὴν 

    <lb n="14"/>τῶν ὀνομάτων 

    <lb n="15"/>σοῦ. ἐὰν <supplied cert="high" reason="lost">ἔλ</supplied>θῃ ὑπ<unclear cert="high" reason="undefined">ὲρ</unclear> 

    <lb n="16"/>ὧν <supplied cert="high" reason="omitted">ἔγραψα</supplied> πέμψον αὐτὸν 

    <lb n="17"/>ἐν <choice><corr>τάχει</corr><sic>ταχι</sic></choice> <choice><corr>ἵνα</corr><sic>εινα</sic></choice> αὐτ<unclear cert="high" reason="undefined">οῖ</unclear><supplied cert="high" reason="lost">ς</supplied> 

    <lb n="18"/><choice><corr>διαστείλῃ</corr><sic>διαστελι</sic></choice>. 

    <lb n="19"/>ἀσπάζεται <choice><corr cert="low">ὑμᾶς</corr><sic>ημας</sic></choice> 

    <lb n="20"/>Σωκράτης. 

    <lb n="21"/><handShift new="m2"/> γινώσκειν σὲ θέλ-

    <lb n="22"/>ω ὅτι ταῦτα τ<supplied cert="high" reason="lost">ὰ γ</supplied>ρ<unclear cert="high" reason="undefined">ά</unclear><supplied cert="high" reason="lost">μμ</supplied>-

    <lb n="23"/>ατα ταρότατα ἔπ-

    <lb n="24"/>εμψα <choice><corr cert="low">ὑμῖν</corr><sic>ημιν</sic></choice> ἕ<unclear cert="high" reason="undefined">νεκ</unclear><supplied cert="low" reason="lost">α </supplied><gap extent="unknown" reason="lost" unit="character"/> 

    <lb n="25"/>ὧν <choice><corr>ἔδωκάς</corr><sic>εδωκες</sic></choice> μοι. 
