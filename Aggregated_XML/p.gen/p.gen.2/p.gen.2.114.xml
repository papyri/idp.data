    <lb n="12"/>τοῦ <expan>α<ex cert="high">ὐτοῦ</ex></expan> <expan>μη<ex cert="high">νὸς</ex></expan> <expan>ἄλ<ex cert="high">λας</ex></expan> πυρ<unclear cert="high" reason="undefined">οῦ</unclear> ἀρτάβας εἴκοσι

