    <lb n="14"/><expan>ἄλ<ex cert="high">λας</ex></expan> π<unclear cert="high" reason="undefined">υ</unclear>ροῦ ἀρτάβας εἴκοσι <unclear cert="high" reason="undefined">δ</unclear>ύο <choice><corr>ἥμισυ</corr><sic>ημισου</sic></choice>

