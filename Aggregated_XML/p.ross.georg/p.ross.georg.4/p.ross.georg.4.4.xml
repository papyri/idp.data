    <lb n="1"/><gap extent="unknown" reason="lost" unit="line"/> 

    <lb n="1"/>πάραυτα οὖν τοῦ δέξασθαί σε τὰ παρόντα γ<unclear cert="high" reason="undefined">ρ</unclear><supplied cert="high" reason="lost">άμμα</supplied>τ<supplied cert="high" reason="lost">α πρὸς</supplied> τ<supplied cert="high" reason="lost">ὴν</supplied> 

    <lb n="2"/>δύναμιν τῶν ἐνταγίων τοὺς τοι<unclear cert="high" reason="undefined">ο</unclear><supplied cert="high" reason="lost">ύ</supplied>τους ἄνδρας 

    <lb n="3"/>νεωτέρους <choice><corr>δυναμένους</corr><sic>δυνομενους</sic></choice> <choice><corr>ἀνθέξεσθαι</corr><sic>ανθεξασθαι</sic></choice> τῷ ἔργῳ 

    <lb n="4"/>ἀπόστειλον δι᾽ ἀποκρισιαρίου σου ὅπως ἐξέλθωσιν 

    <lb n="5"/>διὰ στράτας εἰς <choice><corr>αὐτὰ</corr><sic>αυτην</sic></choice> Ἱεροσόλυμα παρασκευάζων<g type="apostrophe"/> 

    <lb n="6"/>πληρωθῆναι αὐτοὺς τοὺς μισθοὺς καὶ τὴν ἀποτροφὴν αὐτῶν 

    <lb n="7"/>ἀποστέλλων τὸ χρυσίον τῆς δαπάνης τῶν αὐτῶ<unclear cert="high" reason="undefined">ν</unclear> μη<unclear cert="high" reason="undefined">νῶν</unclear> 

    <lb n="8"/>δώδεκα <num value="12"/> δι᾽ αὐτοῦ τοῦ ἀποκρισιαρί<unclear cert="high" reason="undefined">ο</unclear>υ ὅπως χορηγήσῃ α<unclear cert="high" reason="undefined">ὐ</unclear>τὸ 

    <lb n="9"/>εἰς τὸ κάτεργον. ἀλλὰ μὴ ἀμελήσῃς τ<unclear cert="high" reason="undefined">ο</unclear>ῦ τού<unclear cert="high" reason="undefined">τ</unclear>ους ἐκπέμ<g type="apostrophe"/>ψαι 

    <lb n="10"/>καὶ <unclear cert="high" reason="undefined">κιν</unclear>δ<unclear cert="high" reason="undefined">υ</unclear>νεύσῃς. <expan>ἐγρ<ex cert="high">άφη</ex></expan> <expan>μ<unclear cert="high" reason="undefined">η</unclear><ex cert="high">νὶ</ex></expan> <expan>Μ<ex cert="high">ε</ex><unclear cert="high" reason="undefined">χ</unclear><ex cert="high">εὶρ</ex></expan> <num value="20"><unclear cert="high" reason="undefined">κ</unclear></num> <expan>ἰ<ex cert="high">ν</ex>δ<ex cert="high">ικτίονος</ex></expan> <num value="8">η</num>. 

